Dialogue: 0,0:00:36.41,0:00:37.48,Default,,0,0,0,,ĐÊM HALLOWEEN\N2023
Dialogue: 0,0:00:37.67,0:00:39.29,Default,,0,0,0,,{\i1}Bạn muốn nghe chuyện không?{\i0}
Dialogue: 0,0:00:39.91,0:00:42.33,Default,,0,0,0,,{\i1}Tôi phải báo trước, nó hơi nhiễu loạn.{\i0}
Dialogue: 0,0:00:42.62,0:00:46.65,Default,,0,0,0,,{\i1}Và nó xảy ra ở ngay đây,{\i0}\N{\i1}trong thị trấn Vernon yên bình, thơ mộng.{\i0}
Dialogue: 0,0:00:46.85,0:00:48.15,Default,,0,0,0,,BẮC VERNON
Dialogue: 0,0:00:48.35,0:00:52.55,Default,,0,0,0,,{\i1}Ba mươi lăm năm trước,{\i0}\N{\i1}ba cô gái đã bị sát hại một cách tàn nhẫn.{\i0}
Dialogue: 0,0:00:54.26,0:00:56.84,Default,,0,0,0,,{\an8}{\i1}Vào ngày 27 tháng 10 năm 1987,{\i0}
Dialogue: 0,0:00:57.97,0:01:01.46,Default,,0,0,0,,{\i1}Tiffany Clark đã bị thảm sát{\i0}\N{\i1}trong nhà xe của gia đình,{\i0}
Dialogue: 0,0:01:01.66,0:01:05.94,Default,,0,0,0,,{\i1}bị đâm 16 nhát vào đêm{\i0}\N{\i1}sinh nhật tuổi 16 ngọt ngào của mình.{\i0}
Dialogue: 0,0:01:07.02,0:01:10.52,Default,,0,0,0,,{\i1}Tại một nhà gỗ trong rừng,{\i0}\N{\i1}Marisa Song 16 tuổi{\i0}
Dialogue: 0,0:01:11.23,0:01:15.53,Default,,0,0,0,,{\i1}bị giết sau đó hai ngày,{\i0}\N{\i1}cũng bị đâm 16 nhát.{\i0}
Dialogue: 0,0:01:16.49,0:01:18.02,Default,,0,0,0,,{\i1}Và hai ngày sau đó,{\i0}
Dialogue: 0,0:01:18.22,0:01:21.77,Default,,0,0,0,,{\i1}vào đêm Halloween,{\i0}\N{\i1}Heather Hernandez 16 tuổi{\i0}
Dialogue: 0,0:01:21.97,0:01:26.08,Default,,0,0,0,,{\i1}bị đâm 16 nhát ở bãi đỗ xe{\i0}\N{\i1}của công viên giải trí Billy's Boardwalk.{\i0}
Dialogue: 0,0:01:27.12,0:01:29.40,Default,,0,0,0,,{\an8}{\i1}Hung thủ biến mất. Theo các nhân chứng…{\i0}
Dialogue: 0,0:01:29.60,0:01:31.65,Default,,0,0,0,,{\an8}TIẾP TỤC TRUY LÙNG SÁT NHÂN TUỔI 16
Dialogue: 0,0:01:31.85,0:01:35.45,Default,,0,0,0,,{\i1}…Sát Nhân Tuổi 16{\i0}\N{\i1}mặc toàn đồ đen và đeo mặt nạ,{\i0}
Dialogue: 0,0:01:35.65,0:01:37.74,Default,,0,0,0,,{\i1}điều đã trở nên nổi tiếng ở Vernon,{\i0}
Dialogue: 0,0:01:37.94,0:01:42.81,Default,,0,0,0,,{\i1}nơi mà vào mỗi dịp Halloween,{\i0}\N{\i1}người ta vẫn hóa trang thành kẻ sát nhân.{\i0}
Dialogue: 0,0:01:45.18,0:01:49.02,Default,,0,0,0,,Chào mừng đến chuyến du lịch\N{\i1}Podcast Sát Nhân Tuổi 16,{\i0}
Dialogue: 0,0:01:49.36,0:01:52.51,Default,,0,0,0,,được mang đến\Nbởi nhà báo tội phạm Chris Dubasage.
Dialogue: 0,0:01:52.71,0:01:55.53,Default,,0,0,0,,Tôi là người dẫn chương trình,\NChris Dubasage.
Dialogue: 0,0:01:56.78,0:02:02.58,Default,,0,0,0,,Bên trái tôi, nhà của Tiffany Clark,\Nnạn nhân đầu tiên của Sát Nhân Tuổi 16.
Dialogue: 0,0:02:03.29,0:02:06.27,Default,,0,0,0,,Nó đã biến thành quán Zatta Burger.
Dialogue: 0,0:02:06.47,0:02:07.32,Default,,0,0,0,,Chào đoàn du lịch.
Dialogue: 0,0:02:07.51,0:02:10.28,Default,,0,0,0,,Có ai muốn thử\NLỗ Chiên Zatta của bọn tôi không?
Dialogue: 0,0:02:10.48,0:02:12.61,Default,,0,0,0,,- Cảm ơn.\N- Phải, ở ngay đây
Dialogue: 0,0:02:12.81,0:02:15.95,Default,,0,0,0,,chính là nơi Sát Nhân Tuổi 16\Nbắt đầu chuỗi giết chóc
Dialogue: 0,0:02:16.15,0:02:18.45,Default,,0,0,0,,- khắp thị trấn Vernon.\N- Giết ba người.
Dialogue: 0,0:02:18.65,0:02:20.50,Default,,0,0,0,,Mỗi khi nghĩ đến sát nhân hàng loạt,
Dialogue: 0,0:02:20.69,0:02:21.95,Default,,0,0,0,,tôi nghĩ ít nhất là sáu.
Dialogue: 0,0:02:22.15,0:02:24.37,Default,,0,0,0,,Cảm ơn, Angie. Bọn tôi rất cảm kích.
Dialogue: 0,0:02:24.57,0:02:27.59,Default,,0,0,0,,Hãy vỗ tay cho Angie,\Nngười ước có thêm người bị giết.
Dialogue: 0,0:02:27.78,0:02:28.67,Default,,0,0,0,,Tôi…
Dialogue: 0,0:02:28.87,0:02:31.52,Default,,0,0,0,,Được rồi. Hãy sang\Nhiện trường án mạng kế tiếp.
Dialogue: 0,0:02:39.57,0:02:44.20,Default,,0,0,0,,BẢN NĂNG SÁT THỦ
Dialogue: 0,0:02:59.76,0:03:02.51,Default,,0,0,0,,Jamie. Ồn ào quá.
Dialogue: 0,0:03:02.80,0:03:04.89,Default,,0,0,0,,Mẹ chỉ muốn cứu màng nhĩ của con.
Dialogue: 0,0:03:05.60,0:03:09.25,Default,,0,0,0,,Chúa ơi, giá như con chọn ban nhạc\Nmà mẹ không quen ca sĩ chính.
Dialogue: 0,0:03:09.45,0:03:13.34,Default,,0,0,0,,Ý mẹ là, Eddie Royal\Nlà một thằng khốn hồi trung học.
Dialogue: 0,0:03:13.54,0:03:17.22,Default,,0,0,0,,Giờ chú ấy rất tuyệt và sở hữu\Nhội từ thiện làm sạch nước, nên…
Dialogue: 0,0:03:17.42,0:03:19.55,Default,,0,0,0,,Con à, hắn đang giơ ngón giữa kìa.
Dialogue: 0,0:03:19.75,0:03:21.45,Default,,0,0,0,,Vâng, cho nhựa dùng một lần.
Dialogue: 0,0:03:23.41,0:03:27.52,Default,,0,0,0,,Jamie, nếu mượn đồ của mẹ\Nthì hãy để ý đến chúng. Đồ cổ điển đấy.
Dialogue: 0,0:03:27.72,0:03:30.58,Default,,0,0,0,,Con có để ở đó đâu.\NCon đang định quàng lên mà.
Dialogue: 0,0:03:31.50,0:03:34.00,Default,,0,0,0,,- Con sẽ đi gặp Amelia.\N- Chờ đã.
Dialogue: 0,0:03:34.59,0:03:37.74,Default,,0,0,0,,Jamie, con đi gặp Amelia để làm gì?
Dialogue: 0,0:03:37.94,0:03:40.20,Default,,0,0,0,,Cậu ấy có\Nhai vé hòa nhạc Bản Năng Sát Thủ.
Dialogue: 0,0:03:40.40,0:03:41.66,Default,,0,0,0,,Em yêu, xem này.
Dialogue: 0,0:03:41.86,0:03:44.83,Default,,0,0,0,,Norm Dubasage\Nđang đưa tin về cơn bão ở Florida.
Dialogue: 0,0:03:45.03,0:03:46.46,Default,,0,0,0,,{\an8}{\i1}Tom, tối nay Chúa giận rồi.{\i0}
Dialogue: 0,0:03:46.66,0:03:47.54,Default,,0,0,0,,{\an8}Em à, tuyệt thật.
Dialogue: 0,0:03:47.74,0:03:50.17,Default,,0,0,0,,{\an8}- Ông ấy còn chả đội mũ.\N{\i1}- Chúa trong Cựu Ước.{\i0}
Dialogue: 0,0:03:50.37,0:03:52.71,Default,,0,0,0,,Có khi ông ấy được giải Pulitzer thứ hai.
Dialogue: 0,0:03:52.91,0:03:56.63,Default,,0,0,0,,Con yêu, sao con không ở nhà\Nvà cùng bố mẹ phát kẹo Halloween?
Dialogue: 0,0:03:56.83,0:04:00.24,Default,,0,0,0,,- Con thích thế mà.\N- Không, mẹ ơi. Mẹ thích thôi.
Dialogue: 0,0:04:01.32,0:04:03.99,Default,,0,0,0,,Ôi, Chúa ơi, bố! Bố mặc gì thế?
Dialogue: 0,0:04:05.49,0:04:06.87,Default,,0,0,0,,Bố là Zac Efron.
Dialogue: 0,0:04:07.49,0:04:10.90,Default,,0,0,0,,Nhưng, con biết đấy,\Nhồi trước bố từng đô con như cậu ấy.
Dialogue: 0,0:04:11.10,0:04:13.40,Default,,0,0,0,,Được rồi. Còn mẹ, sao mẹ không hóa trang?
Dialogue: 0,0:04:13.60,0:04:16.84,Default,,0,0,0,,Có mà. Mẹ là Molly Ringwald\Ntrong {\i1}The Breakfast Club.{\i0}
Dialogue: 0,0:04:18.88,0:04:22.03,Default,,0,0,0,,- Được rồi.\N- Con yêu, còn ai đi hòa nhạc nữa?
Dialogue: 0,0:04:22.23,0:04:25.08,Default,,0,0,0,,Có gì nghiêm trọng thế?\NCon sẽ về trước giờ giới nghiêm.
Dialogue: 0,0:04:25.28,0:04:28.16,Default,,0,0,0,,Con biết thời điểm này trong năm\Nkhó khăn thế nào mà.
Dialogue: 0,0:04:28.36,0:04:32.67,Default,,0,0,0,,Nhất là giờ đây\Ncon đã bằng tuổi của bọn mẹ hồi đó.
Dialogue: 0,0:04:32.87,0:04:34.30,Default,,0,0,0,,Vậy con không được đi hòa nhạc
Dialogue: 0,0:04:34.49,0:04:37.17,Default,,0,0,0,,vì bạn bè bố mẹ bị giết cách đây 35 năm.
Dialogue: 0,0:04:37.37,0:04:38.26,Default,,0,0,0,,Được rồi.
Dialogue: 0,0:04:38.46,0:04:40.80,Default,,0,0,0,,Mẹ chả bao giờ nói với bà ngoại kiểu đó.
Dialogue: 0,0:04:41.00,0:04:42.89,Default,,0,0,0,,Mẹ có nói gì với bà đâu. Vậy nên…
Dialogue: 0,0:04:43.09,0:04:46.03,Default,,0,0,0,,Con muốn nói là giờ đâu phải năm 1987 nữa.
Dialogue: 0,0:04:46.45,0:04:47.98,Default,,0,0,0,,Bố mẹ có thể dò vị trí con.
Dialogue: 0,0:04:48.18,0:04:52.31,Default,,0,0,0,,Con có xịt hơi cay\Ngắn móc khóa, báo động hiếp dâm.
Dialogue: 0,0:04:52.51,0:04:55.40,Default,,0,0,0,,Bố bắt con học lớp tự vệ từ hồi bảy tuổi.
Dialogue: 0,0:04:55.60,0:04:59.24,Default,,0,0,0,,Mẹ đưa con đá bảo vệ\Nlấy từ một bà thầy bói.
Dialogue: 0,0:04:59.44,0:05:04.16,Default,,0,0,0,,- Bố mẹ chỉ muốn giữ an toàn cho con.\N- Chúa ơi. Mẹ thôi đi, được chứ?
Dialogue: 0,0:05:04.36,0:05:08.85,Default,,0,0,0,,Việc này không liên quan gì đến con.\NƯớc gì bố mẹ quên nó đi.
Dialogue: 0,0:05:12.31,0:05:15.81,Default,,0,0,0,,Tốt thôi, cứ đi đi,\Nnhưng bố con sẽ lái xe chở đi.
Dialogue: 0,0:05:16.15,0:05:19.02,Default,,0,0,0,,Bố sẽ đợi đến khi kết thúc\Nvà lái xe đưa con về.
Dialogue: 0,0:05:20.98,0:05:23.11,Default,,0,0,0,,CHRIS DUBASAGE\NTIN NHẮN
Dialogue: 0,0:05:23.82,0:05:25.28,Default,,0,0,0,,- Sao thế?\N- Không có gì.
Dialogue: 0,0:05:26.03,0:05:28.35,Default,,0,0,0,,Tin nhắn nhóm các bà mẹ ấy mà.
Dialogue: 0,0:05:28.55,0:05:31.45,Default,,0,0,0,,Được rồi. Bố, đi thôi.\NCon không muốn lỡ phần mở màn.
Dialogue: 0,0:05:32.54,0:05:36.00,Default,,0,0,0,,Con yêu. Bảo trọng nhé. Mẹ yêu con!
Dialogue: 0,0:05:44.80,0:05:47.79,Default,,0,0,0,,Khi con ngồi ghế sau\Nnhư thể bố là tài xế Uber,
Dialogue: 0,0:05:47.98,0:05:52.56,Default,,0,0,0,,- việc đó khiến bố hơi tổn thương.\N- Bố, làm ơn đi. Một sao.
Dialogue: 0,0:06:02.15,0:06:04.64,Default,,0,0,0,,Này, bố tưởng con định đi đón Amelia.
Dialogue: 0,0:06:04.84,0:06:06.65,Default,,0,0,0,,Vâng, con nhắn báo ta đến rồi.
Dialogue: 0,0:06:07.61,0:06:12.58,Default,,0,0,0,,- Sao con không gõ cửa?\N- Không, bố à, vậy thì bất lịch sự quá.
Dialogue: 0,0:06:16.91,0:06:17.90,Default,,0,0,0,,- Chào.\N- Chào.
Dialogue: 0,0:06:18.10,0:06:19.03,Default,,0,0,0,,Chào cô Creston.
Dialogue: 0,0:06:19.22,0:06:21.29,Default,,0,0,0,,Chào Jamie. Xem hòa nhạc vui nhé.
Dialogue: 0,0:06:22.92,0:06:24.24,Default,,0,0,0,,Tớ có mang kẹo cần đấy.
Dialogue: 0,0:06:24.44,0:06:25.49,Default,,0,0,0,,- Ơn Chúa.\N- Sativa.
Dialogue: 0,0:06:25.69,0:06:26.87,Default,,0,0,0,,- Ơn Chúa.\N- Có tớ lo.
Dialogue: 0,0:06:27.07,0:06:31.72,Default,,0,0,0,,- Chào Lauren. Tối nay có gì vui không?\N- Tôi đi làm thôi.
Dialogue: 0,0:06:32.26,0:06:35.64,Default,,0,0,0,,Lạ thật, ngành sản xuất\Nkhông ăn mừng Halloween.
Dialogue: 0,0:06:36.23,0:06:38.50,Default,,0,0,0,,Được rồi. Rất vui được gặp cô.
Dialogue: 0,0:06:38.70,0:06:41.42,Default,,0,0,0,,- Đi thôi!\N- Ta cần đến đó như hôm qua.
Dialogue: 0,0:06:41.62,0:06:43.88,Default,,0,0,0,,- Thôi đi nhé?\N- Thôi á? Đi được không?
Dialogue: 0,0:06:44.08,0:06:45.07,Default,,0,0,0,,Rồi. Thì thôi.
Dialogue: 0,0:06:45.61,0:06:48.51,Default,,0,0,0,,- Cho kẹo hay bị ghẹo!\N- Ta có gì ở đây nào?
Dialogue: 0,0:06:48.71,0:06:49.89,Default,,0,0,0,,Halloween vui vẻ.
Dialogue: 0,0:06:50.09,0:06:53.81,Default,,0,0,0,,Cô thích mấy bộ này lắm.\NPhù thủy kiểu truyền thống.
Dialogue: 0,0:06:54.01,0:06:56.73,Default,,0,0,0,,Mấy đứa ơi,\Nđến giờ về nhà rồi. Halloween vui vẻ!
Dialogue: 0,0:06:56.93,0:06:58.91,Default,,0,0,0,,Ừ, Halloween vui vẻ.
Dialogue: 0,0:07:04.67,0:07:07.46,Default,,0,0,0,,Sao họ còn cho mấy thứ này\Nvào gói trộn lẫn nhỉ?
Dialogue: 0,0:07:18.48,0:07:19.48,Default,,0,0,0,,Ôi, Chúa ơi!
Dialogue: 0,0:07:21.60,0:07:24.69,Default,,0,0,0,,Được rồi. Tôi ghét mấy bộ trang phục này.
Dialogue: 0,0:07:27.19,0:07:31.03,Default,,0,0,0,,Xin lỗi, tôi hơi căng thẳng.\NĐi xin kẹo một mình à?
Dialogue: 0,0:07:35.58,0:07:37.20,Default,,0,0,0,,Thế này không vui đâu.
Dialogue: 0,0:07:38.66,0:07:40.50,Default,,0,0,0,,Ôi, Chúa ơi. Không.
Dialogue: 0,0:07:47.75,0:07:49.46,Default,,0,0,0,,{\i1}Đang gọi cảnh sát.{\i0}
Dialogue: 0,0:07:56.60,0:07:59.17,Default,,0,0,0,,Tao đã lo sợ\Nviệc bị giết kể từ hồi 16 tuổi.
Dialogue: 0,0:07:59.37,0:08:01.14,Default,,0,0,0,,Tưởng tao không chuẩn bị sao?
Dialogue: 0,0:08:35.34,0:08:36.89,Default,,0,0,0,,Cảnh sát sẽ sớm đến đây.
Dialogue: 0,0:08:50.78,0:08:51.78,Default,,0,0,0,,Sao chứ?
Dialogue: 0,0:09:01.33,0:09:02.62,Default,,0,0,0,,Không.
Dialogue: 0,0:09:04.92,0:09:06.83,Default,,0,0,0,,Cứu! Cứu với!
Dialogue: 0,0:09:07.96,0:09:08.92,Default,,0,0,0,,Cứu!
Dialogue: 0,0:09:12.01,0:09:13.01,Default,,0,0,0,,Không!
Dialogue: 0,0:09:20.64,0:09:22.27,Default,,0,0,0,,Không!
Dialogue: 0,0:09:27.69,0:09:28.81,Default,,0,0,0,,Cho kẹo hay bị ghẹo!
Dialogue: 0,0:09:50.92,0:09:54.63,Default,,0,0,0,,SÁT NHÂN XUYÊN KHÔNG
Dialogue: 0,0:09:55.84,0:09:58.30,Default,,0,0,0,,Tôi đang ở nhà của gia đình Hughes,
Dialogue: 0,0:09:59.80,0:10:02.06,Default,,0,0,0,,nơi thành viên đáng mến của cộng đồng
Dialogue: 0,0:10:03.10,0:10:06.34,Default,,0,0,0,,và cũng là bạn tôi,\NPam Hughes, vừa bị giết.
Dialogue: 0,0:10:06.53,0:10:08.96,Default,,0,0,0,,PODCAST SÁT NHÂN TUỔI 16\NTHÍCH VÀ ĐĂNG KÝ
Dialogue: 0,0:10:09.16,0:10:10.61,Default,,0,0,0,,Bị đâm 16 nhát.
Dialogue: 0,0:10:12.94,0:10:15.90,Default,,0,0,0,,{\i1}Cảnh sát vẫn chưa đưa ra{\i0}\N{\i1}tuyên bố chính thức.{\i0}
Dialogue: 0,0:10:16.70,0:10:22.66,Default,,0,0,0,,{\i1}Nhưng theo lời các nhân chứng,{\i0}\N{\i1}Sát Nhân Tuổi 16 đã trở lại.{\i0}
Dialogue: 0,0:10:25.00,0:10:26.62,Default,,0,0,0,,{\i1}Tức là không ai được an toàn.{\i0}
Dialogue: 0,0:10:27.16,0:10:30.11,Default,,0,0,0,,Trong các thử thách\Ntôi từng đối mặt khi làm hiệu trưởng,
Dialogue: 0,0:10:30.31,0:10:33.59,Default,,0,0,0,,không thử thách nào\Ngiúp tôi chuẩn bị cho chuyện này,
Dialogue: 0,0:10:34.13,0:10:38.72,Default,,0,0,0,,nói tạm biệt người bạn thân yêu của tôi\Nvà giáo viên hướng nghiệp, Pam Hughes.
Dialogue: 0,0:10:40.26,0:10:43.97,Default,,0,0,0,,Jamie, xin gửi lời cầu nguyện\Nđến gia đình em trong thảm kịch này.
Dialogue: 0,0:10:45.14,0:10:47.50,Default,,0,0,0,,Nhưng ta đâu chỉ đối mặt với nỗi buồn?
Dialogue: 0,0:10:47.70,0:10:50.59,Default,,0,0,0,,Ta còn đối mặt\Nvới sự trở lại của Sát Nhân Tuổi 16.
Dialogue: 0,0:10:50.79,0:10:53.05,Default,,0,0,0,,Tôi muốn nhường lời cho HLV Finkle,
Dialogue: 0,0:10:53.25,0:10:56.61,Default,,0,0,0,,người sẽ nói qua vài động tác tự vệ.
Dialogue: 0,0:10:58.07,0:11:03.78,Default,,0,0,0,,Ừ. Vậy, điều đầu tiên các em muốn làm\Nkhi thấy tên sát nhân là bỏ chạy.
Dialogue: 0,0:11:04.49,0:11:10.08,Default,,0,0,0,,Hãy nhớ, tránh dao. Giữ mạng.
Dialogue: 0,0:11:11.12,0:11:12.29,Default,,0,0,0,,Bầy Quỷ cố lên.
Dialogue: 0,0:11:13.79,0:11:15.75,Default,,0,0,0,,BẦY QUỶ VERNON
Dialogue: 0,0:11:17.30,0:11:19.84,Default,,0,0,0,,Jamie, bọn tôi gặp em chút được chứ?
Dialogue: 0,0:11:22.84,0:11:25.97,Default,,0,0,0,,Em không muốn lắm,\Nnhưng hình như đó là câu hỏi tu từ.
Dialogue: 0,0:11:26.22,0:11:27.22,Default,,0,0,0,,Sao thế ạ?
Dialogue: 0,0:11:27.93,0:11:30.63,Default,,0,0,0,,Tôi biết thật khó khăn\Nkhi mất đi người thân.
Dialogue: 0,0:11:30.83,0:11:33.21,Default,,0,0,0,,Mẹ em đã luôn là một quý cô tuyệt vời.
Dialogue: 0,0:11:33.41,0:11:36.73,Default,,0,0,0,,- Cô ấy đã luôn đối tốt với tôi.\N- Thầy đừng nói "đã" nữa.
Dialogue: 0,0:11:37.78,0:11:38.78,Default,,0,0,0,,Tôi xin lỗi.
Dialogue: 0,0:11:39.69,0:11:42.41,Default,,0,0,0,,Nghe này, cảnh sát trưởng\Nmuốn hỏi em vài câu.
Dialogue: 0,0:11:43.57,0:11:45.81,Default,,0,0,0,,Bố cháu ở đâu vào đêm mẹ cháu bị giết?
Dialogue: 0,0:11:46.01,0:11:48.40,Default,,0,0,0,,Chúa ơi, Kara, cô vào thẳng vấn đề luôn à?
Dialogue: 0,0:11:48.60,0:11:49.94,Default,,0,0,0,,Bố cháu không giết mẹ.
Dialogue: 0,0:11:50.14,0:11:53.28,Default,,0,0,0,,Cháu có biết mối quan hệ\Ncủa mẹ cháu với Chris Dubasage?
Dialogue: 0,0:11:53.48,0:11:55.92,Default,,0,0,0,,Cái ông làm\Npodcast tour án mạng? Cháu không…
Dialogue: 0,0:11:57.67,0:11:59.28,Default,,0,0,0,,Họ không có quan hệ gì.
Dialogue: 0,0:11:59.48,0:12:02.97,Default,,0,0,0,,Bọn tôi kiểm tra điện thoại cô ấy,\Ncả hai nhắn tin rất nhiều.
Dialogue: 0,0:12:03.93,0:12:05.64,Default,,0,0,0,,Bố cháu có hay ghen không?
Dialogue: 0,0:12:07.35,0:12:09.92,Default,,0,0,0,,Cô luôn ghét bố cháu,
Dialogue: 0,0:12:10.12,0:12:13.96,Default,,0,0,0,,nhưng có lẽ đã đến lúc buông bỏ\Nmấy chuyện hồi trung học ngày xưa
Dialogue: 0,0:12:14.16,0:12:18.68,Default,,0,0,0,,mà cô cứ bám víu vào và làm những gì\Nmà ông bố bảo vệ nghèo khổ của cô
Dialogue: 0,0:12:18.88,0:12:22.86,Default,,0,0,0,,không làm được cách đây 35 năm\Nvà bắt kẻ sát nhân chết tiệt này đi.
Dialogue: 0,0:12:34.29,0:12:36.79,Default,,0,0,0,,{\i1}Billy… Hải ly… Vui vẻ.{\i0}
Dialogue: 0,0:12:38.59,0:12:40.76,Default,,0,0,0,,{\i1}Billy… Hải ly… Vui vẻ.{\i0}
Dialogue: 0,0:12:42.42,0:12:44.55,Default,,0,0,0,,{\i1}- Billy… Hải ly… Vui vẻ.{\i0}\N- Xin lỗi.
Dialogue: 0,0:12:46.09,0:12:48.60,Default,,0,0,0,,CÚ RƠI LƯỢNG TỬ
Dialogue: 0,0:12:51.64,0:12:57.48,Default,,0,0,0,,HỘI CHỢ KHOA HỌC TRUNG HỌC VERNON
Dialogue: 0,0:13:01.15,0:13:05.32,Default,,0,0,0,,{\i1}…bốn, ba, hai, một.{\i0}
Dialogue: 0,0:13:07.57,0:13:08.58,Default,,0,0,0,,{\i1}Chuỗi thất bại.{\i0}
Dialogue: 0,0:13:09.66,0:13:13.15,Default,,0,0,0,,Sao họ lại tổ chức hội chợ khoa học\Nở nơi bị ám nhất có thể tưởng vậy?
Dialogue: 0,0:13:13.35,0:13:16.82,Default,,0,0,0,,{\an8}Hiệu trưởng Summers thuê miễn phí.\NĐể đưa người ta trở lại.
Dialogue: 0,0:13:17.02,0:13:19.45,Default,,0,0,0,,{\an8}Đây từng là nơi vui chơi ở Vernon,
Dialogue: 0,0:13:19.64,0:13:22.45,Default,,0,0,0,,{\an8}nhưng giờ chỉ là\Nmột điểm dừng trong tour án mạng.
Dialogue: 0,0:13:22.65,0:13:25.58,Default,,0,0,0,,Đây là dự án của hai cậu à?\NBuồng chụp ảnh?
Dialogue: 0,0:13:25.78,0:13:28.79,Default,,0,0,0,,- Nó hơn cả buồng chụp ảnh, Derek.\N- Vậy nó là cái gì?
Dialogue: 0,0:13:28.99,0:13:31.29,Default,,0,0,0,,Một thứ sẽ thay đổi thế giới mà ta biết.
Dialogue: 0,0:13:31.49,0:13:34.94,Default,,0,0,0,,Nhưng chúc may mắn\Nvới phân ủ của cậu. Hộp đựng phân rau củ.
Dialogue: 0,0:13:35.85,0:13:39.26,Default,,0,0,0,,Khi nào cậu mới nói rằng\Ncậu đang chế tạo cỗ máy thời gian?
Dialogue: 0,0:13:39.46,0:13:41.73,Default,,0,0,0,,Khi tớ tạo ra\Nmáy thời gian hoạt động được.
Dialogue: 0,0:13:42.44,0:13:45.60,Default,,0,0,0,,Nếu bảo người ta\Nrằng nó khả thi và nó không khả thi,
Dialogue: 0,0:13:45.80,0:13:48.56,Default,,0,0,0,,thì cậu chỉ là kẻ mất trí\Nmuốn du hành thời gian.
Dialogue: 0,0:13:48.76,0:13:51.44,Default,,0,0,0,,- Cậu nghĩ cậu làm được?\N- Tớ nghĩ là sắp rồi.
Dialogue: 0,0:13:51.63,0:13:53.94,Default,,0,0,0,,Cậu báo là cậu\Ndùng thiết kế của mẹ cậu rồi chứ?
Dialogue: 0,0:13:54.14,0:13:55.44,Default,,0,0,0,,NHỮNG Ý TƯỞNG LỖI LẠC CỦA LAUREN CRESTON
Dialogue: 0,0:13:55.64,0:13:57.32,Default,,0,0,0,,Không. Mẹ bảo tớ đừng bận tâm.
Dialogue: 0,0:13:57.52,0:14:02.59,Default,,0,0,0,,Tớ nghĩ bà ấy gần làm được rồi,\Nnhưng tự dưng bà ấy lại bỏ cuộc.
Dialogue: 0,0:14:04.26,0:14:06.76,Default,,0,0,0,,Tớ sẽ cho bà ấy thấy\Nbà ấy thực sự lỗi lạc.
Dialogue: 0,0:14:07.51,0:14:10.76,Default,,0,0,0,,- Nếu tớ có thể làm nó hoạt động.\N- Nó bị làm sao?
Dialogue: 0,0:14:11.72,0:14:12.54,Default,,0,0,0,,Tớ không biết.
Dialogue: 0,0:14:12.74,0:14:15.42,Default,,0,0,0,,Tớ nghĩ có liên quan đến Wi-Fi.
Dialogue: 0,0:14:15.62,0:14:18.50,Default,,0,0,0,,- Nó có Wi-Fi à?\N- Ừ. Để đồng bộ với GPS điện thoại.
Dialogue: 0,0:14:18.70,0:14:21.88,Default,,0,0,0,,Không có nó, cậu không thể\Nđảm bảo trở về đúng chỗ
Dialogue: 0,0:14:22.08,0:14:24.36,Default,,0,0,0,,và cậu sẽ trôi nổi trong không gian.
Dialogue: 0,0:14:27.28,0:14:31.07,Default,,0,0,0,,{\an8}Sao nó được thiết lập\Nvề ngày 27 tháng 10 năm 1987?
Dialogue: 0,0:14:33.70,0:14:36.83,Default,,0,0,0,,Vì đó là ngày vụ án mạng đầu tiên xảy ra.
Dialogue: 0,0:14:37.62,0:14:39.40,Default,,0,0,0,,Tớ nghĩ nếu ta có thể ngăn chặn
Dialogue: 0,0:14:39.60,0:14:43.36,Default,,0,0,0,,Sát Nhân Tuổi 16 giết người ngay từ đầu…
Dialogue: 0,0:14:43.56,0:14:45.21,Default,,0,0,0,,Mẹ tớ có thể sẽ vẫn còn sống.
Dialogue: 0,0:15:18.75,0:15:22.48,Default,,0,0,0,,{\i1}Trong số khẩn cấp{\i0}\N{\i1}của{\i0} Podcast Sát Nhân Tuổi {\i1}16,{\i0}
Dialogue: 0,0:15:22.68,0:15:24.59,Default,,0,0,0,,{\i1}tên sát nhân đã trở lại.{\i0}
Dialogue: 0,0:15:25.50,0:15:29.51,Default,,0,0,0,,{\i1}Tiffany Clark, Marisa Song,{\i0}\N{\i1}Heather Hernandez.{\i0}
Dialogue: 0,0:15:30.05,0:15:32.79,Default,,0,0,0,,{\i1}Và giờ, 35 năm sau, là Pam Hughes.{\i0}
Dialogue: 0,0:15:32.99,0:15:34.62,Default,,0,0,0,,{\an8}PODCAST ĐỘC QUYỀN
Dialogue: 0,0:15:34.82,0:15:38.75,Default,,0,0,0,,{\an8}{\i1}Điều gì khiến hắn trở lại?{\i0}\N{\i1}Sao lại vào lúc này? Tại sao là cô ấy?{\i0}
Dialogue: 0,0:15:38.95,0:15:41.75,Default,,0,0,0,,{\i1}Và liệu tên sát nhân{\i0}\N{\i1}có đang nấp trong bóng tối,{\i0}
Dialogue: 0,0:15:41.95,0:15:43.94,Default,,0,0,0,,{\i1}rình mò nạn nhân tiếp theo không?{\i0}
Dialogue: 0,0:15:44.61,0:15:46.65,Default,,0,0,0,,{\i1}Hay hắn đang hòa vào đám đông?{\i0}
Dialogue: 0,0:15:47.44,0:15:51.24,Default,,0,0,0,,{\i1}Cảnh sát một lần nữa không có manh mối{\i0}\N{\i1}về tung tích kẻ sát nhân.{\i0}
Dialogue: 0,0:15:51.45,0:15:55.74,Default,,0,0,0,,{\i1}Hắn có thể trốn ở bất cứ đâu,{\i0}\N{\i1}quan sát, chờ đợi…{\i0}
Dialogue: 0,0:15:57.70,0:15:59.94,Default,,0,0,0,,- Xin lỗi.\N{\i1}- Pam là bạn và bạn cùng lớp{\i0}
Dialogue: 0,0:16:00.14,0:16:04.15,Default,,0,0,0,,{\i1}- của các nạn nhân khác…{\i0}\N- Con đói không? Bố đặt pizza rồi.
Dialogue: 0,0:16:04.35,0:16:06.34,Default,,0,0,0,,Không ạ. Cảm ơn bố.
Dialogue: 0,0:16:07.59,0:16:10.09,Default,,0,0,0,,Bố không biết con nghe podcast đó đấy.
Dialogue: 0,0:16:10.80,0:16:14.70,Default,,0,0,0,,Mẹ là bạn của Chris Dubasage ư?\NCảnh sát trưởng Lim nói họ nhắn tin.
Dialogue: 0,0:16:14.90,0:16:19.12,Default,,0,0,0,,Không. Mẹ con\Nkhông phải bạn của Đồ-Khốn-Basage.
Dialogue: 0,0:16:19.32,0:16:20.79,Default,,0,0,0,,Tên đó là con kền kền
Dialogue: 0,0:16:20.99,0:16:23.92,Default,,0,0,0,,biến những vụ thảm sát\Nthành hoạt động giải trí.
Dialogue: 0,0:16:24.12,0:16:25.73,Default,,0,0,0,,Vậy sao mẹ nhắn tin với chú ta?
Dialogue: 0,0:16:26.02,0:16:28.27,Default,,0,0,0,,Vì mẹ con muốn giúp tìm hung thủ.
Dialogue: 0,0:16:29.23,0:16:31.39,Default,,0,0,0,,Cảnh sát đã bỏ cuộc từ lâu rồi.
Dialogue: 0,0:16:31.59,0:16:34.07,Default,,0,0,0,,Chris là người duy nhất vẫn đang điều tra.
Dialogue: 0,0:16:34.78,0:16:36.56,Default,,0,0,0,,Vậy bố biết chú ấy hồi trung học.
Dialogue: 0,0:16:36.76,0:16:38.83,Default,,0,0,0,,Bọn bố đều biết nhau hết.
Dialogue: 0,0:16:39.87,0:16:43.57,Default,,0,0,0,,Anh ta từng thích mẹ con.\NHồi ấy mẹ con và bố vẫn đang là bạn,
Dialogue: 0,0:16:43.77,0:16:48.03,Default,,0,0,0,,mãi về sau bố mẹ mới yêu nhau,\Nkhi bố mẹ học đại học xong trở về.
Dialogue: 0,0:16:48.23,0:16:50.95,Default,,0,0,0,,Nhưng ơn Chúa bố mẹ\Nkhông yêu nhau hồi trung học,
Dialogue: 0,0:16:51.15,0:16:52.88,Default,,0,0,0,,vì tình cảm sẽ không bền được.
Dialogue: 0,0:16:57.85,0:17:00.81,Default,,0,0,0,,Bố vẫn nhớ khi bố mẹ\Nbiết được mẹ đã mang thai con.
Dialogue: 0,0:17:01.43,0:17:06.00,Default,,0,0,0,,Bố mẹ đang ở Boardwalk\Nvà đi chơi trò Cú Rơi Lượng Tử,
Dialogue: 0,0:17:06.20,0:17:07.23,Default,,0,0,0,,trò cảm giác mạnh ấy?
Dialogue: 0,0:17:09.44,0:17:14.85,Default,,0,0,0,,Cả hai đều nôn thốc nôn tháo.\NVà mẹ con còn nôn dữ dội hơn,
Dialogue: 0,0:17:15.05,0:17:19.83,Default,,0,0,0,,nên bà ấy đã thử thai\Nvà vậy đấy. Bố mẹ có con.
Dialogue: 0,0:17:23.71,0:17:26.62,Default,,0,0,0,,Giúp bố việc này,\Nđừng nghe Chris Dubasage nữa.
Dialogue: 0,0:17:28.71,0:17:30.80,Default,,0,0,0,,Jamie. Chào.
Dialogue: 0,0:17:33.34,0:17:35.26,Default,,0,0,0,,- Chris.\N- Cháu biết rồi.
Dialogue: 0,0:17:36.68,0:17:38.76,Default,,0,0,0,,Rất tiếc về sự mất mát của cháu.
Dialogue: 0,0:17:42.06,0:17:44.71,Default,,0,0,0,,- Chú thu âm nhé?\N- Hai người ngoại tình à?
Dialogue: 0,0:17:44.91,0:17:46.19,Default,,0,0,0,,Dĩ nhiên là không.
Dialogue: 0,0:17:46.56,0:17:49.86,Default,,0,0,0,,Bọn chú chỉ là bạn bè.\NCô ấy muốn giúp tìm hung thủ.
Dialogue: 0,0:17:50.90,0:17:54.07,Default,,0,0,0,,- Tại sao bà ấy lại giấu chuyện đó?\N- Chú không biết.
Dialogue: 0,0:17:55.49,0:17:58.16,Default,,0,0,0,,Ý chú là, bố cháu không thích chú lắm.
Dialogue: 0,0:17:58.91,0:18:02.62,Default,,0,0,0,,Nhưng ông ấy quý bố chú.\NNói ông ấy sẽ được thêm giải Pulitzer.
Dialogue: 0,0:18:03.33,0:18:05.79,Default,,0,0,0,,Podcast của chú giành năm giải Podsy đấy.
Dialogue: 0,0:18:07.08,0:18:09.50,Default,,0,0,0,,- Đâu có giống nhau.\N- Phải.
Dialogue: 0,0:18:11.34,0:18:14.01,Default,,0,0,0,,Mẹ cháu có nói gì với cháu\Nvề mảnh giấy không?
Dialogue: 0,0:18:14.55,0:18:17.18,Default,,0,0,0,,- Mảnh giấy gì?\N- Cô ấy nhận nó vào năm 1987.
Dialogue: 0,0:18:18.01,0:18:21.43,Default,,0,0,0,,Sau vụ sát hại cuối.\NCô ấy thấy nó được nhét vào tủ khóa.
Dialogue: 0,0:18:27.31,0:18:30.65,Default,,0,0,0,,MÀY LÀ KẺ TIẾP THEO\NMỘT NGÀY NÀO ĐÓ
Dialogue: 0,0:18:31.52,0:18:33.23,Default,,0,0,0,,Bà ấy chưa từng kể với cháu.
Dialogue: 0,0:18:33.98,0:18:35.36,Default,,0,0,0,,Cô ấy không kể với ai.
Dialogue: 0,0:18:36.78,0:18:41.57,Default,,0,0,0,,Cô ấy nói cô ấy sợ,\Nrằng nó sẽ trở thành sự thật hay gì đó.
Dialogue: 0,0:18:42.41,0:18:43.79,Default,,0,0,0,,Bà ấy kể cho chú sao?
Dialogue: 0,0:18:43.99,0:18:48.50,Default,,0,0,0,,Cô ấy kể cho chú vì nó là một manh mối.\NAi lại nỡ hành hạ cô ấy như vậy chứ?
Dialogue: 0,0:18:49.08,0:18:53.96,Default,,0,0,0,,Để cô ấy sống cả đời\Ntrong nỗi sợ hãi rằng ngày đó sẽ đến.
Dialogue: 0,0:18:55.21,0:18:56.42,Default,,0,0,0,,Jamie. Đợi đã.
Dialogue: 0,0:18:56.97,0:18:59.34,Default,,0,0,0,,{\an8}{\i1}Billy… Hải ly… Vui vẻ.{\i0}
Dialogue: 0,0:19:01.09,0:19:03.26,Default,,0,0,0,,{\i1}Billy… Hải ly… Vui vẻ.{\i0}
Dialogue: 0,0:19:05.22,0:19:07.48,Default,,0,0,0,,{\i1}Billy… Hải ly… Vui vẻ.{\i0}
Dialogue: 0,0:19:18.11,0:19:19.05,Default,,0,0,0,,{\an8}NHÀ BÚP BÊ KINH DỊ
Dialogue: 0,0:19:19.25,0:19:21.79,Default,,0,0,0,,{\an8}- Jamie. Tớ đây, là tớ.\N- Ôi, Chúa ơi!
Dialogue: 0,0:19:21.99,0:19:24.81,Default,,0,0,0,,- Cậu vừa xịt hơi cay mụ phù thủy à?\N- Một chút.
Dialogue: 0,0:19:25.01,0:19:28.27,Default,,0,0,0,,Tớ cần gặp cậu.\NTớ cần cậu giúp tóm tên Sát Nhân Tuổi 16.
Dialogue: 0,0:19:28.47,0:19:31.57,Default,,0,0,0,,Hắn đã hành hạ mẹ tớ cả đời.\NTớ không thể để hắn thoát…
Dialogue: 0,0:19:31.77,0:19:32.79,Default,,0,0,0,,Jamie!
Dialogue: 0,0:19:33.75,0:19:34.75,Default,,0,0,0,,Chạy đi!
Dialogue: 0,0:19:35.46,0:19:41.30,Default,,0,0,0,,Cứu! Cứu, có ai không, cứu! Cứu với!
Dialogue: 0,0:20:07.58,0:20:08.95,Default,,0,0,0,,{\i1}Khởi động chuỗi.{\i0}
Dialogue: 0,0:20:10.54,0:20:12.54,Default,,0,0,0,,{\i1}Lỗi.{\i0}
Dialogue: 0,0:20:14.71,0:20:20.67,Default,,0,0,0,,{\i1}- Bắt đầu. Năm, bốn, ba, hai, một.{\i0}\N- Tránh xa tôi ra. Có ai không, cứu!
Dialogue: 0,0:21:38.84,0:21:42.28,Default,,0,0,0,,- Nó ở đây này.\N- Xin lỗi, hai người không dùng được.
Dialogue: 0,0:21:42.48,0:21:44.42,Default,,0,0,0,,Nó đang trục trặc.
Dialogue: 0,0:21:45.05,0:21:46.80,Default,,0,0,0,,- Thật à?\N- Nó bị hỏng rồi.
Dialogue: 0,0:21:47.80,0:21:48.70,Default,,0,0,0,,Không dùng được.
Dialogue: 0,0:21:48.90,0:21:50.01,Default,,0,0,0,,Xin lỗi.
Dialogue: 0,0:21:51.72,0:21:54.25,Default,,0,0,0,,{\an8}Nhân tiện, áo của anh rất có vấn đề đấy.
Dialogue: 0,0:21:54.45,0:21:56.02,Default,,0,0,0,,{\an8}THANH TRA LIÊN BANG NGU DẠI
Dialogue: 0,0:21:56.27,0:21:57.69,Default,,0,0,0,,{\an8}Em thích áo của anh mà.
Dialogue: 0,0:22:06.32,0:22:08.70,Default,,0,0,0,,{\an8}{\i1}Hải ly Billy nói hãy vui vẻ nào.{\i0}
Dialogue: 0,0:22:10.12,0:22:12.10,Default,,0,0,0,,{\i1}Hải ly Billy nói hãy vui vẻ nào.{\i0}
Dialogue: 0,0:22:12.30,0:22:13.87,Default,,0,0,0,,- Xin lỗi.\N- Xin lỗi cưng.
Dialogue: 0,0:22:15.46,0:22:17.71,Default,,0,0,0,,Cho hỏi. Đây là năm bao nhiêu ạ?
Dialogue: 0,0:22:18.58,0:22:19.88,Default,,0,0,0,,1987.
Dialogue: 0,0:22:20.79,0:22:21.65,Default,,0,0,0,,Ôi, Chúa ơi!
Dialogue: 0,0:22:21.85,0:22:26.01,Default,,0,0,0,,Chị biết. Sắp hết thập niên 1980 rồi\Nmà chị còn chưa thử cocain nữa.
Dialogue: 0,0:22:28.34,0:22:29.39,Default,,0,0,0,,Chúng làm gì biết.
Dialogue: 0,0:22:29.97,0:22:31.68,Default,,0,0,0,,Em đang tìm Pam Hughes.
Dialogue: 0,0:22:32.60,0:22:34.71,Default,,0,0,0,,- Ai?\N- Miller. Xin lỗi, Pam Miller.
Dialogue: 0,0:22:34.91,0:22:36.42,Default,,0,0,0,,Cô ấy là bạn em.
Dialogue: 0,0:22:36.62,0:22:40.13,Default,,0,0,0,,Chắc cô ấy đang ở trường.\NEm cũng nên thế. Chị cho em đi nhờ.
Dialogue: 0,0:22:40.33,0:22:43.68,Default,,0,0,0,,Gì? Không. Em không thể\Nlên xe cùng chị, chị là người lạ.
Dialogue: 0,0:22:43.87,0:22:45.19,Default,,0,0,0,,Có khi là kẻ sát nhân.
Dialogue: 0,0:22:45.94,0:22:50.36,Default,,0,0,0,,Cưng à, có kẻ sát nhân nào\Nmặc đồ Gloria Vanderbilt không?
Dialogue: 0,0:22:52.87,0:22:53.87,Default,,0,0,0,,Không.
Dialogue: 0,0:22:56.66,0:22:58.62,Default,,0,0,0,,- Cảm ơn chị.\N- Ừ. Không có gì.
Dialogue: 0,0:23:01.25,0:23:02.71,Default,,0,0,0,,- Áo đẹp đấy.\N- Cảm ơn.
Dialogue: 0,0:23:08.59,0:23:10.01,Default,,0,0,0,,TRƯỜNG TRUNG HỌC VERNON
Dialogue: 0,0:23:12.30,0:23:13.33,Default,,0,0,0,,BẦY QUỶ ĐỎ VERNON
Dialogue: 0,0:23:13.53,0:23:15.39,Default,,0,0,0,,Và kỳ thị sắc tộc kìa.
Dialogue: 0,0:23:16.14,0:23:17.39,Default,,0,0,0,,Biết ngay mà.
Dialogue: 0,0:23:18.39,0:23:19.39,Default,,0,0,0,,Chúa ơi!
Dialogue: 0,0:23:23.90,0:23:28.99,Default,,0,0,0,,Xin chào. Em là Jamie… LaFleur.
Dialogue: 0,0:23:29.61,0:23:34.77,Default,,0,0,0,,Và em là học sinh trao đổi\Ntừ đảo Prince Edward, Canada.
Dialogue: 0,0:23:34.97,0:23:38.52,Default,,0,0,0,,Trường cũ của em nói\Nhọ sẽ gửi giấy tờ nhập học cho em.
Dialogue: 0,0:23:38.72,0:23:40.96,Default,,0,0,0,,- Lớp mấy?\N- Mười một.
Dialogue: 0,0:23:42.25,0:23:43.58,Default,,0,0,0,,Lịch học của em đây.
Dialogue: 0,0:23:44.09,0:23:47.20,Default,,0,0,0,,Được rồi. Cô không cần xác nhận gì sao?
Dialogue: 0,0:23:47.40,0:23:51.79,Default,,0,0,0,,"Xác nhận…" Đây là đâu, kho vàng\NFort Knox à? Em trễ giờ thể dục rồi.
Dialogue: 0,0:23:51.98,0:23:55.93,Default,,0,0,0,,Em nhận lịch khác được không?\NEm không thích học thể dục.
Dialogue: 0,0:23:59.56,0:24:01.69,Default,,0,0,0,,Có lựa chọn nào khác không ạ?
Dialogue: 0,0:24:05.94,0:24:07.15,Default,,0,0,0,,Ôi, Chúa ơi.
Dialogue: 0,0:24:09.86,0:24:11.90,Default,,0,0,0,,Đúng rồi. Chuyển đi.
Dialogue: 0,0:24:12.45,0:24:14.39,Default,,0,0,0,,Sao đây lại là đồ trường cấp chứ?
Dialogue: 0,0:24:14.59,0:24:16.70,Default,,0,0,0,,- Đi nào.\N- Nhìn như nhân viên Hooters.
Dialogue: 0,0:24:26.54,0:24:27.71,Default,,0,0,0,,Ôi, Chúa ơi!
Dialogue: 0,0:24:38.97,0:24:41.42,Default,,0,0,0,,Học sinh mới. Vào sân đi.
Dialogue: 0,0:24:41.62,0:24:44.69,Default,,0,0,0,,Em sao? Không. Không, em… em ổn.
Dialogue: 0,0:24:44.89,0:24:48.07,Default,,0,0,0,,"Em? Ổn?" Vào đi! Đi nào!
Dialogue: 0,0:24:48.61,0:24:49.82,Default,,0,0,0,,Thô lỗ quá.
Dialogue: 0,0:24:50.86,0:24:53.64,Default,,0,0,0,,- Hôm nay. Hôm nay. Hôm nay.\N- Được rồi.
Dialogue: 0,0:24:53.84,0:24:55.74,Default,,0,0,0,,- Cảm ơn.\N- Em ở đó rất hợp.
Dialogue: 0,0:24:58.28,0:24:59.29,Default,,0,0,0,,Chúa ơi!
Dialogue: 0,0:25:09.30,0:25:10.42,Default,,0,0,0,,Cái quái gì thế?
Dialogue: 0,0:25:13.38,0:25:14.63,Default,,0,0,0,,Cái quái gì thế?
Dialogue: 0,0:25:31.23,0:25:33.69,Default,,0,0,0,,Sao việc này lại hợp pháp chứ?
Dialogue: 0,0:25:35.66,0:25:38.07,Default,,0,0,0,,Dừng lại. Tôi biết ai làm rồi.
Dialogue: 0,0:25:40.70,0:25:41.91,Default,,0,0,0,,Cúi xuống!
Dialogue: 0,0:25:42.62,0:25:44.86,Default,,0,0,0,,Tôi tìm Pam Miller. Biết cô ấy chứ?
Dialogue: 0,0:25:45.06,0:25:48.13,Default,,0,0,0,,- Ừ, ai mà chả biết Pam.\N- Tuyệt. Cô ấy đâu?
Dialogue: 0,0:25:49.38,0:25:50.34,Default,,0,0,0,,Kia.
Dialogue: 0,0:25:57.59,0:25:58.76,Default,,0,0,0,,Cậu bị loại rồi.
Dialogue: 0,0:25:59.64,0:26:02.25,Default,,0,0,0,,Khoan đã, không,\NPam Miller phải tốt bụng chứ.
Dialogue: 0,0:26:02.45,0:26:05.29,Default,,0,0,0,,Tốt bụng kiểu sợ là\Nmở nhạc to sẽ gây hại màng nhĩ.
Dialogue: 0,0:26:05.49,0:26:08.60,Default,,0,0,0,,Cậu chơi đồ à?\NPam Miller là phù thủy xấu xa ở Vernon.
Dialogue: 0,0:26:09.40,0:26:10.98,Default,,0,0,0,,Cút ra đi, đồ thua cuộc!
Dialogue: 0,0:26:13.78,0:26:16.39,Default,,0,0,0,,Lời khuyên nhé. Lần tới, hãy bắt bóng.
Dialogue: 0,0:26:16.59,0:26:21.02,Default,,0,0,0,,Được chứ? Như vậy tôi sẽ không phải\Nđộng vào thứ máu kinh tởm của em. Tởm lắm.
Dialogue: 0,0:26:21.22,0:26:24.35,Default,,0,0,0,,Được rồi, làm tốt lắm, đội thắng cuộc.
Dialogue: 0,0:26:24.55,0:26:27.61,Default,,0,0,0,,Giá mà tôi có thể nói vậy\Nvới đội thua, mà không thể.
Dialogue: 0,0:26:27.81,0:26:29.23,Default,,0,0,0,,Đi tắm thôi, đi nào.
Dialogue: 0,0:26:29.43,0:26:31.42,Default,,0,0,0,,Cảm ơn, HLV Zane.
Dialogue: 0,0:26:32.21,0:26:35.16,Default,,0,0,0,,Anh trai tớ mua cho chúng ta\Nrất nhiều rượu B & J.
Dialogue: 0,0:26:35.36,0:26:38.05,Default,,0,0,0,,- Chúa ơi, tớ thích rượu BJ lắm.\N- Pam.
Dialogue: 0,0:26:39.47,0:26:40.64,Default,,0,0,0,,Pam Miller?
Dialogue: 0,0:26:41.68,0:26:42.72,Default,,0,0,0,,Ừ.
Dialogue: 0,0:26:49.19,0:26:51.27,Default,,0,0,0,,Chụp ảnh đi, đừng nhìn.
Dialogue: 0,0:26:52.57,0:26:56.61,Default,,0,0,0,,Xin lỗi. Chỉ là cô không như tôi nghĩ.
Dialogue: 0,0:26:58.20,0:27:00.41,Default,,0,0,0,,Tôi là Jamie. Từ Canada.
Dialogue: 0,0:27:01.28,0:27:05.91,Default,,0,0,0,,Tôi xin lỗi. Tôi sẽ không bắt tay cậu,\Nvì tôi không phải một gã 45 tuổi.
Dialogue: 0,0:27:06.62,0:27:07.94,Default,,0,0,0,,Hãy đến sau giờ học,
Dialogue: 0,0:27:08.14,0:27:10.50,Default,,0,0,0,,cậu có thể sắp xếp tiệc\Ntrong lúc tớ làm tóc.
Dialogue: 0,0:27:11.25,0:27:12.11,Default,,0,0,0,,Được rồi.
Dialogue: 0,0:27:12.31,0:27:17.09,Default,,0,0,0,,Ở chỗ tôi thì tiệc sinh nhật\Nbị coi là một việc chán ngắt.
Dialogue: 0,0:27:17.67,0:27:19.78,Default,,0,0,0,,Có lẽ cô nên hủy đi.
Dialogue: 0,0:27:19.98,0:27:21.64,Default,,0,0,0,,Có lẽ cậu nên đi chết đi.
Dialogue: 0,0:27:21.97,0:27:24.60,Default,,0,0,0,,Chúa ơi. Mẹ… acita.
Dialogue: 0,0:27:26.93,0:27:27.93,Default,,0,0,0,,{\i1}Mamacita.{\i0}
Dialogue: 0,0:27:29.44,0:27:30.52,Default,,0,0,0,,Tiếng Tây Ban Nha?
Dialogue: 0,0:27:31.52,0:27:34.73,Default,,0,0,0,,Được rồi… đi chết đi.
Dialogue: 0,0:27:36.82,0:27:39.03,Default,,0,0,0,,Học tiếng Tây Ban Nha ba năm đấy.
Dialogue: 0,0:27:40.28,0:27:43.91,Default,,0,0,0,,Tốt thôi, mẹ không muốn\Nhủy bữa tiệc? Con sẽ hủy bữa tiệc.
Dialogue: 0,0:27:45.16,0:27:48.69,Default,,0,0,0,,Người già, người ốm và người nuôi chó.
Dialogue: 0,0:27:48.89,0:27:51.27,Default,,0,0,0,,- Đó là thứ tự những người anh ghét?\N- Ừ.
Dialogue: 0,0:27:51.47,0:27:53.29,Default,,0,0,0,,Thứ tự hay đấy. Được thôi.
Dialogue: 0,0:27:53.71,0:27:56.90,Default,,0,0,0,,Chào, cháu muốn trình báo\Nvề một tội ác chưa xảy ra.
Dialogue: 0,0:27:57.10,0:27:59.32,Default,,0,0,0,,Nghe thật điên, nhưng hãy tin cháu.
Dialogue: 0,0:27:59.52,0:28:01.08,Default,,0,0,0,,Xem {\i1}Back To The Future{\i0} chưa?
Dialogue: 0,0:28:01.28,0:28:03.83,Default,,0,0,0,,- Chưa, nghe nói hay lắm.\N- Vâng, rất hay.
Dialogue: 0,0:28:04.03,0:28:08.60,Default,,0,0,0,,Nhưng, về cơ bản,\Ncháu như đang sống trong phim đó.
Dialogue: 0,0:28:08.89,0:28:11.63,Default,,0,0,0,,Đó là lý do cháu biết\Ntối nay sẽ có vụ án mạng.
Dialogue: 0,0:28:11.83,0:28:13.19,Default,,0,0,0,,Án mạng ư?
Dialogue: 0,0:28:14.36,0:28:16.47,Default,,0,0,0,,- Chưa từng có án mạng ở Vernon.\N- Vâng.
Dialogue: 0,0:28:16.67,0:28:19.68,Default,,0,0,0,,Tối nay sẽ có án mạng\Ntại bữa tiệc của Tiffany Clark,
Dialogue: 0,0:28:19.88,0:28:21.89,Default,,0,0,0,,sẽ có toàn trẻ con uống rượu,
Dialogue: 0,0:28:22.09,0:28:24.89,Default,,0,0,0,,nên hai chú phải đến đó\Nvà dẹp bữa tiệc đi.
Dialogue: 0,0:28:25.09,0:28:27.98,Default,,0,0,0,,Chúa ơi! Thật sao?
Dialogue: 0,0:28:28.18,0:28:29.90,Default,,0,0,0,,Tôi chưa từng gặp cháu ở đây.
Dialogue: 0,0:28:30.10,0:28:33.86,Default,,0,0,0,,Vâng, cháu là học sinh trao đổi\Ntừ đảo Prince Edward, Canada.
Dialogue: 0,0:28:34.06,0:28:35.71,Default,,0,0,0,,- Nghe như bịa.\N- Không phải.
Dialogue: 0,0:28:36.63,0:28:37.92,Default,,0,0,0,,Nơi đó có thật mà.
Dialogue: 0,0:28:39.88,0:28:43.38,Default,,0,0,0,,Lim. Chú là bố của cảnh sát trưởng.
Dialogue: 0,0:28:44.39,0:28:48.16,Default,,0,0,0,,Biệt danh của Kara\Nlà Cảnh Sát Trưởng? Nghe hay đấy.
Dialogue: 0,0:28:48.36,0:28:53.77,Default,,0,0,0,,Này, cháu đang cho chú cơ hội\Nngăn chặn thảm kịch trước khi nó xảy ra.
Dialogue: 0,0:28:57.61,0:28:59.34,Default,,0,0,0,,- Ai đây?\N- Đó là kẻ sát nhân.
Dialogue: 0,0:28:59.54,0:29:02.64,Default,,0,0,0,,- Hắn đã giết ai?\N- Hắn vẫn chưa giết ai,
Dialogue: 0,0:29:02.84,0:29:06.56,Default,,0,0,0,,nhưng chuyện đó… Chú làm ơn\Nxem {\i1}Back to the Future{\i0} được không?
Dialogue: 0,0:29:06.76,0:29:09.39,Default,,0,0,0,,Tôi không biết lũ gây rối các cháu\Nlàm gì ở Canada,
Dialogue: 0,0:29:09.59,0:29:12.02,Default,,0,0,0,,nhưng ở đây\Nkhông dung thứ cho mấy trò đùa,
Dialogue: 0,0:29:12.22,0:29:15.83,Default,,0,0,0,,nên hãy quay về trường đi,\Nkẻo tôi sẽ bắt cháu vì tội trốn học.
Dialogue: 0,0:29:20.13,0:29:22.26,Default,,0,0,0,,Chúa ơi, sẽ không có ai tin mình.
Dialogue: 0,0:29:23.76,0:29:25.66,Default,,0,0,0,,{\an8}Tôi cũng muốn xem phim đó đấy.
Dialogue: 0,0:29:25.86,0:29:29.64,Default,,0,0,0,,{\an8}Tôi thì không, ghét mấy phim\Ndu hành thời gian. Chúng thật vô lý.
Dialogue: 0,0:29:37.06,0:29:40.52,Default,,0,0,0,,Chào, lại là em đây. Jamie LaFleur.
Dialogue: 0,0:29:41.61,0:29:44.10,Default,,0,0,0,,Em cần biết Lauren Creston học lớp nào.
Dialogue: 0,0:29:44.29,0:29:46.64,Default,,0,0,0,,Em biết nó riêng tư, nhưng khẩn cấp lắm.
Dialogue: 0,0:29:46.84,0:29:50.83,Default,,0,0,0,,- Em có ống EpiPen mà cậu ấy cần…\N- Khoa học Trái Đất, phòng 217.
Dialogue: 0,0:29:51.45,0:29:52.54,Default,,0,0,0,,Cảm ơn cô.
Dialogue: 0,0:29:54.71,0:29:58.00,Default,,0,0,0,,Ôi Chúa ơi.\NĐi máy bay thời này chắc điên lắm.
Dialogue: 0,0:30:01.05,0:30:03.92,Default,,0,0,0,,Chào, Lauren Creston có ở đây không?
Dialogue: 0,0:30:09.01,0:30:12.31,Default,,0,0,0,,Ôi, Chúa ơi! Cô đẹp quá.
Dialogue: 0,0:30:14.06,0:30:15.06,Default,,0,0,0,,Có chuyện gì thế?
Dialogue: 0,0:30:15.85,0:30:17.92,Default,,0,0,0,,Tôi gặp cậu ở sảnh chút được không?
Dialogue: 0,0:30:18.12,0:30:20.47,Default,,0,0,0,,Làm ơn hãy nói\Ncô đã xem {\i1}Back to the Future.{\i0}
Dialogue: 0,0:30:20.66,0:30:22.22,Default,,0,0,0,,- Dĩ nhiên rồi.\N- Ơn Chúa.
Dialogue: 0,0:30:22.42,0:30:25.97,Default,,0,0,0,,Hãy coi cháu là Marty McFly.\NBa mươi lăm năm nữa,
Dialogue: 0,0:30:26.17,0:30:28.56,Default,,0,0,0,,con gái cô Amelia, bạn thân nhất của cháu,
Dialogue: 0,0:30:28.76,0:30:32.73,Default,,0,0,0,,cùng chế tạo cỗ máy thời gian\Ndựa trên bản vẽ trong vở. Vở của cô!
Dialogue: 0,0:30:32.93,0:30:36.48,Default,,0,0,0,,Nó là {\i1}Những ý tưởng lỗi lạc{\i0}\N{\i1}và bất thường của Lauren Creston,{\i0}
Dialogue: 0,0:30:36.68,0:30:39.78,Default,,0,0,0,,và cháu dùng máy thời gian\Nđể thoát khỏi tên sát nhân
Dialogue: 0,0:30:39.98,0:30:42.86,Default,,0,0,0,,vừa tái xuất sau thời gian dài im lìm
Dialogue: 0,0:30:43.06,0:30:45.99,Default,,0,0,0,,sau chuỗi giết chóc\Nđầu tiên của hắn, bắt đầu từ tối nay.
Dialogue: 0,0:30:46.19,0:30:50.18,Default,,0,0,0,,Nên cháu cần cô giúp bắt kẻ sát nhân,\Nđể hắn không thể giết mẹ cháu.
Dialogue: 0,0:30:52.26,0:30:56.21,Default,,0,0,0,,Vì hắn đã giết…\NHắn đã giết mẹ cháu, ở tương lai,
Dialogue: 0,0:30:56.41,0:30:59.05,Default,,0,0,0,,và cháu cũng cần cô giúp tìm ra cách
Dialogue: 0,0:30:59.24,0:31:02.13,Default,,0,0,0,,điều chỉnh cỗ máy thời gian\Nđể đưa cháu về nhà.
Dialogue: 0,0:31:02.33,0:31:06.68,Default,,0,0,0,,Vì Amelia nói nó cần Wi-Fi để hoạt động\Nvà thời này chưa có thứ đó.
Dialogue: 0,0:31:06.88,0:31:08.49,Default,,0,0,0,,Tình hình không ổn lắm.
Dialogue: 0,0:31:11.57,0:31:14.31,Default,,0,0,0,,- Không ai biết về cuốn vở của tôi.\N- Trừ cháu.
Dialogue: 0,0:31:14.51,0:31:18.04,Default,,0,0,0,,Vì cháu đến từ tương lai.\NCháu có bằng chứng đây.
Dialogue: 0,0:31:22.17,0:31:24.53,Default,,0,0,0,,- Cỗ máy đây à?\N- Là điện thoại của cháu.
Dialogue: 0,0:31:24.73,0:31:26.42,Default,,0,0,0,,Cô xem muốn cỗ máy không?
Dialogue: 0,0:31:28.22,0:31:31.41,Default,,0,0,0,,Khoan, tôi không hiểu.\NNếu tên sát nhân nhắm vào Nhóm Molly,
Dialogue: 0,0:31:31.61,0:31:33.87,Default,,0,0,0,,sao hắn lại đợi 35 năm để giết mẹ cháu?
Dialogue: 0,0:31:34.07,0:31:37.00,Default,,0,0,0,,- Nhóm Molly là ai?\N- Tiffany, Marisa, Heather, Pam.
Dialogue: 0,0:31:37.20,0:31:39.71,Default,,0,0,0,,Họ tự gọi là Nhóm Molly,\Ntheo Molly Ringwald.
Dialogue: 0,0:31:39.91,0:31:43.67,Default,,0,0,0,,- Họ bị ám ảnh. Còn ăn mặc giống cô ấy.\N- Không ngờ họ thân vậy.
Dialogue: 0,0:31:43.87,0:31:46.38,Default,,0,0,0,,Cháu tưởng họ quen nhau vì đây là Vernon
Dialogue: 0,0:31:46.58,0:31:48.03,Default,,0,0,0,,và ai cũng biết nhau.
Dialogue: 0,0:31:54.12,0:31:55.33,Default,,0,0,0,,Nó đây.
Dialogue: 0,0:31:56.74,0:31:57.75,Default,,0,0,0,,Cỗ máy thời gian.
Dialogue: 0,0:31:59.83,0:32:02.57,Default,,0,0,0,,- Thú vị đấy.\N- Lạ lắm. Trước đó nó không chạy,
Dialogue: 0,0:32:02.77,0:32:06.78,Default,,0,0,0,,nhưng khi hắn tấn công cháu\Nvà đâm dao vào bảng, nó tự dưng hoạt động.
Dialogue: 0,0:32:06.98,0:32:09.12,Default,,0,0,0,,Chắc nó cần thêm kim loại dẫn.
Dialogue: 0,0:32:09.31,0:32:11.84,Default,,0,0,0,,Phải. "Thêm kim loại dẫn".\NCháu đã nói thế.
Dialogue: 0,0:32:13.43,0:32:15.87,Default,,0,0,0,,Túi cô toàn dụng cụ sao? Không có sách à?
Dialogue: 0,0:32:16.07,0:32:17.97,Default,,0,0,0,,Sách đọc hết rồi, dụng cụ có ích hơn.
Dialogue: 0,0:32:19.98,0:32:22.21,Default,,0,0,0,,- Cái gì đây?\N- Ôi, Chúa ơi!
Dialogue: 0,0:32:22.41,0:32:23.92,Default,,0,0,0,,- Súng bắn đinh đấy.\N- Xin lỗi.
Dialogue: 0,0:32:24.12,0:32:26.72,Default,,0,0,0,,Không thể sửa cỗ máy\Ncon gái tương lai tôi chế tạo
Dialogue: 0,0:32:26.92,0:32:30.22,Default,,0,0,0,,- nếu tôi bị giết vì bất cẩn.\N- Cô đón nhận vụ này dễ thật.
Dialogue: 0,0:32:30.42,0:32:33.01,Default,,0,0,0,,Sáng chế du hành thời gian\Nthì sẽ phải nghĩ tới
Dialogue: 0,0:32:33.21,0:32:35.43,Default,,0,0,0,,khả năng người tương lai cần giúp.
Dialogue: 0,0:32:35.63,0:32:37.81,Default,,0,0,0,,- Vụ kẻ sát nhân thì bất ngờ đấy.\N- Vâng.
Dialogue: 0,0:32:38.01,0:32:41.56,Default,,0,0,0,,Nhân tiện, nếu cháu\Nngăn chặn được vụ giết người xảy ra,
Dialogue: 0,0:32:41.76,0:32:44.61,Default,,0,0,0,,nó có gây ra\Nvết rách không-thời gian không?
Dialogue: 0,0:32:44.81,0:32:47.45,Default,,0,0,0,,Chịu. Có nhiều giả thuyết\Nvề du hành thời gian.
Dialogue: 0,0:32:47.64,0:32:49.61,Default,,0,0,0,,Thứ mà tôi thấy hứa hẹn nhất…
Dialogue: 0,0:32:49.81,0:32:51.99,Default,,0,0,0,,Cháu biết cơ chế lượng tử không?
Dialogue: 0,0:32:52.19,0:32:55.59,Default,,0,0,0,,Cháu xem {\i1}Avengers: Endgame{\i0} rồi.\NNhưng cháu không hiểu lắm.
Dialogue: 0,0:32:56.35,0:32:59.97,Default,,0,0,0,,Được rồi, hãy tưởng tượng\Nrằng thời gian như dòng sông.
Dialogue: 0,0:33:00.81,0:33:03.96,Default,,0,0,0,,Cháu nhảy khỏi dòng sông,\Nlên thượng nguồn, rồi nhảy xuống.
Dialogue: 0,0:33:04.16,0:33:06.09,Default,,0,0,0,,Nó liên tục chảy, vậy nên,
Dialogue: 0,0:33:06.29,0:33:08.76,Default,,0,0,0,,mọi thay đổi đều ảnh hưởng đến hạ nguồn.
Dialogue: 0,0:33:08.96,0:33:12.15,Default,,0,0,0,,Vậy nếu cháu làm mọi thứ tốt lên\Nthì mọi chuyện sẽ ổn?
Dialogue: 0,0:33:12.86,0:33:15.82,Default,,0,0,0,,- Lý thuyết là vậy.\N- Và nếu thời gian cứ trôi,
Dialogue: 0,0:33:16.95,0:33:18.52,Default,,0,0,0,,chuyện gì sẽ xảy ra ở nhà?
Dialogue: 0,0:33:18.72,0:33:20.62,Default,,0,0,0,,{\i1}Hải ly Billy nói hãy vui vẻ nào.{\i0}
Dialogue: 0,0:33:21.54,0:33:24.52,Default,,0,0,0,,{\i1}- Billy… Hải ly… Vui vẻ.{\i0}\N- Hãy đưa lũ trẻ ở hội chợ
Dialogue: 0,0:33:24.72,0:33:26.82,Default,,0,0,0,,ra khỏi đây và thắt chặt an ninh.
Dialogue: 0,0:33:27.02,0:33:29.11,Default,,0,0,0,,Kara, Jamie đâu? Thấy con bé chưa?
Dialogue: 0,0:33:29.31,0:33:30.63,Default,,0,0,0,,Vẫn chưa.
Dialogue: 0,0:33:32.76,0:33:36.20,Default,,0,0,0,,Anh làm gì thế? Ghi chép cho tập mới à?
Dialogue: 0,0:33:36.40,0:33:39.25,Default,,0,0,0,,Ngăn không cho người ta điều tra\Nlà không hay đâu.
Dialogue: 0,0:33:39.45,0:33:42.29,Default,,0,0,0,,- Đó là việc anh làm à?\N- Tôi là phóng viên điều tra.
Dialogue: 0,0:33:42.49,0:33:45.39,Default,,0,0,0,,Không, đấy là bố anh.\NAnh là hướng dẫn viên.
Dialogue: 0,0:33:45.89,0:33:49.26,Default,,0,0,0,,Chú Hughes. Chú Hughes.\NCháu biết Jamie đi đâu rồi.
Dialogue: 0,0:33:49.46,0:33:50.84,Default,,0,0,0,,Cậu ấy đã dùng máy thời gian.
Dialogue: 0,0:33:51.04,0:33:53.51,Default,,0,0,0,,Cháu chế tạo máy thời gian\Ncho hội chợ khoa học.
Dialogue: 0,0:33:53.71,0:33:57.20,Default,,0,0,0,,- Chúa ơi, con bé kỳ quặc như mẹ nó.\N- Jamie đang ở năm 1987.
Dialogue: 0,0:33:57.57,0:34:00.31,Default,,0,0,0,,Cháu không thấy cỗ máy,\Nchắc nó đã hoạt động.
Dialogue: 0,0:34:00.51,0:34:02.60,Default,,0,0,0,,Thích khoa học viễn tưởng? Viết sách đi.
Dialogue: 0,0:34:02.80,0:34:06.67,Default,,0,0,0,,Nhưng đây là thế giới thật,\Ntôi sẽ giải vụ án này.
Dialogue: 0,0:34:10.29,0:34:14.17,Default,,0,0,0,,- Kara!\N- Chú có vài câu hỏi tiếp theo.
Dialogue: 0,0:34:15.01,0:34:16.01,Default,,0,0,0,,Câu hỏi thứ nhất.
Dialogue: 0,0:34:17.59,0:34:19.87,Default,,0,0,0,,- Tôi thực sự phải đi cùng ư?\N- Vâng.
Dialogue: 0,0:34:20.07,0:34:23.10,Default,,0,0,0,,Có án mạng ở bữa tiệc này,\Ncháu không đi một mình đâu.
Dialogue: 0,0:34:25.02,0:34:27.69,Default,,0,0,0,,- Cảm ơn đã cho cháu mượn đồ.\N- Không có gì.
Dialogue: 0,0:34:30.11,0:34:31.82,Default,,0,0,0,,Chúa ơi, nơi này đẹp quá.
Dialogue: 0,0:34:32.02,0:34:34.55,Default,,0,0,0,,Không thể tin\Nhọ biến nó thành Zatta Burger.
Dialogue: 0,0:34:34.75,0:34:37.99,Default,,0,0,0,,- Ở đây có Zatta Burger chưa?\N- Chẳng biết đó là gì luôn.
Dialogue: 0,0:34:42.70,0:34:44.23,Default,,0,0,0,,Sao rồi? Vào đi.
Dialogue: 0,0:34:44.43,0:34:48.44,Default,,0,0,0,,Này, cậu không thể vào.\NCó quy định trang phục.
Dialogue: 0,0:34:48.64,0:34:51.19,Default,,0,0,0,,- Cái gì? Làm gì có.\N- Ừ, có đấy.
Dialogue: 0,0:34:51.39,0:34:52.53,Default,,0,0,0,,Cấm trang sức mọt sách.
Dialogue: 0,0:34:52.73,0:34:55.99,Default,,0,0,0,,Nó là bản sao vi mạch đầu tiên,\Nđánh dấu bình minh của…
Dialogue: 0,0:34:56.19,0:34:57.47,Default,,0,0,0,,Im đi, thằng đụt.
Dialogue: 0,0:34:58.26,0:35:01.35,Default,,0,0,0,,Này! Thôi đi!
Dialogue: 0,0:35:01.68,0:35:06.23,Default,,0,0,0,,Khỉ thật. Này, Kara. Nhìn xem.\NĐụt Summers có đứa bạn mọt sách này.
Dialogue: 0,0:35:14.94,0:35:17.97,Default,,0,0,0,,Được rồi, nghe này. Tôi phải tìm Tiffany.
Dialogue: 0,0:35:18.17,0:35:19.85,Default,,0,0,0,,- Thế à.\N- Động chạm ngoài ý muốn.
Dialogue: 0,0:35:20.05,0:35:22.56,Default,,0,0,0,,- Động chạm ngoài ý muốn.\N- Khách ngoài ý muốn.
Dialogue: 0,0:35:22.76,0:35:25.83,Default,,0,0,0,,Có chỗ chơi {\i1}Dungeons & Dragons{\i0}\Ncuối phố kìa. Chúc vui.
Dialogue: 0,0:35:26.62,0:35:27.62,Default,,0,0,0,,Không, đợi đã.
Dialogue: 0,0:35:27.96,0:35:31.83,Default,,0,0,0,,Này! Này, đợi đã.\NChúa ơi, tôi phải vào đó.
Dialogue: 0,0:35:33.21,0:35:34.71,Default,,0,0,0,,Tôi biết lối vào khác.
Dialogue: 0,0:35:45.93,0:35:48.83,Default,,0,0,0,,Cậu là học sinh trao đổi,\Nphải không? Từ Canada?
Dialogue: 0,0:35:49.03,0:35:51.09,Default,,0,0,0,,Ừ, đảo Prince Edward.
Dialogue: 0,0:35:51.29,0:35:53.67,Default,,0,0,0,,Tôi từng đến đó vào mùa hè với dì tôi. Ừ.
Dialogue: 0,0:35:53.87,0:35:57.99,Default,,0,0,0,,Cậu có biết độ tuổi trung bình trên đảo\Nlà 49,5 không? Tuyệt nhỉ?
Dialogue: 0,0:35:58.69,0:36:01.91,Default,,0,0,0,,Tuyệt lắm. Vâng, tôi sẽ ở cùng Lauren.
Dialogue: 0,0:36:03.16,0:36:05.66,Default,,0,0,0,,- Ừ.\N- Được rồi. Vòng hai.
Dialogue: 0,0:36:08.58,0:36:11.92,Default,,0,0,0,,Này, Doug. Này, sao cậu\Nkhông nâng bọn tôi lên?
Dialogue: 0,0:36:12.79,0:36:16.20,Default,,0,0,0,,Được. Tôi sẽ vòng ra trước\Nvà các cậu có thể cho tôi vào.
Dialogue: 0,0:36:16.39,0:36:18.55,Default,,0,0,0,,- Tuyệt.\N- Được rồi.
Dialogue: 0,0:36:19.34,0:36:20.84,Default,,0,0,0,,Cảm ơn.
Dialogue: 0,0:36:21.47,0:36:24.55,Default,,0,0,0,,Được rồi, Doug. Cảm ơn.
Dialogue: 0,0:36:28.81,0:36:31.27,Default,,0,0,0,,- Mời phu nhân.\N- Ôi, Chúa ơi!
Dialogue: 0,0:36:50.50,0:36:53.46,Default,,0,0,0,,Được rồi. Được rồi, ta phải tìm Tiffany.
Dialogue: 0,0:37:00.30,0:37:01.55,Default,,0,0,0,,Ôi, Chúa ơi!
Dialogue: 0,0:37:07.85,0:37:10.83,Default,,0,0,0,,- Anh chàng đó tuyệt thật. Phải chứ?\N- Blake Hughes.
Dialogue: 0,0:37:11.03,0:37:15.56,Default,,0,0,0,,Đó là Blake Hughes sao?\NÔi, Chúa ơi. Đó là bố cháu.
Dialogue: 0,0:37:22.57,0:37:24.43,Default,,0,0,0,,- Blake Hughes là bố cháu?\N- Vâng.
Dialogue: 0,0:37:24.63,0:37:25.78,Default,,0,0,0,,Còn Pam là mẹ cháu?
Dialogue: 0,0:37:30.12,0:37:31.44,Default,,0,0,0,,Blake đang cặp Tiffany.
Dialogue: 0,0:37:31.64,0:37:33.69,Default,,0,0,0,,Họ đã hẹn hò kể từ Spring Fling.
Dialogue: 0,0:37:33.89,0:37:37.46,Default,,0,0,0,,Tôi khá chắc là họ cắm sừng nhau.\NCả nhóm đó đều hứng tình.
Dialogue: 0,0:37:38.84,0:37:39.82,Default,,0,0,0,,Ôi, Chúa ơi!
Dialogue: 0,0:37:40.02,0:37:42.91,Default,,0,0,0,,- Ừ. Họ đấy. Nhóm Molly.\N- Mừng tuổi 16 ngọt ngào.
Dialogue: 0,0:37:43.11,0:37:45.95,Default,,0,0,0,,Tất cả đều ăn mặc\Nnhư một phiên bản Molly Ringwald.
Dialogue: 0,0:37:46.15,0:37:47.34,Default,,0,0,0,,Cậu làm được rồi.
Dialogue: 0,0:37:48.76,0:37:54.27,Default,,0,0,0,,Được rồi. Cháu ở đây để ngăn chặn án mạng.\NVậy nên, ngăn chặn án mạng thôi.
Dialogue: 0,0:38:01.53,0:38:05.53,Default,,0,0,0,,- Tiệc vui lắm, các cô.\N- Cậu làm gì ở đây?
Dialogue: 0,0:38:06.82,0:38:09.06,Default,,0,0,0,,Biết điều gì\Nlàm bữa tiệc này vui hơn không?
Dialogue: 0,0:38:09.26,0:38:11.14,Default,,0,0,0,,Cảnh sát. Gọi họ thôi.
Dialogue: 0,0:38:11.34,0:38:12.52,Default,,0,0,0,,Tôi thích Sting.
Dialogue: 0,0:38:12.72,0:38:14.73,Default,,0,0,0,,Ý hay đấy.
Dialogue: 0,0:38:14.93,0:38:17.92,Default,,0,0,0,,Rồi họ có thể chơi bài\N{\i1}Đừng đứng gần tôi quá.{\i0}
Dialogue: 0,0:38:19.75,0:38:20.57,Default,,0,0,0,,- Cảm ơn.\N- Này,
Dialogue: 0,0:38:20.77,0:38:23.20,Default,,0,0,0,,cái kiểu cô gái xấu tính này\Nlỗi thời lắm rồi.
Dialogue: 0,0:38:23.40,0:38:25.66,Default,,0,0,0,,Phụ nữ nên nâng đỡ nhau.
Dialogue: 0,0:38:25.86,0:38:28.91,Default,,0,0,0,,Thế tôi nâng hai ngón này lên,\Nvà cậu biến đi thì sao.
Dialogue: 0,0:38:29.11,0:38:32.33,Default,,0,0,0,,Được rồi. Các cậu,\Ncười lên vì kỷ yếu nào. Đúng thế!
Dialogue: 0,0:38:32.53,0:38:34.08,Default,,0,0,0,,Đừng chụp, Đồ-Khốn-Basage.
Dialogue: 0,0:38:34.28,0:38:37.06,Default,,0,0,0,,Chỗ nào cũng có bia,\Nmuốn Tiff bị phạt cả đời à?
Dialogue: 0,0:38:39.31,0:38:43.82,Default,,0,0,0,,- Chụp nữa là tôi đập máy ảnh cậu đấy.\N- Đâu cần phải gắt thế.
Dialogue: 0,0:38:45.49,0:38:48.16,Default,,0,0,0,,Randy, làm giúp tớ một việc.
Dialogue: 0,0:38:50.07,0:38:53.49,Default,,0,0,0,,Này! Này, gì vậy? Làm trò gì thế?
Dialogue: 0,0:39:04.59,0:39:07.09,Default,,0,0,0,,Đó là người thật à?
Dialogue: 0,0:39:07.63,0:39:09.59,Default,,0,0,0,,Tôi đã lo cậu quên cho tôi vào.
Dialogue: 0,0:39:10.01,0:39:12.14,Default,,0,0,0,,Mừng khi thấy cậu cũng bị bắt nạt.
Dialogue: 0,0:39:17.31,0:39:21.05,Default,,0,0,0,,Cô sẽ thành người khốn khổ,\Nkhông biết hạnh phúc thực sự là gì,
Dialogue: 0,0:39:21.25,0:39:22.92,Default,,0,0,0,,và có một mái tóc tầm thường
Dialogue: 0,0:39:23.12,0:39:25.44,Default,,0,0,0,,khiến đầu cô trông như quả bóng bàn.
Dialogue: 0,0:39:26.78,0:39:29.57,Default,,0,0,0,,Nó cụ thể một cách kỳ lạ.
Dialogue: 0,0:39:34.08,0:39:35.70,Default,,0,0,0,,Xin lỗi.
Dialogue: 0,0:39:36.83,0:39:40.42,Default,,0,0,0,,Blakey, đừng. Làm ơn, làm ơn đừng làm thế!
Dialogue: 0,0:39:40.79,0:39:41.57,Default,,0,0,0,,Sao vậy?
Dialogue: 0,0:39:41.77,0:39:44.53,Default,,0,0,0,,Đứa khốn nào đó mách anh ấy\Nlà tôi hôn Eddie Royal.
Dialogue: 0,0:39:44.73,0:39:47.49,Default,,0,0,0,,- Cô hôn Eddie Royal?\N- Ừ.
Dialogue: 0,0:39:47.69,0:39:51.72,Default,,0,0,0,,- Giọng ca của Bản Năng Sát Thủ?\N- Ở Canada cũng biết nhạc của anh ấy?
Dialogue: 0,0:39:52.47,0:39:55.18,Default,,0,0,0,,Anh ấy nổi tiếng à? Eddie!
Dialogue: 0,0:39:56.68,0:39:58.96,Default,,0,0,0,,- Khoan, Tiffany.\N- Biết cô ta nói gì không?
Dialogue: 0,0:39:59.16,0:40:01.67,Default,,0,0,0,,- Cô ta hôn Eddie.\N- Có vẻ lại chia tay rồi.
Dialogue: 0,0:40:01.87,0:40:03.52,Default,,0,0,0,,- Làm trò quá.\N- Tôi cần uống.
Dialogue: 0,0:40:04.31,0:40:06.27,Default,,0,0,0,,Tiffany?
Dialogue: 0,0:40:07.40,0:40:09.43,Default,,0,0,0,,Tiffany bị khủng hoảng rồi.
Dialogue: 0,0:40:09.63,0:40:12.35,Default,,0,0,0,,Đã đến lúc làm bạn bè tốt\Nvà ở cùng cô ấy cả tối.
Dialogue: 0,0:40:12.55,0:40:14.12,Default,,0,0,0,,Đừng bỏ cô ấy một mình.
Dialogue: 0,0:40:15.20,0:40:16.20,Default,,0,0,0,,Randy!
Dialogue: 0,0:40:18.75,0:40:20.25,Default,,0,0,0,,- Nào, đi thôi.\N- Cái gì?
Dialogue: 0,0:40:24.00,0:40:25.21,Default,,0,0,0,,Sao cậu làm được thế?
Dialogue: 0,0:40:27.38,0:40:29.17,Default,,0,0,0,,Mẹ tôi dạy tôi tự vệ.
Dialogue: 0,0:40:32.30,0:40:34.51,Default,,0,0,0,,Tiffany.
Dialogue: 0,0:40:43.27,0:40:46.22,Default,,0,0,0,,Eddie, lâu nay lẽ ra em nên yêu anh.
Dialogue: 0,0:40:46.41,0:40:48.15,Default,,0,0,0,,May là Jamie giúp em nhận ra.
Dialogue: 0,0:40:49.07,0:40:51.18,Default,,0,0,0,,Ở trong phòng bố mẹ em có sao không?
Dialogue: 0,0:40:51.38,0:40:54.64,Default,,0,0,0,,Em biết, nhưng nghe nói\NZack Ritrovatti và Valerie Mosco
Dialogue: 0,0:40:54.84,0:40:56.81,Default,,0,0,0,,đang mây mưa trong phòng của em.
Dialogue: 0,0:40:57.01,0:40:59.74,Default,,0,0,0,,Hơn nữa, em luôn muốn\Nquan hệ trên giường nước.
Dialogue: 0,0:41:01.12,0:41:04.67,Default,,0,0,0,,Khoan. Em phải đi tè. Uống BJ nhiều quá.
Dialogue: 0,0:41:05.96,0:41:07.92,Default,,0,0,0,,Mong thổi kèn lần nữa không sao.
Dialogue: 0,0:41:09.67,0:41:12.22,Default,,0,0,0,,Không thổi kèn đâu.\NCác anh tè từ chỗ đó mà.
Dialogue: 0,0:41:13.30,0:41:17.10,Default,,0,0,0,,Đừng đi đâu cả.\NEm sẽ quay lại ngay, ngôi sao nhạc rock ạ.
Dialogue: 0,0:41:21.47,0:41:22.71,Default,,0,0,0,,Mẹ kiếp.
Dialogue: 0,0:41:22.91,0:41:23.85,Default,,0,0,0,,Tiffany!
Dialogue: 0,0:41:28.69,0:41:29.55,Default,,0,0,0,,{\an8}ÁN MẠNG
Dialogue: 0,0:41:29.75,0:41:30.94,Default,,0,0,0,,{\an8}Còn 58%. Khỉ thật!
Dialogue: 0,0:41:31.53,0:41:32.76,Default,,0,0,0,,VỤ 214: TIFFANY CLARK
Dialogue: 0,0:41:32.96,0:41:36.91,Default,,0,0,0,,{\i1}Thi thể Tiffany ở trong nhà xe,{\i0}\N{\i1}thời điểm tử vong, ngay sau nửa đêm.{\i0}
Dialogue: 0,0:41:39.16,0:41:40.79,Default,,0,0,0,,Cô đến từ tương lai à?
Dialogue: 0,0:41:41.58,0:41:43.37,Default,,0,0,0,,Phải. Nhà xe ở đâu?
Dialogue: 0,0:41:45.96,0:41:47.04,Default,,0,0,0,,Cảm ơn.
Dialogue: 0,0:41:56.72,0:41:58.55,Default,,0,0,0,,Anh ấy sẽ viết bài hát về nó.
Dialogue: 0,0:42:05.98,0:42:07.10,Default,,0,0,0,,Eddie?
Dialogue: 0,0:42:11.23,0:42:12.44,Default,,0,0,0,,Anh đang trốn à?
Dialogue: 0,0:42:13.36,0:42:16.86,Default,,0,0,0,,Tiffany?
Dialogue: 0,0:42:18.82,0:42:20.24,Default,,0,0,0,,Vì em sẽ tìm được anh…
Dialogue: 0,0:42:23.33,0:42:24.33,Default,,0,0,0,,Tiffany?
Dialogue: 0,0:42:27.75,0:42:30.34,Default,,0,0,0,,- Anh bảo sẽ khóa cửa mà!\N- Anh khóa rồi.
Dialogue: 0,0:42:32.34,0:42:35.92,Default,,0,0,0,,Khoan. Mình không theo đuổi lũ con trai.
Dialogue: 0,0:42:37.76,0:42:39.05,Default,,0,0,0,,Họ theo đuổi mình chứ.
Dialogue: 0,0:42:40.47,0:42:43.85,Default,,0,0,0,,- Khoan, Tiffany lẽ ra phải ở đây.\N- Có lịch à?
Dialogue: 0,0:42:45.06,0:42:46.06,Default,,0,0,0,,Mình đã thay đổi nó.
Dialogue: 0,0:42:47.35,0:42:49.65,Default,,0,0,0,,Chúa ơi, Tiffany đâu? Tiffany?
Dialogue: 0,0:42:50.48,0:42:53.78,Default,,0,0,0,,Ra đây nào, Eddie. Nước ấm lắm.
Dialogue: 0,0:43:06.75,0:43:08.58,Default,,0,0,0,,Cái mặt nạ đó là sao?
Dialogue: 0,0:43:12.88,0:43:14.25,Default,,0,0,0,,Anh sẽ tháo nó ra chứ?
Dialogue: 0,0:43:17.51,0:43:20.64,Default,,0,0,0,,Nếu muốn làm tình với em\Nthì anh phải tháo.
Dialogue: 0,0:43:23.10,0:43:25.18,Default,,0,0,0,,Em không thích trò kỳ cục này đâu.
Dialogue: 0,0:43:26.22,0:43:27.85,Default,,0,0,0,,Eddie, anh làm quái gì thế?
Dialogue: 0,0:43:33.02,0:43:34.07,Default,,0,0,0,,Cái quái gì thế?
Dialogue: 0,0:43:40.86,0:43:42.45,Default,,0,0,0,,PHÒNG NGỦ PHỤ HUYNH\NCẤM VÀO!
Dialogue: 0,0:43:58.09,0:44:01.22,Default,,0,0,0,,Tiffany?
Dialogue: 0,0:44:09.81,0:44:11.02,Default,,0,0,0,,Tiffany?
Dialogue: 0,0:44:15.23,0:44:16.15,Default,,0,0,0,,Cô không được…
Dialogue: 0,0:44:17.53,0:44:18.53,Default,,0,0,0,,Đừng để hắn thoát.
Dialogue: 0,0:44:56.27,0:44:58.63,Default,,0,0,0,,Có rắc rối là luôn có mặt Blake Hughes.
Dialogue: 0,0:44:58.83,0:45:02.49,Default,,0,0,0,,Dennis, tha cháu đi.\NCháu đã nói cho Ponch những gì cháu biết.
Dialogue: 0,0:45:04.16,0:45:06.56,Default,,0,0,0,,Còn cậu này, muốn thẩm vấn cậu ta không?
Dialogue: 0,0:45:06.76,0:45:09.91,Default,,0,0,0,,Không, để cậu ta đi.\NCậu ta là bạn của con gái tôi.
Dialogue: 0,0:45:10.29,0:45:12.50,Default,,0,0,0,,- Cảm ơn bố.\N- Không có gì, con yêu.
Dialogue: 0,0:45:16.42,0:45:19.03,Default,,0,0,0,,Chà, xem ai này.\NNgười bạn Canada mới của ta.
Dialogue: 0,0:45:19.23,0:45:22.24,Default,,0,0,0,,Các chú đã có thể ngăn việc này.\NCháu đã cố cảnh báo.
Dialogue: 0,0:45:22.44,0:45:25.54,Default,,0,0,0,,- Và cháu ở đâu khi nó xảy ra?\N- Các chú nghi ngờ cháu?
Dialogue: 0,0:45:25.74,0:45:28.25,Default,,0,0,0,,Chưa từng có án mạng\Nđến khi cháu xuất hiện.
Dialogue: 0,0:45:28.45,0:45:32.04,Default,,0,0,0,,- Cháu có vẻ biết quá nhiều về nó.\N- Cháu đã cố ngăn nó!
Dialogue: 0,0:45:32.24,0:45:37.31,Default,,0,0,0,,Cậu ấy nói sự thật, thưa Cảnh sát trưởng.\NCậu ấy chỉ muốn bảo vệ Tiffany.
Dialogue: 0,0:45:42.11,0:45:45.11,Default,,0,0,0,,Đây là hiện trường thực sự.\NKhông ai được đi đâu.
Dialogue: 0,0:45:48.62,0:45:50.12,Default,,0,0,0,,Ra khỏi đây thôi.
Dialogue: 0,0:45:53.58,0:45:55.17,Default,,0,0,0,,Chờ đã, mẹ. Pam!
Dialogue: 0,0:45:57.63,0:46:01.30,Default,,0,0,0,,Pam! Cô không thể về nhà một mình,\Ncó kẻ sát nhân đang tự do.
Dialogue: 0,0:46:01.84,0:46:03.21,Default,,0,0,0,,Nhà tôi đâu có xa.
Dialogue: 0,0:46:08.64,0:46:10.31,Default,,0,0,0,,Rất tiếc về bạn của cô.
Dialogue: 0,0:46:12.10,0:46:14.23,Default,,0,0,0,,Tôi cũng không thực sự quý cậu ấy.
Dialogue: 0,0:46:14.77,0:46:18.05,Default,,0,0,0,,Cậu ấy quá xấu tính.\NVà còn không hài hước, như kiểu…
Dialogue: 0,0:46:18.25,0:46:21.47,Default,,0,0,0,,Như khi tôi thuyết phục Lisa Vitzlaki\Nrằng Randy yêu cậu ấy,
Dialogue: 0,0:46:21.67,0:46:23.84,Default,,0,0,0,,nên cậu ấy đã hát {\i1}Làm em ngạt thở.{\i0}
Dialogue: 0,0:46:24.04,0:46:25.99,Default,,0,0,0,,- Có vui đâu.\N- Bọn tôi thấy hài.
Dialogue: 0,0:46:27.24,0:46:28.49,Default,,0,0,0,,Và Tiffany thích nó.
Dialogue: 0,0:46:29.95,0:46:33.08,Default,,0,0,0,,Cậu ấy không phải đứa tuyệt vời\Nnhưng vẫn là bạn tôi.
Dialogue: 0,0:46:35.41,0:46:37.33,Default,,0,0,0,,Sao cô biết cậu ấy gặp nguy hiểm?
Dialogue: 0,0:46:41.75,0:46:42.75,Default,,0,0,0,,Tôi là thầy bói.
Dialogue: 0,0:46:44.17,0:46:47.88,Default,,0,0,0,,Và tôi tiên đoán được rằng\NTiffany sẽ bị giết tối nay.
Dialogue: 0,0:46:49.97,0:46:51.39,Default,,0,0,0,,Tôi đã cố ngăn điều đó.
Dialogue: 0,0:46:53.22,0:46:55.43,Default,,0,0,0,,Nhưng có vẻ tôi đã không làm được.
Dialogue: 0,0:46:57.73,0:47:02.02,Default,,0,0,0,,Nhưng có lẽ với sự giúp đỡ của cô,\Nta có thể ngăn các vụ còn lại.
Dialogue: 0,0:47:03.69,0:47:04.82,Default,,0,0,0,,Các vụ còn lại?
Dialogue: 0,0:47:06.32,0:47:07.76,Default,,0,0,0,,Sẽ có thêm nữa ư?
Dialogue: 0,0:47:07.96,0:47:09.95,Default,,0,0,0,,Marisa và Heather.
Dialogue: 0,0:47:12.66,0:47:15.08,Default,,0,0,0,,Cả cô nữa. Tất cả các cô đều gặp nguy.
Dialogue: 0,0:47:19.75,0:47:21.25,Default,,0,0,0,,Cô thấy đấy, đây là…
Dialogue: 0,0:47:22.54,0:47:23.71,Default,,0,0,0,,Đây là…
Dialogue: 0,0:47:25.30,0:47:26.71,Default,,0,0,0,,Đá tâm linh.
Dialogue: 0,0:47:38.43,0:47:39.48,Default,,0,0,0,,Trông đúng đấy.
Dialogue: 0,0:47:42.90,0:47:47.65,Default,,0,0,0,,- Cô có thể nhìn bao xa trong tương lai?\N- Cho đến năm 2023.
Dialogue: 0,0:47:50.15,0:47:55.16,Default,,0,0,0,,{\an8}{\i1}Các người đang vi phạm trực tiếp{\i0}\N{\i1}Điều 113, Mục 9 của Bộ luật Hình sự.{\i0}
Dialogue: 0,0:47:55.62,0:47:59.02,Default,,0,0,0,,{\i1}- Giờ các người có năm giây để tuân thủ.{\i0}\N{\i1}- Cứu! Cứu với!{\i0}
Dialogue: 0,0:47:59.22,0:48:02.67,Default,,0,0,0,,{\i1}Năm, bốn, ba, hai, một.{\i0}
Dialogue: 0,0:48:02.96,0:48:05.92,Default,,0,0,0,,{\i1}Giờ tôi đã có quyền sử dụng vũ lực.{\i0}
Dialogue: 0,0:48:20.02,0:48:21.69,Default,,0,0,0,,Máy móc có giết ta không?
Dialogue: 0,0:48:23.94,0:48:27.61,Default,,0,0,0,,Không. Máy móc không giết hết chúng ta.
Dialogue: 0,0:48:28.40,0:48:32.01,Default,,0,0,0,,Chúng chỉ xé toạc cơ cấu xã hội
Dialogue: 0,0:48:32.21,0:48:35.87,Default,,0,0,0,,- qua các điệu nhảy trên TikTok.\N- Chúng dùng điệu nhảy?
Dialogue: 0,0:48:36.95,0:48:39.27,Default,,0,0,0,,Không ngờ Pam Miller\Ncuồng khoa học viễn tưởng.
Dialogue: 0,0:48:39.47,0:48:42.23,Default,,0,0,0,,- Cậu hé răng là tôi giết cậu.\N- Được rồi.
Dialogue: 0,0:48:42.43,0:48:44.83,Default,,0,0,0,,Tập trung nào. Tôi…
Dialogue: 0,0:48:49.17,0:48:54.64,Default,,0,0,0,,Tôi tiên đoán được rằng\NMarisa sẽ là nạn nhân tiếp theo.
Dialogue: 0,0:48:55.47,0:48:56.41,Default,,0,0,0,,Ôi, không!
Dialogue: 0,0:48:56.61,0:48:59.21,Default,,0,0,0,,Và nó sẽ xảy ra vào ngày kia,
Dialogue: 0,0:48:59.41,0:49:02.59,Default,,0,0,0,,- tại một nhà gỗ trong rừng.\N- Ôi, Chúa ơi!
Dialogue: 0,0:49:02.79,0:49:08.19,Default,,0,0,0,,Pam, cô có nghĩ ra\Nai không ưa Nhóm Molly không?
Dialogue: 0,0:49:08.94,0:49:14.91,Default,,0,0,0,,Có lẽ cuộc đời ai đó đã bị các cô hủy hoại\Nhay để lại tổn thương tâm lý
Dialogue: 0,0:49:15.11,0:49:21.02,Default,,0,0,0,,khiến họ muốn săn lùng các cô\Nvà giết các cô theo kiểu nghi lễ nào đó?
Dialogue: 0,0:49:21.22,0:49:23.71,Default,,0,0,0,,Không. Không có ai cả.
Dialogue: 0,0:49:26.21,0:49:31.05,Default,,0,0,0,,Ý tôi là… Có lẽ là Alvin chăng? Bọn tôi\Ntừng giễu cợt miếng che mắt của cậu ta.
Dialogue: 0,0:49:31.76,0:49:33.26,Default,,0,0,0,,Và Wendy.
Dialogue: 0,0:49:34.43,0:49:40.17,Default,,0,0,0,,Và Tyler, và Amy Wolf.\NRon Biến Thái. Trish Béo.
Dialogue: 0,0:49:40.36,0:49:45.46,Default,,0,0,0,,Tôi đoán là Lisa Vitzlaki, vì ta đã nói\Nrằng việc hát bài kia là không hài hước.
Dialogue: 0,0:49:45.66,0:49:49.26,Default,,0,0,0,,Và Jenny Hudson với Jenny Grisante.
Dialogue: 0,0:49:49.46,0:49:52.47,Default,,0,0,0,,Mấy đứa ơi,\Ntình hình thế nào, các con cần gì…
Dialogue: 0,0:49:52.67,0:49:55.82,Default,,0,0,0,,Chúa ơi, mẹ, bọn con ổn! Bọn con ổn!
Dialogue: 0,0:49:57.16,0:50:00.10,Default,,0,0,0,,- Ừ, mẹ biết các con đã trải qua nhiều…\N- Đúng rồi.
Dialogue: 0,0:50:00.30,0:50:04.87,Default,,0,0,0,,Và mẹ đang khiến chuyện tệ hơn đấy!\NMẹ đi đi được không? Kệ bọn con!
Dialogue: 0,0:50:08.71,0:50:11.05,Default,,0,0,0,,Bảo sao bà không bao giờ đến thăm.
Dialogue: 0,0:50:12.38,0:50:17.18,Default,,0,0,0,,Cô không nên nói với mẹ kiểu đó.\NCô không bao giờ biết…
Dialogue: 0,0:50:19.76,0:50:21.10,Default,,0,0,0,,Bà ấy chỉ quan tâm cô.
Dialogue: 0,0:50:22.31,0:50:25.64,Default,,0,0,0,,Thì sao, cậu có\Nmối quan hệ tốt đẹp với mẹ ư?
Dialogue: 0,0:50:26.31,0:50:29.02,Default,,0,0,0,,Không. Nhưng giá mà tôi có.
Dialogue: 0,0:50:30.27,0:50:32.82,Default,,0,0,0,,Và giá như tôi được nói yêu bà nhiều hơn.
Dialogue: 0,0:50:34.32,0:50:38.61,Default,,0,0,0,,- Tôi chỉ không hiểu thứ bà ấy…\N- Này. Tôi chia sẻ thân tình như bạn bè.
Dialogue: 0,0:50:39.20,0:50:42.31,Default,,0,0,0,,Chả ai muốn nghe cậu nói\Nyêu mẹ ra sao đâu, được chứ?
Dialogue: 0,0:50:42.51,0:50:44.23,Default,,0,0,0,,Được rồi, tôi xin lỗi.
Dialogue: 0,0:50:44.43,0:50:48.32,Default,,0,0,0,,Còn Randy, có thể là nghi phạm không?\NĐó là thằng bị thịt, to xác.
Dialogue: 0,0:50:48.52,0:50:52.88,Default,,0,0,0,,Không. Randy yêu Tiffany. Cậu ta\Nđã mời cậu ấy đi Spring Fling năm ngoái.
Dialogue: 0,0:50:53.96,0:50:57.47,Default,,0,0,0,,Cậu ta còn mời cả Marisa.\NVà Heather. Và tôi nữa.
Dialogue: 0,0:50:58.47,0:51:00.75,Default,,0,0,0,,Tôi hiểu được lý do cậu ta giận bọn tôi.
Dialogue: 0,0:51:00.95,0:51:03.71,Default,,0,0,0,,Cậu ta đâu thể giận\Nvì bị từ chối, thế điên quá.
Dialogue: 0,0:51:03.91,0:51:05.50,Default,,0,0,0,,Tôi nên hẹn hò cậu ta không?
Dialogue: 0,0:51:05.70,0:51:08.71,Default,,0,0,0,,Đề phòng cậu ta\Nlà kẻ sát nhân và sẽ không giết tôi?
Dialogue: 0,0:51:08.91,0:51:11.67,Default,,0,0,0,,Không! Đừng yêu Randy!\NĐó không phải người cô sẽ kết đôi.
Dialogue: 0,0:51:11.87,0:51:13.86,Default,,0,0,0,,- Vậy tôi sẽ kết đôi với ai?\N- Blake!
Dialogue: 0,0:51:16.36,0:51:20.53,Default,,0,0,0,,Thật sao? Blake! Ôi, Chúa ơi!
Dialogue: 0,0:51:23.16,0:51:26.02,Default,,0,0,0,,Cậu biết không,\Ntôi đã luôn thầm yêu Blake.
Dialogue: 0,0:51:26.22,0:51:27.69,Default,,0,0,0,,Tôi không thể phản bội Tiffany.
Dialogue: 0,0:51:27.89,0:51:29.69,Default,,0,0,0,,Tốt, bởi vì cô và Blake
Dialogue: 0,0:51:29.89,0:51:33.65,Default,,0,0,0,,sẽ không yêu nhau\Nmột thời gian, sau nhiều năm nữa.
Dialogue: 0,0:51:33.85,0:51:36.49,Default,,0,0,0,,Nếu bọn tôi là của nhau\Nvà thời điểm không quan trọng,
Dialogue: 0,0:51:36.69,0:51:41.12,Default,,0,0,0,,và Tiffany đã chết…\NTôi sẽ được làm tình với Blake!
Dialogue: 0,0:51:41.32,0:51:43.20,Default,,0,0,0,,Không, hai người quá hứng tình
Dialogue: 0,0:51:43.40,0:51:45.79,Default,,0,0,0,,và sẽ cắm sừng nhau rồi làm hỏng chuyện.
Dialogue: 0,0:51:45.99,0:51:48.04,Default,,0,0,0,,Cả hai sẽ không cùng chơi Cú Rơi Lượng Tử,
Dialogue: 0,0:51:48.24,0:51:50.59,Default,,0,0,0,,nơi tìm ra một điều thực sự quan trọng.
Dialogue: 0,0:51:50.79,0:51:53.48,Default,,0,0,0,,Không được ngủ với Blake\Nđến khi học đại học.
Dialogue: 0,0:51:54.48,0:51:57.14,Default,,0,0,0,,Được rồi, tuyệt.\NVậy hai cậu tìm kẻ sát nhân,
Dialogue: 0,0:51:57.33,0:52:00.07,Default,,0,0,0,,còn tôi sẽ sửa máy thời… Báo thời gian.
Dialogue: 0,0:52:01.24,0:52:03.78,Default,,0,0,0,,Máy báo thời gian.\NTên mỹ miều của đồng hồ.
Dialogue: 0,0:52:21.13,0:52:23.85,Default,,0,0,0,,Vậy đó là Cỗ Máy Thời Gian 2.0?
Dialogue: 0,0:52:26.26,0:52:30.35,Default,,0,0,0,,Không, đây chỉ là một đống rác rẻ tiền\Ncháu mua từ Craigslist.
Dialogue: 0,0:52:33.35,0:52:34.77,Default,,0,0,0,,Chú làm gì ở đây?
Dialogue: 0,0:52:35.11,0:52:38.19,Default,,0,0,0,,Chú biết mọi thứ về mấy vụ án mạng này.
Dialogue: 0,0:52:38.94,0:52:42.01,Default,,0,0,0,,Song mấy ngày trước,\Nchú nói Tiffany bị giết ở nhà xe.
Dialogue: 0,0:52:42.21,0:52:43.52,Default,,0,0,0,,Nhưng không phải.
Dialogue: 0,0:52:43.71,0:52:46.68,Default,,0,0,0,,Cô ấy bị giết trên giường nước\Ncủa bố mẹ. Tình tiết cơ bản.
Dialogue: 0,0:52:46.88,0:52:49.95,Default,,0,0,0,,- Sao chú có thể quên điều đó?\N- Hiệu ứng Mandela.
Dialogue: 0,0:52:51.46,0:52:52.94,Default,,0,0,0,,Đó là hiện tượng tâm lý
Dialogue: 0,0:52:53.14,0:52:55.57,Default,,0,0,0,,trong đó ta nhớ\Nnhững điều không hề xảy ra.
Dialogue: 0,0:52:55.77,0:52:58.20,Default,,0,0,0,,Như việc họ nghĩ\NNelson Mandela đã chết trong tù.
Dialogue: 0,0:52:58.40,0:53:01.20,Default,,0,0,0,,Nhưng có giả thuyết rằng\Nnhững ký ức đó không sai,
Dialogue: 0,0:53:01.40,0:53:06.76,Default,,0,0,0,,chúng chỉ là thứ vương lại từ một quá khứ\Nđã bị thay đổi qua du hành thời gian.
Dialogue: 0,0:53:08.89,0:53:10.81,Default,,0,0,0,,Này, khoan đã, đó là gì vậy?
Dialogue: 0,0:53:11.35,0:53:12.42,Default,,0,0,0,,"Đâm cỗ máy".
Dialogue: 0,0:53:12.62,0:53:16.13,Default,,0,0,0,,Cảnh sát nghĩ đó chỉ là\Nlời bài hát heavy metal nào đó.
Dialogue: 0,0:53:16.33,0:53:18.48,Default,,0,0,0,,Dẫn tới việc họ điều tra Eddie Royal.
Dialogue: 0,0:53:19.36,0:53:20.93,Default,,0,0,0,,- Eddie Royal?\N- Phải.
Dialogue: 0,0:53:21.13,0:53:23.20,Default,,0,0,0,,Giọng ca của {\i1}Cách Một Giường Nước?{\i0}
Dialogue: 0,0:53:25.24,0:53:27.53,Default,,0,0,0,,Sao họ nghĩ thế? Chú ấy siêu trầm mà.
Dialogue: 0,0:53:28.08,0:53:30.79,Default,,0,0,0,,Lạ thật. Chú cứ tưởng…
Dialogue: 0,0:53:31.54,0:53:33.23,Default,,0,0,0,,Chắc đó là hiệu ứng Mandela.
Dialogue: 0,0:53:33.43,0:53:36.13,Default,,0,0,0,,- Chữ J ở cuối là gì vậy?\N- Đó là chữ J à?
Dialogue: 0,0:53:36.79,0:53:38.67,Default,,0,0,0,,Ai cũng nghĩ là cổ tự của quỷ.
Dialogue: 0,0:53:39.34,0:53:41.16,Default,,0,0,0,,Chú còn ảnh của tối hôm đó chứ?
Dialogue: 0,0:53:41.36,0:53:43.99,Default,,0,0,0,,Ừ, ảnh hiện trường chính thức
Dialogue: 0,0:53:44.19,0:53:46.39,Default,,0,0,0,,và ảnh chú chụp ở bữa tiệc\Ncho cuốn kỷ yếu.
Dialogue: 0,0:53:50.64,0:53:52.73,Default,,0,0,0,,- Cậu ấy kia, đúng rồi!\N- Trời đất!
Dialogue: 0,0:53:53.85,0:53:56.38,Default,,0,0,0,,Ôi, Chúa ơi! Jamie đã trở về năm 1987.
Dialogue: 0,0:53:56.58,0:54:01.63,Default,,0,0,0,,Mảnh giấy là thông điệp cho cháu.\N"Đâm cỗ máy. Đâm cỗ máy".
Dialogue: 0,0:54:01.83,0:54:04.43,Default,,0,0,0,,Chắc dao của kẻ sát nhân\Nđã đâm vào chất dẫn
Dialogue: 0,0:54:04.63,0:54:07.56,Default,,0,0,0,,và Jamie muốn bảo cháu rằng\Ncần thêm kim loại dẫn!
Dialogue: 0,0:54:07.76,0:54:09.81,Default,,0,0,0,,Có chắc không phải cổ tự của quỷ không?
Dialogue: 0,0:54:10.01,0:54:12.81,Default,,0,0,0,,Không phải cổ tự.\NJamie đang cố ngăn vụ án mạng.
Dialogue: 0,0:54:13.01,0:54:14.91,Default,,0,0,0,,Nhưng họ vẫn chết.
Dialogue: 0,0:54:15.79,0:54:19.88,Default,,0,0,0,,Phải, Tiffany chết, nhưng các vụ sát hại\NHeather và Marisa vẫn chưa xảy ra.
Dialogue: 0,0:54:20.88,0:54:25.42,Default,,0,0,0,,Được rồi, thời gian xảy ra cùng lúc,\Nnhư một dòng sông vậy. Đúng thế.
Dialogue: 0,0:54:28.30,0:54:29.37,Default,,0,0,0,,THỜI SỰ KZFD
Dialogue: 0,0:54:29.57,0:54:33.37,Default,,0,0,0,,Xin chào. Được biết tối qua\Ncảnh sát đã tạm giam Eddie Coburn,
Dialogue: 0,0:54:33.57,0:54:36.63,Default,,0,0,0,,còn được các học sinh\Ngọi là Eddie Royal, để thẩm vấn.
Dialogue: 0,0:54:36.83,0:54:41.57,Default,,0,0,0,,Sao họ lại nghĩ đó là Eddie?\NHọ đang phí thời gian, đâu phải chú ấy.
Dialogue: 0,0:54:42.15,0:54:45.14,Default,,0,0,0,,{\an8}Thảm kịch này\Nđã làm rung chuyển cộng đồng Vernon.
Dialogue: 0,0:54:45.34,0:54:48.99,Default,,0,0,0,,{\an8}Từ {\i1}Thời sự KZFD,{\i0} tôi là Norm Dubasage.
Dialogue: 0,0:54:55.25,0:54:58.40,Default,,0,0,0,,Nhìn kìa. Họ sẽ nhớ cô ấy. Hầu như là vậy.
Dialogue: 0,0:54:58.60,0:55:00.29,Default,,0,0,0,,{\an8}XUỐNG ĐỊA NGỤC ĐI TIFFANY!
Dialogue: 0,0:55:04.05,0:55:07.55,Default,,0,0,0,,Nếu cháu làm hỏng chuyện\Nbố mẹ cháu yêu nhau và kết hôn,
Dialogue: 0,0:55:08.30,0:55:10.01,Default,,0,0,0,,liệu cháu có biến mất không?
Dialogue: 0,0:55:11.14,0:55:14.04,Default,,0,0,0,,{\i1}Back to the Future{\i0} sai ở chỗ đó.\NCháu sẽ không biến mất.
Dialogue: 0,0:55:14.24,0:55:15.63,Default,,0,0,0,,Đâu phải phép thuật chứ.
Dialogue: 0,0:55:15.82,0:55:17.75,Default,,0,0,0,,Nếu bố mẹ cháu không kết hôn và có con,
Dialogue: 0,0:55:17.95,0:55:20.25,Default,,0,0,0,,về cơ bản cháu sẽ không có nơi để trở về,
Dialogue: 0,0:55:20.45,0:55:23.88,Default,,0,0,0,,vì mọi thứ sẽ khác đi\Nvà sẽ không có ai biết cháu là ai.
Dialogue: 0,0:55:24.08,0:55:25.94,Default,,0,0,0,,- Tôi giữ chỗ ăn trưa nhé!\N- Tuyệt.
Dialogue: 0,0:55:32.53,0:55:34.87,Default,,0,0,0,,Chào. Mọi người sao rồi?
Dialogue: 0,0:55:35.91,0:55:37.66,Default,,0,0,0,,Cậu lại mặc áo khoác đó à?
Dialogue: 0,0:55:38.79,0:55:40.98,Default,,0,0,0,,Ở đây không có ai thật lòng sao?
Dialogue: 0,0:55:41.18,0:55:43.04,Default,,0,0,0,,Tôi thật lòng với cái này!
Dialogue: 0,0:55:45.25,0:55:47.78,Default,,0,0,0,,Tớ chắc họ sẽ đề cập\Nđến gì đó trong kỷ yếu.
Dialogue: 0,0:55:47.98,0:55:49.72,Default,,0,0,0,,Như năm ngoái khi Trish Béo chết.
Dialogue: 0,0:55:50.59,0:55:51.68,Default,,0,0,0,,Tôi thích áo của cậu.
Dialogue: 0,0:55:55.01,0:55:58.79,Default,,0,0,0,,Trish Béo là ai?\NHơn nữa, không được gọi cô ấy thế.
Dialogue: 0,0:55:58.99,0:56:00.90,Default,,0,0,0,,Ừ, không thế nữa. Cậu ấy chết rồi.
Dialogue: 0,0:56:01.60,0:56:03.84,Default,,0,0,0,,- Sao cô ấy chết?\N- Tai nạn xe hơi.
Dialogue: 0,0:56:04.04,0:56:05.84,Default,,0,0,0,,Họ nói nồng độ cồn của cậu ta quá cao
Dialogue: 0,0:56:06.04,0:56:09.22,Default,,0,0,0,,- và xe quấn quanh cây.\N- Vì vậy con gái không nên lái xe.
Dialogue: 0,0:56:09.42,0:56:13.22,Default,,0,0,0,,Không, Randy, đó là lý do\Nngười ta không nên lái xe khi say.
Dialogue: 0,0:56:13.42,0:56:15.98,Default,,0,0,0,,- Tôi lái xe tốt hơn khi say.\N- Không hề.
Dialogue: 0,0:56:16.18,0:56:18.60,Default,,0,0,0,,Tôi đảm bảo luôn, không phải thế.
Dialogue: 0,0:56:18.80,0:56:20.79,Default,,0,0,0,,Đó là điều con gái sẽ nói.
Dialogue: 0,0:56:21.58,0:56:22.88,Default,,0,0,0,,Blake, đợi đã!
Dialogue: 0,0:56:39.14,0:56:41.13,Default,,0,0,0,,- Cậu vẽ cái đó à?\N- Ừ.
Dialogue: 0,0:56:41.33,0:56:44.63,Default,,0,0,0,,Nó đẹp quá. Cậu đúng là họa sĩ tài năng.
Dialogue: 0,0:56:44.83,0:56:48.05,Default,,0,0,0,,Không biết nữa,\Ncó lẽ tớ nên hủy bữa tiệc của mình.
Dialogue: 0,0:56:48.25,0:56:51.22,Default,,0,0,0,,- Giờ tớ thấy mất ngon rồi.\N- Nhưng sinh nhật cậu mà!
Dialogue: 0,0:56:51.42,0:56:57.12,Default,,0,0,0,,Này, sao ta không đi chơi xa với nhau?\NChỉ ba chúng ta thôi.
Dialogue: 0,0:56:57.83,0:57:00.91,Default,,0,0,0,,Cả Pam nữa. Pam! Pam?
Dialogue: 0,0:57:03.25,0:57:04.90,Default,,0,0,0,,Ừ! Ừ, thế chắc vui lắm.
Dialogue: 0,0:57:05.10,0:57:07.65,Default,,0,0,0,,Ừ. Ta có thể vào thành phố chẳng hạn.
Dialogue: 0,0:57:07.85,0:57:10.53,Default,,0,0,0,,Một nơi đẹp đẽ, đông đúc,
Dialogue: 0,0:57:10.73,0:57:14.05,Default,,0,0,0,,nhiều ánh sáng,\Nkhông ở ngoại ô, không ở nhà gỗ.
Dialogue: 0,0:57:14.47,0:57:17.58,Default,,0,0,0,,Có thể sẽ vui nếu qua đêm\Nở căn hộ của bố mẹ tôi.
Dialogue: 0,0:57:17.78,0:57:23.00,Default,,0,0,0,,Phải! Đúng rồi! Tiệc sinh nhật\Nở căn hộ! Tôi thích vụ đó. Làm thôi!
Dialogue: 0,0:57:23.20,0:57:26.76,Default,,0,0,0,,Ngoài ra, tôi đã nghĩ,\Nmột cách hay để tưởng nhớ Tiffany
Dialogue: 0,0:57:26.96,0:57:31.01,Default,,0,0,0,,và đền đáp môi trường\Nsẽ là trồng một chiếc cây vinh danh cô ấy.
Dialogue: 0,0:57:31.21,0:57:32.55,Default,,0,0,0,,Tôi sẽ làm đơn kiến nghị,
Dialogue: 0,0:57:32.75,0:57:35.97,Default,,0,0,0,,nên nếu mọi người có thể viết tên mình ra
Dialogue: 0,0:57:36.17,0:57:40.48,Default,,0,0,0,,một cách rõ ràng ở đây bằng chữ in hoa,\Nđể tôi xem chữ viết tay mọi người.
Dialogue: 0,0:57:40.68,0:57:42.65,Default,,0,0,0,,- Tiffany ghét tự nhiên.\N- Dị ứng cây.
Dialogue: 0,0:57:42.85,0:57:44.58,Default,,0,0,0,,Đền đáp môi trường này này!
Dialogue: 0,0:57:46.04,0:57:47.79,Default,,0,0,0,,Không phải tay vừa sờ háng đâu.
Dialogue: 0,0:57:48.34,0:57:54.30,Default,,0,0,0,,Vậy là… Bi kịch\Nđã xảy ra và thật đáng buồn.
Dialogue: 0,0:57:55.09,0:57:59.37,Default,,0,0,0,,Nhưng ta không thể làm gì\Nngoại trừ sống tiếp.
Dialogue: 0,0:57:59.57,0:58:02.47,Default,,0,0,0,,Vì vậy các em hãy lấy cốc becher ra.
Dialogue: 0,0:58:04.10,0:58:06.10,Default,,0,0,0,,LaFleur, hãy bắt cặp với Damon.
Dialogue: 0,0:58:09.06,0:58:10.94,Default,,0,0,0,,Xin chào, cốc becher!
Dialogue: 0,0:58:12.94,0:58:17.16,Default,,0,0,0,,Cốc becher! Xin chào. Cảm ơn.
Dialogue: 0,0:58:22.29,0:58:23.79,Default,,0,0,0,,{\an8}BẮN VÀO ĐẦU BỌN NÓ\NTẠI SAO
Dialogue: 0,0:58:31.96,0:58:34.42,Default,,0,0,0,,Đến lúc mở sinh nhật ở căn hộ rồi!
Dialogue: 0,0:58:35.01,0:58:38.58,Default,,0,0,0,,Trong sinh nhật tôi,\Nta sẽ khoe hàng nếu có gã xe tải bấm còi.
Dialogue: 0,0:58:38.78,0:58:40.20,Default,,0,0,0,,Cậu nên biết,
Dialogue: 0,0:58:40.40,0:58:44.50,Default,,0,0,0,,căn hộ có một vài quy tắc.\NNếu tôi chỉ tay vào cậu, cậu sẽ phải uống.
Dialogue: 0,0:58:44.70,0:58:46.89,Default,,0,0,0,,Ta sẽ say quắc cần câu!
Dialogue: 0,0:58:47.77,0:58:49.34,Default,,0,0,0,,Damon bị sao thế?
Dialogue: 0,0:58:49.54,0:58:51.69,Default,,0,0,0,,- Damon?\N- Cộng sự thí nghiệm của tôi.
Dialogue: 0,0:58:52.98,0:58:55.68,Default,,0,0,0,,- Ý cậu là Lurch.\N- Sao, cậu thích cậu ta à?
Dialogue: 0,0:58:55.88,0:58:57.33,Default,,0,0,0,,Chả ai bắt chuyện cậu ta.
Dialogue: 0,0:58:57.53,0:59:01.06,Default,,0,0,0,,Cậu ta ở trại cải tạo cả năm lớp tám\Nvì đánh nhau quá nhiều.
Dialogue: 0,0:59:01.26,0:59:04.23,Default,,0,0,0,,Ừ, và rồi cậu ta nghỉ\Ngần hết năm ngoái vì Trish Béo.
Dialogue: 0,0:59:04.43,0:59:06.77,Default,,0,0,0,,Cậu ta chắc là\Nhọc sinh già nhất lịch sử Mỹ.
Dialogue: 0,0:59:06.97,0:59:10.38,Default,,0,0,0,,"Vì Trish" là sao?
Dialogue: 0,0:59:10.92,0:59:12.04,Default,,0,0,0,,Đó là em gái cậu ta.
Dialogue: 0,0:59:13.09,0:59:17.07,Default,,0,0,0,,Được rồi. Sao cô không nhắc đến điều này\Nkhi nói về các nghi phạm?
Dialogue: 0,0:59:17.27,0:59:19.37,Default,,0,0,0,,Tôi có nói về Trish Béo mà.
Dialogue: 0,0:59:19.57,0:59:22.29,Default,,0,0,0,,Ừ, và cô không nói\Ncô ấy có người anh bạo lực!
Dialogue: 0,0:59:22.49,0:59:25.21,Default,,0,0,0,,Vậy thì sao? Bọn tôi có làm gì Lurch đâu.
Dialogue: 0,0:59:25.41,0:59:26.96,Default,,0,0,0,,Ừ, thực ra tớ thích Lurch.
Dialogue: 0,0:59:27.16,0:59:29.38,Default,,0,0,0,,Cậu ta làm\Nở Billy's Boardwalk. Từng thấy tớ
Dialogue: 0,0:59:29.58,0:59:31.80,Default,,0,0,0,,thổi kèn cho Reid DiMento\Nở Nhà Búp Bê Kinh Dị
Dialogue: 0,0:59:32.00,0:59:35.13,Default,,0,0,0,,- và cậu ta không mách lẻo.\N- Trời! Cậu thổi kèn ở đó?
Dialogue: 0,0:59:35.33,0:59:36.97,Default,,0,0,0,,- Ở đó có ai không?\N- Có thể.
Dialogue: 0,0:59:37.17,0:59:39.89,Default,,0,0,0,,Nhưng nếu vào Nhà Búp Bê Kinh Dị,
Dialogue: 0,0:59:40.09,0:59:43.20,Default,,0,0,0,,cậu chỉ có thể tự trách mình\Nnếu thấy thứ không muốn thấy.
Dialogue: 0,0:59:44.41,0:59:48.37,Default,,0,0,0,,Việc đó làm tớ nhớ Tiffany quá.\NCậu ấy rất ghét thổi kèn.
Dialogue: 0,0:59:49.12,0:59:50.62,Default,,0,0,0,,"Nước tiểu đi ra từ đó".
Dialogue: 0,0:59:55.88,0:59:57.03,Default,,0,0,0,,Cậu tới giờ ngủ à?
Dialogue: 0,0:59:57.23,0:59:59.13,Default,,0,0,0,,Gần đây không được ngủ mấy.
Dialogue: 0,1:00:12.77,1:00:14.94,Default,,0,0,0,,Đến nơi rồi!
Dialogue: 0,1:00:18.74,1:00:21.28,Default,,0,0,0,,Cái quái gì thế?
Dialogue: 0,1:00:28.04,1:00:30.23,Default,,0,0,0,,Cái quái gì thế? Cái gì đây?
Dialogue: 0,1:00:30.43,1:00:33.82,Default,,0,0,0,,- Căn hộ của bố mẹ tôi.\N- Không, đây đâu phải căn hộ!
Dialogue: 0,1:00:34.02,1:00:35.38,Default,,0,0,0,,Không phải ư?
Dialogue: 0,1:00:35.71,1:00:37.53,Default,,0,0,0,,Chắc tôi nghĩ đến nhà nghỉ dưỡng.
Dialogue: 0,1:00:37.73,1:00:41.74,Default,,0,0,0,,Đây là một căn nhà gỗ kinh dị\Nở nơi khỉ ho cò gáy!
Dialogue: 0,1:00:41.94,1:00:43.58,Default,,0,0,0,,Cô nói sẽ vào thành phố mà.
Dialogue: 0,1:00:43.78,1:00:46.91,Default,,0,0,0,,- Tôi có nói thế đâu.\N- Cô ám chỉ rõ ràng!
Dialogue: 0,1:00:47.11,1:00:49.79,Default,,0,0,0,,- Ôi, Chúa ơi, có sao đâu?\N- Heather, chuyện là
Dialogue: 0,1:00:49.99,1:00:53.80,Default,,0,0,0,,có một kẻ sát nhân ngoài vòng pháp luật\Nđang nhắm vào nhóm các cô,
Dialogue: 0,1:00:54.00,1:00:56.02,Default,,0,0,0,,và cô đưa chúng ta vào rừng!
Dialogue: 0,1:00:56.86,1:00:57.98,Default,,0,0,0,,Ôi, Chúa ơi!
Dialogue: 0,1:00:58.48,1:01:00.11,Default,,0,0,0,,Tớ quên mang vodka rồi.
Dialogue: 0,1:01:00.69,1:01:04.22,Default,,0,0,0,,Cái gì? Được rồi.\NKhông thành vấn đề. Vì ta đi ngay bây giờ.
Dialogue: 0,1:01:04.42,1:01:06.45,Default,,0,0,0,,Nào, mọi người, vào lại xe đi.
Dialogue: 0,1:01:09.79,1:01:13.79,Default,,0,0,0,,Ôi, Chúa ơi! Mọi người,\Nvào trong đi, mau! Tên sát nhân đấy!
Dialogue: 0,1:01:14.88,1:01:17.34,Default,,0,0,0,,- Tiệc tùng đây!\N- Tiệc tùng!
Dialogue: 0,1:01:18.05,1:01:21.34,Default,,0,0,0,,- Hãy bảo cậu đem vodka đi!\N- Tớ có bao giờ thiếu nó đâu?
Dialogue: 0,1:01:22.38,1:01:24.97,Default,,0,0,0,,Tớ cũng đến, và tớ đem bánh brownie này!
Dialogue: 0,1:01:26.55,1:01:29.83,Default,,0,0,0,,- Tưởng chỉ có bốn chúng ta thôi.\N- Có sao đâu?
Dialogue: 0,1:01:30.03,1:01:32.06,Default,,0,0,0,,Cậu định chơi les cuối tuần này à?
Dialogue: 0,1:01:33.56,1:01:35.50,Default,,0,0,0,,- Chào. Tiệc tùng thôi.\N- Chào.
Dialogue: 0,1:01:35.70,1:01:36.63,Default,,0,0,0,,Tởm quá.
Dialogue: 0,1:01:36.83,1:01:40.22,Default,,0,0,0,,Câu đó tởm, không phải\Nngười đồng tính tởm. Họ rất tuyệt.
Dialogue: 0,1:01:40.42,1:01:42.74,Default,,0,0,0,,Cái câu đó… mang ý kỳ thị lắm đấy.
Dialogue: 0,1:01:43.40,1:01:45.64,Default,,0,0,0,,Các cậu phải uống bù đấy. Tớ hai chai rồi.
Dialogue: 0,1:01:45.84,1:01:47.37,Default,,0,0,0,,Và tớ lái rất ổn.
Dialogue: 0,1:01:49.24,1:01:52.45,Default,,0,0,0,,Được rồi, toàn bộ cửa và cửa sổ đã khóa.
Dialogue: 0,1:01:53.12,1:01:54.90,Default,,0,0,0,,Nó đang xảy ra.
Dialogue: 0,1:01:55.10,1:01:57.49,Default,,0,0,0,,Chúng ta đều ở đây. Nơi xảy ra án mạng.
Dialogue: 0,1:01:57.68,1:01:58.79,Default,,0,0,0,,Nhà gỗ án mạng!
Dialogue: 0,1:02:00.17,1:02:01.86,Default,,0,0,0,,Nhà gỗ án mạng, cụng nào!
Dialogue: 0,1:02:02.06,1:02:04.41,Default,,0,0,0,,Rồi, mọi người. Đây là điều ta phải làm.
Dialogue: 0,1:02:04.61,1:02:08.14,Default,,0,0,0,,Mọi người, nghe này.\NCó những quy tắc cho tối nay.
Dialogue: 0,1:02:08.97,1:02:10.39,Default,,0,0,0,,- Cút đi!\N- Không!
Dialogue: 0,1:02:10.81,1:02:14.69,Default,,0,0,0,,Này, mẹ… nóng. Nóng bỏ mẹ!
Dialogue: 0,1:02:15.77,1:02:18.67,Default,,0,0,0,,Chúa ơi, cậu ấy thực sự\Nmuốn chơi les với tớ cuối tuần này.
Dialogue: 0,1:02:18.87,1:02:23.80,Default,,0,0,0,,Được rồi, mọi người, điều này\Nrất quan trọng. Đừng ai đi đâu một mình.
Dialogue: 0,1:02:24.00,1:02:25.76,Default,,0,0,0,,Nhất là cô, Marisa.
Dialogue: 0,1:02:25.96,1:02:29.07,Default,,0,0,0,,Và không ai được ra ngoài\Nvì bất kỳ lý do gì.
Dialogue: 0,1:02:32.33,1:02:34.12,Default,,0,0,0,,Tớ sẽ trần truồng tắm nước nóng.
Dialogue: 0,1:02:36.67,1:02:38.71,Default,,0,0,0,,- Cụng! Đúng thế!\N- Phải rồi!
Dialogue: 0,1:02:40.59,1:02:43.76,Default,,0,0,0,,Có một tên sát nhân đang tự do. Tôi không…
Dialogue: 0,1:02:48.59,1:02:49.64,Default,,0,0,0,,Nghe thấy không?
Dialogue: 0,1:02:50.80,1:02:54.02,Default,,0,0,0,,Bình tĩnh đi. Ăn thêm brownie này.
Dialogue: 0,1:02:54.98,1:02:57.63,Default,,0,0,0,,Không, tôi ăn năm cái rồi.
Dialogue: 0,1:02:57.83,1:03:01.19,Default,,0,0,0,,Năm? Bảo sao cậu bị hoang tưởng.
Dialogue: 0,1:03:02.65,1:03:04.44,Default,,0,0,0,,- Bánh cần à?\N- Ừ.
Dialogue: 0,1:03:06.15,1:03:08.06,Default,,0,0,0,,Tôi trộm ít cần từ tủ tang chứng.
Dialogue: 0,1:03:08.26,1:03:11.23,Default,,0,0,0,,- Trong mấy cái này chắc phải nửa cân cần.\N- Nửa cân?
Dialogue: 0,1:03:11.42,1:03:14.12,Default,,0,0,0,,Được rồi, tức là mọi người đang phê lòi.
Dialogue: 0,1:03:18.12,1:03:20.42,Default,,0,0,0,,- Ừ.\N- Chết tiệt!
Dialogue: 0,1:03:21.75,1:03:24.74,Default,,0,0,0,,Khoan, sao tôi không cảm thấy gì?
Dialogue: 0,1:03:24.94,1:03:27.42,Default,,0,0,0,,Đô của cậu coi bộ mạnh đấy.
Dialogue: 0,1:03:28.47,1:03:31.22,Default,,0,0,0,,Tôi còn sót lại một ít\Nnếu cậu muốn cuốn điếu.
Dialogue: 0,1:03:32.64,1:03:35.77,Default,,0,0,0,,Cái gì đây? Chỉ là đất thôi.
Dialogue: 0,1:03:36.68,1:03:40.73,Default,,0,0,0,,- Nhìn mấy cái cành này!\N- Vì nó lấy từ dưới đất mà.
Dialogue: 0,1:03:41.56,1:03:44.44,Default,,0,0,0,,Cần những năm 1980 dở thật.
Dialogue: 0,1:03:45.19,1:03:49.81,Default,,0,0,0,,Tôi có thể đưa cô kẹo dẻo to từng này\Nvà có nhiều cần gấp 100 lần chỗ này.
Dialogue: 0,1:03:50.01,1:03:52.74,Default,,0,0,0,,- Cậu vứt đồ của tôi.\N- Xin lỗi.
Dialogue: 0,1:03:53.66,1:03:55.02,Default,,0,0,0,,Cậu điên thật đấy.
Dialogue: 0,1:03:55.22,1:03:56.25,Default,,0,0,0,,Được rồi.
Dialogue: 0,1:03:57.08,1:03:59.25,Default,,0,0,0,,Này! Này, mọi người!
Dialogue: 0,1:04:01.29,1:04:03.75,Default,,0,0,0,,Mọi người! Vào trong thôi.
Dialogue: 0,1:04:05.84,1:04:08.97,Default,,0,0,0,,- Này, mọi người, trong nhà có pizza đấy!\N- Pizza?
Dialogue: 0,1:04:09.68,1:04:13.04,Default,,0,0,0,,- Pizza! Pizza! Pizza! Pizza!\N- Pizza! Pizza! Pizza! Pizza!
Dialogue: 0,1:04:13.24,1:04:15.87,Default,,0,0,0,,Được rồi, hai người. Xê ra nào!
Dialogue: 0,1:04:16.07,1:04:20.23,Default,,0,0,0,,Trời ạ, Canada. Căng thế?\NĐể tôi rót cho cậu một ly.
Dialogue: 0,1:04:21.48,1:04:23.80,Default,,0,0,0,,Hai người phải bốn năm nữa mới yêu nhau!
Dialogue: 0,1:04:24.00,1:04:27.55,Default,,0,0,0,,- Hứng tình quá rồi, quá sớm!\N- Ôi, Chúa ơi, cậu thôi đi.
Dialogue: 0,1:04:27.75,1:04:29.11,Default,,0,0,0,,Cậu nói như mẹ tôi ấy.
Dialogue: 0,1:04:31.07,1:04:33.07,Default,,0,0,0,,Pam, tôi chỉ trông chừng cô thôi.
Dialogue: 0,1:04:33.53,1:04:36.16,Default,,0,0,0,,Nếu cậu không vui thì không được dự tiệc.
Dialogue: 0,1:04:36.99,1:04:38.41,Default,,0,0,0,,Tận hưởng cây cối đi!
Dialogue: 0,1:04:40.33,1:04:41.33,Default,,0,0,0,,Cái gì?
Dialogue: 0,1:04:42.04,1:04:43.90,Default,,0,0,0,,Jamie đang tận hưởng môi trường.
Dialogue: 0,1:04:44.10,1:04:46.32,Default,,0,0,0,,- Mọi người! Này!\N- Thật ư? Cậu ấy gào kìa.
Dialogue: 0,1:04:46.52,1:04:48.88,Default,,0,0,0,,- Cậu ta sẽ ổn. Ở Canada mà.\N- Chết tiệt!
Dialogue: 0,1:04:49.21,1:04:51.76,Default,,0,0,0,,Tôi nên để hắn băm vằm mấy người ra.
Dialogue: 0,1:04:53.72,1:04:56.51,Default,,0,0,0,,Cậu ta nói có pizza,\Nnhưng có thấy pizza đâu.
Dialogue: 0,1:04:57.01,1:04:59.14,Default,,0,0,0,,Chắc cậu ta giấu ở đâu đó.
Dialogue: 0,1:04:59.89,1:05:02.09,Default,,0,0,0,,- Tìm nó thôi.\N- Cậu ta thật lén lút.
Dialogue: 0,1:05:02.29,1:05:06.86,Default,,0,0,0,,Này, tớ không nghĩ là có pizza. Tớ nghĩ…
Dialogue: 0,1:05:07.52,1:05:10.57,Default,,0,0,0,,Không biết nữa,\Ncó lẽ cậu ta chỉ muốn ta vào trong.
Dialogue: 0,1:05:12.07,1:05:13.18,Default,,0,0,0,,Trời ạ! Tớ đói quá!
Dialogue: 0,1:05:13.38,1:05:16.87,Default,,0,0,0,,Tớ có thể làm pizza cho chúng ta.
Dialogue: 0,1:05:17.87,1:05:21.41,Default,,0,0,0,,Cậu có thể làm…\NÝ cậu là cậu có đồ làm pizza?
Dialogue: 0,1:05:22.16,1:05:25.75,Default,,0,0,0,,- Ý tớ là, ta có thể ứng biến.\N- Ứng biến?
Dialogue: 0,1:05:26.08,1:05:28.09,Default,,0,0,0,,- Đúng! Ứng biến thôi.\N- Được rồi.
Dialogue: 0,1:05:43.77,1:05:46.02,Default,,0,0,0,,Khỉ thật! Mình khóa hết cửa sổ rồi.
Dialogue: 0,1:05:53.86,1:05:56.07,Default,,0,0,0,,Kara!
Dialogue: 0,1:05:57.28,1:06:02.50,Default,,0,0,0,,Hình như có ai đó trong nhà.\NCó ai đó trong nhà.
Dialogue: 0,1:06:03.29,1:06:04.69,Default,,0,0,0,,Tôi đang ở trong nhà.
Dialogue: 0,1:06:04.89,1:06:07.83,Default,,0,0,0,,- Không.\N- Cậu giấu pizza ở đâu?
Dialogue: 0,1:06:08.54,1:06:11.05,Default,,0,0,0,,Tôi nghĩ có ai đó trong nhà.
Dialogue: 0,1:06:11.80,1:06:12.80,Default,,0,0,0,,Kara!
Dialogue: 0,1:06:14.26,1:06:16.43,Default,,0,0,0,,Ta có đủ nguyên liệu làm pizza Hawaii.
Dialogue: 0,1:06:18.18,1:06:20.18,Default,,0,0,0,,Cậu muốn mấy lát táo?
Dialogue: 0,1:06:20.68,1:06:22.35,Default,,0,0,0,,Không biết nửa, chắc là một.
Dialogue: 0,1:06:23.23,1:06:25.48,Default,,0,0,0,,Ôi, Chúa ơi, tay cậu!
Dialogue: 0,1:06:38.49,1:06:41.44,Default,,0,0,0,,- Liệu tớ có chết không?\N- Tớ không nghĩ vậy.
Dialogue: 0,1:06:41.63,1:06:43.06,Default,,0,0,0,,Nếu tớ tìm được băng gạc.
Dialogue: 0,1:06:43.26,1:06:45.40,Default,,0,0,0,,Không thể bảo mẹ là tớ cắt trúng tay
Dialogue: 0,1:06:45.60,1:06:48.82,Default,,0,0,0,,vì tớ bị phê\Nsau khi uống bốn ly rượu và nửa cân cần!
Dialogue: 0,1:06:49.02,1:06:51.11,Default,,0,0,0,,Bảo bà ấy là cậu bị tai nạn xe máy.
Dialogue: 0,1:06:51.31,1:06:55.22,Default,,0,0,0,,Cậu nói đúng.\NTớ có thể nói dối. Cậu thông minh thật.
Dialogue: 0,1:06:59.68,1:07:02.58,Default,,0,0,0,,Tớ sẽ làm cocktail
Dialogue: 0,1:07:02.78,1:07:07.94,Default,,0,0,0,,nếu ai đó muốn\Nthôi tỏ ra thảm hại và bắt đầu vui vẻ.
Dialogue: 0,1:07:37.01,1:07:39.43,Default,,0,0,0,,- Cái gì vậy?\N- Tớ không biết.
Dialogue: 0,1:07:40.22,1:07:44.83,Default,,0,0,0,,Ôi, Chúa ơi! Chúa ơi.\NKẻ sát nhân sẽ đến đây!
Dialogue: 0,1:07:45.03,1:07:45.92,Default,,0,0,0,,Cái gì?
Dialogue: 0,1:07:46.12,1:07:47.21,Default,,0,0,0,,Jamie bảo tớ thế.
Dialogue: 0,1:07:47.41,1:07:49.42,Default,,0,0,0,,Máu chảy từ tay đã khiến tớ tỉnh ra.
Dialogue: 0,1:07:49.62,1:07:52.72,Default,,0,0,0,,Cậu ta không muốn chơi les,\Ncậu ta là thầy bói.
Dialogue: 0,1:07:52.91,1:07:54.80,Default,,0,0,0,,Kẻ sát nhân sẽ đến và giết Marisa!
Dialogue: 0,1:07:55.00,1:07:56.23,Default,,0,0,0,,Marisa?
Dialogue: 0,1:07:57.78,1:07:59.65,Default,,0,0,0,,Đi nào, chặn cửa đi!
Dialogue: 0,1:08:04.33,1:08:06.29,Default,,0,0,0,,Heather, chơi bóng bia không?
Dialogue: 0,1:08:09.58,1:08:10.67,Default,,0,0,0,,Heather?
Dialogue: 0,1:08:11.67,1:08:12.67,Default,,0,0,0,,Bóng bia!
Dialogue: 0,1:08:29.73,1:08:32.77,Default,,0,0,0,,Randy?
Dialogue: 0,1:08:38.32,1:08:40.03,Default,,0,0,0,,Không, này!
Dialogue: 0,1:08:42.07,1:08:43.20,Default,,0,0,0,,Heather!
Dialogue: 0,1:08:45.58,1:08:46.58,Default,,0,0,0,,Heather!
Dialogue: 0,1:08:47.49,1:08:49.00,Default,,0,0,0,,Heather, tỉnh dậy!
Dialogue: 0,1:08:50.12,1:08:54.29,Default,,0,0,0,,Heather, tỉnh dậy! Heather!
Dialogue: 0,1:08:57.13,1:08:59.78,Default,,0,0,0,,- Này.\N- Nhạc bị sao thế?
Dialogue: 0,1:08:59.98,1:09:01.49,Default,,0,0,0,,Cá là Blake và Pam không muốn
Dialogue: 0,1:09:01.69,1:09:04.59,Default,,0,0,0,,ta nghe thấy họ nện thùm thụp vào tường.
Dialogue: 0,1:09:06.89,1:09:08.33,Default,,0,0,0,,Xoạc trong bồn tắm không?
Dialogue: 0,1:09:08.53,1:09:12.96,Default,,0,0,0,,Ừ! Nhưng tớ dạo đầu tệ lắm\Nnên cậu phải chủ động đấy.
Dialogue: 0,1:09:13.16,1:09:14.23,Default,,0,0,0,,Được rồi.
Dialogue: 0,1:09:21.65,1:09:23.78,Default,,0,0,0,,- Giỏi thì vào đi, sát nhân!\N- Tốt đấy.
Dialogue: 0,1:09:24.11,1:09:26.03,Default,,0,0,0,,Cho tôi vào! Hắn ở trong nhà!
Dialogue: 0,1:09:27.49,1:09:29.77,Default,,0,0,0,,Pam! Pam, tên sát nhân ở trong nhà!
Dialogue: 0,1:09:29.97,1:09:31.37,Default,,0,0,0,,Sao cậu lên được đây?
Dialogue: 0,1:09:31.75,1:09:34.40,Default,,0,0,0,,- Hắn sẽ giết Heather.\N- Tôi tưởng là Marisa?
Dialogue: 0,1:09:34.60,1:09:35.46,Default,,0,0,0,,Randy?
Dialogue: 0,1:09:52.10,1:09:53.81,Default,,0,0,0,,Ôi, Chúa ơi, Heather!
Dialogue: 0,1:09:54.52,1:09:55.94,Default,,0,0,0,,- Heather!\N- Heather!
Dialogue: 0,1:09:56.81,1:09:59.73,Default,,0,0,0,,Heather! Heather à! Ra khỏi đó đi!
Dialogue: 0,1:10:03.65,1:10:04.65,Default,,0,0,0,,Heather!
Dialogue: 0,1:10:10.29,1:10:13.25,Default,,0,0,0,,Ai đó, cứu tớ! Cứu tớ với!
Dialogue: 0,1:10:28.22,1:10:30.35,Default,,0,0,0,,Tránh xa cậu ấy ra!
Dialogue: 0,1:11:28.20,1:11:29.20,Default,,0,0,0,,Cô ấy còn sống.
Dialogue: 0,1:11:49.01,1:11:51.70,Default,,0,0,0,,Rồi, mấy đứa, rắc rối thực sự đây. Tôi có…
Dialogue: 0,1:11:51.90,1:11:55.06,Default,,0,0,0,,Của chú đây, Cảnh sát trưởng. Cẩn thận.
Dialogue: 0,1:11:57.85,1:11:58.75,Default,,0,0,0,,Cái gì đây?
Dialogue: 0,1:11:58.95,1:12:01.59,Default,,0,0,0,,Cháu đã dùng thanh củi\Nđánh vào đầu kẻ sát nhân
Dialogue: 0,1:12:01.79,1:12:04.32,Default,,0,0,0,,và dùng khăn giấy đó thấm máu của hắn.
Dialogue: 0,1:12:06.53,1:12:07.82,Default,,0,0,0,,Các chú có ADN của hắn.
Dialogue: 0,1:12:08.95,1:12:10.74,Default,,0,0,0,,- Đó là cái gì?\N- ADN?
Dialogue: 0,1:12:11.28,1:12:13.45,Default,,0,0,0,,Nó kiểu, cháu không…
Dialogue: 0,1:12:14.03,1:12:15.60,Default,,0,0,0,,Không biết nó viết tắt của gì.
Dialogue: 0,1:12:15.80,1:12:21.19,Default,,0,0,0,,Ai cũng… Nó là thứ của riêng từng người.
Dialogue: 0,1:12:21.39,1:12:23.96,Default,,0,0,0,,Ta nhập nó\Nvào cơ sở dữ liệu toàn thế giới.
Dialogue: 0,1:12:27.80,1:12:32.39,Default,,0,0,0,,Chắc chắn rồi, cô bé. Ta sẽ khởi động\Ncơ sở dữ liệu DMA toàn thế giới.
Dialogue: 0,1:12:33.80,1:12:34.97,Default,,0,0,0,,Tôi sẽ phá án này.
Dialogue: 0,1:12:37.47,1:12:39.79,Default,,0,0,0,,- Được rồi, nghiêm túc nào.\N- Trời ạ!
Dialogue: 0,1:12:39.99,1:12:41.92,Default,,0,0,0,,Còn ai biết mấy đứa định đến đó?
Dialogue: 0,1:12:42.12,1:12:45.34,Default,,0,0,0,,- Không ai cả.\N- Lurch thấy bọn cháu nói ở lớp khoa học.
Dialogue: 0,1:12:45.54,1:12:46.97,Default,,0,0,0,,Có lẽ cậu ta đã nghe được.
Dialogue: 0,1:12:47.17,1:12:50.20,Default,,0,0,0,,Các cháu từng đưa ai khác\Nđến đó chưa? Các nhóm khác?
Dialogue: 0,1:12:51.28,1:12:53.70,Default,,0,0,0,,Nhỡ không có kẻ nào đi theo ta đến đó?
Dialogue: 0,1:12:54.45,1:12:57.64,Default,,0,0,0,,- Ý cậu là gì?\N- Tức là, cậu ở đâu khi chuyện đó xảy ra?
Dialogue: 0,1:12:57.84,1:12:59.60,Default,,0,0,0,,- Khoan đã.\N- Jamie làm tớ sợ.
Dialogue: 0,1:12:59.80,1:13:01.98,Default,,0,0,0,,- Tớ nấp trong tủ.\N- Không nghe tiếng hét à?
Dialogue: 0,1:13:02.18,1:13:04.57,Default,,0,0,0,,Tiếng nhạc ồn lắm.\NBố, con không làm gì cả.
Dialogue: 0,1:13:04.77,1:13:05.75,Default,,0,0,0,,Bố biết, Kara.
Dialogue: 0,1:13:06.09,1:13:06.92,Default,,0,0,0,,Mẹ kiếp!
Dialogue: 0,1:13:08.30,1:13:09.30,Default,,0,0,0,,Kara?
Dialogue: 0,1:13:10.55,1:13:11.72,Default,,0,0,0,,Giờ ta làm gì?
Dialogue: 0,1:13:13.64,1:13:14.64,Default,,0,0,0,,Không biết nữa.
Dialogue: 0,1:13:15.51,1:13:18.08,Default,,0,0,0,,Marisa lẽ ra phải là\Nngười chết trong nhà gỗ.
Dialogue: 0,1:13:18.28,1:13:19.56,Default,,0,0,0,,Khoan, sao cơ?
Dialogue: 0,1:13:20.35,1:13:21.35,Default,,0,0,0,,Cậu ta là thầy bói.
Dialogue: 0,1:13:23.02,1:13:24.52,Default,,0,0,0,,Giờ hết rồi.
Dialogue: 0,1:13:25.90,1:13:29.47,Default,,0,0,0,,Tôi là Norm Dubasage từ {\i1}Thời sự KZFD.{\i0}
Dialogue: 0,1:13:29.67,1:13:35.47,Default,,0,0,0,,{\an8}TỰ VỆ\NHÃY CHỌN VŨ KHÍ CHO BẠN
Dialogue: 0,1:13:35.67,1:13:39.44,Default,,0,0,0,,{\an8}Nỗi sợ tràn ngập Vernon\Nkhi cộng đồng khóc thương nạn nhân thứ hai
Dialogue: 0,1:13:39.64,1:13:42.44,Default,,0,0,0,,{\an8}và cảnh sát vẫn chưa bắt được ai.
Dialogue: 0,1:13:42.64,1:13:44.52,Default,,0,0,0,,Giờ, thật không may, tôi phải nói…
Dialogue: 0,1:13:44.72,1:13:46.50,Default,,0,0,0,,- Cháu sao rồi?\N- Không ổn lắm.
Dialogue: 0,1:13:46.92,1:13:49.50,Default,,0,0,0,,Thứ duy nhất\Ncháu thay đổi được là thứ tự chết.
Dialogue: 0,1:13:50.13,1:13:52.57,Default,,0,0,0,,Cháu nghĩ có thể có hai kẻ sát nhân,
Dialogue: 0,1:13:52.77,1:13:54.34,Default,,0,0,0,,như phim {\i1}Tiếng hét{\i0} ấy.
Dialogue: 0,1:13:55.18,1:13:56.14,Default,,0,0,0,,Tiếng hét?
Dialogue: 0,1:13:56.97,1:13:58.71,Default,,0,0,0,,Phải rồi. Chưa có phim đó.
Dialogue: 0,1:13:58.91,1:14:02.48,Default,,0,0,0,,Đó là phim tương lai cứ tưởng\NDrew Barrymore đóng chính nhưng không.
Dialogue: 0,1:14:02.98,1:14:05.48,Default,,0,0,0,,Thôi bỏ đi. Cỗ máy thời gian sao rồi ạ?
Dialogue: 0,1:14:06.10,1:14:08.02,Default,,0,0,0,,Có tin tốt và tin xấu.
Dialogue: 0,1:14:08.82,1:14:11.32,Default,,0,0,0,,Tôi đã tạo ra một mẫu thử ở lớp Mua sắm.
Dialogue: 0,1:14:11.73,1:14:13.55,Default,,0,0,0,,- Trời!\N- Nhưng không hoạt động.
Dialogue: 0,1:14:13.75,1:14:15.07,Default,,0,0,0,,Thật ấn tượng.
Dialogue: 0,1:14:16.49,1:14:19.31,Default,,0,0,0,,- Chúa ơi. Có tín hiệu.\N- Sao lại thế?
Dialogue: 0,1:14:19.51,1:14:22.44,Default,,0,0,0,,Cháu đoán họ tưởng\Nđó là một dạng cổ ngữ của quỷ.
Dialogue: 0,1:14:22.64,1:14:23.86,Default,,0,0,0,,{\an8}EDDIE ROYAL\NCỰU NGHI PHẠM
Dialogue: 0,1:14:24.06,1:14:28.13,Default,,0,0,0,,{\an8}Như đã nói, cháu không phải\Nkẻ nổi loạn. Và một bạn gái nữa đã chết.
Dialogue: 0,1:14:29.13,1:14:32.30,Default,,0,0,0,,{\an8}Một trải nghiệm như vậy,\Nnó thay đổi con người ta.
Dialogue: 0,1:14:34.55,1:14:37.62,Default,,0,0,0,,Khi đàn ông giãi bày,\Nđiều đó thật lôi cuốn.
Dialogue: 0,1:14:37.82,1:14:40.81,Default,,0,0,0,,Từ KZFD, tôi là Norm Dubasage.
Dialogue: 0,1:14:41.51,1:14:42.68,Default,,0,0,0,,Và hết.
Dialogue: 0,1:14:44.73,1:14:46.63,Default,,0,0,0,,Không. Wi-Fi đâu rồi?
Dialogue: 0,1:14:46.83,1:14:48.30,Default,,0,0,0,,- Eddie!\N- Eddie, em yêu anh.
Dialogue: 0,1:14:48.50,1:14:49.51,Default,,0,0,0,,Quay lại đi.
Dialogue: 0,1:14:49.71,1:14:51.80,Default,,0,0,0,,- Vệ tinh TV.\N- Eddie, là em, Charlene.
Dialogue: 0,1:14:52.00,1:14:54.59,Default,,0,0,0,,Nếu câu trộm\Ntín hiệu xe van TV vào bộ chuyển,
Dialogue: 0,1:14:54.79,1:14:58.01,Default,,0,0,0,,nó có thể sẽ đưa được cháu về nhà.\NVà giờ là tin xấu.
Dialogue: 0,1:14:58.21,1:15:00.52,Default,,0,0,0,,Cháu tưởng\Nmẫu thử không hoạt động là tin xấu.
Dialogue: 0,1:15:00.72,1:15:03.56,Default,,0,0,0,,Không, đó chỉ là\Nđiều bình thường của quy trình.
Dialogue: 0,1:15:03.76,1:15:04.69,Default,,0,0,0,,Thử và lỗi.
Dialogue: 0,1:15:04.89,1:15:07.52,Default,,0,0,0,,Tin xấu là, với tốc độ cạn pin của cháu,
Dialogue: 0,1:15:07.72,1:15:10.86,Default,,0,0,0,,cháu có khoảng bảy tiếng rưỡi\Ntrước khi kẹt ở đây vĩnh viễn.
Dialogue: 0,1:15:11.06,1:15:14.16,Default,,0,0,0,,Khỉ gió! Đó chính là\Nthời điểm Heather bị giết.
Dialogue: 0,1:15:14.36,1:15:15.97,Default,,0,0,0,,Hoặc lẽ ra phải bị giết.
Dialogue: 0,1:15:16.72,1:15:19.58,Default,,0,0,0,,- Ở đâu?\N- Lễ hội Halloween. Ở Billy's.
Dialogue: 0,1:15:19.78,1:15:23.29,Default,,0,0,0,,Họ luôn phát sóng lễ hội đó.\NPhải rồi. Đó là cách đưa cháu về.
Dialogue: 0,1:15:23.49,1:15:25.83,Default,,0,0,0,,Ta sẽ cướp sóng truyền hình ở Boardwalk.
Dialogue: 0,1:15:26.03,1:15:28.96,Default,,0,0,0,,Gặp tôi ở Billy's sau giờ học,\Nphải đảm bảo nó hoạt động
Dialogue: 0,1:15:29.16,1:15:32.17,Default,,0,0,0,,- để đưa cháu về.\N- Cháu chưa thể về khi chưa cản được hắn.
Dialogue: 0,1:15:32.37,1:15:36.34,Default,,0,0,0,,Được rồi, nếu tối nay không đưa cháu về,\Nhãy ôn Thuyết kinh tế Reagan,
Dialogue: 0,1:15:36.54,1:15:38.91,Default,,0,0,0,,vì thứ Tư ta có bài kiểm tra Xã hội học.
Dialogue: 0,1:15:45.37,1:15:48.04,Default,,0,0,0,,Thôi nào! Này!
Dialogue: 0,1:15:49.04,1:15:52.09,Default,,0,0,0,,Này! Hứng tình quá rồi. Quá sớm.
Dialogue: 0,1:16:14.90,1:16:16.32,Default,,0,0,0,,NHÀ VỆ SINH\NMỜI QUA
Dialogue: 0,1:17:01.70,1:17:04.57,Default,,0,0,0,,Chúa ơi, mùi kinh quá. Được rồi.
Dialogue: 0,1:17:06.99,1:17:07.99,Default,,0,0,0,,Được rồi.
Dialogue: 0,1:17:12.00,1:17:13.79,Default,,0,0,0,,Chúa ơi, nếu đây là thi thể…
Dialogue: 0,1:17:18.30,1:17:19.34,Default,,0,0,0,,Ơn Chúa!
Dialogue: 0,1:17:23.26,1:17:24.30,Default,,0,0,0,,Cút đi!
Dialogue: 0,1:17:26.43,1:17:27.89,Default,,0,0,0,,Tốt thôi!
Dialogue: 0,1:17:28.77,1:17:31.43,Default,,0,0,0,,Lấy gì thì lấy,\Nlàm ơn để lại trò {\i1}ThunderCats.{\i0}
Dialogue: 0,1:17:33.10,1:17:35.51,Default,,0,0,0,,Tôi đâu muốn lấy trò chơi điện tử của cậu.
Dialogue: 0,1:17:35.70,1:17:37.44,Default,,0,0,0,,Đó là thứ duy nhất đáng giá.
Dialogue: 0,1:17:40.36,1:17:42.32,Default,,0,0,0,,Sao cậu đỗ xe ngoài tiệc của Tiffany?
Dialogue: 0,1:17:43.32,1:17:44.49,Default,,0,0,0,,Tôi muốn tham gia.
Dialogue: 0,1:17:45.16,1:17:48.66,Default,,0,0,0,,Tôi ngồi ngoài nghe nhạc Meat Loaf\Nđể lên tinh thần, nhưng…
Dialogue: 0,1:17:49.95,1:17:53.25,Default,,0,0,0,,Tôi biết mọi người nghĩ tôi đáng sợ,\Nnhưng tôi không giết người.
Dialogue: 0,1:17:56.42,1:17:58.40,Default,,0,0,0,,- Có chuyện gì thế?\N- Doug.
Dialogue: 0,1:17:58.60,1:18:01.30,Default,,0,0,0,,Chỉ giám thị mới được\Nra ngoài trong giờ học.
Dialogue: 0,1:18:02.05,1:18:05.22,Default,,0,0,0,,- Bọn tôi chỉ…\N- Chơi điện tử?
Dialogue: 0,1:18:06.89,1:18:11.04,Default,,0,0,0,,Ừ. Biết là hơi đụt, nhưng tôi muốn\Nlàm trò chơi điện tử một ngày nào đó.
Dialogue: 0,1:18:11.24,1:18:14.46,Default,,0,0,0,,Cậu chơi {\i1}Death Wish 3{\i0} chưa?\NĐồ họa tuyệt lắm.
Dialogue: 0,1:18:14.66,1:18:16.98,Default,,0,0,0,,Khi cậu giết ai đó,\Nchúng nổ thành sương hồng.
Dialogue: 0,1:18:17.94,1:18:20.18,Default,,0,0,0,,Vào trong đi. Ngoài này không an toàn.
Dialogue: 0,1:18:20.37,1:18:23.22,Default,,0,0,0,,Ở với tôi sẽ an toàn.\NTôi học karate tám năm rồi.
Dialogue: 0,1:18:23.42,1:18:25.81,Default,,0,0,0,,- Ở đâu?\N- Võ đường Tom. Ở khu mua sắm.
Dialogue: 0,1:18:26.01,1:18:28.06,Default,,0,0,0,,Cảnh sát lượn qua đây mỗi hai phút.
Dialogue: 0,1:18:28.26,1:18:30.35,Default,,0,0,0,,Cảnh sát trưởng Lim không phá án được đâu.
Dialogue: 0,1:18:30.55,1:18:33.44,Default,,0,0,0,,Chú ấy tin đó là Eddie\Ndù tôi đã bảo là không thể.
Dialogue: 0,1:18:33.64,1:18:36.98,Default,,0,0,0,,Biết là không phải Eddie,\Nnhưng sao cậu biết không phải Eddie?
Dialogue: 0,1:18:37.18,1:18:40.07,Default,,0,0,0,,Cậu ấy kể với tôi\Nvề việc cậu ấy hôn hít Tiffany
Dialogue: 0,1:18:40.27,1:18:42.88,Default,,0,0,0,,nhưng đã bỏ đi\Nngay khi cậu ấy nói không thổi kèn.
Dialogue: 0,1:18:43.84,1:18:48.30,Default,,0,0,0,,Nghĩ mà xem, nếu chịu thổi kèn,\Ncó khi cậu ấy vẫn còn sống.
Dialogue: 0,1:18:49.22,1:18:52.14,Default,,0,0,0,,Ừ, đừng lấy đó làm bài học nhé.
Dialogue: 0,1:18:58.44,1:19:00.72,Default,,0,0,0,,- Chào.\N- Chào. Bắt được sát nhân chưa?
Dialogue: 0,1:19:00.92,1:19:03.36,Default,,0,0,0,,- Chưa. Buồng chụp ảnh hoạt động chưa?\N- Chưa.
Dialogue: 0,1:19:05.11,1:19:08.01,Default,,0,0,0,,Trong này có\Nmáy phát siêu tương lai, mà hỏng rồi.
Dialogue: 0,1:19:08.21,1:19:11.14,Default,,0,0,0,,Nó không thể phát ra đủ điện\Ncho cỗ máy thời gian.
Dialogue: 0,1:19:11.34,1:19:13.06,Default,,0,0,0,,Máy thời gian đó à? Thế thôi ư?
Dialogue: 0,1:19:13.26,1:19:17.46,Default,,0,0,0,,Hãy tưởng tượng\Nđây là máy tính và đây là nút nguồn.
Dialogue: 0,1:19:18.50,1:19:21.11,Default,,0,0,0,,- Được rồi, tức là ta cần nút nguồn mới.\N- Ừ.
Dialogue: 0,1:19:21.31,1:19:25.09,Default,,0,0,0,,Một thứ có thể tạo ra hàng tấn lực.\NĐủ để phá vỡ trọng lực.
Dialogue: 0,1:19:28.63,1:19:30.64,Default,,0,0,0,,Như Cú Rơi Lượng Tử?
Dialogue: 0,1:19:41.23,1:19:43.98,Default,,0,0,0,,Và ta đã trở lại Trái Đất.
Dialogue: 0,1:19:44.44,1:19:47.18,Default,,0,0,0,,Cảm ơn nhiều vì đã chơi Cú Rơi Lượng Tử,
Dialogue: 0,1:19:47.38,1:19:50.35,Default,,0,0,0,,làm ơn hãy ra khỏi máy trước khi nôn
Dialogue: 0,1:19:50.55,1:19:54.41,Default,,0,0,0,,và tận hưởng thời gian còn lại trong ngày\Ntại Billy's Boardwalk.
Dialogue: 0,1:19:56.70,1:19:58.69,Default,,0,0,0,,Chào. Thành phố cử bọn cháu đến.
Dialogue: 0,1:19:58.89,1:20:02.11,Default,,0,0,0,,Ta phải đóng cửa trò chơi,\Nmột trong những tấm bên ngoài…
Dialogue: 0,1:20:02.31,1:20:05.59,Default,,0,0,0,,Cú Rơi Lượng Tử dừng,\NNed được nghỉ. Chỉ cần nói vậy.
Dialogue: 0,1:20:09.93,1:20:11.34,Default,,0,0,0,,Chú ấy đi luôn kìa.
Dialogue: 0,1:20:12.76,1:20:14.31,Default,,0,0,0,,Thập niên 1980 điên rồ thật.
Dialogue: 0,1:20:17.60,1:20:20.34,Default,,0,0,0,,Được rồi. Tôi ở đây,\Ncháu đi bắt kẻ sát nhân.
Dialogue: 0,1:20:20.54,1:20:22.15,Default,,0,0,0,,- Ừ, chúc may mắn.\N- Cảm ơn.
Dialogue: 0,1:20:24.90,1:20:27.76,Default,,0,0,0,,Như đã nói với Pam, tôi là thầy bói.
Dialogue: 0,1:20:27.96,1:20:30.05,Default,,0,0,0,,Tôi tưởng Heather sẽ bị giết tối nay,
Dialogue: 0,1:20:30.25,1:20:32.56,Default,,0,0,0,,tại Lễ hội Halloween, trong bãi đỗ xe,
Dialogue: 0,1:20:32.76,1:20:35.64,Default,,0,0,0,,mà cô ấy chết rồi\Nnên có lẽ hắn sẽ săn lùng Marisa.
Dialogue: 0,1:20:35.84,1:20:37.62,Default,,0,0,0,,- Ôi, Chúa ơi!\N- Hoặc là tôi.
Dialogue: 0,1:20:38.37,1:20:41.02,Default,,0,0,0,,Điều gì cũng có thể xảy ra\Nnếu tiên đoán của cậu sai.
Dialogue: 0,1:20:41.22,1:20:45.38,Default,,0,0,0,,Ừ, ý là, không ai an toàn cả,\Nnên nếu ai muốn bỏ đi thì đi ngay đi.
Dialogue: 0,1:20:51.05,1:20:52.13,Default,,0,0,0,,Tôi muốn bắt hắn.
Dialogue: 0,1:20:53.68,1:20:56.29,Default,,0,0,0,,Kể cả nếu hắn giết Marisa\Nvà còn lại vẫn sống…
Dialogue: 0,1:20:56.49,1:20:58.92,Default,,0,0,0,,- Ôi, Chúa ơi!\N- Tớ không muốn sống cả đời
Dialogue: 0,1:20:59.12,1:21:01.96,Default,,0,0,0,,biết rằng kẻ tâm thần giết bạn mình\Nvẫn nhởn nhơ.
Dialogue: 0,1:21:02.16,1:21:04.61,Default,,0,0,0,,Tớ cũng thế.\NTiêu diệt thằng khốn này, nhé?
Dialogue: 0,1:21:05.40,1:21:08.55,Default,,0,0,0,,- Tớ không muốn làm mồi nhử.\N- Cô sẽ ổn thôi.
Dialogue: 0,1:21:08.75,1:21:13.52,Default,,0,0,0,,Ngay khi cô thấy hắn, hãy ấn nút này.\NĐó là báo động hiếp dâm, nó kêu rất to,
Dialogue: 0,1:21:13.71,1:21:15.03,Default,,0,0,0,,và nếu cô sợ…
Dialogue: 0,1:21:15.91,1:21:20.29,Default,,0,0,0,,Được rồi, đấy,\Ngiờ cô đã biết nó hoạt động ra sao.
Dialogue: 0,1:21:20.96,1:21:24.48,Default,,0,0,0,,Nhưng nhỡ hắn cắt cổ cậu ấy\Ntrước khi cậu ấy kịp bấm thì sao?
Dialogue: 0,1:21:24.68,1:21:26.65,Default,,0,0,0,,- Nói gì thế?\N- Hắn không làm thế.
Dialogue: 0,1:21:26.85,1:21:30.59,Default,,0,0,0,,- Hắn luôn đâm nạn nhân 16 nhát.\N- Nói vậy cũng không đỡ hơn đâu!
Dialogue: 0,1:21:30.79,1:21:33.83,Default,,0,0,0,,Cô sẽ ổn thôi.\NHãy đi đến Nhà Búp Bê Kinh Dị
Dialogue: 0,1:21:34.03,1:21:36.45,Default,,0,0,0,,như thể cô định gặp ai ở đó\Nđể thổi kèn họ.
Dialogue: 0,1:21:36.65,1:21:38.35,Default,,0,0,0,,Ừ, cậu làm thế suốt mà.
Dialogue: 0,1:21:40.06,1:21:40.83,Default,,0,0,0,,Phải rồi.
Dialogue: 0,1:21:41.03,1:21:43.77,Default,,0,0,0,,- Và bọn tôi sẽ ở đó, chờ đợi.\N- Được rồi.
Dialogue: 0,1:21:46.27,1:21:49.03,Default,,0,0,0,,Mình sẽ thổi kèn ai đó, chỉ vậy thôi.
Dialogue: 0,1:21:49.98,1:21:52.15,Default,,0,0,0,,Thổi kèn trong Nhà Búp Bê Kinh Dị.
Dialogue: 0,1:21:52.90,1:21:53.90,Default,,0,0,0,,Mình làm được mà.
Dialogue: 0,1:22:07.96,1:22:08.90,Default,,0,0,0,,Hãy tỏ ra tự nhiên.
Dialogue: 0,1:22:09.10,1:22:12.45,Default,,0,0,0,,Phòng khi kẻ sát nhân\Nđi theo cô thay vì Marisa.
Dialogue: 0,1:22:12.65,1:22:14.70,Default,,0,0,0,,- Ta không muốn hắn… Chúa ơi!\N- Kara!
Dialogue: 0,1:22:14.90,1:22:16.30,Default,,0,0,0,,Cậu làm gì ở đây?
Dialogue: 0,1:22:17.35,1:22:19.79,Default,,0,0,0,,Halloween mà. Mọi người đâu?
Dialogue: 0,1:22:19.99,1:22:22.63,Default,,0,0,0,,Tớ cảm giác\Ncác cậu tránh mặt tớ sau vụ nhà gỗ.
Dialogue: 0,1:22:22.83,1:22:24.42,Default,,0,0,0,,Blake làm các cậu xa lánh tớ.
Dialogue: 0,1:22:24.62,1:22:26.44,Default,,0,0,0,,- Không, không có.\N- Đâu có.
Dialogue: 0,1:22:27.44,1:22:31.38,Default,,0,0,0,,Họ đang đi chơi tàu lượn và Jamie,\Ncậu ta thấy không khỏe.
Dialogue: 0,1:22:31.58,1:22:32.68,Default,,0,0,0,,Ừ, hơi đau bụng.
Dialogue: 0,1:22:32.88,1:22:35.72,Default,,0,0,0,,Lạ thật. Mọi người\Ncứ hỏi tớ ở đâu khi Heather bị giết,
Dialogue: 0,1:22:35.92,1:22:37.87,Default,,0,0,0,,nhưng không ai chết trước khi cậu đến.
Dialogue: 0,1:23:01.01,1:23:02.01,Default,,0,0,0,,Này!
Dialogue: 0,1:23:07.94,1:23:10.82,Default,,0,0,0,,Ôi, Chúa ơi. Chú thấy Marisa.
Dialogue: 0,1:23:12.44,1:23:15.47,Default,,0,0,0,,Chú vừa nhớ ra đã thấy cô ấy\Nở Boardwalk, đêm Halloween,
Dialogue: 0,1:23:15.67,1:23:18.43,Default,,0,0,0,,và tên sát nhân đi theo cô ấy.\NPhải đưa Jamie ra khỏi đó.
Dialogue: 0,1:23:18.63,1:23:19.57,Default,,0,0,0,,Vâng, cháu biết.
Dialogue: 0,1:23:24.04,1:23:25.16,Default,,0,0,0,,Đưa cháu kim loại dẫn.
Dialogue: 0,1:23:31.67,1:23:32.70,Default,,0,0,0,,Được rồi.
Dialogue: 0,1:23:32.90,1:23:37.22,Default,,0,0,0,,NHÀ BÚP BÊ KINH DỊ
Dialogue: 0,1:23:47.77,1:23:49.52,Default,,0,0,0,,{\i1}Đi xuống à?{\i0}
Dialogue: 0,1:24:03.58,1:24:04.83,Default,,0,0,0,,Mình ghét nơi này.
Dialogue: 0,1:24:10.67,1:24:11.83,Default,,0,0,0,,Randy.
Dialogue: 0,1:24:18.30,1:24:22.39,Default,,0,0,0,,Randy, tớ đây.
Dialogue: 0,1:24:41.82,1:24:43.12,Default,,0,0,0,,Mình ghét nơi này!
Dialogue: 0,1:24:48.70,1:24:53.92,Default,,0,0,0,,{\an8}MẸ ĐANG QUAN SÁT\NMẸ ĐANG ĐỢI
Dialogue: 0,1:25:02.22,1:25:03.47,Default,,0,0,0,,Giống quần của mình.
Dialogue: 0,1:25:06.43,1:25:07.43,Default,,0,0,0,,Không.
Dialogue: 0,1:25:28.58,1:25:31.16,Default,,0,0,0,,Randy, cậu có ở đây không?
Dialogue: 0,1:25:35.63,1:25:39.88,Default,,0,0,0,,Tớ đang ở một mình trong bếp\Nvà đã sẵn sàng thổi kèn cậu.
Dialogue: 0,1:25:52.10,1:25:53.10,Default,,0,0,0,,Khỉ thật!
Dialogue: 0,1:25:55.98,1:25:58.02,Default,,0,0,0,,Báo động hiếp dâm!
Dialogue: 0,1:26:01.53,1:26:03.90,Default,,0,0,0,,Tôi lấy được dao rồi! Khỉ thật!
Dialogue: 0,1:26:13.00,1:26:13.94,Default,,0,0,0,,Randy!
Dialogue: 0,1:26:14.14,1:26:15.37,Default,,0,0,0,,Này, thằng khốn!
Dialogue: 0,1:26:42.36,1:26:44.28,Default,,0,0,0,,Đã bảo tớ không phải tên sát nhân mà!
Dialogue: 0,1:26:46.03,1:26:47.39,Default,,0,0,0,,Đồ thật à?
Dialogue: 0,1:26:47.59,1:26:50.16,Default,,0,0,0,,Ừ, bố tớ đưa.\NCó kẻ sát nhân đang tự do mà.
Dialogue: 0,1:26:53.58,1:26:56.04,Default,,0,0,0,,Ta đã làm được. Ta đã cản được hắn.
Dialogue: 0,1:27:05.93,1:27:06.88,Default,,0,0,0,,Doug?
Dialogue: 0,1:27:09.26,1:27:11.04,Default,,0,0,0,,Sao Doug lại muốn giết chúng ta?
Dialogue: 0,1:27:11.24,1:27:15.84,Default,,0,0,0,,- Đụt Summers cho tớ ăn đòn?\N- Cậu ta học karate. Ở Võ đường Tom.
Dialogue: 0,1:27:16.03,1:27:18.23,Default,,0,0,0,,Nhưng bọn mình có làm gì Doug đâu!
Dialogue: 0,1:27:20.61,1:27:23.15,Default,,0,0,0,,- Chờ đã, cái gì kia?\N- Trang sức mọt sách.
Dialogue: 0,1:27:23.53,1:27:24.99,Default,,0,0,0,,Đó là mề đay à?
Dialogue: 0,1:27:28.66,1:27:29.66,Default,,0,0,0,,Trish Béo!
Dialogue: 0,1:27:30.62,1:27:32.73,Default,,0,0,0,,Sao cậu ta có ảnh của Trish Béo?
Dialogue: 0,1:27:32.93,1:27:34.60,Default,,0,0,0,,Họ là một cặp.
Dialogue: 0,1:27:34.80,1:27:37.61,Default,,0,0,0,,Nhớ không, chuyến dã ngoại\Nđi Bảo tàng Field ấy?
Dialogue: 0,1:27:37.81,1:27:41.19,Default,,0,0,0,,Niềng răng của họ mắc vào nhau\Nvì họ hôn nhau trên xe buýt.
Dialogue: 0,1:27:41.39,1:27:42.29,Default,,0,0,0,,Thế thì điên quá!
Dialogue: 0,1:27:42.84,1:27:46.70,Default,,0,0,0,,Cậu ta săn lùng và giết các cô,\Nvì các cô trêu chọc bạn gái cậu ta?
Dialogue: 0,1:27:46.90,1:27:49.74,Default,,0,0,0,,Ôi, Chúa ơi. Chắc cậu ta\Nđã biết về vụ ngủ qua đêm.
Dialogue: 0,1:27:49.94,1:27:51.01,Default,,0,0,0,,Vụ ngủ qua đêm nào?
Dialogue: 0,1:27:52.55,1:27:54.26,Default,,0,0,0,,Cái đêm cậu ta chết, bọn tôi…
Dialogue: 0,1:27:55.31,1:27:58.09,Default,,0,0,0,,Có tin đồn Trish đã quan hệ với HLV Zane.
Dialogue: 0,1:27:58.29,1:27:59.84,Default,,0,0,0,,Bọn tôi muốn biết có thật không
Dialogue: 0,1:28:00.04,1:28:02.13,Default,,0,0,0,,nên Tiffany rủ cậu ta đến\Nvà chuốc cho say.
Dialogue: 0,1:28:02.33,1:28:03.80,Default,,0,0,0,,- {\i1}Uống thêm đi.{\i0}\N- Chúa ơi!
Dialogue: 0,1:28:04.00,1:28:05.97,Default,,0,0,0,,Cậu ta chối, nhưng Tiffany cứ hỏi,
Dialogue: 0,1:28:06.17,1:28:08.39,Default,,0,0,0,,cậu ta khóc\Nrồi nhốt mình trong phòng Tiffany.
Dialogue: 0,1:28:08.59,1:28:09.64,Default,,0,0,0,,{\i1}Để tôi yên!{\i0}
Dialogue: 0,1:28:09.84,1:28:13.20,Default,,0,0,0,,Và cậu ta hẳn đã gọi cho Doug!\NSau đó thì cậu ta đi về.
Dialogue: 0,1:28:13.74,1:28:16.40,Default,,0,0,0,,Các cô chuốc cô ấy say\Nrồi để cô ấy lái xe về?
Dialogue: 0,1:28:16.60,1:28:18.40,Default,,0,0,0,,Bọn tôi cố cản, nhưng cậu ta giận quá!
Dialogue: 0,1:28:18.60,1:28:22.17,Default,,0,0,0,,Jamie! Ôi, Chúa ơi, là Doug sao?
Dialogue: 0,1:28:22.46,1:28:24.57,Default,,0,0,0,,Ta phải đi ngay. Tôi làm nó hoạt động rồi,
Dialogue: 0,1:28:24.77,1:28:27.09,Default,,0,0,0,,- mà điện thoại gần hết pin.\N- Sao cô có thể?
Dialogue: 0,1:28:27.76,1:28:30.83,Default,,0,0,0,,- Cô đã giết người!\N- Tôi có giết ai đâu.
Dialogue: 0,1:28:31.03,1:28:32.39,Default,,0,0,0,,Có đấy!
Dialogue: 0,1:28:32.64,1:28:36.67,Default,,0,0,0,,Cô giả vờ làm bạn với cô ấy,\Nbắt nạt cô ấy, làm cô ấy say xỉn,
Dialogue: 0,1:28:36.87,1:28:38.38,Default,,0,0,0,,và để cô ấy lái xe về!
Dialogue: 0,1:28:38.58,1:28:39.60,Default,,0,0,0,,Pam không ở đó.
Dialogue: 0,1:28:40.94,1:28:43.44,Default,,0,0,0,,Tiff và tôi cãi nhau vào mùa hè.
Dialogue: 0,1:28:44.19,1:28:45.77,Default,,0,0,0,,Bọn tôi cạch mặt mấy tháng.
Dialogue: 0,1:28:47.19,1:28:49.35,Default,,0,0,0,,Tôi không bao giờ làm chuyện như vậy.
Dialogue: 0,1:28:49.54,1:28:52.85,Default,,0,0,0,,- Cậu nghĩ tôi là con quái vật à?\N- Jamie, phải đi thôi.
Dialogue: 0,1:28:53.05,1:28:55.69,Default,,0,0,0,,Vậy tại sao Doug\Nthề sẽ giết cô ngày nào đó?
Dialogue: 0,1:28:55.88,1:28:57.79,Default,,0,0,0,,- Làm gì có.\N- Cậu ta định thế.
Dialogue: 0,1:28:58.79,1:29:00.21,Default,,0,0,0,,Cậu ta định để lại lời nhắn.
Dialogue: 0,1:29:02.42,1:29:03.42,Default,,0,0,0,,Mảnh giấy này.
Dialogue: 0,1:29:07.50,1:29:10.13,Default,,0,0,0,,Đâu quan trọng, phải không?\NCậu ta chết rồi.
Dialogue: 0,1:29:17.85,1:29:19.60,Default,,0,0,0,,Bố còn đưa tao cái này nữa!
Dialogue: 0,1:29:21.14,1:29:23.21,Default,,0,0,0,,- Thằng khốn!\N- Hắn đến từ tương lai!
Dialogue: 0,1:29:23.41,1:29:25.55,Default,,0,0,0,,Tôi đã phá mặt nạ!\NĐó là Doug trưởng thành!
Dialogue: 0,1:29:25.75,1:29:28.18,Default,,0,0,0,,Cháu đã giết Doug!\NĐâu còn Doug trưởng thành nữa!
Dialogue: 0,1:29:28.38,1:29:30.94,Default,,0,0,0,,- Vậy sao hắn đến được đây?\N- Amelia.
Dialogue: 0,1:29:37.33,1:29:40.20,Default,,0,0,0,,Ôi, Chúa ơi. Amelia.
Dialogue: 0,1:29:41.71,1:29:43.07,Default,,0,0,0,,Amelia!
Dialogue: 0,1:29:43.27,1:29:45.54,Default,,0,0,0,,Amelia. Amelia, nghe thấy mẹ không?
Dialogue: 0,1:29:57.47,1:29:58.76,Default,,0,0,0,,Ôi, Chúa ơi!
Dialogue: 0,1:29:59.68,1:30:01.98,Default,,0,0,0,,- Chạy! Chạy đi!\N- Chạy đi!
Dialogue: 0,1:30:09.65,1:30:13.30,Default,,0,0,0,,- Cú Rơi Lượng Tử hiệu quả thật chứ?\N- Miễn là KZFD phát sóng trực tiếp!
Dialogue: 0,1:30:13.50,1:30:15.85,Default,,0,0,0,,{\an8}Và từ KZFD, tôi là Norm Dubasage,
Dialogue: 0,1:30:16.05,1:30:18.85,Default,,0,0,0,,{\an8}- chúc quý vị…\N- Đừng dừng quay! Sát nhân ở đây!
Dialogue: 0,1:30:19.05,1:30:20.52,Default,,0,0,0,,{\an8}- Cứ quay đi.\N- Đợi chút.
Dialogue: 0,1:30:20.72,1:30:24.71,Default,,0,0,0,,{\an8}Có vẻ như là một cuộc rượt đuổi\Nvà một cô gái trẻ vừa cảnh báo…
Dialogue: 0,1:30:27.17,1:30:29.36,Default,,0,0,0,,{\an8}Đừng dừng lại. Cứ quay đi.
Dialogue: 0,1:30:29.56,1:30:31.70,Default,,0,0,0,,Trời ơi, ông ấy vừa bị đâm vào đầu!
Dialogue: 0,1:30:31.90,1:30:34.24,Default,,0,0,0,,Cháu chưa thể đi.\NChưa cản được hắn. Mẹ cháu!
Dialogue: 0,1:30:34.44,1:30:37.37,Default,,0,0,0,,Giờ đi hoặc không bao giờ.\NTôi sẽ đưa cháu về lúc đó.
Dialogue: 0,1:30:37.57,1:30:39.16,Default,,0,0,0,,Cháu có thể cứu mẹ.
Dialogue: 0,1:30:39.36,1:30:42.29,Default,,0,0,0,,Đây là nơi duy nhất\Nkhông bị ảnh hưởng bởi lực ly tâm.
Dialogue: 0,1:30:42.49,1:30:44.38,Default,,0,0,0,,Khi đã đủ tốc độ, cháu phải ở trong này.
Dialogue: 0,1:30:44.58,1:30:45.80,Default,,0,0,0,,Bị ngã ra ngoài thì sao?
Dialogue: 0,1:30:45.99,1:30:48.76,Default,,0,0,0,,Cháu sẽ bị ném vào tường\Nvới lực phóng tên lửa
Dialogue: 0,1:30:48.96,1:30:50.78,Default,,0,0,0,,và có thể bị nổ tung. Chúc may mắn.
Dialogue: 0,1:30:50.98,1:30:52.28,Default,,0,0,0,,Chào con tôi hộ tôi.
Dialogue: 0,1:30:54.40,1:30:55.72,Default,,0,0,0,,- Đưa mẹ cháu cái này.\N- Ừ.
Dialogue: 0,1:30:55.92,1:30:57.24,Default,,0,0,0,,- Tạm biệt.\N- Tạm biệt.
Dialogue: 0,1:30:57.82,1:30:58.74,Default,,0,0,0,,Đi thôi.
Dialogue: 0,1:31:00.12,1:31:02.91,Default,,0,0,0,,Cô ra khỏi đây đi! Đi đi. Ra ngoài!
Dialogue: 0,1:31:03.66,1:31:05.52,Default,,0,0,0,,- Đi!\N- Cháu sẽ đưa hắn đi cùng!
Dialogue: 0,1:31:05.72,1:31:07.62,Default,,0,0,0,,Còn hơn để hắn ở đây! Đi đi!
Dialogue: 0,1:31:14.92,1:31:16.13,Default,,0,0,0,,Mẹ ơi, đừng!
Dialogue: 0,1:31:27.98,1:31:30.17,Default,,0,0,0,,- Mẹ làm gì ở đây?\N- Không biết nữa!
Dialogue: 0,1:31:30.37,1:31:31.30,Default,,0,0,0,,Cậu tiên đoán là
Dialogue: 0,1:31:31.50,1:31:33.88,Default,,0,0,0,,tôi sẽ ở Cú Rơi Lượng Tử\Ncho thời khắc trọng đại.
Dialogue: 0,1:31:34.08,1:31:35.94,Default,,0,0,0,,Tôi nghĩ có liên quan đến cậu.
Dialogue: 0,1:31:36.86,1:31:38.61,Default,,0,0,0,,Hai ta đã từng làm việc này.
Dialogue: 0,1:31:46.66,1:31:48.00,Default,,0,0,0,,Hãy hạ tên khốn này!
Dialogue: 0,1:32:43.01,1:32:44.05,Default,,0,0,0,,Không!
Dialogue: 0,1:32:45.35,1:32:46.35,Default,,0,0,0,,Không!
Dialogue: 0,1:33:01.95,1:33:03.12,Default,,0,0,0,,Không!
Dialogue: 0,1:33:15.63,1:33:16.96,Default,,0,0,0,,Không!
Dialogue: 0,1:33:28.35,1:33:32.23,Default,,0,0,0,,- Ông đã giết mẹ tôi!\N- Có vẻ là hai lần rồi.
Dialogue: 0,1:33:32.77,1:33:33.94,Default,,0,0,0,,Và bố ông nữa!
Dialogue: 0,1:33:34.77,1:33:36.23,Default,,0,0,0,,Đó là cơm thêm thôi.
Dialogue: 0,1:33:36.65,1:33:38.59,Default,,0,0,0,,Ta vẫn còn sống ở năm 1987.
Dialogue: 0,1:33:38.79,1:33:42.26,Default,,0,0,0,,Giờ, Chris không phải\Nsống cùng ông bố rác rưởi,
Dialogue: 0,1:33:42.46,1:33:46.53,Default,,0,0,0,,người quan tâm đến di sản ông ta\Nhơn chính con trai ông ta nữa!
Dialogue: 0,1:33:48.08,1:33:51.69,Default,,0,0,0,,Mẹ tôi chưa từng nhận được\Ngiấy nhắn trong tủ đồ năm 1987.
Dialogue: 0,1:33:51.89,1:33:56.29,Default,,0,0,0,,Ông viết nó sau khi bà ấy chết.\NTại sao ông lại làm việc này?
Dialogue: 0,1:33:56.71,1:33:57.92,Default,,0,0,0,,Nối tiếp câu chuyện.
Dialogue: 0,1:33:58.50,1:34:02.70,Default,,0,0,0,,Thật khó để duy trì sự hứng thú\Nvới ba vụ án mạng từ 35 năm trước.
Dialogue: 0,1:34:02.90,1:34:06.43,Default,,0,0,0,,Nhưng giờ Sát Nhân Tuổi 16\Nđã trở lại và hắn là ông ba bị.
Dialogue: 0,1:34:07.55,1:34:10.60,Default,,0,0,0,,Hắn có thể ở mọi nơi,\Nmọi lúc. Không thể bị giết.
Dialogue: 0,1:34:11.64,1:34:13.56,Default,,0,0,0,,Hắn là Michael Myers.
Dialogue: 0,1:34:14.85,1:34:16.13,Default,,0,0,0,,Còn ta là chuyên gia.
Dialogue: 0,1:34:16.33,1:34:19.80,Default,,0,0,0,,Dù vậy, ta không hề biết\Ntên sát nhân ban đầu là Doug,
Dialogue: 0,1:34:20.00,1:34:21.26,Default,,0,0,0,,nên cảm ơn cô vì việc đó.
Dialogue: 0,1:34:21.46,1:34:24.51,Default,,0,0,0,,Người ta càng quan tâm\Nvề Sát Nhân Tuổi 16,
Dialogue: 0,1:34:24.71,1:34:28.81,Default,,0,0,0,,ta càng trở nên nổi tiếng. Có lẽ,\Nngày nào đó, ta sẽ có giải Pulitzer.
Dialogue: 0,1:34:29.01,1:34:31.02,Default,,0,0,0,,Sẽ không có ai biết về ông,
Dialogue: 0,1:34:31.22,1:34:33.73,Default,,0,0,0,,bởi ông sẽ chết\Ntrong cỗ máy thời gian của tôi!
Dialogue: 0,1:34:33.93,1:34:35.67,Default,,0,0,0,,Nếu ta ở trung tâm thì không.
Dialogue: 0,1:34:58.52,1:35:01.19,Default,,0,0,0,,Ta có cả hai con dao rồi!
Dialogue: 0,1:35:07.95,1:35:09.93,Default,,0,0,0,,Cô không nên du hành thời gian.
Dialogue: 0,1:35:10.13,1:35:12.79,Default,,0,0,0,,Và ông không nên động đến gia đình tôi!
Dialogue: 0,1:35:18.04,1:35:19.08,Default,,0,0,0,,Tận hưởng đi nhé.
Dialogue: 0,1:35:25.05,1:35:26.01,Default,,0,0,0,,Sương hồng.
Dialogue: 0,1:35:29.55,1:35:31.87,Default,,0,0,0,,{\i1}- Thời gian như dòng sông.{\i0}\N{\i1}- Cậu bị loại rồi!{\i0}
Dialogue: 0,1:35:32.07,1:35:33.50,Default,,0,0,0,,{\i1}Tôi là Jamie. Từ Canada.{\i0}
Dialogue: 0,1:35:33.70,1:35:36.54,Default,,0,0,0,,{\i1}Chả ai muốn nghe{\i0}\N{\i1}cậu nói yêu mẹ ra sao đâu.{\i0}
Dialogue: 0,1:35:36.74,1:35:39.59,Default,,0,0,0,,{\i1}- Trời ạ, Canada. Căng thế?{\i0}\N{\i1}- Cháu đã cố ngăn nó!{\i0}
Dialogue: 0,1:35:39.79,1:35:43.01,Default,,0,0,0,,{\i1}- Tôi thích áo của cậu.{\i0}\N{\i1}- Đó là điều con gái sẽ nói.{\i0}
Dialogue: 0,1:35:43.21,1:35:45.28,Default,,0,0,0,,{\i1}Con yêu. Bảo trọng nhé. Mẹ yêu con!{\i0}
Dialogue: 0,1:36:07.09,1:36:08.18,Default,,0,0,0,,Mẹ!
Dialogue: 0,1:36:10.18,1:36:11.51,Default,,0,0,0,,Ôi, Chúa ơi!
Dialogue: 0,1:36:14.85,1:36:15.85,Default,,0,0,0,,Mẹ!
Dialogue: 0,1:36:19.19,1:36:20.19,Default,,0,0,0,,Mẹ!
Dialogue: 0,1:36:22.31,1:36:24.65,Default,,0,0,0,,Mẹ! Mẹ ơi!
Dialogue: 0,1:36:27.24,1:36:30.24,Default,,0,0,0,,Mẹ!
Dialogue: 0,1:36:31.62,1:36:33.53,Default,,0,0,0,,Con yêu! Con yêu à.
Dialogue: 0,1:36:34.12,1:36:36.83,Default,,0,0,0,,Mẹ! Mẹ, mẹ vẫn ổn.
Dialogue: 0,1:36:39.92,1:36:41.61,Default,,0,0,0,,- Con yêu mẹ.\N- Mẹ cũng yêu con.
Dialogue: 0,1:36:41.81,1:36:43.32,Default,,0,0,0,,- Sao vậy?\N- Chuyện gì thế?
Dialogue: 0,1:36:43.52,1:36:45.63,Default,,0,0,0,,- Bố! Bố ơi.\N- Sao thế?
Dialogue: 0,1:36:45.83,1:36:48.01,Default,,0,0,0,,Con à, sao mặt con đầy máu vậy?
Dialogue: 0,1:36:48.67,1:36:50.59,Default,,0,0,0,,Con bị ngã xe ấy mà.
Dialogue: 0,1:36:51.22,1:36:52.33,Default,,0,0,0,,- Chúa ơi!\N- Chúa ơi!
Dialogue: 0,1:36:52.53,1:36:53.47,Default,,0,0,0,,Ôi, Chúa ơi!
Dialogue: 0,1:36:55.89,1:36:57.33,Default,,0,0,0,,Hai ngươi làm gì ở đây?
Dialogue: 0,1:36:57.53,1:36:59.46,Default,,0,0,0,,Con nói "làm gì ở đây" là sao?
Dialogue: 0,1:36:59.66,1:37:01.59,Default,,0,0,0,,Hàng năm vẫn tụ tập ngày Halloween mà.
Dialogue: 0,1:37:01.79,1:37:05.84,Default,,0,0,0,,Phải rồi. Đó là lý do\Nmẹ ăn mặc như Molly Ringwald.
Dialogue: 0,1:37:06.04,1:37:08.26,Default,,0,0,0,,Mẹ ư? Không. Mẹ đã mặc đồ hóa trang đâu.
Dialogue: 0,1:37:08.46,1:37:12.91,Default,,0,0,0,,Không, năm nay, mẹ con và bố\Nsẽ hóa thân thành ông Smith và bà Smith.
Dialogue: 0,1:37:13.82,1:37:15.16,Default,,0,0,0,,Phải. Đúng thế.
Dialogue: 0,1:37:16.87,1:37:18.95,Default,,0,0,0,,Cô gặp cháu bên ngoài chút nhé?
Dialogue: 0,1:37:19.96,1:37:20.96,Default,,0,0,0,,Xin lỗi.
Dialogue: 0,1:37:21.41,1:37:24.28,Default,,0,0,0,,Cô rất muốn\Nở Cú Rơi Lượng Tử khi cháu đến nơi
Dialogue: 0,1:37:24.48,1:37:25.59,Default,,0,0,0,,nhưng tính sai giờ.
Dialogue: 0,1:37:26.21,1:37:29.36,Default,,0,0,0,,Cô tạo ra du hành thời gian,\Nnhưng quên mất quy ước giờ mùa hè.
Dialogue: 0,1:37:29.56,1:37:31.72,Default,,0,0,0,,Thật mừng khi không cần đến thứ này.
Dialogue: 0,1:37:31.97,1:37:33.41,Default,,0,0,0,,Chúa ơi, cô mang súng?
Dialogue: 0,1:37:33.61,1:37:35.62,Default,,0,0,0,,Cô đâu biết ai sẽ bước ra từ chỗ đó!
Dialogue: 0,1:37:35.82,1:37:38.04,Default,,0,0,0,,Cô đã đợi 35 năm để xem ai thắng.
Dialogue: 0,1:37:38.24,1:37:40.85,Default,,0,0,0,,- Có nhiều thứ phải cập nhật cho cháu.\N- Vâng.
Dialogue: 0,1:37:42.64,1:37:43.92,Default,,0,0,0,,{\i1}Những điểm khác biệt.{\i0}
Dialogue: 0,1:37:44.12,1:37:47.51,Default,,0,0,0,,Cô phải đoán vài thứ, dĩ nhiên,\Nnhưng về tổng thể chắc là đúng.
Dialogue: 0,1:37:47.71,1:37:51.14,Default,,0,0,0,,Chào. Xin lỗi, cháu đến trễ.\NJason và Veronica đang đến.
Dialogue: 0,1:37:51.34,1:37:52.82,Default,,0,0,0,,Hai người làm gì ngoài này?
Dialogue: 0,1:37:53.41,1:37:56.27,Default,,0,0,0,,Giúp cô bé làm bài tập khoa học ấy mà.
Dialogue: 0,1:37:56.47,1:37:58.91,Default,,0,0,0,,- Bọn cô vào ngay.\N- Máu giả đẹp đấy.
Dialogue: 0,1:37:59.79,1:38:01.16,Default,,0,0,0,,- Nhìn như thật.\N- Cảm ơn.
Dialogue: 0,1:38:04.46,1:38:05.48,Default,,0,0,0,,Ai thế?
Dialogue: 0,1:38:05.68,1:38:10.45,Default,,0,0,0,,Ừ, cháu có nhớ đã từng cố\Nchia rẽ bố mẹ cháu hồi trung học chứ?
Dialogue: 0,1:38:10.65,1:38:15.93,Default,,0,0,0,,Việc đó không thành. Họ lập tức thành đôi\Nvà đó là anh trai 34 tuổi của cháu.
Dialogue: 0,1:38:17.01,1:38:18.85,Default,,0,0,0,,Sao chuyện đó cứ xảy ra thế?
Dialogue: 0,1:38:19.81,1:38:21.15,Default,,0,0,0,,Và tên cậu bé là Jamie.
Dialogue: 0,1:38:21.35,1:38:22.58,Default,,0,0,0,,Bọn cháu cùng tên Jamie?
Dialogue: 0,1:38:22.78,1:38:25.31,Default,,0,0,0,,Colette, con yêu,\Ngiúp anh con dọn bàn nhé?
Dialogue: 0,1:38:27.98,1:38:31.43,Default,,0,0,0,,- Vâng, con vào ngay.\N- Được rồi. Và bà ngoại con gọi.
Dialogue: 0,1:38:31.63,1:38:34.49,Default,,0,0,0,,Bà gửi lời yêu con\Nvà hẹn gặp con vào lễ Tạ Ơn.
Dialogue: 0,1:38:37.37,1:38:39.70,Default,,0,0,0,,- Cháu nên đọc luôn đi.\N- Vâng.
Dialogue: 0,1:38:46.25,1:38:47.96,Default,,0,0,0,,Mẹ kiếp du hành thời gian.
Dialogue: 0,1:38:52.55,1:38:56.41,Default,,0,0,0,,{\an8}Cháu có anh trai 34 tuổi tên Jamie\N(xem phần 'Colette'),
Dialogue: 0,1:38:56.61,1:38:59.47,Default,,0,0,0,,{\an8}có chồng tên Jason\Nvà con gái ba tuổi tên Veronica.
Dialogue: 0,1:39:00.01,1:39:03.25,Default,,0,0,0,,Cô là kỹ sư sinh học\N(đừng lo vì cô là kỹ sư sinh học tốt!)
Dialogue: 0,1:39:03.45,1:39:07.35,Default,,0,0,0,,Amelia vẫn là bạn thân của cháu\Nvà là một thiên tài.
Dialogue: 0,1:39:07.94,1:39:09.26,Default,,0,0,0,,Và giờ là tin sốc:
Dialogue: 0,1:39:09.46,1:39:12.13,Default,,0,0,0,,Randy là hiệu trưởng\Ntrường Trung học Vernon.
Dialogue: 0,1:39:12.33,1:39:14.69,Default,,0,0,0,,Nhớ hồi chú ấy ném cháu xuống\Nvà ợ vào mặt cháu chứ?
Dialogue: 0,1:39:15.53,1:39:18.89,Default,,0,0,0,,Kara là cảnh sát trưởng.\NCô ấy vui khi cần sa hợp pháp.
Dialogue: 0,1:39:19.09,1:39:21.94,Default,,0,0,0,,Bánh brownie của cô ấy\Nphê như "Snoop Dogg".
Dialogue: 0,1:39:22.14,1:39:24.35,Default,,0,0,0,,{\an8}Lurch tạo ra đế chế trò chơi\Ncó tên "Sương Hồng".
Dialogue: 0,1:39:24.55,1:39:28.58,Default,,0,0,0,,{\an8}Và lập ra tổ chức chống bắt nạt\Nnhằm vinh danh em gái Trish.
Dialogue: 0,1:39:29.67,1:39:30.99,Default,,0,0,0,,{\an8}Eddie Royal hát cho ban nhạc\Ncó tên Cách Một Giường Nước.
Dialogue: 0,1:39:31.19,1:39:33.07,Default,,0,0,0,,{\an8}Chú ấy nói với Rolling Stone\Nrằng chú ấy chọn cái tên đó bởi vì
Dialogue: 0,1:39:33.27,1:39:35.63,Default,,0,0,0,,{\an8}"đó là khoảng cách\Ngiữa chú ấy và cái chết".
Dialogue: 0,1:39:36.55,1:39:39.37,Default,,0,0,0,,{\an8}Cái chết của bố gây sang chấn cho Chris.
Dialogue: 0,1:39:39.57,1:39:42.21,Default,,0,0,0,,{\an8}Cô nghe nói chú ấy đang tu ở Ấn Độ.
Dialogue: 0,1:39:42.41,1:39:44.22,Default,,0,0,0,,{\an8}Cô vẫn để ý chú ấy, phòng khi…
Dialogue: 0,1:43:22.94,1:43:24.94,Default,,0,0,0,,Giám sát sáng tạo\NAnnie Kim
