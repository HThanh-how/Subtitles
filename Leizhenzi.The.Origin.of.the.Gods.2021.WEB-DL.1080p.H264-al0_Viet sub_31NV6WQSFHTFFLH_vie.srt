Dialogue: 0,0:00:01.45,0:00:02.50,Default,,0,0,0,,[Tương truyền,]
Dialogue: 0,0:00:02.50,0:00:05.70,Default,,0,0,0,,[khi xưa Thiên Tôn đã bày ra trận Phong Thần,]
Dialogue: 0,0:00:06.41,0:00:08.82,Default,,0,0,0,,[nếu sinh linh trên thế gian\Nchịu cảnh lầm than,]
Dialogue: 0,0:00:09.10,0:00:11.02,Default,,0,0,0,,[sẽ có Tướng Tinh trên trời,]
Dialogue: 0,0:00:11.02,0:00:13.77,Default,,0,0,0,,[trời thả Thần Linh hạ phàm,]
Dialogue: 0,0:00:14.14,0:00:16.46,Default,,0,0,0,,[cùng nhau thảo phạt hôn quân,]
Dialogue: 0,0:00:16.82,0:00:18.86,Default,,0,0,0,,[thay triều đổi đại.]
Dialogue: 0,0:00:20.54,0:00:23.42,Default,,0,0,0,,Tâm như nước tĩnh.
Dialogue: 0,0:00:24.18,0:00:27.30,Default,,0,0,0,,Tâm như nước tĩnh.
Dialogue: 0,0:00:28.29,0:00:30.61,Default,,0,0,0,,Tâm như nước tĩnh.
Dialogue: 0,0:00:29.94,0:00:32.41,Default,,0,0,0,,[Tiên nhân Vân Trung Tử]
Dialogue: 0,0:00:31.37,0:00:32.94,Default,,0,0,0,,Tâm như nước tĩnh!
Dialogue: 0,0:00:32.94,0:00:34.98,Default,,0,0,0,,Tâm như nước tĩnh!
Dialogue: 0,0:01:06.42,0:01:07.01,Default,,0,0,0,,Hừ!
Dialogue: 0,0:01:08.42,0:01:11.46,Default,,0,0,0,,Một đốm lửa nhỏ có thể thiêu cháy cả đồng cỏ
Dialogue: 0,0:01:11.86,0:01:14.82,Default,,0,0,0,,nhưng tâm như nước tĩnh...
Dialogue: 0,0:01:17.46,0:01:18.02,Default,,0,0,0,,Á!
Dialogue: 0,0:01:24.46,0:01:25.73,Default,,0,0,0,,Trận Phong Thần.
Dialogue: 0,0:02:15.89,0:02:17.62,Default,,0,0,0,,Hôm nay trận đã khởi động,
Dialogue: 0,0:02:18.65,0:02:20.90,Default,,0,0,0,,ngươi bị lửa của Tướng Tinh đốt cháy,
Dialogue: 0,0:02:21.17,0:02:23.10,Default,,0,0,0,,cũng là số kiếp của ngươi.
Dialogue: 0,0:02:23.93,0:02:25.13,Default,,0,0,0,,Hãy cùng hắn
Dialogue: 0,0:02:25.54,0:02:27.06,Default,,0,0,0,,vào trần gian đi.
Dialogue: 0,0:02:46.34,0:02:48.94,Default,,0,0,0,,[Phủ Tây Bá hầu, Tây Kỳ]
Dialogue: 0,0:02:46.42,0:02:47.90,Default,,0,0,0,,[Lớn thay cái đức đầu cả của Kiền,]
Dialogue: 0,0:02:48.18,0:02:49.77,Default,,0,0,0,,[muôn vật nhờ đó khởi đầu.]
Dialogue: 0,0:02:50.30,0:02:51.94,Default,,0,0,0,,[Lưu chuyển lửa trời,]
Dialogue: 0,0:02:52.14,0:02:53.82,Default,,0,0,0,,[cai quản từ đầu đến cuối,]
Dialogue: 0,0:02:54.17,0:02:55.82,Default,,0,0,0,,[sáu ngôi đều theo thì vận mà nên.]
Dialogue: 0,0:02:56.37,0:02:57.86,Default,,0,0,0,,[Hào "cửu ngũ".]
Dialogue: 0,0:03:03.30,0:03:04.82,Default,,0,0,0,,[Càn khôn mới ổn định.]
Dialogue: 0,0:03:08.17,0:03:10.72,Default,,0,0,0,,[Tây Bá hầu Cơ Xương]
Dialogue: 0,0:03:27.02,0:03:28.06,Default,,0,0,0,,Đừng qua đây!
Dialogue: 0,0:03:31.66,0:03:32.98,Default,,0,0,0,,Cứu với!
Dialogue: 0,0:03:34.86,0:03:36.26,Default,,0,0,0,,Đừng qua đây!
Dialogue: 0,0:03:37.62,0:03:39.10,Default,,0,0,0,,Tránh ra! Tránh ra!
Dialogue: 0,0:03:43.74,0:03:45.22,Default,,0,0,0,,Đừng qua đây!
Dialogue: 0,0:03:55.68,0:03:57.40,Default,,0,0,0,,[Lầu Trích Tinh, Triều Ca]
Dialogue: 0,0:04:00.14,0:04:01.58,Default,,0,0,0,,Này, các người xem kìa,
Dialogue: 0,0:04:01.58,0:04:02.98,Default,,0,0,0,,trên trời có thứ gì vậy?
Dialogue: 0,0:04:02.98,0:04:04.10,Default,,0,0,0,,Chắc là đom đóm.
Dialogue: 0,0:04:04.42,0:04:05.34,Default,,0,0,0,,Vừa Lập xuân,
Dialogue: 0,0:04:05.34,0:04:06.74,Default,,0,0,0,,đom đóm ở đâu ra?
Dialogue: 0,0:04:07.18,0:04:08.14,Default,,0,0,0,,Đúng đó.
Dialogue: 0,0:05:00.10,0:05:02.48,Default,,0,0,0,,[Triều Ca, Văn Thái sư]
Dialogue: 0,0:05:01.66,0:05:03.14,Default,,0,0,0,,Thiên thạch Tứ Thú,
Dialogue: 0,0:05:03.42,0:05:04.62,Default,,0,0,0,,châu chấu trên trời xuất hiện,
Dialogue: 0,0:05:05.65,0:05:07.26,Default,,0,0,0,,Phong Thần khởi động,
Dialogue: 0,0:05:07.78,0:05:09.22,Default,,0,0,0,,Tướng Tinh đến.
Dialogue: 0,0:05:10.26,0:05:11.50,Default,,0,0,0,,Tộc Vũ Nhân
Dialogue: 0,0:05:11.50,0:05:12.82,Default,,0,0,0,,có một pháp khí
Dialogue: 0,0:05:13.70,0:05:14.58,Default,,0,0,0,,tên là
Dialogue: 0,0:05:14.74,0:05:16.21,Default,,0,0,0,,đôi cánh Phong Lôi.
Dialogue: 0,0:05:16.65,0:05:17.70,Default,,0,0,0,,Con của Tướng Tinh
Dialogue: 0,0:05:17.90,0:05:20.34,Default,,0,0,0,,chỉ có hợp thành một thể\Nvới đôi cánh Phong Lôi
Dialogue: 0,0:05:20.86,0:05:22.18,Default,,0,0,0,,mới có thể cưỡi gió
Dialogue: 0,0:05:22.46,0:05:24.18,Default,,0,0,0,,lên thẳng Cửu Trùng Thiên.
Dialogue: 0,0:05:24.66,0:05:25.77,Default,,0,0,0,,Tộc Vũ Nhân?
Dialogue: 0,0:05:26.14,0:05:26.86,Default,,0,0,0,,Có điều,
Dialogue: 0,0:05:27.21,0:05:28.82,Default,,0,0,0,,bổn thái sư sẽ không cho chúng
Dialogue: 0,0:05:29.38,0:05:31.49,Default,,0,0,0,,có cơ hội diệt Ân Thương ta đâu.
Dialogue: 0,0:05:54.14,0:05:56.90,Default,,0,0,0,,Đừng phản kháng vô ích nữa,
Dialogue: 0,0:05:57.22,0:05:59.42,Default,,0,0,0,,nếu không muốn diệt tộc từ đây
Dialogue: 0,0:05:59.94,0:06:04.02,Default,,0,0,0,,thì giao đôi cánh Phong Lôi\Nvà Tướng Tinh ra đây.
Dialogue: 0,0:06:00.19,0:06:02.96,Default,,0,0,0,,[Tộc trưởng tộc Vũ Nhân]
Dialogue: 0,0:06:06.14,0:06:07.42,Default,,0,0,0,,Mơ giữa ban ngày!
Dialogue: 0,0:06:08.06,0:06:11.10,Default,,0,0,0,,Vậy thì đừng trách ta không khách sáo.
Dialogue: 0,0:06:27.14,0:06:28.34,Default,,0,0,0,,Vân Trung Tử.
Dialogue: 0,0:06:30.06,0:06:32.42,Default,,0,0,0,,Sao ngươi lại thành bộ dạng quỷ ma thế này?
Dialogue: 0,0:06:32.77,0:06:33.77,Default,,0,0,0,,Văn lão tặc,
Dialogue: 0,0:06:34.42,0:06:36.78,Default,,0,0,0,,ngươi không cảm thấy rất buồn cười à?
Dialogue: 0,0:06:40.22,0:06:41.38,Default,,0,0,0,,Đôi cánh Phong Lôi
Dialogue: 0,0:06:41.86,0:06:43.22,Default,,0,0,0,,một khi đã hợp thể
Dialogue: 0,0:06:43.50,0:06:44.90,Default,,0,0,0,,thì ngày lấy nó ra
Dialogue: 0,0:06:45.09,0:06:47.30,Default,,0,0,0,,cũng là lúc hồn bay phách tán.
Dialogue: 0,0:06:48.14,0:06:49.61,Default,,0,0,0,,Con của Tướng Tinh,
Dialogue: 0,0:06:50.42,0:06:52.10,Default,,0,0,0,,đạo của chúng sinh,
Dialogue: 0,0:06:52.54,0:06:54.66,Default,,0,0,0,,giao hết cho ngươi đó.
Dialogue: 0,0:07:01.86,0:07:03.22,Default,,0,0,0,,Con của Tướng Tinh!
Dialogue: 0,0:07:54.82,0:07:55.85,Default,,0,0,0,,Vân Trung Tử.
Dialogue: 0,0:07:57.38,0:07:59.50,Default,,0,0,0,,Ngươi lại làm hỏng chuyện tốt của lão phu!
Dialogue: 0,0:08:01.54,0:08:03.02,Default,,0,0,0,,Lão tặc Văn Trọng nhà ngươi,
Dialogue: 0,0:08:03.70,0:08:05.62,Default,,0,0,0,,có Vân Trung Tử ta ở đây,
Dialogue: 0,0:08:05.74,0:08:07.58,Default,,0,0,0,,đừng mơ làm gì đến Tướng Tinh.
Dialogue: 0,0:08:08.82,0:08:10.06,Default,,0,0,0,,Vậy thì xem thử
Dialogue: 0,0:08:10.18,0:08:11.62,Default,,0,0,0,,pháp lực của ai
Dialogue: 0,0:08:12.13,0:08:13.18,Default,,0,0,0,,mạnh hơn.
Dialogue: 0,0:08:22.10,0:08:23.70,Default,,0,0,0,,Nhân pháp Địa, Địa pháp Thiên, Thiên pháp Đạo,
Dialogue: 0,0:08:23.70,0:08:26.09,Default,,0,0,0,,Đạo pháp Tự nhiên!
Dialogue: 0,0:09:29.42,0:09:30.10,Default,,0,0,0,,Tiên trưởng!
Dialogue: 0,0:09:30.45,0:09:31.89,Default,,0,0,0,,Đứa bé này chính là Tướng Tinh,
Dialogue: 0,0:09:32.38,0:09:33.65,Default,,0,0,0,,ta sẽ giao nó cho ngươi.
Dialogue: 0,0:09:34.26,0:09:35.46,Default,,0,0,0,,Nó đã có tên rồi,
Dialogue: 0,0:09:35.73,0:09:36.70,Default,,0,0,0,,tên là Tân Hoàn,
Dialogue: 0,0:09:37.02,0:09:38.93,Default,,0,0,0,,trong người có đôi cánh Phong Lôi.
Dialogue: 0,0:09:39.14,0:09:39.81,Default,,0,0,0,,Mười tám năm sau
Dialogue: 0,0:09:40.10,0:09:42.33,Default,,0,0,0,,nhất định sẽ khởi động trận Phong Thần.
Dialogue: 0,0:09:42.66,0:09:43.70,Default,,0,0,0,,Được.
Dialogue: 0,0:09:46.82,0:09:48.86,Default,,0,0,0,,Đứa này cũng là hậu nhân của tộc Vũ Nhân,
Dialogue: 0,0:09:49.46,0:09:50.37,Default,,0,0,0,,ta sẽ đưa nó về núi
Dialogue: 0,0:09:50.50,0:09:51.30,Default,,0,0,0,,nuôi nấng trưởng thành.
Dialogue: 0,0:10:08.94,0:10:09.54,Default,,0,0,0,,Tiên trưởng,
Dialogue: 0,0:10:10.50,0:10:12.58,Default,,0,0,0,,ta thấy đứa bé này có duyên với ta,
Dialogue: 0,0:10:13.58,0:10:14.98,Default,,0,0,0,,hay là ta đưa cả hai đứa về.
Dialogue: 0,0:10:15.42,0:10:16.01,Default,,0,0,0,,Đặt tên nó
Dialogue: 0,0:10:16.45,0:10:17.58,Default,,0,0,0,,là Lôi Chấn Tử đi.
Dialogue: 0,0:10:22.44,0:10:27.63,Default,,0,0,0,,[Lôi Chấn Tử: Phong Thần Duyên Khởi]
Dialogue: 0,0:10:32.61,0:10:34.42,Default,,0,0,0,,Nhảy đi! Nhảy mau lên!
Dialogue: 0,0:10:34.42,0:10:35.58,Default,,0,0,0,,Mau nhảy đi!
Dialogue: 0,0:10:35.58,0:10:36.30,Default,,0,0,0,,Ngươi nhảy đi chứ, Lôi Chấn Tử!
Dialogue: 0,0:10:36.30,0:10:37.50,Default,,0,0,0,,Đứng lâu quá trời mà không chịu nhảy.
Dialogue: 0,0:10:37.50,0:10:38.22,Default,,0,0,0,,Có phải sợ tè ra quần rồi không? Nhanh lên đi.
Dialogue: 0,0:10:38.22,0:10:40.02,Default,,0,0,0,,Chẳng phải ngươi nói chờ ngươi mười tám tuổi\Nsẽ có cánh thần trời sinh sao?
Dialogue: 0,0:10:40.02,0:10:41.02,Default,,0,0,0,,Nhanh lên đi, đờ ra đó làm gì?
Dialogue: 0,0:10:41.02,0:10:42.18,Default,,0,0,0,,Ta nhìn mà sốt ruột giùm luôn đó.
Dialogue: 0,0:10:42.46,0:10:43.50,Default,,0,0,0,,Nó là thằng nhát gan! Nhảy đi!
Dialogue: 0,0:10:43.50,0:10:44.78,Default,,0,0,0,,Chỉ có mấy mét thôi mà,
Dialogue: 0,0:10:44.78,0:10:46.39,Default,,0,0,0,,đừng nói là ngươi không bay qua được nha.
Dialogue: 0,0:10:46.48,0:10:48.33,Default,,0,0,0,,Đứng lâu quá trời mà không chịu nhảy.
Dialogue: 0,0:10:48.62,0:10:49.22,Default,,0,0,0,,Nhảy đi!
Dialogue: 0,0:10:49.33,0:10:50.94,Default,,0,0,0,,Hắn tưởng hắn là Tân Hoàn chắc?
Dialogue: 0,0:10:51.26,0:10:52.78,Default,,0,0,0,,Câm mồm, đừng nhắc hắn với ta!
Dialogue: 0,0:10:54.98,0:10:56.66,Default,,0,0,0,,Vậy ngươi nhảy xuống mau đi!
Dialogue: 0,0:10:56.66,0:10:57.69,Default,,0,0,0,,Nhảy đi!
Dialogue: 0,0:10:57.69,0:11:00.01,Default,,0,0,0,,Nhảy đi đồ nhát gan,\Nđừng nói ngươi sợ chết nha.
Dialogue: 0,0:11:00.01,0:11:00.89,Default,,0,0,0,,Mau nhảy đi, nhanh lên!
Dialogue: 0,0:11:00.98,0:11:01.38,Default,,0,0,0,,Nhanh lên!
Dialogue: 0,0:11:01.50,0:11:02.38,Default,,0,0,0,,Nhảy đi!
Dialogue: 0,0:11:02.38,0:11:03.50,Default,,0,0,0,,Không phải đồ nhát gan thì nhảy nhanh đi!
Dialogue: 0,0:11:06.74,0:11:07.58,Default,,0,0,0,,Nhảy nhanh đi, đồ nhát gan.
Dialogue: 0,0:11:21.34,0:11:22.34,Default,,0,0,0,,Cười ta!
Dialogue: 0,0:11:22.62,0:11:23.58,Default,,0,0,0,,Cười ta hả!
Dialogue: 0,0:11:23.58,0:11:24.33,Default,,0,0,0,,Không không không không.
Dialogue: 0,0:11:24.42,0:11:24.86,Default,,0,0,0,,Là hắn!
Dialogue: 0,0:11:24.94,0:11:25.58,Default,,0,0,0,,Là hắn!
Dialogue: 0,0:11:25.65,0:11:26.22,Default,,0,0,0,,Không phải ta!
Dialogue: 0,0:11:26.22,0:11:26.53,Default,,0,0,0,,Là hắn!
Dialogue: 0,0:11:26.53,0:11:27.14,Default,,0,0,0,,Ai?
Dialogue: 0,0:11:27.50,0:11:28.06,Default,,0,0,0,,Là hắn!
Dialogue: 0,0:11:28.06,0:11:28.54,Default,,0,0,0,,Không phải!
Dialogue: 0,0:11:28.78,0:11:29.58,Default,,0,0,0,,Hắn, là hắn!
Dialogue: 0,0:11:29.58,0:11:30.14,Default,,0,0,0,,Đều là hắn cả!
Dialogue: 0,0:11:30.22,0:11:31.25,Default,,0,0,0,,- Không phải ta...\N- Cầm mồm!
Dialogue: 0,0:11:32.30,0:11:33.06,Default,,0,0,0,,Xin lỗi, xin lỗi.
Dialogue: 0,0:11:33.06,0:11:34.22,Default,,0,0,0,,Biết người này là ai không?
Dialogue: 0,0:11:34.58,0:11:35.62,Default,,0,0,0,,Lôi Chấn Tử,
Dialogue: 0,0:11:36.06,0:11:37.06,Default,,0,0,0,,ta bảo kê.
Dialogue: 0,0:11:37.97,0:11:38.62,Default,,0,0,0,,Hiểu chưa?
Dialogue: 0,0:11:39.02,0:11:40.66,Default,,0,0,0,,Hiểu rồi, hiểu rồi, biết rồi.
Dialogue: 0,0:11:40.93,0:11:42.58,Default,,0,0,0,,Chẳng phải muốn nhìn ta nhảy sao?\NNgẩng đầu lên,
Dialogue: 0,0:11:42.69,0:11:43.62,Default,,0,0,0,,nhìn ta nhảy nè!
Dialogue: 0,0:11:43.74,0:11:44.30,Default,,0,0,0,,Nhìn nè!
Dialogue: 0,0:11:44.30,0:11:44.86,Default,,0,0,0,,Không dám nữa.
Dialogue: 0,0:11:44.86,0:11:46.94,Default,,0,0,0,,Lần sau đừng để ta\Nnhìn thấy các ngươi ở con phố này.
Dialogue: 0,0:11:47.09,0:11:47.53,Default,,0,0,0,,Cút!
Dialogue: 0,0:11:48.50,0:11:49.41,Default,,0,0,0,,Vâng, vâng.
Dialogue: 0,0:11:49.66,0:11:50.86,Default,,0,0,0,,Đi mau, đi mau!
Dialogue: 0,0:11:50.90,0:11:51.93,Default,,0,0,0,,Vừa nhảy vừa cút!
Dialogue: 0,0:11:53.86,0:11:54.69,Default,,0,0,0,,Nhảy cao lên!
Dialogue: 0,0:11:56.89,0:11:57.74,Default,,0,0,0,,Hoàn gia tha mạng.
Dialogue: 0,0:11:57.90,0:11:58.50,Default,,0,0,0,,Hoàn gia tha mạng.
Dialogue: 0,0:11:58.50,0:11:59.78,Default,,0,0,0,,Lần sau không dám nữa đâu, Hoàn gia.
Dialogue: 0,0:12:00.02,0:12:01.10,Default,,0,0,0,,- Hoàn gia tha mạng.\N- Cút mau!
Dialogue: 0,0:12:02.58,0:12:03.42,Default,,0,0,0,,Nhảy!
Dialogue: 0,0:12:08.82,0:12:09.53,Default,,0,0,0,,Tân Hoàn!
Dialogue: 0,0:12:10.18,0:12:11.82,Default,,0,0,0,,Ai bảo đệ lại lo chuyện bao đồng nữa?
Dialogue: 0,0:12:12.34,0:12:13.98,Default,,0,0,0,,Chuyện của đệ đệ\Nsao lại là lo chuyện bao đồng chứ?
Dialogue: 0,0:12:14.54,0:12:16.06,Default,,0,0,0,,Ta không muốn thấy ai bắt nạt đệ đệ ta.
Dialogue: 0,0:12:16.57,0:12:18.26,Default,,0,0,0,,Ta đã nói với đệ bao nhiêu lần rồi,
Dialogue: 0,0:12:18.98,0:12:19.94,Default,,0,0,0,,đệ là đệ đệ của ta!
Dialogue: 0,0:12:20.50,0:12:21.09,Default,,0,0,0,,Được được được,
Dialogue: 0,0:12:22.22,0:12:23.22,Default,,0,0,0,,ta là đệ đệ của huynh.
Dialogue: 0,0:12:24.42,0:12:25.66,Default,,0,0,0,,Huynh muốn nói sao cũng được,
Dialogue: 0,0:12:26.93,0:12:28.34,Default,,0,0,0,,nói chung ta bảo vệ huynh là được rồi.
Dialogue: 0,0:12:29.18,0:12:31.30,Default,,0,0,0,,Còn về ca ca tỷ tỷ đệ đệ muội muội hay gì đó,
Dialogue: 0,0:12:34.02,0:12:34.73,Default,,0,0,0,,sao cũng được.
Dialogue: 0,0:12:40.01,0:12:40.78,Default,,0,0,0,,Chấn Nhi,
Dialogue: 0,0:12:42.54,0:12:43.90,Default,,0,0,0,,vi phu đã nói bao nhiêu lần rồi,
Dialogue: 0,0:12:44.26,0:12:46.22,Default,,0,0,0,,không cho phép con ra ngoài\Nlàm chuyện ngông cuồng,
Dialogue: 0,0:12:46.58,0:12:47.74,Default,,0,0,0,,sao con không nghe chứ?
Dialogue: 0,0:12:50.13,0:12:51.06,Default,,0,0,0,,Con chỉ muốn chứng minh
Dialogue: 0,0:12:51.06,0:12:52.10,Default,,0,0,0,,con không kém cỏi hơn Tân Hoàn,
Dialogue: 0,0:12:52.45,0:12:53.46,Default,,0,0,0,,con có gì sai chứ?
Dialogue: 0,0:12:54.18,0:12:55.02,Default,,0,0,0,,Lôi Chấn Tử,
Dialogue: 0,0:12:55.58,0:12:56.66,Default,,0,0,0,,không được nói chuyện với nghĩa phụ như vậy.
Dialogue: 0,0:12:56.85,0:12:57.78,Default,,0,0,0,,Tại sao lại không được?
Dialogue: 0,0:12:58.34,0:12:59.66,Default,,0,0,0,,Câu nệ lễ nghi, xem trọng phép tắc,
Dialogue: 0,0:12:59.66,0:13:00.66,Default,,0,0,0,,nhìn mà chán.
Dialogue: 0,0:13:01.66,0:13:04.94,Default,,0,0,0,,Tiểu gia đây sống vui sống khoẻ là được.
Dialogue: 0,0:13:05.02,0:13:05.74,Default,,0,0,0,,Con...
Dialogue: 0,0:13:06.46,0:13:07.22,Default,,0,0,0,,Hầu gia,
Dialogue: 0,0:13:07.50,0:13:08.66,Default,,0,0,0,,người bớt giận đi.
Dialogue: 0,0:13:09.49,0:13:10.46,Default,,0,0,0,,Có điều Lôi Chấn Tử,
Dialogue: 0,0:13:10.54,0:13:11.86,Default,,0,0,0,,ngươi cứng đầu thật đấy!
Dialogue: 0,0:13:12.57,0:13:13.82,Default,,0,0,0,,Không giống như Tân công tử,
Dialogue: 0,0:13:14.06,0:13:15.50,Default,,0,0,0,,tư chất hơn người.
Dialogue: 0,0:13:16.62,0:13:18.22,Default,,0,0,0,,Ta nói này, sao cô càng khuyên càng tệ vậy?
Dialogue: 0,0:13:20.34,0:13:21.58,Default,,0,0,0,,Hầu gia,
Dialogue: 0,0:13:21.62,0:13:22.90,Default,,0,0,0,,người xem hắn kìa.
Dialogue: 0,0:13:23.13,0:13:24.74,Default,,0,0,0,,Tốt xấu gì ta cũng do tiên trưởng cử đến,
Dialogue: 0,0:13:24.94,0:13:26.70,Default,,0,0,0,,bình thường hắn toàn bắt nạt ta như vậy.
Dialogue: 0,0:13:27.06,0:13:28.14,Default,,0,0,0,,Mấy trăm tuổi của ta
Dialogue: 0,0:13:28.26,0:13:29.38,Default,,0,0,0,,sống uổng mất rồi.
Dialogue: 0,0:13:29.54,0:13:30.46,Default,,0,0,0,,Ngồi ngay ngắn lại!
Dialogue: 0,0:13:30.46,0:13:30.82,Default,,0,0,0,,Không dậy!
Dialogue: 0,0:13:31.22,0:13:32.05,Default,,0,0,0,,Con dậy ngay cho ta!
Dialogue: 0,0:13:32.22,0:13:33.25,Default,,0,0,0,,Con dậy ngay cho ta!
Dialogue: 0,0:13:33.25,0:13:34.14,Default,,0,0,0,,Không dậy, không dậy, con không dậy!
Dialogue: 0,0:13:34.14,0:13:34.78,Default,,0,0,0,,Con...
Dialogue: 0,0:13:38.38,0:13:38.86,Default,,0,0,0,,Phụ thân,
Dialogue: 0,0:13:39.74,0:13:41.14,Default,,0,0,0,,hôm nay người gọi bọn con đến
Dialogue: 0,0:13:41.34,0:13:42.26,Default,,0,0,0,,có chuyện gì không?
Dialogue: 0,0:13:44.30,0:13:45.10,Default,,0,0,0,,Tại con cả,
Dialogue: 0,0:13:45.30,0:13:46.78,Default,,0,0,0,,suýt nữa làm hỏng chuyện lớn.
Dialogue: 0,0:13:48.45,0:13:48.98,Default,,0,0,0,,Hoàn Nhi,
Dialogue: 0,0:13:50.06,0:13:51.06,Default,,0,0,0,,năm xưa tiên trưởng
Dialogue: 0,0:13:51.34,0:13:52.70,Default,,0,0,0,,đã đặt một cây côn vàng
Dialogue: 0,0:13:52.94,0:13:53.94,Default,,0,0,0,,trong rừng Phong Thần Giới.
Dialogue: 0,0:13:54.66,0:13:55.34,Default,,0,0,0,,Cây côn này
Dialogue: 0,0:13:55.58,0:13:57.98,Default,,0,0,0,,do chí khí anh hào từ khắp nơi hợp thành,
Dialogue: 0,0:13:58.54,0:14:00.58,Default,,0,0,0,,dùng màn sương Tây Kỳ tạo thành kết giới,
Dialogue: 0,0:14:00.82,0:14:02.46,Default,,0,0,0,,bảo vệ cho con bình an suốt mười tám năm.
Dialogue: 0,0:14:03.54,0:14:04.61,Default,,0,0,0,,Tiên trưởng nói rõ,
Dialogue: 0,0:14:05.90,0:14:07.66,Default,,0,0,0,,chờ khi con đến tuổi trưởng thành
Dialogue: 0,0:14:07.74,0:14:09.50,Default,,0,0,0,,thì có thể đích thân lấy cây côn vàng đó
Dialogue: 0,0:14:09.74,0:14:11.02,Default,,0,0,0,,đi đến núi Chung Nam
Dialogue: 0,0:14:11.18,0:14:11.86,Default,,0,0,0,,tu luyện.
Dialogue: 0,0:14:12.65,0:14:13.26,Default,,0,0,0,,Được.
Dialogue: 0,0:14:13.66,0:14:14.46,Default,,0,0,0,,Con sẽ đi ngay.
Dialogue: 0,0:14:15.78,0:14:16.49,Default,,0,0,0,,Con cũng đi nữa.
Dialogue: 0,0:14:17.05,0:14:18.86,Default,,0,0,0,,Con cảm thấy con là ca ca của Tân Hoàn,
Dialogue: 0,0:14:19.18,0:14:20.34,Default,,0,0,0,,con có nghĩa vụ.
Dialogue: 0,0:14:20.34,0:14:21.10,Default,,0,0,0,,Không được làm càn!
Dialogue: 0,0:14:22.10,0:14:23.14,Default,,0,0,0,,Cây côn vàng này,
Dialogue: 0,0:14:23.38,0:14:25.02,Default,,0,0,0,,chỉ có Tướng Tinh mới có thể lấy được.
Dialogue: 0,0:14:25.26,0:14:25.78,Default,,0,0,0,,Hầu gia,
Dialogue: 0,0:14:26.14,0:14:27.90,Default,,0,0,0,,người cứ cho Lôi Chấn Tử đi cùng đi.
Dialogue: 0,0:14:28.05,0:14:28.81,Default,,0,0,0,,Có ta đây,
Dialogue: 0,0:14:29.02,0:14:30.74,Default,,0,0,0,,ta sẽ trông chừng Lôi Chấn Tử giúp người.
Dialogue: 0,0:14:32.62,0:14:33.65,Default,,0,0,0,,Có còn muốn đi không?
Dialogue: 0,0:14:33.82,0:14:34.34,Default,,0,0,0,,Đi.
Dialogue: 0,0:14:35.82,0:14:37.38,Default,,0,0,0,,Cho con đi học hỏi thêm cũng được.
Dialogue: 0,0:14:37.61,0:14:38.61,Default,,0,0,0,,Nhưng mà có một chuyện,
Dialogue: 0,0:14:39.37,0:14:41.62,Default,,0,0,0,,không được quấy loạn Tân Hoàn.
Dialogue: 0,0:14:41.74,0:14:42.69,Default,,0,0,0,,Đa tạ hầu gia!
Dialogue: 0,0:14:48.14,0:14:49.90,Default,,0,0,0,,Lôi Chấn Tử, nhanh lên!
Dialogue: 0,0:14:55.70,0:14:56.50,Default,,0,0,0,,Nhàm chán.
Dialogue: 0,0:14:58.82,0:14:59.62,Default,,0,0,0,,Lôi Chấn Tử,
Dialogue: 0,0:14:59.94,0:15:00.61,Default,,0,0,0,,ngươi ngoan ngoãn đi,
Dialogue: 0,0:15:00.78,0:15:01.42,Default,,0,0,0,,không được quậy phá!
Dialogue: 0,0:15:02.18,0:15:03.22,Default,,0,0,0,,Hai người nhàm chán thật đó.
Dialogue: 0,0:15:09.38,0:15:11.66,Default,,0,0,0,,Hai người đứng lại cho ta!
Dialogue: 0,0:15:15.78,0:15:17.62,Default,,0,0,0,,Tân Hoàn, mấy người chờ ta với!
Dialogue: 0,0:15:18.14,0:15:20.34,Default,,0,0,0,,Mấy ngày này đi muốn gãy chân luôn rồi.
Dialogue: 0,0:15:30.94,0:15:31.90,Default,,0,0,0,,Là ở đây.
Dialogue: 0,0:15:32.50,0:15:33.86,Default,,0,0,0,,Qua ngọn núi này là đến rồi.
Dialogue: 0,0:16:04.94,0:16:06.58,Default,,0,0,0,,Mấy người nói xem hình vẽ trên đồ đằng này,
Dialogue: 0,0:16:06.58,0:16:08.54,Default,,0,0,0,,có phải là chuyện trận Phong Thần không?
Dialogue: 0,0:16:09.98,0:16:10.78,Default,,0,0,0,,Phụ thân có nói
Dialogue: 0,0:16:11.94,0:16:14.06,Default,,0,0,0,,những bia mộ\Ntrong rừng Thần Bia đều trống rỗng,
Dialogue: 0,0:16:14.77,0:16:16.66,Default,,0,0,0,,chờ đến ngày trận Phong Thần khởi động,
Dialogue: 0,0:16:17.62,0:16:18.90,Default,,0,0,0,,các thần linh trở về.
Dialogue: 0,0:16:19.98,0:16:20.86,Default,,0,0,0,,Hai người xem nè.
Dialogue: 0,0:16:23.06,0:16:24.18,Default,,0,0,0,,Ở đây có chữ!
Dialogue: 0,0:16:28.85,0:16:31.30,Default,,0,0,0,,"Thiên hạ chi hành, đại đạo vi công,
Dialogue: 0,0:16:31.49,0:16:32.69,Default,,0,0,0,,thiên..."
Dialogue: 0,0:16:33.02,0:16:34.98,Default,,0,0,0,,Đây là gì vậy?
Dialogue: 0,0:16:35.66,0:16:37.74,Default,,0,0,0,,"Phong Thần bị phá,\Nvĩnh viễn không thành thần."
Dialogue: 0,0:16:38.82,0:16:39.94,Default,,0,0,0,,Dù doạ ai vậy?
Dialogue: 0,0:16:39.94,0:16:41.17,Default,,0,0,0,,Ta cũng đâu muốn thành thần.
Dialogue: 0,0:16:41.34,0:16:42.37,Default,,0,0,0,,Đâu phải ai cũng như ngươi,
Dialogue: 0,0:16:42.37,0:16:43.46,Default,,0,0,0,,không có chí lớn.
Dialogue: 0,0:16:43.90,0:16:44.98,Default,,0,0,0,,Ta có thể thành thần hay không,
Dialogue: 0,0:16:44.98,0:16:46.38,Default,,0,0,0,,trông cậy vào ngươi cả đấy, Tân Hoàn!
Dialogue: 0,0:16:47.02,0:16:48.86,Default,,0,0,0,,Cô có thành thần cũng chỉ là con bướm quèn.
Dialogue: 0,0:16:48.94,0:16:50.46,Default,,0,0,0,,Còn nói nữa, còn nói nữa chứ.
Dialogue: 0,0:16:58.10,0:16:59.42,Default,,0,0,0,,Đệ làm gì vậy Tân Hoàn?
Dialogue: 0,0:16:59.54,0:17:00.82,Default,,0,0,0,,Đi mau lên.
Dialogue: 0,0:17:36.14,0:17:36.58,Default,,0,0,0,,Cứ...
Dialogue: 0,0:17:36.98,0:17:38.22,Default,,0,0,0,,dễ dàng vậy à?
Dialogue: 0,0:17:38.49,0:17:40.06,Default,,0,0,0,,Không có chút mơ hồ nào cho bọn ta sao?
Dialogue: 0,0:17:40.34,0:17:41.30,Default,,0,0,0,,Lôi Chấn Tử,
Dialogue: 0,0:17:41.50,0:17:42.66,Default,,0,0,0,,nghiêm túc tí đi.
Dialogue: 0,0:18:08.46,0:18:10.14,Default,,0,0,0,,Nói gì có đó.
Dialogue: 0,0:18:11.82,0:18:12.58,Default,,0,0,0,,Cái bia đá nát này
Dialogue: 0,0:18:12.70,0:18:14.06,Default,,0,0,0,,sao lại có tên ta nữa vậy?
Dialogue: 0,0:18:15.30,0:18:16.42,Default,,0,0,0,,Có gì đâu chứ?
Dialogue: 0,0:18:16.94,0:18:17.86,Default,,0,0,0,,Nếu sau này
Dialogue: 0,0:18:17.86,0:18:19.50,Default,,0,0,0,,Tân Hoàn khởi động trận Phong Thần rồi,
Dialogue: 0,0:18:19.90,0:18:21.42,Default,,0,0,0,,chúng ta đều có\Nbia Phong Thần của chính mình.
Dialogue: 0,0:18:21.58,0:18:22.42,Default,,0,0,0,,Đúng không Tân Hoàn?
Dialogue: 0,0:18:25.90,0:18:27.62,Default,,0,0,0,,Khởi động trận Phong Thần
Dialogue: 0,0:18:28.42,0:18:30.18,Default,,0,0,0,,là ngày Ân Thương diệt vong,
Dialogue: 0,0:18:30.50,0:18:31.90,Default,,0,0,0,,anh hào được phong thần
Dialogue: 0,0:18:32.02,0:18:32.90,Default,,0,0,0,,ghi vào bảng Phong Thần.
Dialogue: 0,0:18:32.90,0:18:33.70,Default,,0,0,0,,Được rồi, được rồi, Tân Hoàn,
Dialogue: 0,0:18:33.70,0:18:34.42,Default,,0,0,0,,đừng đọc nữa.
Dialogue: 0,0:18:34.42,0:18:35.62,Default,,0,0,0,,Ta nghe mà nhức hết cả đầu.
Dialogue: 0,0:18:36.30,0:18:37.01,Default,,0,0,0,,Nói chung là
Dialogue: 0,0:18:37.01,0:18:38.98,Default,,0,0,0,,ai dám cản đường Tân Hoàn nhà bọn ta,
Dialogue: 0,0:18:39.33,0:18:40.78,Default,,0,0,0,,tiểu gia đây sẽ đánh nát bọn chúng.
Dialogue: 0,0:18:41.38,0:18:42.29,Default,,0,0,0,,Không được, không được, không được.
Dialogue: 0,0:18:43.02,0:18:43.86,Default,,0,0,0,,Lôi Chấn Tử,
Dialogue: 0,0:18:43.86,0:18:44.78,Default,,0,0,0,,ngươi đừng làm càn.
Dialogue: 0,0:18:45.66,0:18:46.34,Default,,0,0,0,,Không được,
Dialogue: 0,0:18:46.74,0:18:48.34,Default,,0,0,0,,phá huỷ bia Phong Thần\Nthì vĩnh viễn không thành thần,
Dialogue: 0,0:18:49.01,0:18:50.54,Default,,0,0,0,,cái giá này có phải hơi lớn quá không?
Dialogue: 0,0:18:55.93,0:18:57.02,Default,,0,0,0,,Ta là ca ca của đệ,
Dialogue: 0,0:18:57.10,0:18:57.78,Default,,0,0,0,,nhớ rõ,
Dialogue: 0,0:18:57.86,0:18:59.13,Default,,0,0,0,,ta bảo vệ đệ.
Dialogue: 0,0:19:00.82,0:19:01.62,Default,,0,0,0,,Tránh ra!
Dialogue: 0,0:19:10.30,0:19:12.58,Default,,0,0,0,,Dám cản đường của Tân Hoàn nhà ta.
Dialogue: 0,0:19:32.34,0:19:33.45,Default,,0,0,0,,Ta đùa thôi mà,
Dialogue: 0,0:19:34.45,0:19:35.77,Default,,0,0,0,,ta còn chưa dùng sức nữa.
Dialogue: 0,0:19:36.38,0:19:36.74,Default,,0,0,0,,Ta...
Dialogue: 0,0:19:36.90,0:19:38.46,Default,,0,0,0,,Bây giờ ta nghiêm túc đó nha.
Dialogue: 0,0:20:09.97,0:20:10.94,Default,,0,0,0,,Còn ngây ra đó làm gì?
Dialogue: 0,0:20:10.94,0:20:11.85,Default,,0,0,0,,Mau đi lấy đi!
Dialogue: 0,0:20:12.29,0:20:14.34,Default,,0,0,0,,Không thì lát nữa\Nlại có quỷ quái xuất hiện bây giờ.
Dialogue: 0,0:20:14.62,0:20:15.30,Default,,0,0,0,,Câm miệng,
Dialogue: 0,0:20:15.50,0:20:16.78,Default,,0,0,0,,ngươi đừng nói ăn nói lung tung.
Dialogue: 0,0:20:51.50,0:20:52.30,Default,,0,0,0,,Cái này...
Dialogue: 0,0:20:52.66,0:20:53.74,Default,,0,0,0,,Không phải chứ?
Dialogue: 0,0:20:56.14,0:20:57.14,Default,,0,0,0,,Côn vàng này,
Dialogue: 0,0:20:57.38,0:20:59.62,Default,,0,0,0,,rõ ràng Tướng Tinh\Nphải rút ra được mới phải chứ?
Dialogue: 0,0:21:07.41,0:21:08.29,Default,,0,0,0,,Cây côn chết tiệt này,
Dialogue: 0,0:21:08.29,0:21:09.66,Default,,0,0,0,,ta không tin!
Dialogue: 0,0:21:11.02,0:21:12.30,Default,,0,0,0,,Lôi Chấn Tử!
Dialogue: 0,0:21:22.30,0:21:22.94,Default,,0,0,0,,Lôi Chấn Tử!
Dialogue: 0,0:21:25.94,0:21:28.21,Default,,0,0,0,,Cây côn vàng này\Nchỉ có Tướng Tinh mới lấy được,
Dialogue: 0,0:21:28.50,0:21:29.58,Default,,0,0,0,,ngươi cũng không phải...
Dialogue: 0,0:22:17.03,0:22:18.57,Default,,0,0,0,,[Lầu Trích Tinh, Triều Ca]
Dialogue: 0,0:22:34.47,0:22:35.35,Default,,0,0,0,,Chúc mừng Thái sư,
Dialogue: 0,0:22:35.47,0:22:36.77,Default,,0,0,0,,cuối cùng cũng thành công.
Dialogue: 0,0:22:37.01,0:22:38.44,Default,,0,0,0,,Côn vàng xuất thế,
Dialogue: 0,0:22:38.56,0:22:40.13,Default,,0,0,0,,trận Tru Tiên đã bố trí xong.
Dialogue: 0,0:22:40.65,0:22:41.85,Default,,0,0,0,,Theo lão phu
Dialogue: 0,0:22:42.13,0:22:44.01,Default,,0,0,0,,đến Tây Kỳ gặp bạn cũ thôi.
Dialogue: 0,0:22:48.23,0:22:49.61,Default,,0,0,0,,Sao công tử vẫn chưa về chứ?
Dialogue: 0,0:22:49.61,0:22:50.41,Default,,0,0,0,,Không biết nữa.
Dialogue: 0,0:22:50.41,0:22:51.60,Default,,0,0,0,,Có khi nào xảy ra chuyện gì không?
Dialogue: 0,0:22:51.75,0:22:52.86,Default,,0,0,0,,Không phải chứ?
Dialogue: 0,0:22:52.95,0:22:54.45,Default,,0,0,0,,Công tử là Tướng Tinh mà.
Dialogue: 0,0:22:54.55,0:22:55.41,Default,,0,0,0,,Đúng đó.
Dialogue: 0,0:22:56.26,0:22:57.64,Default,,0,0,0,,Công tử về rồi!
Dialogue: 0,0:22:57.64,0:22:59.67,Default,,0,0,0,,Công tử đã về rồi!
Dialogue: 0,0:22:59.99,0:23:01.91,Default,,0,0,0,,Chúc mừng, chúc mừng, chúc mừng.
Dialogue: 0,0:23:01.91,0:23:04.02,Default,,0,0,0,,Chúc mừng công tử.
Dialogue: 0,0:23:04.02,0:23:05.85,Default,,0,0,0,,Chúc mừng nha, chúc mừng.
Dialogue: 0,0:23:05.85,0:23:06.73,Default,,0,0,0,,Tốt quá.
Dialogue: 0,0:23:08.73,0:23:10.87,Default,,0,0,0,,Chúc mừng, chúc mừng.
Dialogue: 0,0:23:10.87,0:23:12.67,Default,,0,0,0,,Chúc mừng công tử, chúc mừng.
Dialogue: 0,0:23:12.67,0:23:15.43,Default,,0,0,0,,Chúc mừng, chúc mừng công tử.
Dialogue: 0,0:23:16.02,0:23:18.51,Default,,0,0,0,,Chúc mừng, chúc mừng.
Dialogue: 0,0:23:18.97,0:23:20.86,Default,,0,0,0,,Không nhìn lầm mà.
Dialogue: 0,0:23:20.86,0:23:21.55,Default,,0,0,0,,Đúng đó.
Dialogue: 0,0:23:23.05,0:23:25.06,Default,,0,0,0,,Từ nhỏ Hoàn Nhi đã chăm chỉ hiếu học,
Dialogue: 0,0:23:25.86,0:23:27.82,Default,,0,0,0,,cũng không uổng công\Nmười mấy năm ta bồi dưỡng.
Dialogue: 0,0:23:28.82,0:23:29.46,Default,,0,0,0,,Sau này,
Dialogue: 0,0:23:30.58,0:23:31.85,Default,,0,0,0,,phải nhờ vào tiên trưởng rồi đó.
Dialogue: 0,0:23:33.61,0:23:34.14,Default,,0,0,0,,Chờ đã!
Dialogue: 0,0:23:34.98,0:23:36.34,Default,,0,0,0,,Hình như có gì đó sai sai.
Dialogue: 0,0:23:41.90,0:23:42.82,Default,,0,0,0,,Sao có thể...
Dialogue: 0,0:23:43.38,0:23:44.73,Default,,0,0,0,,Không phải hắn mới đúng chứ.
Dialogue: 0,0:23:44.73,0:23:46.02,Default,,0,0,0,,Sao cây côn này lại ở trong tay hắn?
Dialogue: 0,0:23:46.02,0:23:46.46,Default,,0,0,0,,Đúng đó.
Dialogue: 0,0:23:46.46,0:23:47.58,Default,,0,0,0,,Chẳng phải chỉ có Tướng Tinh\Nmới lấy được sao?
Dialogue: 0,0:23:47.58,0:23:49.90,Default,,0,0,0,,Chẳng lẽ người rút được côn vàng\Nlà Lôi Chấn Tử?
Dialogue: 0,0:23:51.30,0:23:52.94,Default,,0,0,0,,Sao cây côn vàng lại ở trong tay hắn?
Dialogue: 0,0:23:52.94,0:23:53.58,Default,,0,0,0,,Đúng đó!
Dialogue: 0,0:23:54.41,0:23:55.82,Default,,0,0,0,,Câm mồm hết cho ta!
Dialogue: 0,0:23:57.05,0:23:59.25,Default,,0,0,0,,Cây côn vàng này\Nvốn là của Tân Hoàn lấy được.
Dialogue: 0,0:24:09.26,0:24:09.86,Default,,0,0,0,,Tân Hoàn.
Dialogue: 0,0:24:10.30,0:24:10.90,Default,,0,0,0,,Quỳ xuống!
Dialogue: 0,0:24:14.46,0:24:15.14,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,0:24:15.50,0:24:16.53,Default,,0,0,0,,bái kiến tiên trưởng đi.
Dialogue: 0,0:24:19.58,0:24:20.37,Default,,0,0,0,,Bái kiến tiên trưởng.
Dialogue: 0,0:24:21.22,0:24:22.78,Default,,0,0,0,,Lão đạo sĩ tóc xoăn ở đâu ra,
Dialogue: 0,0:24:22.94,0:24:24.38,Default,,0,0,0,,ra vẻ gì chứ?
Dialogue: 0,0:24:29.17,0:24:30.17,Default,,0,0,0,,Ông làm vậy là sao?
Dialogue: 0,0:24:30.38,0:24:31.18,Default,,0,0,0,,Ông trả cây côn cho ta!
Dialogue: 0,0:24:31.18,0:24:31.78,Default,,0,0,0,,Lôi Chấn Tử!
Dialogue: 0,0:24:32.58,0:24:33.50,Default,,0,0,0,,Không được vô lễ!
Dialogue: 0,0:24:35.42,0:24:36.46,Default,,0,0,0,,Không sao, không sao.
Dialogue: 0,0:24:37.70,0:24:39.94,Default,,0,0,0,,Hôm nay Tân Hoàn nhận mệnh Tướng Tinh
Dialogue: 0,0:24:40.29,0:24:41.90,Default,,0,0,0,,lấy côn vàng về đây.
Dialogue: 0,0:24:42.46,0:24:43.22,Default,,0,0,0,,Ngày mai,
Dialogue: 0,0:24:43.34,0:24:44.90,Default,,0,0,0,,ta sẽ đưa hắn về núi Chung Nam,
Dialogue: 0,0:24:45.22,0:24:47.02,Default,,0,0,0,,chờ sau khi tu luyện thành tài rồi,
Dialogue: 0,0:24:47.18,0:24:47.82,Default,,0,0,0,,nhất định
Dialogue: 0,0:24:48.46,0:24:49.85,Default,,0,0,0,,sẽ cứu thiên hạ.
Dialogue: 0,0:24:56.98,0:24:58.01,Default,,0,0,0,,Chúc đệ năm nào cũng như hôm nay,
Dialogue: 0,0:24:58.01,0:24:58.74,Default,,0,0,0,,tuổi nào cũng như bây giờ.
Dialogue: 0,0:24:58.74,0:25:00.14,Default,,0,0,0,,Được rồi, đừng nhiều lời nữa.
Dialogue: 0,0:25:00.14,0:25:00.86,Default,,0,0,0,,Nói chung,
Dialogue: 0,0:25:00.86,0:25:01.66,Default,,0,0,0,,có chuyện gì cứ đến tìm ta.
Dialogue: 0,0:25:02.02,0:25:03.10,Default,,0,0,0,,Nào, cảm ơn tiên trưởng đi.
Dialogue: 0,0:25:03.34,0:25:03.90,Default,,0,0,0,,Mau mau mau.
Dialogue: 0,0:25:03.90,0:25:04.93,Default,,0,0,0,,Cảm ơn tiên trưởng.
Dialogue: 0,0:25:13.14,0:25:13.94,Default,,0,0,0,,Lôi Chấn Tử.
Dialogue: 0,0:25:15.78,0:25:17.14,Default,,0,0,0,,Chẳng lẽ đệ không muốn thành thần sao?
Dialogue: 0,0:25:22.06,0:25:23.22,Default,,0,0,0,,Thật ra đệ không cần phải vậy.
Dialogue: 0,0:25:24.78,0:25:25.50,Default,,0,0,0,,Không đáng.
Dialogue: 0,0:25:27.05,0:25:28.50,Default,,0,0,0,,Chẳng có gì mà không đáng cả.
Dialogue: 0,0:25:29.02,0:25:29.98,Default,,0,0,0,,Tiểu gia ta bằng lòng.
Dialogue: 0,0:25:36.22,0:25:38.42,Default,,0,0,0,,Tiên trưởng!
Dialogue: 0,0:25:39.37,0:25:40.98,Default,,0,0,0,,Ngươi làm gì vậy?
Dialogue: 0,0:25:42.33,0:25:43.02,Default,,0,0,0,,Quan trọng là
Dialogue: 0,0:25:43.21,0:25:46.58,Default,,0,0,0,,ông bảo ta đi theo Tướng Tinh khởi động trận\Nthì có thể thành thần.
Dialogue: 0,0:25:47.14,0:25:47.82,Default,,0,0,0,,Nhưng bây giờ
Dialogue: 0,0:25:47.94,0:25:49.26,Default,,0,0,0,,ông nhận nhầm người rồi,
Dialogue: 0,0:25:49.49,0:25:50.62,Default,,0,0,0,,ta phải đi theo ai đây hả?
Dialogue: 0,0:25:51.10,0:25:52.01,Default,,0,0,0,,Tốt xấu gì
Dialogue: 0,0:25:52.54,0:25:53.98,Default,,0,0,0,,ông cũng phải gợi ý cho ta chứ.
Dialogue: 0,0:25:57.06,0:25:58.86,Default,,0,0,0,,Ông nói gì đi!
Dialogue: 0,0:26:01.14,0:26:01.49,Default,,0,0,0,,Được!
Dialogue: 0,0:26:01.74,0:26:02.66,Default,,0,0,0,,Ta nói ngươi biết.
Dialogue: 0,0:26:03.98,0:26:04.50,Default,,0,0,0,,Nào.
Dialogue: 0,0:26:04.74,0:26:05.45,Default,,0,0,0,,Nào nào nào.
Dialogue: 0,0:26:08.06,0:26:08.78,Default,,0,0,0,,Sao cơ?
Dialogue: 0,0:26:09.26,0:26:09.94,Default,,0,0,0,,Tiên trưởng,
Dialogue: 0,0:26:10.17,0:26:11.02,Default,,0,0,0,,ông nói gì...
Dialogue: 0,0:26:12.66,0:26:13.94,Default,,0,0,0,,Tiên trưởng!
Dialogue: 0,0:26:16.77,0:26:18.18,Default,,0,0,0,,Được phụ thân thương yêu,
Dialogue: 0,0:26:18.42,0:26:19.62,Default,,0,0,0,,nuôi nấng mười tám năm,
Dialogue: 0,0:26:20.46,0:26:22.49,Default,,0,0,0,,con nhất định sẽ\Nchuyên tâm học đạo cùng tiên trưởng,
Dialogue: 0,0:26:22.90,0:26:24.42,Default,,0,0,0,,hoàn thành nguyện vọng thiên hạ.
Dialogue: 0,0:26:29.70,0:26:30.94,Default,,0,0,0,,Hoàn Nhi, đứng lên đi.
Dialogue: 0,0:26:34.81,0:26:35.50,Default,,0,0,0,,Chuyến này đi,
Dialogue: 0,0:26:35.98,0:26:37.30,Default,,0,0,0,,nhất định phải bảo trọng.
Dialogue: 0,0:26:38.45,0:26:40.21,Default,,0,0,0,,Sau này cho dù xảy ra chuyện gì,
Dialogue: 0,0:26:41.34,0:26:42.58,Default,,0,0,0,,con cũng phải luôn tin rằng
Dialogue: 0,0:26:43.61,0:26:45.62,Default,,0,0,0,,vi phu không hề tính toán gì với con cả.
Dialogue: 0,0:27:05.17,0:27:06.17,Default,,0,0,0,,Phụ thân làm gì vậy...
Dialogue: 0,0:27:07.54,0:27:09.25,Default,,0,0,0,,Hoàn Nhi là Tướng Tinh,
Dialogue: 0,0:27:09.98,0:27:11.34,Default,,0,0,0,,về lý nên nhận lạy này.
Dialogue: 0,0:27:12.30,0:27:13.10,Default,,0,0,0,,Cái lạy này,
Dialogue: 0,0:27:13.46,0:27:15.06,Default,,0,0,0,,không phải vì bách tính trong thiên hạ,
Dialogue: 0,0:27:15.53,0:27:17.66,Default,,0,0,0,,mà là vì bản thân Cơ Xương ta.
Dialogue: 0,0:27:22.78,0:27:23.86,Default,,0,0,0,,Có đi không hả?
Dialogue: 0,0:27:24.18,0:27:25.86,Default,,0,0,0,,Viên đan trong lò luyện đan của ta
Dialogue: 0,0:27:25.97,0:27:27.41,Default,,0,0,0,,sắp khét luôn rồi.
Dialogue: 0,0:27:30.93,0:27:31.74,Default,,0,0,0,,Hoàn Nhi,
Dialogue: 0,0:27:32.57,0:27:33.34,Default,,0,0,0,,đi đi.
Dialogue: 0,0:27:50.78,0:27:52.34,Default,,0,0,0,,Phiền phức.
Dialogue: 0,0:27:58.02,0:27:58.74,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,0:27:59.02,0:28:00.02,Default,,0,0,0,,ngươi bỏ đi như vậy sao?
Dialogue: 0,0:28:00.14,0:28:01.22,Default,,0,0,0,,Lôi Chấn Tử phải làm sao?
Dialogue: 0,0:28:08.38,0:28:09.14,Default,,0,0,0,,Tân Hoàn!
Dialogue: 0,0:28:13.49,0:28:14.22,Default,,0,0,0,,Xin lỗi.
Dialogue: 0,0:28:25.49,0:28:26.86,Default,,0,0,0,,Lôi Chấn Tử!
Dialogue: 0,0:29:02.98,0:29:03.62,Default,,0,0,0,,Tiên trưởng,
Dialogue: 0,0:29:05.18,0:29:06.26,Default,,0,0,0,,ngừng lại ở đây
Dialogue: 0,0:29:06.74,0:29:08.66,Default,,0,0,0,,là có chuyện gì muốn căn dặn Tân Hoàn sao?
Dialogue: 0,0:29:15.50,0:29:16.30,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,0:29:17.78,0:29:19.78,Default,,0,0,0,,ngươi không phải Tướng Tinh.
Dialogue: 0,0:29:24.57,0:29:26.06,Default,,0,0,0,,Nếu tiên trưởng đã biết,
Dialogue: 0,0:29:27.38,0:29:28.50,Default,,0,0,0,,sao lại không vạch trần ta?
Dialogue: 0,0:29:30.46,0:29:31.73,Default,,0,0,0,,Năm xưa,
Dialogue: 0,0:29:32.34,0:29:33.70,Default,,0,0,0,,khi Tướng Tinh xuất thế,
Dialogue: 0,0:29:34.66,0:29:36.18,Default,,0,0,0,,tộc trưởng của tộc Vũ Nhân
Dialogue: 0,0:29:36.26,0:29:37.70,Default,,0,0,0,,trong lúc sắp chết
Dialogue: 0,0:29:38.65,0:29:41.85,Default,,0,0,0,,đã đưa đôi cánh Phong Lôi vào cơ thể ngươi.
Dialogue: 0,0:29:42.70,0:29:43.34,Default,,0,0,0,,Ta nghĩ,
Dialogue: 0,0:29:44.10,0:29:46.46,Default,,0,0,0,,hắn cũng muốn bảo vệ Tướng Tinh thật sự.
Dialogue: 0,0:29:47.30,0:29:48.54,Default,,0,0,0,,Chỉ trách bọn ta năm xưa
Dialogue: 0,0:29:48.77,0:29:50.58,Default,,0,0,0,,hiểu lầm ngươi là Tướng Tinh,
Dialogue: 0,0:29:51.34,0:29:53.78,Default,,0,0,0,,mãi đến khi rút côn vàng ra
Dialogue: 0,0:29:54.34,0:29:55.46,Default,,0,0,0,,mới biết được
Dialogue: 0,0:29:56.10,0:29:56.98,Default,,0,0,0,,Lôi Chấn Tử
Dialogue: 0,0:29:57.38,0:29:59.26,Default,,0,0,0,,mới là Tướng Tinh thật sự.
Dialogue: 0,0:30:00.98,0:30:01.74,Default,,0,0,0,,Tướng Tinh
Dialogue: 0,0:30:02.41,0:30:04.73,Default,,0,0,0,,không thể không có đôi cánh Phong Lôi.
Dialogue: 0,0:30:05.82,0:30:06.26,Default,,0,0,0,,Cho nên...
Dialogue: 0,0:30:06.26,0:30:07.98,Default,,0,0,0,,Cho nên ông mới dùng thủ đoạn này
Dialogue: 0,0:30:09.26,0:30:11.10,Default,,0,0,0,,để ép ta giao đôi cánh Phong Lôi ra.
Dialogue: 0,0:30:13.34,0:30:15.34,Default,,0,0,0,,Uổng cho ông là thần tiên đức cao vọng trọng.
Dialogue: 0,0:30:17.26,0:30:17.90,Default,,0,0,0,,Hoàn Nhi!
Dialogue: 0,0:30:26.50,0:30:27.30,Default,,0,0,0,,Phụ thân.
Dialogue: 0,0:30:47.66,0:30:48.41,Default,,0,0,0,,Hoàn Nhi.
Dialogue: 0,0:30:49.94,0:30:50.61,Default,,0,0,0,,Cái cúi đầu ban nãy
Dialogue: 0,0:30:50.86,0:30:51.78,Default,,0,0,0,,của vi phu
Dialogue: 0,0:30:53.22,0:30:55.01,Default,,0,0,0,,là lời xin lỗi cả đời của ta dành cho con.
Dialogue: 0,0:30:56.86,0:30:57.78,Default,,0,0,0,,Vương thượng vô đạo,
Dialogue: 0,0:30:58.85,0:31:00.54,Default,,0,0,0,,Tướng Tinh cần có đôi cánh Phong Lôi
Dialogue: 0,0:31:00.70,0:31:02.37,Default,,0,0,0,,mới có thể bay vào trận Phong Thần.
Dialogue: 0,0:31:03.34,0:31:04.02,Default,,0,0,0,,Cho nên...
Dialogue: 0,0:31:05.13,0:31:06.33,Default,,0,0,0,,không thể không hy sinh con.
Dialogue: 0,0:31:07.98,0:31:09.02,Default,,0,0,0,,Nhưng thiên hạ
Dialogue: 0,0:31:09.97,0:31:11.42,Default,,0,0,0,,sẽ luôn nhớ đến con.
Dialogue: 0,0:31:17.45,0:31:18.33,Default,,0,0,0,,Đệ đệ có biết không?
Dialogue: 0,0:31:22.46,0:31:23.30,Default,,0,0,0,,Nếu nó muốn,
Dialogue: 0,0:31:24.14,0:31:24.98,Default,,0,0,0,,con sẽ trả cho nó.
Dialogue: 0,0:31:27.66,0:31:29.18,Default,,0,0,0,,Nhưng hai người không thể lừa gạt con.
Dialogue: 0,0:31:32.06,0:31:33.42,Default,,0,0,0,,Mười tám năm rồi, phụ thân,
Dialogue: 0,0:31:36.10,0:31:37.38,Default,,0,0,0,,mọi chuyện mà người làm,
Dialogue: 0,0:31:38.26,0:31:40.89,Default,,0,0,0,,chẳng lẽ đều vì con là Tướng Tinh sao?
Dialogue: 0,0:31:41.21,0:31:41.86,Default,,0,0,0,,Không phải.
Dialogue: 0,0:31:42.02,0:31:42.94,Default,,0,0,0,,Không phải đâu, Hoàn Nhi.
Dialogue: 0,0:31:44.98,0:31:45.73,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,0:31:46.34,0:31:47.26,Default,,0,0,0,,ngươi cứ đi trước,
Dialogue: 0,0:31:47.46,0:31:49.30,Default,,0,0,0,,sau này trên bảng Phong Thần\Nchắc chắn sẽ có tên ngươi.
Dialogue: 0,0:31:50.06,0:31:50.70,Default,,0,0,0,,Ông nói vậy là sao?
Dialogue: 0,0:31:50.98,0:31:51.74,Default,,0,0,0,,Tiên trưởng!
Dialogue: 0,0:31:52.65,0:31:54.82,Default,,0,0,0,,Đôi cánh Phong Thần\Nvà tính mạng của Vũ Nhân liên kết với nhau,
Dialogue: 0,0:31:54.97,0:31:56.02,Default,,0,0,0,,lấy ra sẽ hồn bay phách tán.
Dialogue: 0,0:31:56.38,0:31:57.49,Default,,0,0,0,,Hai người muốn giết con thật sao?
Dialogue: 0,0:31:57.74,0:31:58.90,Default,,0,0,0,,Vì thiên hạ chúng sinh,
Dialogue: 0,0:31:59.06,0:32:00.06,Default,,0,0,0,,đừng trách bần đạo!
Dialogue: 0,0:32:01.62,0:32:03.10,Default,,0,0,0,,Thì ra Kim Tiên Đại La
Dialogue: 0,0:32:03.26,0:32:05.70,Default,,0,0,0,,lại âm thầm làm những chuyện\Nđáng xấu hổ như vậy.
Dialogue: 0,0:32:26.18,0:32:26.97,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,0:32:27.22,0:32:28.70,Default,,0,0,0,,lão phu sẽ đưa đi trước.
Dialogue: 0,0:32:29.06,0:32:29.90,Default,,0,0,0,,Hoàn Nhi!
Dialogue: 0,0:32:31.86,0:32:32.34,Default,,0,0,0,,Tiên trưởng,
Dialogue: 0,0:32:32.50,0:32:33.18,Default,,0,0,0,,Hoàn Nhi nó...
Dialogue: 0,0:32:50.70,0:32:52.90,Default,,0,0,0,,Đây là nơi ở cũ của tộc Vũ Nhân.
Dialogue: 0,0:32:53.98,0:32:57.30,Default,,0,0,0,,Tộc Vũ Nhân có thể cảm nhận được\Nbất hạnh của quá khứ,
Dialogue: 0,0:32:57.62,0:32:59.65,Default,,0,0,0,,hình ảnh không thể nào giả được.
Dialogue: 0,0:33:00.74,0:33:02.14,Default,,0,0,0,,Ngươi có thể xem thử,
Dialogue: 0,0:33:02.62,0:33:03.50,Default,,0,0,0,,hôm đó
Dialogue: 0,0:33:04.46,0:33:05.58,Default,,0,0,0,,đã xảy ra chuyện gì.
Dialogue: 0,0:33:42.30,0:33:43.90,Default,,0,0,0,,Diệt cả tộc các ngươi,
Dialogue: 0,0:33:44.65,0:33:46.73,Default,,0,0,0,,là vì lấy được Tướng Tinh của trời,
Dialogue: 0,0:33:47.38,0:33:49.10,Default,,0,0,0,,lót đường cho ta xưng vương.
Dialogue: 0,0:34:09.62,0:34:10.61,Default,,0,0,0,,Cơ Xương
Dialogue: 0,0:34:11.02,0:34:13.42,Default,,0,0,0,,là tên tiểu nhân mưu đồ đoạt lợi,
Dialogue: 0,0:34:14.21,0:34:15.86,Default,,0,0,0,,giết cả tộc của ngươi,
Dialogue: 0,0:34:16.10,0:34:19.41,Default,,0,0,0,,còn mơ tưởng lợi dụng ngươi\Nkhởi động trận Phong Thần,
Dialogue: 0,0:34:19.94,0:34:21.42,Default,,0,0,0,,tính toán đủ đường.
Dialogue: 0,0:34:22.58,0:34:23.86,Default,,0,0,0,,Sau khi biết được sự thật
Dialogue: 0,0:34:24.30,0:34:26.34,Default,,0,0,0,,thì giết nhanh gọn lẹ.
Dialogue: 0,0:34:26.73,0:34:28.02,Default,,0,0,0,,Ngươi nói loại người này
Dialogue: 0,0:34:28.33,0:34:30.82,Default,,0,0,0,,còn đáng để ngươi tin tưởng không?
Dialogue: 0,0:34:31.73,0:34:32.94,Default,,0,0,0,,Không đáng.
Dialogue: 0,0:34:36.57,0:34:39.25,Default,,0,0,0,,Cần Tân Hoàn này chỉ vì\Nhoàn thành sứ mệnh Tướng Tinh,
Dialogue: 0,0:34:40.86,0:34:42.50,Default,,0,0,0,,kết quả tộc nhân vứt bỏ ta,
Dialogue: 0,0:34:42.85,0:34:44.21,Default,,0,0,0,,nghĩa phụ muốn giết ta.
Dialogue: 0,0:34:45.86,0:34:47.82,Default,,0,0,0,,Ta luôn là thế thân của Lôi Chấn Tử.
Dialogue: 0,0:34:48.42,0:34:49.86,Default,,0,0,0,,Ta luôn phải gánh vác danh nghĩa của hắn,
Dialogue: 0,0:34:49.86,0:34:51.97,Default,,0,0,0,,sống trong sự ca tụng của mọi người.
Dialogue: 0,0:34:55.93,0:34:57.14,Default,,0,0,0,,Thật sự không đáng.
Dialogue: 0,0:34:58.62,0:35:00.10,Default,,0,0,0,,Không đáng!
Dialogue: 0,0:35:01.65,0:35:04.90,Default,,0,0,0,,Vậy ngươi có muốn thay thế Lôi Chấn Tử không?
Dialogue: 0,0:35:05.85,0:35:07.90,Default,,0,0,0,,Người chứng minh cho tộc Vũ Nhân
Dialogue: 0,0:35:07.90,0:35:09.22,Default,,0,0,0,,chỉ có thể là ngươi.
Dialogue: 0,0:35:15.70,0:35:16.62,Default,,0,0,0,,Đúng.
Dialogue: 0,0:35:18.10,0:35:19.42,Default,,0,0,0,,Chỉ có thể là ta.
Dialogue: 0,0:35:22.10,0:35:23.70,Default,,0,0,0,,Chỉ cần ngươi theo ta,
Dialogue: 0,0:35:24.65,0:35:27.10,Default,,0,0,0,,ta có thể khiến\Nđôi cánh Phong Lôi trưởng thành,
Dialogue: 0,0:35:28.14,0:35:30.42,Default,,0,0,0,,hấp thụ sức mạnh Tướng Tinh của Lôi Chấn Tử
Dialogue: 0,0:35:30.81,0:35:32.57,Default,,0,0,0,,thì có thể trở thành Tướng Tinh.
Dialogue: 0,0:35:33.57,0:35:35.58,Default,,0,0,0,,Ngươi có bằng lòng không?
Dialogue: 0,0:35:50.50,0:35:52.42,Default,,0,0,0,,Văn Thái sư sẽ giết Tân Hoàn!
Dialogue: 0,0:35:52.42,0:35:53.14,Default,,0,0,0,,Nghĩa phụ,
Dialogue: 0,0:35:53.54,0:35:54.86,Default,,0,0,0,,con bằng lòng đơn thân độc mã
Dialogue: 0,0:35:54.86,0:35:56.33,Default,,0,0,0,,đi đến Triều Ca cứu Tân Hoàn về.
Dialogue: 0,0:35:58.46,0:35:59.18,Default,,0,0,0,,Chấn Nhi.
Dialogue: 0,0:36:00.50,0:36:01.17,Default,,0,0,0,,Chấn Nhi!
Dialogue: 0,0:36:01.94,0:36:02.93,Default,,0,0,0,,Không được làm càn!
Dialogue: 0,0:36:03.70,0:36:05.69,Default,,0,0,0,,Chuyện quan trọng trước mắt\Nlà bảo vệ Tướng Tinh.
Dialogue: 0,0:36:08.14,0:36:09.86,Default,,0,0,0,,Tân Hoàn không phải Tướng Tinh sao?
Dialogue: 0,0:36:10.86,0:36:11.94,Default,,0,0,0,,Tiểu tử thối này,
Dialogue: 0,0:36:12.22,0:36:14.10,Default,,0,0,0,,ai là Tướng Tinh\Nmà trong lòng ngươi còn không rõ sao?
Dialogue: 0,0:36:14.78,0:36:15.62,Default,,0,0,0,,Ta nói ngươi biết,
Dialogue: 0,0:36:15.62,0:36:16.46,Default,,0,0,0,,một tháng sau
Dialogue: 0,0:36:16.46,0:36:18.30,Default,,0,0,0,,là lúc trận Phong Thần khởi động,
Dialogue: 0,0:36:18.54,0:36:19.94,Default,,0,0,0,,ngươi muốn bay vào trận Phong Thần
Dialogue: 0,0:36:20.10,0:36:21.70,Default,,0,0,0,,thì nhất định phải lấy được pháp khí\Ncủa tộc Vũ Nhân,
Dialogue: 0,0:36:21.70,0:36:23.02,Default,,0,0,0,,đôi cánh Phong Lôi.
Dialogue: 0,0:36:25.38,0:36:26.78,Default,,0,0,0,,Chuyện này quan trọng lắm đó.
Dialogue: 0,0:36:32.86,0:36:33.74,Default,,0,0,0,,Chấn Nhi,
Dialogue: 0,0:36:34.46,0:36:36.38,Default,,0,0,0,,cho nên con phải lấy đôi cánh Phong Lôi về
Dialogue: 0,0:36:37.05,0:36:38.98,Default,,0,0,0,,mới có thể khởi động trận.
Dialogue: 0,0:36:45.73,0:36:47.17,Default,,0,0,0,,Ý của nghĩa phụ là
Dialogue: 0,0:36:48.05,0:36:50.18,Default,,0,0,0,,muốn con lấy đồ của Tân Hoàn?
Dialogue: 0,0:36:51.78,0:36:53.82,Default,,0,0,0,,Gì mà đồ của hắn?
Dialogue: 0,0:36:53.82,0:36:55.50,Default,,0,0,0,,Đó vốn là đồ của ngươi.
Dialogue: 0,0:36:55.50,0:36:56.38,Default,,0,0,0,,Nếu ngươi không lấy,
Dialogue: 0,0:36:56.38,0:36:57.78,Default,,0,0,0,,thiên hạ nhất định sẽ diệt vong.
Dialogue: 0,0:37:00.37,0:37:01.10,Default,,0,0,0,,Trước mắt,
Dialogue: 0,0:37:01.46,0:37:04.10,Default,,0,0,0,,trận Tru Tiên của Văn Thái sư\Nđã hình thành rồi,
Dialogue: 0,0:37:06.09,0:37:08.94,Default,,0,0,0,,chờ khi đôi cánh Phong Lôi\Ncủa Tân Hoàn trưởng thành,
Dialogue: 0,0:37:09.26,0:37:10.26,Default,,0,0,0,,trận Tru Tiên này
Dialogue: 0,0:37:10.46,0:37:11.98,Default,,0,0,0,,chắc chắn sẽ phá huỷ trận Phong Thần.
Dialogue: 0,0:37:12.90,0:37:13.66,Default,,0,0,0,,Đến lúc đó,
Dialogue: 0,0:37:13.82,0:37:14.70,Default,,0,0,0,,thế gian
Dialogue: 0,0:37:14.98,0:37:16.38,Default,,0,0,0,,sẽ vĩnh viễn không có ngày bình yên.
Dialogue: 0,0:37:17.65,0:37:19.10,Default,,0,0,0,,Đúng đó.
Dialogue: 0,0:37:20.26,0:37:22.05,Default,,0,0,0,,Vậy Tân Hoàn có nguy hiểm không?
Dialogue: 0,0:37:23.22,0:37:24.34,Default,,0,0,0,,Không đâu.
Dialogue: 0,0:37:25.10,0:37:27.74,Default,,0,0,0,,Chẳng phải chỉ là món pháp khí nhỏ xíu sao?
Dialogue: 0,0:37:29.62,0:37:30.26,Default,,0,0,0,,Được.
Dialogue: 0,0:37:30.98,0:37:32.05,Default,,0,0,0,,Con theo hai người.
Dialogue: 0,0:37:33.26,0:37:34.69,Default,,0,0,0,,Nhưng con muốn Hạnh Nhi đi cùng con!
Dialogue: 0,0:37:45.34,0:37:46.94,Default,,0,0,0,,Tiên trưởng!
Dialogue: 0,0:37:47.38,0:37:48.18,Default,,0,0,0,,Ta không đi!
Dialogue: 0,0:37:48.18,0:37:51.09,Default,,0,0,0,,Lão đạo sĩ tóc xoăn này mau thả ta xuống!
Dialogue: 0,0:37:51.66,0:37:54.38,Default,,0,0,0,,Sắp rơi xuống rồi!
Dialogue: 0,0:37:58.50,0:38:00.14,Default,,0,0,0,,Cứu mạng!
Dialogue: 0,0:38:04.46,0:38:06.18,Default,,0,0,0,,Tiên trưởng!
Dialogue: 0,0:38:13.85,0:38:15.38,Default,,0,0,0,,Ngươi có sao không?
Dialogue: 0,0:38:25.30,0:38:26.73,Default,,0,0,0,,Đau quá, đau quá đi.
Dialogue: 0,0:38:27.38,0:38:28.26,Default,,0,0,0,,Cái cây phất trần này,
Dialogue: 0,0:38:28.38,0:38:29.78,Default,,0,0,0,,ngươi mắc vào mông ta rồi!
Dialogue: 0,0:38:30.81,0:38:31.74,Default,,0,0,0,,Tiên trưởng.
Dialogue: 0,0:38:32.62,0:38:33.30,Default,,0,0,0,,Lão già,
Dialogue: 0,0:38:33.74,0:38:34.77,Default,,0,0,0,,mau dạy pháp thuật cho ta đi!
Dialogue: 0,0:38:34.77,0:38:36.34,Default,,0,0,0,,Ta phải đi tìm Tân Hoàn rồi đưa đệ ấy về.
Dialogue: 0,0:38:40.46,0:38:42.25,Default,,0,0,0,,Sao ngươi mở miệng mà không nói gì vậy?
Dialogue: 0,0:38:46.18,0:38:47.38,Default,,0,0,0,,Lão đạo sĩ tóc xoăn này,
Dialogue: 0,0:38:47.38,0:38:48.41,Default,,0,0,0,,mau dạy ta pháp thuật,
Dialogue: 0,0:38:48.41,0:38:49.54,Default,,0,0,0,,ta phải đi tìm Tân Hoàn,
Dialogue: 0,0:38:49.73,0:38:51.58,Default,,0,0,0,,đưa đệ ấy về!
Dialogue: 0,0:38:52.86,0:38:54.66,Default,,0,0,0,,Chuyện này thì ngươi khỏi phải lo.
Dialogue: 0,0:38:55.50,0:38:57.30,Default,,0,0,0,,Hắn sẽ tự đến tìm ngươi thôi.
Dialogue: 0,0:39:05.78,0:39:07.14,Default,,0,0,0,,Đây là trận Tru Tiên
Dialogue: 0,0:39:07.26,0:39:09.22,Default,,0,0,0,,mà bổn thái sư bỏ ra
Dialogue: 0,0:39:09.46,0:39:11.38,Default,,0,0,0,,tâm huyết mười tám năm luyện thành.
Dialogue: 0,0:39:11.38,0:39:12.93,Default,,0,0,0,,Vậy hắn đã luyện thành chưa?
Dialogue: 0,0:39:14.06,0:39:15.66,Default,,0,0,0,,Thanh long phương Đông,
Dialogue: 0,0:39:15.78,0:39:17.22,Default,,0,0,0,,Huyền vũ phương Bắc,
Dialogue: 0,0:39:17.22,0:39:18.94,Default,,0,0,0,,Bạch hổ phương Tây,
Dialogue: 0,0:39:18.94,0:39:20.27,Default,,0,0,0,,Chu tước phương Nam.
Dialogue: 0,0:39:20.27,0:39:21.34,Default,,0,0,0,,Tứ đại trận nhãn
Dialogue: 0,0:39:21.34,0:39:23.10,Default,,0,0,0,,cần phải khởi động hết
Dialogue: 0,0:39:23.34,0:39:24.58,Default,,0,0,0,,mới có thể khởi động.
Dialogue: 0,0:39:25.46,0:39:27.65,Default,,0,0,0,,Muốn khởi động tứ đại trận nhãn
Dialogue: 0,0:39:27.99,0:39:30.40,Default,,0,0,0,,thì có ma lực cực mạnh thúc đẩy.
Dialogue: 0,0:39:32.94,0:39:33.82,Default,,0,0,0,,Ta muốn ngươi
Dialogue: 0,0:39:34.22,0:39:35.58,Default,,0,0,0,,từ bỏ tạp niệm,
Dialogue: 0,0:39:35.86,0:39:37.25,Default,,0,0,0,,tĩnh tâm tu hành,
Dialogue: 0,0:39:37.69,0:39:40.30,Default,,0,0,0,,hỗ trợ trận Tru Tiên hình hành,
Dialogue: 0,0:39:41.14,0:39:42.86,Default,,0,0,0,,cùng nhau gia tăng công lực.
Dialogue: 0,0:39:43.06,0:39:45.06,Default,,0,0,0,,Vậy hắn đi đâu để tìm ma lực cực mạnh?
Dialogue: 0,0:39:46.66,0:39:49.25,Default,,0,0,0,,Xoá bỏ tâm trí của Tân Hoàn,
Dialogue: 0,0:39:49.74,0:39:50.98,Default,,0,0,0,,dẫn vào ma đạo.
Dialogue: 0,0:39:51.26,0:39:52.98,Default,,0,0,0,,Phải đến khi nào ta mới có thể\Ntu luyện thành công?
Dialogue: 0,0:39:54.74,0:39:57.34,Default,,0,0,0,,Chờ khi bốn phương vị Chu tước, Huyền vũ,
Dialogue: 0,0:39:57.34,0:39:58.86,Default,,0,0,0,,Thanh long, Bạch hổ sáng lên,
Dialogue: 0,0:39:59.10,0:40:01.01,Default,,0,0,0,,trận Tru Tiên khởi động,
Dialogue: 0,0:40:01.18,0:40:03.06,Default,,0,0,0,,đôi cánh Phong Lôi trưởng thành,
Dialogue: 0,0:40:03.26,0:40:05.34,Default,,0,0,0,,công pháp của ngươi sẽ tự thành.
Dialogue: 0,0:40:05.34,0:40:06.62,Default,,0,0,0,,Ta không muốn làm hại đệ ấy,
Dialogue: 0,0:40:07.38,0:40:09.02,Default,,0,0,0,,ta chỉ muốn trở thành Tướng Tinh.
Dialogue: 0,0:40:09.46,0:40:10.98,Default,,0,0,0,,Ta nhất định sẽ đưa Tân Hoàn trở về,
Dialogue: 0,0:40:11.66,0:40:12.57,Default,,0,0,0,,đệ ấy là huynh đệ của ta.
Dialogue: 0,0:40:12.86,0:40:14.26,Default,,0,0,0,,Ngươi có nhiệm vụ quan trọng hơn.
Dialogue: 0,0:40:14.42,0:40:15.78,Default,,0,0,0,,Lấy đôi cánh Phong Thần về.
Dialogue: 0,0:40:15.94,0:40:17.85,Default,,0,0,0,,Chỉ cần ngươi cắt đứt vướng bận,
Dialogue: 0,0:40:18.38,0:40:20.62,Default,,0,0,0,,ngươi sẽ là Tướng Tinh mà người đời
Dialogue: 0,0:40:21.02,0:40:22.02,Default,,0,0,0,,kính nể.
Dialogue: 0,0:40:48.06,0:40:49.65,Default,,0,0,0,,Ta mới là Tướng Tinh thật sự,
Dialogue: 0,0:40:50.38,0:40:52.49,Default,,0,0,0,,không ai có thể thay thế vị trí của ta.
Dialogue: 0,0:40:53.86,0:40:55.42,Default,,0,0,0,,Đôi cánh Phong Lôi,
Dialogue: 0,0:40:55.62,0:40:57.13,Default,,0,0,0,,ngươi chính là Tướng Tinh.
Dialogue: 0,0:40:57.26,0:40:58.42,Default,,0,0,0,,Tân Hoàn của ta,
Dialogue: 0,0:40:58.94,0:41:00.26,Default,,0,0,0,,ở phía sau ngươi
Dialogue: 0,0:41:00.46,0:41:01.97,Default,,0,0,0,,là vạn ngươi kính nể,
Dialogue: 0,0:41:02.26,0:41:03.98,Default,,0,0,0,,chúng thần trốn tránh.
Dialogue: 0,0:41:04.58,0:41:05.98,Default,,0,0,0,,Hãy cho chúng xem thử
Dialogue: 0,0:41:06.30,0:41:10.02,Default,,0,0,0,,ai mới là con của Phong Lôi thật sự.
Dialogue: 0,0:41:13.34,0:41:14.17,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,0:41:14.98,0:41:16.78,Default,,0,0,0,,ta nhất định sẽ đưa đệ trở về.
Dialogue: 0,0:41:23.85,0:41:24.29,Default,,0,0,0,,Tiên trưởng.
Dialogue: 0,0:41:37.86,0:41:38.58,Default,,0,0,0,,Tiên trưởng,
Dialogue: 0,0:41:38.90,0:41:39.94,Default,,0,0,0,,ông có được không vậy?
Dialogue: 0,0:41:41.01,0:41:42.14,Default,,0,0,0,,Sao ta có thể không được chứ?
Dialogue: 0,0:41:42.58,0:41:43.86,Default,,0,0,0,,Tiên căn của Tướng Tinh
Dialogue: 0,0:41:43.94,0:41:45.26,Default,,0,0,0,,khó tìm thấy lắm.
Dialogue: 0,0:41:46.34,0:41:47.66,Default,,0,0,0,,Đây là lần đầu tiên ta châm đấy.
Dialogue: 0,0:41:48.30,0:41:48.90,Default,,0,0,0,,Lần đần tiên?
Dialogue: 0,0:41:48.90,0:41:49.61,Default,,0,0,0,,Ngươi ngồi yên đi.
Dialogue: 0,0:41:49.61,0:41:50.26,Default,,0,0,0,,Bỏ đi, không làm nữa.
Dialogue: 0,0:41:50.26,0:41:50.90,Default,,0,0,0,,- Không làm nữa! Không làm nữa!\N- Đừng, đừng đừng!
Dialogue: 0,0:41:50.90,0:41:51.58,Default,,0,0,0,,Ta không làm nữa!
Dialogue: 0,0:41:51.58,0:41:53.86,Default,,0,0,0,,Người khác muốn châm, ta còn không làm đấy.
Dialogue: 0,0:42:00.49,0:42:01.14,Default,,0,0,0,,Thế nào?
Dialogue: 0,0:42:03.06,0:42:03.78,Default,,0,0,0,,Không đau chút nào.
Dialogue: 0,0:42:03.78,0:42:04.62,Default,,0,0,0,,Tiên trưởng,
Dialogue: 0,0:42:04.70,0:42:06.06,Default,,0,0,0,,ông châm sai rồi.
Dialogue: 0,0:42:16.01,0:42:18.50,Default,,0,0,0,,Tại sao?
Dialogue: 0,0:42:19.18,0:42:21.17,Default,,0,0,0,,Tại sao cánh không chịu mọc ra?
Dialogue: 0,0:42:30.26,0:42:31.58,Default,,0,0,0,,Tại sao lại nhìn trộm?
Dialogue: 0,0:42:36.13,0:42:38.10,Default,,0,0,0,,Quả nhiên ngươi không phụ kỳ vọng của ta.
Dialogue: 0,0:42:39.42,0:42:40.65,Default,,0,0,0,,Khoảng thời gian này,
Dialogue: 0,0:42:40.90,0:42:42.34,Default,,0,0,0,,ngươi tu luyện trong trận,
Dialogue: 0,0:42:42.94,0:42:44.90,Default,,0,0,0,,trận Tru Tiên đã khởi động.
Dialogue: 0,0:42:45.22,0:42:45.86,Default,,0,0,0,,Uy lực của Chu tước,
Dialogue: 0,0:42:46.14,0:42:46.90,Default,,0,0,0,,Huyền vũ
Dialogue: 0,0:42:47.06,0:42:48.98,Default,,0,0,0,,đã tăng lên rất nhiều
Dialogue: 0,0:42:49.42,0:42:51.02,Default,,0,0,0,,còn công lực của ngươi tăng lên
Dialogue: 0,0:42:51.34,0:42:52.90,Default,,0,0,0,,cũng ngày một lớn mạnh.
Dialogue: 0,0:42:53.41,0:42:54.34,Default,,0,0,0,,Xem ra,
Dialogue: 0,0:42:54.50,0:42:57.10,Default,,0,0,0,,kế hoạch phá huỷ\Ntrận Phong Thần của chúng ta
Dialogue: 0,0:42:57.37,0:42:59.18,Default,,0,0,0,,không còn xa vời nữa.
Dialogue: 0,0:43:01.14,0:43:02.18,Default,,0,0,0,,Vẫn không đủ.
Dialogue: 0,0:43:03.42,0:43:04.26,Default,,0,0,0,,Bây giờ
Dialogue: 0,0:43:04.70,0:43:07.49,Default,,0,0,0,,Lôi Chấn Tử đã được\NVân Trung Tử nhận làm đồ đệ.
Dialogue: 0,0:43:08.22,0:43:08.93,Default,,0,0,0,,Đến lúc đó,
Dialogue: 0,0:43:09.06,0:43:11.78,Default,,0,0,0,,hắn nhất định sẽ quay về\Nlấy đôi cánh Phong Lôi đi.
Dialogue: 0,0:43:12.10,0:43:12.82,Default,,0,0,0,,Ngươi phải cần
Dialogue: 0,0:43:13.26,0:43:14.97,Default,,0,0,0,,sớm ngày cắt bỏ tất cả.
Dialogue: 0,0:43:15.34,0:43:16.46,Default,,0,0,0,,Nhưng Lôi Chấn Tử sẽ không...
Dialogue: 0,0:43:16.46,0:43:17.10,Default,,0,0,0,,Ngây thơ!
Dialogue: 0,0:43:18.86,0:43:19.74,Default,,0,0,0,,Ta có một kế
Dialogue: 0,0:43:20.46,0:43:22.26,Default,,0,0,0,,có thể khiến người cắt bỏ chờ mong,
Dialogue: 0,0:43:22.82,0:43:24.30,Default,,0,0,0,,ngươi có muốn thử không?
Dialogue: 0,0:43:27.10,0:43:28.66,Default,,0,0,0,,Cưỡng chế mở tiên căn
Dialogue: 0,0:43:28.66,0:43:31.78,Default,,0,0,0,,phải chịu đựng sự đau khổ\Nmà người thường khó mà chịu được.
Dialogue: 0,0:43:31.94,0:43:33.10,Default,,0,0,0,,Xé tim rạch phổi,
Dialogue: 0,0:43:33.30,0:43:35.06,Default,,0,0,0,,người thường không thể nào chịu được,
Dialogue: 0,0:43:35.21,0:43:36.86,Default,,0,0,0,,nhưng cũng phải thử xem.
Dialogue: 0,0:43:36.97,0:43:38.62,Default,,0,0,0,,Còn mười bảy châm nữa!
Dialogue: 0,0:43:44.62,0:43:47.18,Default,,0,0,0,,Thiên linh nhập châm, tiên căn đại thành!
Dialogue: 0,0:44:13.86,0:44:16.18,Default,,0,0,0,,Bây giờ ta cảm thấy người tràn đầy sinh lực!
Dialogue: 0,0:44:16.81,0:44:18.06,Default,,0,0,0,,Tiên trưởng!
Dialogue: 0,0:44:19.26,0:44:20.10,Default,,0,0,0,,Ông có sao không tiên trưởng?
Dialogue: 0,0:44:20.10,0:44:20.86,Default,,0,0,0,,Không sao.
Dialogue: 0,0:44:25.05,0:44:26.46,Default,,0,0,0,,Quả nhiên là Tướng Tinh.
Dialogue: 0,0:44:27.86,0:44:29.98,Default,,0,0,0,,Ngươi muốn chiến thắng Tân Hoàn\Ntrong thời gian ngắn
Dialogue: 0,0:44:31.06,0:44:33.94,Default,,0,0,0,,thì phải uống công thức độc môn của ta.
Dialogue: 0,0:44:40.78,0:44:41.38,Default,,0,0,0,,Lão già,
Dialogue: 0,0:44:41.74,0:44:43.18,Default,,0,0,0,,ta thấy chắc là đủ rồi.
Dialogue: 0,0:44:43.30,0:44:44.26,Default,,0,0,0,,Còn lại,
Dialogue: 0,0:44:44.62,0:44:45.34,Default,,0,0,0,,ngày mai chúng ta hãy thử.
Dialogue: 0,0:44:45.82,0:44:47.26,Default,,0,0,0,,Ngươi nói gì vậy hả tiểu tử thối?
Dialogue: 0,0:44:47.38,0:44:48.30,Default,,0,0,0,,Ngươi đừng không biết tốt xấu!
Dialogue: 0,0:44:48.46,0:44:49.37,Default,,0,0,0,,Trên đầu ông còn châm kìa!
Dialogue: 0,0:44:49.46,0:44:50.38,Default,,0,0,0,,Tiên trưởng!
Dialogue: 0,0:45:14.22,0:45:15.22,Default,,0,0,0,,Là Hoàn Nhi sao?
Dialogue: 0,0:45:35.54,0:45:36.30,Default,,0,0,0,,Cơ Xương,
Dialogue: 0,0:45:38.98,0:45:40.34,Default,,0,0,0,,chúng ta lại gặp nhau rồi.
Dialogue: 0,0:45:43.78,0:45:44.66,Default,,0,0,0,,Lôi Chấn Tử,
Dialogue: 0,0:45:44.81,0:45:45.54,Default,,0,0,0,,ngươi đừng sợ,
Dialogue: 0,0:45:45.90,0:45:47.57,Default,,0,0,0,,ta tin ngươi làm được mà.
Dialogue: 0,0:45:53.14,0:45:53.54,Default,,0,0,0,,Nào,
Dialogue: 0,0:45:53.66,0:45:54.30,Default,,0,0,0,,tiểu tử thối,
Dialogue: 0,0:45:55.49,0:45:56.90,Default,,0,0,0,,với trình độ của ngươi,
Dialogue: 0,0:45:57.30,0:45:58.98,Default,,0,0,0,,ta nhắm mắt cũng đánh thắng được.
Dialogue: 0,0:46:17.81,0:46:18.46,Default,,0,0,0,,Tiểu tử thối,
Dialogue: 0,0:46:18.54,0:46:19.22,Default,,0,0,0,,đừng đắc ý,
Dialogue: 0,0:46:19.38,0:46:20.46,Default,,0,0,0,,ngươi nhìn dưới chân đi kìa.
Dialogue: 0,0:46:35.90,0:46:36.90,Default,,0,0,0,,Chó ăn phân.
Dialogue: 0,0:46:38.06,0:46:38.78,Default,,0,0,0,,Có phục không?
Dialogue: 0,0:46:38.94,0:46:39.86,Default,,0,0,0,,Không phục thì đứng lên.
Dialogue: 0,0:46:40.98,0:46:41.74,Default,,0,0,0,,Lại lần nữa!
Dialogue: 0,0:46:42.30,0:46:42.86,Default,,0,0,0,,Được được được.
Dialogue: 0,0:46:43.01,0:46:43.82,Default,,0,0,0,,Đứng lên, đứng lên.
Dialogue: 0,0:46:47.09,0:46:47.62,Default,,0,0,0,,Không hay rồi.
Dialogue: 0,0:46:48.82,0:46:49.34,Default,,0,0,0,,Lôi Chấn Tử,
Dialogue: 0,0:46:49.73,0:46:52.42,Default,,0,0,0,,[Tây Bá hầu gặp nạn]
Dialogue: 0,0:46:49.94,0:46:51.54,Default,,0,0,0,,Tân Hoàn bắt Tây Bá hầu đi rồi.
Dialogue: 0,0:46:57.06,0:46:58.62,Default,,0,0,0,,Lão già, ta xin ông đó,
Dialogue: 0,0:46:58.62,0:47:00.58,Default,,0,0,0,,ông cho ta đi cứu nghĩa phụ đi, được không?
Dialogue: 0,0:47:00.98,0:47:03.10,Default,,0,0,0,,Ta và Tân Hoàn còn chút chuyện\Ncần phải giải quyết nữa.
Dialogue: 0,0:47:05.10,0:47:06.66,Default,,0,0,0,,Ngươi có thấy phiền không hả?
Dialogue: 0,0:47:07.54,0:47:08.70,Default,,0,0,0,,Ngươi uống nhiều đan dược như vậy
Dialogue: 0,0:47:09.34,0:47:11.02,Default,,0,0,0,,mà công lực không hề tăng lên chút nào.
Dialogue: 0,0:47:11.14,0:47:11.94,Default,,0,0,0,,Ngươi đi làm gì hả?
Dialogue: 0,0:47:11.94,0:47:12.90,Default,,0,0,0,,Ngươi đi tìm đường chết sao?
Dialogue: 0,0:47:13.49,0:47:14.74,Default,,0,0,0,,Ngươi chết thì không sao
Dialogue: 0,0:47:14.90,0:47:16.34,Default,,0,0,0,,nhưng ta biết ăn nói\Nvới Thiên Tôn thế nào đây?
Dialogue: 0,0:47:16.98,0:47:17.82,Default,,0,0,0,,Ta mặc kệ,
Dialogue: 0,0:47:17.89,0:47:18.74,Default,,0,0,0,,nói chung ta phải đi,
Dialogue: 0,0:47:18.74,0:47:19.78,Default,,0,0,0,,đừng ai cản ta.
Dialogue: 0,0:47:21.62,0:47:22.14,Default,,0,0,0,,Chờ đã!
Dialogue: 0,0:47:24.49,0:47:25.22,Default,,0,0,0,,Tiên trưởng,
Dialogue: 0,0:47:25.42,0:47:26.25,Default,,0,0,0,,có thể...
Dialogue: 0,0:47:35.74,0:47:37.25,Default,,0,0,0,,Cái này lại là gì nữa đây?
Dialogue: 0,0:47:38.02,0:47:38.66,Default,,0,0,0,,Thứ này
Dialogue: 0,0:47:39.45,0:47:41.18,Default,,0,0,0,,là quả thánh Kim Long
Dialogue: 0,0:47:42.30,0:47:43.53,Default,,0,0,0,,có sức mạnh cực lớn,
Dialogue: 0,0:47:44.34,0:47:46.17,Default,,0,0,0,,nếu bị nó nuốt vào
Dialogue: 0,0:47:46.66,0:47:49.42,Default,,0,0,0,,thì Văn Thái sư\Ncũng phải nể ngươi ba phần đấy.
Dialogue: 0,0:47:52.42,0:47:53.14,Default,,0,0,0,,Ta cần có nó,
Dialogue: 0,0:47:53.42,0:47:54.38,Default,,0,0,0,,xin hãy cho ta.
Dialogue: 0,0:47:56.73,0:47:58.10,Default,,0,0,0,,Tuy ngươi là Tướng Tinh
Dialogue: 0,0:47:58.54,0:48:00.46,Default,,0,0,0,,nhưng cũng phải nói lời không hay trước.
Dialogue: 0,0:48:01.54,0:48:02.38,Default,,0,0,0,,Ngươi muốn nó
Dialogue: 0,0:48:02.89,0:48:04.42,Default,,0,0,0,,hoà hợp hoàn toàn cùng ngươi,
Dialogue: 0,0:48:05.18,0:48:08.34,Default,,0,0,0,,nhận được sức mạnh siêu cấp của thần,
Dialogue: 0,0:48:09.14,0:48:10.58,Default,,0,0,0,,thì phải chấp nhận số mệnh
Dialogue: 0,0:48:11.13,0:48:12.13,Default,,0,0,0,,bị quả thánh Kim Long
Dialogue: 0,0:48:12.74,0:48:14.14,Default,,0,0,0,,nuốt chửng.
Dialogue: 0,0:48:14.86,0:48:15.97,Default,,0,0,0,,Ngươi có bằng lòng không?
Dialogue: 0,0:48:17.85,0:48:18.46,Default,,0,0,0,,Tiên trưởng,
Dialogue: 0,0:48:18.70,0:48:19.49,Default,,0,0,0,,ông không thể làm vậy.
Dialogue: 0,0:48:19.90,0:48:22.13,Default,,0,0,0,,Nếu Lôi Chấn Tử\Nbị quả thánh Kim Long nuốt chửng,
Dialogue: 0,0:48:22.38,0:48:22.94,Default,,0,0,0,,hắn sẽ biến thành...
Dialogue: 0,0:48:27.53,0:48:28.62,Default,,0,0,0,,Ngươi căng thẳng gì chứ?
Dialogue: 0,0:48:30.14,0:48:31.10,Default,,0,0,0,,Quả thánh Kim Long
Dialogue: 0,0:48:31.81,0:48:33.42,Default,,0,0,0,,chỉ có thể ký kết khế ước
Dialogue: 0,0:48:33.94,0:48:35.45,Default,,0,0,0,,với người tình nguyện.
Dialogue: 0,0:48:36.62,0:48:37.98,Default,,0,0,0,,Còn có thành công hay không,
Dialogue: 0,0:48:38.58,0:48:40.06,Default,,0,0,0,,phải xem bản thân ngươi.
Dialogue: 0,0:48:41.61,0:48:42.22,Default,,0,0,0,,Ngươi đừng đồng ý với ông ấy.
Dialogue: 0,0:48:42.22,0:48:42.70,Default,,0,0,0,,Ta chấp nhận.
Dialogue: 0,0:48:44.78,0:48:46.22,Default,,0,0,0,,Nếu hy sinh một mình ta
Dialogue: 0,0:48:46.22,0:48:47.50,Default,,0,0,0,,mà có thể cứu tất cả mọi người,
Dialogue: 0,0:48:47.94,0:48:48.70,Default,,0,0,0,,cũng lời lắm rồi.
Dialogue: 0,0:48:49.26,0:48:50.46,Default,,0,0,0,,Hạnh Nhi, ta không còn thời gian nữa.
Dialogue: 0,0:48:51.70,0:48:52.86,Default,,0,0,0,,Xin hãy cho ta quả thánh.
Dialogue: 0,0:48:54.54,0:48:55.10,Default,,0,0,0,,Được.
Dialogue: 0,0:49:13.98,0:49:15.34,Default,,0,0,0,,Tây Bá hầu!
Dialogue: 0,0:49:19.38,0:49:20.82,Default,,0,0,0,,Sao ông lại đối xử với ta như vậy?
Dialogue: 0,0:49:22.18,0:49:23.62,Default,,0,0,0,,Giết hết tộc Vũ Nhân của ta,
Dialogue: 0,0:49:24.10,0:49:25.38,Default,,0,0,0,,lại bày kế cho ta vào chỗ chết,
Dialogue: 0,0:49:26.70,0:49:28.58,Default,,0,0,0,,chẳng lẽ chỉ vì ta không phải Tướng Tinh sao?
Dialogue: 0,0:49:29.98,0:49:30.78,Default,,0,0,0,,Ông chờ đó,
Dialogue: 0,0:49:31.82,0:49:33.70,Default,,0,0,0,,bây giờ ta sẽ dùng ông dụ Lôi Chấn Tử đến.
Dialogue: 0,0:49:33.70,0:49:34.66,Default,,0,0,0,,Ta xem thử hắn
Dialogue: 0,0:49:34.90,0:49:36.22,Default,,0,0,0,,sẽ lựa chọn thế nào.
Dialogue: 0,0:49:37.02,0:49:37.82,Default,,0,0,0,,Hoàn Nhi,
Dialogue: 0,0:49:38.98,0:49:41.13,Default,,0,0,0,,tộc Vũ Nhân không phải do ta giết,
Dialogue: 0,0:49:42.14,0:49:45.18,Default,,0,0,0,,con đừng để trúng gian kế của Văn Trọng,\NHoàn Nhi.
Dialogue: 0,0:49:47.26,0:49:49.74,Default,,0,0,0,,Lôi Chấn Tử là huynh đệ ruột của con,
Dialogue: 0,0:49:50.02,0:49:51.62,Default,,0,0,0,,con không thể giết nó!
Dialogue: 0,0:49:53.42,0:49:54.58,Default,,0,0,0,,Không thể giết hắn?
Dialogue: 0,0:49:57.62,0:49:58.41,Default,,0,0,0,,Hay lắm.
Dialogue: 0,0:50:00.10,0:50:01.02,Default,,0,0,0,,Ta không thể giết hắn
Dialogue: 0,0:50:01.02,0:50:02.46,Default,,0,0,0,,thì ông có thể giết ta sao?
Dialogue: 0,0:50:06.62,0:50:07.50,Default,,0,0,0,,Hoàn Nhi,
Dialogue: 0,0:50:09.29,0:50:10.86,Default,,0,0,0,,ta không còn lựa chọn nào khác.
Dialogue: 0,0:50:12.10,0:50:13.34,Default,,0,0,0,,Đây là số trời, Hoàn Nhi!
Dialogue: 0,0:50:13.34,0:50:15.45,Default,,0,0,0,,Ta mặc kệ số trời!
Dialogue: 0,0:50:18.21,0:50:19.09,Default,,0,0,0,,Hoàn Nhi!
Dialogue: 0,0:50:30.74,0:50:31.41,Default,,0,0,0,,Hầu gia!
Dialogue: 0,0:50:35.78,0:50:36.50,Default,,0,0,0,,Hầu gia!
Dialogue: 0,0:50:36.90,0:50:37.57,Default,,0,0,0,,Hầu gia!
Dialogue: 0,0:50:38.14,0:50:40.06,Default,,0,0,0,,Tân Hoàn, đệ có biết\Nđệ đang làm gì không?
Dialogue: 0,0:50:40.38,0:50:41.53,Default,,0,0,0,,Đây là nghĩa phụ của chúng ta mà!
Dialogue: 0,0:50:41.53,0:50:42.62,Default,,0,0,0,,Ta không có nghĩa phụ!
Dialogue: 0,0:50:44.93,0:50:45.70,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,0:50:45.93,0:50:47.74,Default,,0,0,0,,ngươi đừng tin lời Văn Trọng lừa gạt.
Dialogue: 0,0:50:50.18,0:50:50.89,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,0:50:51.70,0:50:52.50,Default,,0,0,0,,đừng quậy nữa,
Dialogue: 0,0:50:53.34,0:50:54.54,Default,,0,0,0,,đưa đôi cánh Phong Lôi cho ta,
Dialogue: 0,0:50:55.29,0:50:56.46,Default,,0,0,0,,ta có thể bảo vệ đệ.
Dialogue: 0,0:51:05.26,0:51:07.42,Default,,0,0,0,,Đây là sự lựa chọn của ngươi sao, Lôi Chấn Tử?
Dialogue: 0,0:51:09.42,0:51:10.82,Default,,0,0,0,,Ngươi khiến ta thất vọng quá.
Dialogue: 0,0:51:15.78,0:51:16.70,Default,,0,0,0,,Trước khi ngươi chưa đến đây,
Dialogue: 0,0:51:17.70,0:51:19.66,Default,,0,0,0,,ta vẫn còn chút hy vọng với ngươi,
Dialogue: 0,0:51:21.38,0:51:23.10,Default,,0,0,0,,ta tưởng ngươi\Ngiống như những gì ngươi đã nói,
Dialogue: 0,0:51:23.10,0:51:25.06,Default,,0,0,0,,không màng đến thân phận Tướng Tinh,
Dialogue: 0,0:51:26.90,0:51:29.66,Default,,0,0,0,,không lấy mạng ta giống như Tây Bá hầu.
Dialogue: 0,0:51:31.54,0:51:32.57,Default,,0,0,0,,Không phải đâu Tân Hoàn.
Dialogue: 0,0:51:32.98,0:51:34.33,Default,,0,0,0,,Đệ đang nói lung tung gì vậy?
Dialogue: 0,0:51:35.10,0:51:37.06,Default,,0,0,0,,Đệ từng tin vào mưu kế của tên Văn lão tặc đó.
Dialogue: 0,0:51:37.94,0:51:39.58,Default,,0,0,0,,Ta chỉ đến lấy đôi cánh Phong Lôi về,
Dialogue: 0,0:51:39.58,0:51:40.49,Default,,0,0,0,,cứu nguy cho thiên hạ,
Dialogue: 0,0:51:40.58,0:51:41.98,Default,,0,0,0,,không hề có suy nghĩ nào khác.
Dialogue: 0,0:51:42.62,0:51:43.69,Default,,0,0,0,,Đệ đệ ngoan.
Dialogue: 0,0:51:45.89,0:51:47.62,Default,,0,0,0,,Ngươi có biết năm xưa vì muốn bảo vệ ngươi,
Dialogue: 0,0:51:47.62,0:51:49.38,Default,,0,0,0,,họ mới cố gắng nhét đôi cánh này cho ta,
Dialogue: 0,0:51:49.38,0:51:51.46,Default,,0,0,0,,khiến ta làm con rối suốt mười tám năm không?
Dialogue: 0,0:51:56.06,0:51:56.89,Default,,0,0,0,,Ngươi có biết
Dialogue: 0,0:51:59.14,0:52:00.38,Default,,0,0,0,,nếu ngươi lấy đôi cánh của ta
Dialogue: 0,0:52:02.22,0:52:03.82,Default,,0,0,0,,thì ta sẽ hồn bay phách tán không?
Dialogue: 0,0:52:07.38,0:52:08.94,Default,,0,0,0,,Tân Hoàn, đệ đang nói gì vậy?
Dialogue: 0,0:52:16.54,0:52:18.10,Default,,0,0,0,,Những người nói là yêu thương ta
Dialogue: 0,0:52:19.22,0:52:20.74,Default,,0,0,0,,đều mong ta chết.
Dialogue: 0,0:52:22.97,0:52:23.54,Default,,0,0,0,,Tân Hoàn...
Dialogue: 0,0:52:23.54,0:52:25.66,Default,,0,0,0,,Người ta bảo vệ cũng muốn ta chết.
Dialogue: 0,0:52:27.66,0:52:29.62,Default,,0,0,0,,Là các ngươi đã ép ta vào đường cùng.
Dialogue: 0,0:52:31.65,0:52:34.02,Default,,0,0,0,,Vậy Tân Hoàn ta sẽ chặt đứt nỗi vướng bận này,
Dialogue: 0,0:52:35.02,0:52:37.70,Default,,0,0,0,,tự tác thành cho bản thân!
Dialogue: 0,0:53:09.06,0:53:10.22,Default,,0,0,0,,Tân Hoàn, đệ điên rồi!
Dialogue: 0,0:53:20.98,0:53:21.58,Default,,0,0,0,,Tân Hoàn!
Dialogue: 0,0:53:21.70,0:53:22.54,Default,,0,0,0,,Đệ nói rõ ràng đi,
Dialogue: 0,0:53:23.06,0:53:25.10,Default,,0,0,0,,đôi cánh Phong Lôi\Ncó liên quan gì đến tính mạng đệ?
Dialogue: 0,0:53:51.58,0:53:52.29,Default,,0,0,0,,Tân Hoàn!
Dialogue: 0,0:53:52.41,0:53:52.82,Default,,0,0,0,,Đừng mà!
Dialogue: 0,0:53:59.30,0:54:00.65,Default,,0,0,0,,Hạnh Nhi!
Dialogue: 0,0:54:43.13,0:54:44.22,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,0:54:44.58,0:54:46.38,Default,,0,0,0,,đó là đệ đệ của ngươi.
Dialogue: 0,0:55:06.33,0:55:07.82,Default,,0,0,0,,Câm mồm!
Dialogue: 0,0:55:47.05,0:55:48.30,Default,,0,0,0,,Đừng giận nghĩa phụ nữa,
Dialogue: 0,0:55:48.62,0:55:49.50,Default,,0,0,0,,có ca ca ở đây,
Dialogue: 0,0:55:49.57,0:55:50.57,Default,,0,0,0,,ta sẽ bảo vệ đệ.
Dialogue: 0,0:55:51.06,0:55:51.62,Default,,0,0,0,,Nè.
Dialogue: 0,0:56:09.26,0:56:10.02,Default,,0,0,0,,Lôi Chấn Tử.
Dialogue: 0,0:56:13.82,0:56:14.54,Default,,0,0,0,,Các ngươi đi đi.
Dialogue: 0,0:56:40.02,0:56:42.38,Default,,0,0,0,,Xem ra Hoàn Nhi đã thật sự nhập ma rồi.
Dialogue: 0,0:56:44.06,0:56:45.10,Default,,0,0,0,,May mà hôm nay
Dialogue: 0,0:56:45.38,0:56:46.45,Default,,0,0,0,,có Chấn Nhi ở đây,
Dialogue: 0,0:56:47.06,0:56:47.77,Default,,0,0,0,,nếu không
Dialogue: 0,0:56:48.10,0:56:49.02,Default,,0,0,0,,thì Cơ Xương ta
Dialogue: 0,0:56:49.42,0:56:50.74,Default,,0,0,0,,khó mà giữ mạng.
Dialogue: 0,0:56:53.38,0:56:54.98,Default,,0,0,0,,Xem ra Văn lão tặc đó
Dialogue: 0,0:56:56.10,0:56:58.86,Default,,0,0,0,,đã hoàn thành bước cuối cùng\Ncủa trận Tru Tiên rồi.
Dialogue: 0,0:57:06.30,0:57:06.98,Default,,0,0,0,,Hầu gia,
Dialogue: 0,0:57:07.78,0:57:08.62,Default,,0,0,0,,hình như
Dialogue: 0,0:57:09.30,0:57:10.46,Default,,0,0,0,,Lôi Chấn Tử có gì đó không đúng.
Dialogue: 0,0:57:14.66,0:57:15.34,Default,,0,0,0,,Chấn Nhi,
Dialogue: 0,0:57:15.74,0:57:16.54,Default,,0,0,0,,con sao vậy?
Dialogue: 0,0:57:20.22,0:57:20.89,Default,,0,0,0,,Chấn Nhi?
Dialogue: 0,0:57:30.42,0:57:31.10,Default,,0,0,0,,Nghĩa phụ,
Dialogue: 0,0:57:34.82,0:57:36.29,Default,,0,0,0,,con có thể làm Tướng Tinh,
Dialogue: 0,0:57:37.70,0:57:39.49,Default,,0,0,0,,có thể mở trận Phong Thần,
Dialogue: 0,0:57:42.18,0:57:44.18,Default,,0,0,0,,nhưng mọi người chưa từng nói với con,
Dialogue: 0,0:57:45.78,0:57:47.98,Default,,0,0,0,,lấy đôi cánh Phong Lôi\Ntrong người Tân Hoàn ra
Dialogue: 0,0:57:48.14,0:57:49.78,Default,,0,0,0,,thì sẽ lấy mạng đệ ấy.
Dialogue: 0,0:57:53.70,0:57:54.50,Default,,0,0,0,,Tiểu tử thối,
Dialogue: 0,0:57:54.77,0:57:55.58,Default,,0,0,0,,bọn ta làm như vậy
Dialogue: 0,0:57:55.65,0:57:56.21,Default,,0,0,0,,chẳng phải vì...
Dialogue: 0,0:57:56.21,0:57:57.50,Default,,0,0,0,,Sợ ta hành động theo cảm tính,
Dialogue: 0,0:57:58.78,0:58:00.61,Default,,0,0,0,,sợ ta không nhẫn tâm giết Tân Hoàn,
Dialogue: 0,0:58:01.98,0:58:03.86,Default,,0,0,0,,sợ ta hại cả thiên hạ này.
Dialogue: 0,0:58:07.46,0:58:09.58,Default,,0,0,0,,Nhưng chúng ta nghĩ cho thiên hạ,
Dialogue: 0,0:58:11.17,0:58:12.98,Default,,0,0,0,,thì ai sẽ nghĩ cho chúng ta?
Dialogue: 0,0:58:25.46,0:58:26.45,Default,,0,0,0,,Đúng vậy.
Dialogue: 0,0:58:28.38,0:58:29.42,Default,,0,0,0,,Nhưng làm anh hùng,
Dialogue: 0,0:58:30.30,0:58:31.38,Default,,0,0,0,,làm quân vương
Dialogue: 0,0:58:33.74,0:58:35.98,Default,,0,0,0,,thì trời định phải cô độc cả kiếp này.
Dialogue: 0,0:58:38.06,0:58:39.65,Default,,0,0,0,,Tuy Cơ Xương ta có trăm con
Dialogue: 0,0:58:39.94,0:58:41.18,Default,,0,0,0,,nhưng đều chết bất đắc kỳ tử,
Dialogue: 0,0:58:41.82,0:58:43.50,Default,,0,0,0,,không ai có thể tránh khỏi.
Dialogue: 0,0:58:46.42,0:58:48.69,Default,,0,0,0,,Ta cũng chỉ có thể chấp nhận thôi.
Dialogue: 0,0:58:54.22,0:58:55.17,Default,,0,0,0,,Chấn Nhi,
Dialogue: 0,0:58:57.37,0:58:58.62,Default,,0,0,0,,vi phụ hy vọng
Dialogue: 0,0:58:59.94,0:59:01.70,Default,,0,0,0,,con cũng có thể.
Dialogue: 0,0:59:04.26,0:59:04.62,Default,,0,0,0,,Phụ thân...
Dialogue: 0,0:59:04.62,0:59:05.41,Default,,0,0,0,,Cái lạy này,
Dialogue: 0,0:59:05.78,0:59:08.78,Default,,0,0,0,,ta thay bách tính trong thiên hạ\Ntiễn biệt Tướng Tinh.
Dialogue: 0,0:59:16.02,0:59:16.54,Default,,0,0,0,,Phụ thân,
Dialogue: 0,0:59:18.82,0:59:20.90,Default,,0,0,0,,con biết tầm quan trọng của trận Phong Thần,
Dialogue: 0,0:59:22.45,0:59:23.82,Default,,0,0,0,,nhưng Tân Hoàn cũng quan trọng.
Dialogue: 0,0:59:30.34,0:59:31.10,Default,,0,0,0,,Xin lỗi,
Dialogue: 0,0:59:32.46,0:59:34.06,Default,,0,0,0,,con phải đưa Tân Hoàn trở về.
Dialogue: 0,0:59:35.42,0:59:37.02,Default,,0,0,0,,Trận Tru Tiên đó quá lạnh lẽo
Dialogue: 0,0:59:38.41,0:59:39.54,Default,,0,0,0,,với đệ ấy.
Dialogue: 0,0:59:42.37,0:59:46.54,Default,,0,0,0,,Không giết được Lôi Chấn Tử,\Nmãi mãi không thể thành Tướng Tinh.
Dialogue: 0,0:59:46.98,0:59:48.02,Default,,0,0,0,,Xem ra,
Dialogue: 0,0:59:48.09,0:59:49.62,Default,,0,0,0,,ta nhìn lầm ngươi rồi.
Dialogue: 0,0:59:50.22,0:59:52.66,Default,,0,0,0,,Ngươi vốn không thể giang cánh ra được,
Dialogue: 0,0:59:53.49,0:59:54.94,Default,,0,0,0,,uổng cho ta phí hết tâm tư
Dialogue: 0,0:59:55.30,0:59:57.30,Default,,0,0,0,,nâng cao tu vi cho ngươi.
Dialogue: 0,1:00:02.66,1:00:03.61,Default,,0,0,0,,Ta có thể!
Dialogue: 0,1:00:04.54,1:00:07.18,Default,,0,0,0,,Ta sẽ cho ngươi một cơ hội cuối cùng,
Dialogue: 0,1:00:09.94,1:00:11.90,Default,,0,0,0,,nhập trận thành ma.
Dialogue: 0,1:00:15.54,1:00:16.66,Default,,0,0,0,,Quân Hắc Long,
Dialogue: 0,1:00:16.69,1:00:19.10,Default,,0,0,0,,áp sát mọi hướng biên giới Tây Kỳ.
Dialogue: 0,1:00:21.94,1:00:22.74,Default,,0,0,0,,Hầu gia,
Dialogue: 0,1:00:23.06,1:00:25.90,Default,,0,0,0,,quân Hắc Long của Triều Ca\Nđã áp sát biên giới Ung Châu,
Dialogue: 0,1:00:25.90,1:00:27.14,Default,,0,0,0,,không đến ba ngày nữa,
Dialogue: 0,1:00:27.14,1:00:28.54,Default,,0,0,0,,sẽ tiến công vào Tây Kỳ ta.
Dialogue: 0,1:00:30.42,1:00:32.82,Default,,0,0,0,,Bọn ta cúc cung tận tuỵ cho triều đình,
Dialogue: 0,1:00:33.18,1:00:34.62,Default,,0,0,0,,chưa gặp tai hoạ gì,
Dialogue: 0,1:00:35.34,1:00:38.62,Default,,0,0,0,,thiên hạ lê dân phải sống thế nào đây?
Dialogue: 0,1:00:40.70,1:00:41.82,Default,,0,0,0,,Truyền lệnh của ta,
Dialogue: 0,1:00:42.70,1:00:43.90,Default,,0,0,0,,toàn biên giới Tây Kỳ
Dialogue: 0,1:00:44.26,1:00:45.58,Default,,0,0,0,,vào trạng thái chuẩn bị chiến đấu.
Dialogue: 0,1:00:46.46,1:00:47.34,Default,,0,0,0,,Thề chết
Dialogue: 0,1:00:48.34,1:00:49.54,Default,,0,0,0,,chiến đấu với Triều Ca!
Dialogue: 0,1:00:49.66,1:00:53.54,Default,,0,0,0,,Bọn ta bằng lòng theo Tây bá hầu!
Dialogue: 0,1:00:53.58,1:00:55.90,Default,,0,0,0,,Bọn ta bằng lòng theo Tây bá hầu!
Dialogue: 0,1:00:55.90,1:00:58.58,Default,,0,0,0,,Bọn ta bằng lòng theo Tây bá hầu!
Dialogue: 0,1:01:06.86,1:01:08.22,Default,,0,0,0,,Ta không phải con rối,
Dialogue: 0,1:01:08.90,1:01:10.98,Default,,0,0,0,,ta là Tướng Tinh trên trời của tộc Vũ Nhân.
Dialogue: 0,1:01:11.81,1:01:13.38,Default,,0,0,0,,Ta phải giết ngươi,
Dialogue: 0,1:01:14.82,1:01:16.18,Default,,0,0,0,,Lôi Chấn Tử.
Dialogue: 0,1:01:32.10,1:01:32.78,Default,,0,0,0,,Thái sư,
Dialogue: 0,1:01:32.93,1:01:34.42,Default,,0,0,0,,phía Bạch hổ đã khởi động rồi.
Dialogue: 0,1:01:35.38,1:01:37.66,Default,,0,0,0,,Trận Tru Tiên sắp khởi động,
Dialogue: 0,1:01:38.30,1:01:39.86,Default,,0,0,0,,chỉ có Ân Thương ta
Dialogue: 0,1:01:40.50,1:01:41.90,Default,,0,0,0,,trường thọ!
Dialogue: 0,1:01:43.86,1:01:44.58,Default,,0,0,0,,Đi mau!
Dialogue: 0,1:01:58.42,1:01:59.10,Default,,0,0,0,,Tân Hoàn!
Dialogue: 0,1:01:59.45,1:02:00.33,Default,,0,0,0,,Đệ đang làm gì vậy?
Dialogue: 0,1:02:00.42,1:02:01.62,Default,,0,0,0,,Mau đi với ta đi!
Dialogue: 0,1:02:16.22,1:02:17.98,Default,,0,0,0,,Tân Hoàn đã đánh mất tâm trí\Ntrong trận Tru Tiên rồi,
Dialogue: 0,1:02:18.90,1:02:20.10,Default,,0,0,0,,sẽ không nghe chúng ta khuyên đâu.
Dialogue: 0,1:02:21.38,1:02:23.18,Default,,0,0,0,,Chúng ta phải nghĩ cách dụ đệ ấy\Nra khỏi lầu Trích Tinh.
Dialogue: 0,1:02:24.97,1:02:25.53,Default,,0,0,0,,Đi!
Dialogue: 0,1:02:55.50,1:02:56.21,Default,,0,0,0,,Tân Hoàn!
Dialogue: 0,1:02:57.18,1:02:57.86,Default,,0,0,0,,Tân Hoàn!
Dialogue: 0,1:02:58.02,1:02:59.38,Default,,0,0,0,,Tân Hoàn, đệ tỉnh táo lại đi!
Dialogue: 0,1:03:16.54,1:03:16.98,Default,,0,0,0,,Hạnh Nhi!
Dialogue: 0,1:03:37.58,1:03:38.62,Default,,0,0,0,,Tân Hoàn!
Dialogue: 0,1:03:40.45,1:03:41.30,Default,,0,0,0,,Hạnh Nhi!
Dialogue: 0,1:03:55.70,1:03:56.61,Default,,0,0,0,,Hạnh Nhi!
Dialogue: 0,1:04:05.26,1:04:05.90,Default,,0,0,0,,Hạnh Nhi!
Dialogue: 0,1:04:05.90,1:04:06.90,Default,,0,0,0,,Cô không sao chứ, Hạnh Nhi?
Dialogue: 0,1:04:13.66,1:04:14.90,Default,,0,0,0,,Tân Hoàn, đệ điên rồi!
Dialogue: 0,1:04:15.42,1:04:16.78,Default,,0,0,0,,Cả Hạnh Nhi mà đệ cũng ra tay?
Dialogue: 0,1:04:18.70,1:04:19.66,Default,,0,0,0,,Tân Hoàn...
Dialogue: 0,1:04:20.10,1:04:21.62,Default,,0,0,0,,Tân Hoàn không cố ý đâu.
Dialogue: 0,1:04:21.69,1:04:22.82,Default,,0,0,0,,Cô đừng nói nữa Hạnh Nhi,
Dialogue: 0,1:04:23.26,1:04:24.66,Default,,0,0,0,,cô nhìn bộ dạng của đệ ấy bây giờ đi,
Dialogue: 0,1:04:25.06,1:04:26.38,Default,,0,0,0,,đệ ấy đã tẩu hoả nhập ma rồi!
Dialogue: 0,1:04:27.14,1:04:28.22,Default,,0,0,0,,Ta từng nói,
Dialogue: 0,1:04:29.18,1:04:30.17,Default,,0,0,0,,gặp nhau lần nữa
Dialogue: 0,1:04:30.94,1:04:33.22,Default,,0,0,0,,là lúc ta lấy mạng ngươi.
Dialogue: 0,1:04:35.10,1:04:35.98,Default,,0,0,0,,Được.
Dialogue: 0,1:04:37.06,1:04:39.14,Default,,0,0,0,,Hôm nay ta sẽ đánh cho đệ tỉnh lại!
Dialogue: 0,1:04:40.50,1:04:41.70,Default,,0,0,0,,Lôi Chấn Tử!
Dialogue: 0,1:04:42.46,1:04:43.46,Default,,0,0,0,,Buồn cười!
Dialogue: 0,1:05:13.85,1:05:14.78,Default,,0,0,0,,Lôi Chấn Tử!
Dialogue: 0,1:05:28.30,1:05:29.46,Default,,0,0,0,,Tân Hoàn, đệ đừng ngốc vậy nữa,
Dialogue: 0,1:05:29.69,1:05:30.34,Default,,0,0,0,,về với ta,
Dialogue: 0,1:05:30.54,1:05:32.02,Default,,0,0,0,,nói chuyện rõ ràng với nghĩa phụ.
Dialogue: 0,1:05:32.94,1:05:34.65,Default,,0,0,0,,Ta không có nghĩa phụ!
Dialogue: 0,1:06:35.98,1:06:36.82,Default,,0,0,0,,Lôi Chấn Tử!
Dialogue: 0,1:06:50.38,1:06:51.21,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,1:06:51.50,1:06:53.14,Default,,0,0,0,,ngươi có biết ngươi đang làm gì không?
Dialogue: 0,1:07:05.85,1:07:07.46,Default,,0,0,0,,Lôi Chấn Tử!
Dialogue: 0,1:07:08.54,1:07:09.69,Default,,0,0,0,,Đi mau!
Dialogue: 0,1:07:10.10,1:07:11.42,Default,,0,0,0,,Lôi Chấn Tử!
Dialogue: 0,1:08:20.10,1:08:22.26,Default,,0,0,0,,Rốt cuộc Cơ Xương ta đã làm gì
Dialogue: 0,1:08:23.45,1:08:25.58,Default,,0,0,0,,mà trong một tháng ngắn ngủi
Dialogue: 0,1:08:26.18,1:08:28.10,Default,,0,0,0,,lại mất đi hai đứa con.
Dialogue: 0,1:08:29.82,1:08:30.74,Default,,0,0,0,,Trời cao,
Dialogue: 0,1:08:30.98,1:08:32.49,Default,,0,0,0,,chúng thần ơi,
Dialogue: 0,1:08:33.49,1:08:36.41,Default,,0,0,0,,rốt cuộc các người muốn Cơ Xương làm gì?
Dialogue: 0,1:08:38.98,1:08:40.98,Default,,0,0,0,,Chấn Nhi!
Dialogue: 0,1:08:47.90,1:08:49.33,Default,,0,0,0,,Mọi người, mọi người, mọi người,
Dialogue: 0,1:08:50.33,1:08:53.02,Default,,0,0,0,,đây là phất trần ta mới làm xong nè,
Dialogue: 0,1:08:53.58,1:08:55.94,Default,,0,0,0,,ông xem có hợp với bộ đồ của ta không?
Dialogue: 0,1:08:55.94,1:08:56.66,Default,,0,0,0,,Tiên trưởng!
Dialogue: 0,1:08:58.22,1:08:59.22,Default,,0,0,0,,Tiên trưởng!
Dialogue: 0,1:09:00.29,1:09:01.86,Default,,0,0,0,,Hạnh Nhi đã về rồi.
Dialogue: 0,1:09:02.78,1:09:03.78,Default,,0,0,0,,Cô ấy nói ta biết
Dialogue: 0,1:09:04.77,1:09:05.86,Default,,0,0,0,,Lôi Chấn Tử
Dialogue: 0,1:09:06.14,1:09:08.40,Default,,0,0,0,,đã bị Tân Hoàn giết rồi.
Dialogue: 0,1:09:08.70,1:09:09.73,Default,,0,0,0,,Chết rồi?
Dialogue: 0,1:09:11.29,1:09:12.69,Default,,0,0,0,,Vậy ta cũng hết cách rồi.
Dialogue: 0,1:09:12.90,1:09:13.74,Default,,0,0,0,,Tiên trưởng!
Dialogue: 0,1:09:18.18,1:09:19.26,Default,,0,0,0,,Chấn Nhi.
Dialogue: 0,1:09:19.78,1:09:22.10,Default,,0,0,0,,Chấn Nhi của ta!
Dialogue: 0,1:09:25.98,1:09:26.98,Default,,0,0,0,,Tên Lôi Chấn Tử này
Dialogue: 0,1:09:26.98,1:09:28.45,Default,,0,0,0,,chắc sẽ không chết đâu.
Dialogue: 0,1:09:28.97,1:09:30.58,Default,,0,0,0,,- Chẳng phải hắn bị Tân Hoàn giết rồi sao?\N- Đúng đó.
Dialogue: 0,1:09:30.58,1:09:32.05,Default,,0,0,0,,Sao có thể không chết được?
Dialogue: 0,1:09:32.05,1:09:33.18,Default,,0,0,0,,Đúng thật.
Dialogue: 0,1:09:33.18,1:09:35.02,Default,,0,0,0,,Ông ấy đang nói lung tung gì vậy?
Dialogue: 0,1:09:35.22,1:09:35.98,Default,,0,0,0,,Tiên trưởng,
Dialogue: 0,1:09:36.29,1:09:37.41,Default,,0,0,0,,ông nói vậy là sao?
Dialogue: 0,1:09:39.90,1:09:41.90,Default,,0,0,0,,Quả thánh Kim Long của ta
Dialogue: 0,1:09:42.33,1:09:44.18,Default,,0,0,0,,không phải vật phàm tục.
Dialogue: 0,1:09:44.98,1:09:47.74,Default,,0,0,0,,Lôi Chấn Tử đã ăn quả thánh Kim Long,
Dialogue: 0,1:09:47.94,1:09:49.90,Default,,0,0,0,,thì sẽ được kim thân hộ thể,
Dialogue: 0,1:09:49.90,1:09:50.94,Default,,0,0,0,,hắn không chết được đâu.
Dialogue: 0,1:09:53.06,1:09:55.18,Default,,0,0,0,,Sau này khi mở trận Phong Thần,
Dialogue: 0,1:09:55.34,1:09:58.38,Default,,0,0,0,,Lôi Chấn Tử\Nmượn sức mạnh quả thánh Kim Long của ta
Dialogue: 0,1:09:58.38,1:09:59.58,Default,,0,0,0,,nuốt chửng hắn vào,
Dialogue: 0,1:09:59.58,1:10:00.65,Default,,0,0,0,,sau đó hắn sẽ...
Dialogue: 0,1:10:02.66,1:10:03.34,Default,,0,0,0,,Tiên trưởng,
Dialogue: 0,1:10:03.70,1:10:04.62,Default,,0,0,0,,nếu bị nuốt chửng
Dialogue: 0,1:10:04.98,1:10:05.98,Default,,0,0,0,,thì sẽ như thế nào?
Dialogue: 0,1:10:06.54,1:10:06.98,Default,,0,0,0,,Chuyện này...
Dialogue: 0,1:10:07.22,1:10:07.54,Default,,0,0,0,,Chuyện này...
Dialogue: 0,1:10:07.58,1:10:09.57,Default,,0,0,0,,Thiên cơ không thể tiết lộ.
Dialogue: 0,1:10:10.06,1:10:12.26,Default,,0,0,0,,Còn phải xem bản thân Tướng Tinh\Nlựa chọn thế nào.
Dialogue: 0,1:10:13.46,1:10:15.78,Default,,0,0,0,,Đôi cánh Hắc Ám của Tân Hoàn đã mở ra rồi,
Dialogue: 0,1:10:15.94,1:10:16.70,Default,,0,0,0,,vậy chẳng phải chứng tỏ
Dialogue: 0,1:10:16.86,1:10:18.81,Default,,0,0,0,,trận Tru Tiên đã khởi động rồi sao?
Dialogue: 0,1:10:19.14,1:10:19.46,Default,,0,0,0,,Vậy...
Dialogue: 0,1:10:20.22,1:10:20.57,Default,,0,0,0,,Vậy...
Dialogue: 0,1:10:21.34,1:10:22.50,Default,,0,0,0,,Vậy Lôi Chấn Tử
Dialogue: 0,1:10:22.89,1:10:24.70,Default,,0,0,0,,chắc là bị bắt vào lầu Trích Tinh rồi.
Dialogue: 0,1:10:25.70,1:10:26.34,Default,,0,0,0,,Chuyện không thể chậm trễ.
Dialogue: 0,1:10:26.34,1:10:26.90,Default,,0,0,0,,Hạnh Nhi,
Dialogue: 0,1:10:26.90,1:10:28.93,Default,,0,0,0,,ngươi mau theo ta đi tìm tiểu tử thối đó.
Dialogue: 0,1:10:30.25,1:10:30.78,Default,,0,0,0,,Vâng.
Dialogue: 0,1:10:33.14,1:10:34.62,Default,,0,0,0,,Nhanh nhanh nhanh.
Dialogue: 0,1:11:21.85,1:11:24.74,Default,,0,0,0,,Quả nhiên lão phu không nhìn lầm ngươi.
Dialogue: 0,1:11:26.34,1:11:27.42,Default,,0,0,0,,Thái sư,
Dialogue: 0,1:11:28.54,1:11:30.26,Default,,0,0,0,,mong ước của ông hoàn thành rồi,
Dialogue: 0,1:11:32.97,1:11:33.85,Default,,0,0,0,,còn ta thì sao?
Dialogue: 0,1:11:35.82,1:11:37.22,Default,,0,0,0,,Mong ước của ngươi
Dialogue: 0,1:11:38.50,1:11:39.74,Default,,0,0,0,,ở ngay đây.
Dialogue: 0,1:11:42.02,1:11:43.46,Default,,0,0,0,,Hoàn thành mong ước,
Dialogue: 0,1:11:44.22,1:11:46.66,Default,,0,0,0,,cũng phải hỏi tiểu gia đây có đồng ý không.
Dialogue: 0,1:11:48.54,1:11:49.38,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,1:11:50.89,1:11:52.54,Default,,0,0,0,,đệ đành lòng giết ta thật sao?
Dialogue: 0,1:11:53.70,1:11:54.70,Default,,0,0,0,,Lôi Chấn Tử,
Dialogue: 0,1:11:55.18,1:11:56.29,Default,,0,0,0,,đừng tưởng
Dialogue: 0,1:11:56.46,1:11:58.46,Default,,0,0,0,,ngươi có quả thánh Kim Long hộ thân
Dialogue: 0,1:11:59.02,1:12:02.33,Default,,0,0,0,,thì Tân Hoàn không thể hấp thụ\Nsức mạnh Tướng Tinh.
Dialogue: 0,1:12:03.26,1:12:05.46,Default,,0,0,0,,Ta sẽ cho ngươi tận mắt nhìn thấy
Dialogue: 0,1:12:05.58,1:12:07.70,Default,,0,0,0,,trận Phong Thần sẽ bị huỷ hoại thế nào,
Dialogue: 0,1:12:07.94,1:12:08.74,Default,,0,0,0,,Tân Hoàn
Dialogue: 0,1:12:09.10,1:12:11.02,Default,,0,0,0,,sẽ trở thành Tướng Tinh thế nào.
Dialogue: 0,1:12:13.10,1:12:13.62,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,1:12:14.10,1:12:14.86,Default,,0,0,0,,ngươi đi theo ta.
Dialogue: 0,1:12:17.34,1:12:17.85,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,1:12:18.46,1:12:19.22,Default,,0,0,0,,đệ đừng nghe hắn!
Dialogue: 0,1:12:19.66,1:12:20.38,Default,,0,0,0,,Đừng đi!
Dialogue: 0,1:12:42.58,1:12:43.34,Default,,0,0,0,,Bây giờ
Dialogue: 0,1:12:43.98,1:12:45.90,Default,,0,0,0,,còn thiếu một món bảo vật cuối cùng.
Dialogue: 0,1:12:47.82,1:12:48.77,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,1:12:48.94,1:12:51.18,Default,,0,0,0,,ngươi sắp thành công rồi.
Dialogue: 0,1:12:56.57,1:12:57.50,Default,,0,0,0,,Chỉ thiếu
Dialogue: 0,1:12:58.01,1:13:01.02,Default,,0,0,0,,máu của đôi cánh Thiên Lôi đã nhập ma.
Dialogue: 0,1:13:08.66,1:13:09.58,Default,,0,0,0,,Lôi Chấn Tử!
Dialogue: 0,1:13:29.06,1:13:30.57,Default,,0,0,0,,Là ông!
Dialogue: 0,1:13:31.22,1:13:32.06,Default,,0,0,0,,Đúng vậy.
Dialogue: 0,1:13:32.33,1:13:33.42,Default,,0,0,0,,Mọi chuyện trước đây
Dialogue: 0,1:13:33.82,1:13:36.10,Default,,0,0,0,,chỉ là cảnh tượng giả tạo mà ta đã sắp đặt.
Dialogue: 0,1:13:36.94,1:13:38.05,Default,,0,0,0,,Ngươi vẫn chưa biết sao?
Dialogue: 0,1:13:39.02,1:13:42.22,Default,,0,0,0,,Ngươi đã hấp thụ tất cả linh lực\Ncủa xương cốt tộc Vũ Nhân
Dialogue: 0,1:13:42.38,1:13:44.58,Default,,0,0,0,,mới mở ra được đôi cánh Phong Lôi.
Dialogue: 0,1:13:45.06,1:13:45.97,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,1:13:46.46,1:13:47.26,Default,,0,0,0,,ngươi
Dialogue: 0,1:13:47.62,1:13:50.26,Default,,0,0,0,,mới chính là tội nhân thiên cổ.
Dialogue: 0,1:13:59.86,1:14:01.66,Default,,0,0,0,,Ngươi phải chôn cùng bọn chúng!
Dialogue: 0,1:14:40.33,1:14:41.02,Default,,0,0,0,,Tân Hoàn!
Dialogue: 0,1:14:43.38,1:14:43.98,Default,,0,0,0,,Văn lão tặc,
Dialogue: 0,1:14:44.26,1:14:45.30,Default,,0,0,0,,ông đã làm gì Tân Hoàn?
Dialogue: 0,1:14:47.34,1:14:48.25,Default,,0,0,0,,Hắn à?
Dialogue: 0,1:14:48.66,1:14:49.62,Default,,0,0,0,,Luyện trận.
Dialogue: 0,1:14:50.26,1:14:50.98,Default,,0,0,0,,Tế trận.
Dialogue: 0,1:14:51.82,1:14:53.09,Default,,0,0,0,,Phá huỷ Phong Thần,
Dialogue: 0,1:14:53.22,1:14:54.50,Default,,0,0,0,,trở thành Tướng Tinh.
Dialogue: 0,1:14:55.38,1:14:56.05,Default,,0,0,0,,Ta
Dialogue: 0,1:14:56.38,1:14:58.25,Default,,0,0,0,,cho hắn được như ý muốn.
Dialogue: 0,1:14:59.62,1:15:00.89,Default,,0,0,0,,Xin lỗi.
Dialogue: 0,1:15:05.58,1:15:07.10,Default,,0,0,0,,Ta sẽ giết ông!
Dialogue: 0,1:15:19.22,1:15:19.78,Default,,0,0,0,,Không hay rồi.
Dialogue: 0,1:15:19.78,1:15:20.74,Default,,0,0,0,,Mau đưa Tân Hoàn đi!
Dialogue: 0,1:15:22.54,1:15:23.86,Default,,0,0,0,,Trận Tru Tiên
Dialogue: 0,1:15:24.26,1:15:25.53,Default,,0,0,0,,đã khởi động rồi.
Dialogue: 0,1:15:26.06,1:15:26.66,Default,,0,0,0,,Hôm nay
Dialogue: 0,1:15:27.18,1:15:27.78,Default,,0,0,0,,sẽ là
Dialogue: 0,1:15:28.17,1:15:30.14,Default,,0,0,0,,thời khắc huỷ thiên diệt địa.
Dialogue: 0,1:15:31.22,1:15:31.98,Default,,0,0,0,,Ta
Dialogue: 0,1:15:32.10,1:15:33.82,Default,,0,0,0,,tức là Đạo trời.
Dialogue: 0,1:16:04.81,1:16:06.06,Default,,0,0,0,,Trận Phong Thần à,
Dialogue: 0,1:16:06.13,1:16:07.22,Default,,0,0,0,,sớm không mở muộn không mở,
Dialogue: 0,1:16:07.22,1:16:08.50,Default,,0,0,0,,sao lại cứ mở vào lúc này chứ?
Dialogue: 0,1:16:14.54,1:16:15.37,Default,,0,0,0,,Lôi Chấn Tử,
Dialogue: 0,1:16:15.54,1:16:17.50,Default,,0,0,0,,bây giờ chỉ có ngươi có thể\Nkhởi động trận Phong Thần thôi.
Dialogue: 0,1:16:22.82,1:16:23.62,Default,,0,0,0,,Lôi Chấn tử,
Dialogue: 0,1:16:23.94,1:16:24.90,Default,,0,0,0,,ta đưa đôi cánh cho đệ.
Dialogue: 0,1:16:25.74,1:16:26.42,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,1:16:26.74,1:16:27.94,Default,,0,0,0,,bây giờ ngươi đang bị thương,
Dialogue: 0,1:16:28.06,1:16:29.62,Default,,0,0,0,,không có đôi cánh thì ngươi sẽ chết đó.
Dialogue: 0,1:16:31.50,1:16:32.06,Default,,0,0,0,,Không,
Dialogue: 0,1:16:32.62,1:16:33.94,Default,,0,0,0,,ta muốn đưa Tân Hoàn đi cùng.
Dialogue: 0,1:16:34.46,1:16:35.29,Default,,0,0,0,,Không thể chậm trễ nữa,
Dialogue: 0,1:16:35.29,1:16:36.10,Default,,0,0,0,,các ngươi đi mau đi!
Dialogue: 0,1:16:37.42,1:16:37.98,Default,,0,0,0,,Lôi Chấn Tử!
Dialogue: 0,1:16:37.98,1:16:38.42,Default,,0,0,0,,Đi!
Dialogue: 0,1:16:47.54,1:16:48.18,Default,,0,0,0,,Tân Hoàn!
Dialogue: 0,1:16:49.54,1:16:50.30,Default,,0,0,0,,Tân Hoàn!
Dialogue: 0,1:16:55.97,1:16:56.66,Default,,0,0,0,,Văn lão tặc!
Dialogue: 0,1:17:05.98,1:17:07.02,Default,,0,0,0,,Ta cầm chân Văn lão tặc,
Dialogue: 0,1:17:07.14,1:17:08.34,Default,,0,0,0,,nhân lúc đó lên trời đi.
Dialogue: 0,1:17:16.33,1:17:17.70,Default,,0,0,0,,Hạnh Nhi, chăm sóc cho Tân Hoàn.
Dialogue: 0,1:18:25.34,1:18:25.97,Default,,0,0,0,,Tân Hoàn!
Dialogue: 0,1:18:35.82,1:18:36.62,Default,,0,0,0,,Lôi Chấn Tử!
Dialogue: 0,1:18:42.98,1:18:43.78,Default,,0,0,0,,Hạnh Nhi!
Dialogue: 0,1:19:30.65,1:19:31.30,Default,,0,0,0,,Hạnh Nhi!
Dialogue: 0,1:19:54.90,1:19:55.50,Default,,0,0,0,,Hạnh Nhi!
Dialogue: 0,1:19:57.17,1:19:57.90,Default,,0,0,0,,Bướm nhỏ!
Dialogue: 0,1:19:59.66,1:20:00.82,Default,,0,0,0,,Sao cô ngốc quá vậy?
Dialogue: 0,1:20:01.74,1:20:03.26,Default,,0,0,0,,Ai bảo cô đỡ giúp ta chứ?
Dialogue: 0,1:20:07.10,1:20:08.17,Default,,0,0,0,,Lôi Chấn Tử,
Dialogue: 0,1:20:09.90,1:20:11.82,Default,,0,0,0,,ta ghét ngươi lúc nào cũng trêu chọc ta.
Dialogue: 0,1:20:12.70,1:20:13.74,Default,,0,0,0,,Có lẽ ngươi...
Dialogue: 0,1:20:14.10,1:20:15.33,Default,,0,0,0,,từ đầu,
Dialogue: 0,1:20:16.46,1:20:18.46,Default,,0,0,0,,ta bị lửa của Tướng Tinh làm bị thương,
Dialogue: 0,1:20:19.17,1:20:20.54,Default,,0,0,0,,trời đã định
Dialogue: 0,1:20:20.54,1:20:22.34,Default,,0,0,0,,duyên phận của ta và ngươi.
Dialogue: 0,1:20:23.66,1:20:25.70,Default,,0,0,0,,Ta sẽ luôn bảo vệ ngươi.
Dialogue: 0,1:20:28.66,1:20:30.17,Default,,0,0,0,,Ta không cho phép cô chết!
Dialogue: 0,1:20:31.05,1:20:32.86,Default,,0,0,0,,Ta vẫn chưa cãi nhau với cô đủ mà!
Dialogue: 0,1:20:50.22,1:20:50.90,Default,,0,0,0,,Không hay rồi.
Dialogue: 0,1:20:51.34,1:20:52.26,Default,,0,0,0,,Không kịp nữa rồi.
Dialogue: 0,1:20:52.58,1:20:54.10,Default,,0,0,0,,Mau lên, nhân lúc này!
Dialogue: 0,1:20:55.74,1:20:56.94,Default,,0,0,0,,Mau đi đi,
Dialogue: 0,1:20:58.94,1:21:00.65,Default,,0,0,0,,hoàn thành sứ mệnh của các ngươi.
Dialogue: 0,1:21:19.45,1:21:20.86,Default,,0,0,0,,Vô ích thôi.
Dialogue: 0,1:21:21.54,1:21:23.06,Default,,0,0,0,,Không ai có thể
Dialogue: 0,1:21:23.94,1:21:27.02,Default,,0,0,0,,cản trận Tru Tiên của ta.
Dialogue: 0,1:21:52.82,1:21:54.06,Default,,0,0,0,,Tân Hoàn, đệ làm gì vậy?
Dialogue: 0,1:21:54.66,1:21:56.02,Default,,0,0,0,,Hạnh Nhi đã chết rồi,
Dialogue: 0,1:21:56.18,1:21:57.66,Default,,0,0,0,,đệ không thể có chuyện gì nữa!
Dialogue: 0,1:21:59.22,1:22:01.78,Default,,0,0,0,,Ta không muốn gánh vác\Nsố mệnh của người khác mà sống nữa.
Dialogue: 0,1:22:03.06,1:22:04.10,Default,,0,0,0,,Mệt lắm rồi.
Dialogue: 0,1:22:05.38,1:22:06.70,Default,,0,0,0,,Nhưng may mà có đệ,
Dialogue: 0,1:22:10.26,1:22:11.90,Default,,0,0,0,,khởi động trận Phong Thần
Dialogue: 0,1:22:12.14,1:22:13.62,Default,,0,0,0,,luôn là ước mơ của ta,
Dialogue: 0,1:22:16.62,1:22:17.94,Default,,0,0,0,,nhưng bây giờ tỉnh mộng rồi,
Dialogue: 0,1:22:19.18,1:22:20.82,Default,,0,0,0,,chỉ có thể để đệ khởi động nó thôi.
Dialogue: 0,1:22:21.46,1:22:23.02,Default,,0,0,0,,Đệ cũng là người được chọn tốt nhất.
Dialogue: 0,1:22:23.90,1:22:24.98,Default,,0,0,0,,Rắm chó!
Dialogue: 0,1:22:25.62,1:22:27.22,Default,,0,0,0,,Nếu đệ muốn ta gọi đệ là ca ca
Dialogue: 0,1:22:27.38,1:22:29.02,Default,,0,0,0,,thì phải ngoan ngoãn nghe lời ta!
Dialogue: 0,1:22:35.49,1:22:36.54,Default,,0,0,0,,Lôi Chấn Tử,
Dialogue: 0,1:22:37.10,1:22:38.26,Default,,0,0,0,,ta bảo vệ đệ,
Dialogue: 0,1:22:38.98,1:22:39.86,Default,,0,0,0,,hiểu không?
Dialogue: 0,1:23:06.26,1:23:07.34,Default,,0,0,0,,Ca!
Dialogue: 0,1:23:09.06,1:23:09.81,Default,,0,0,0,,Ca!
Dialogue: 0,1:23:26.70,1:23:28.73,Default,,0,0,0,,Không ai có thể huỷ hoại
Dialogue: 0,1:23:29.38,1:23:31.90,Default,,0,0,0,,trận Tru Tiên!
Dialogue: 0,1:23:50.26,1:23:51.38,Default,,0,0,0,,[Quả thánh Kim Long]
Dialogue: 0,1:23:51.46,1:23:53.34,Default,,0,0,0,,[chỉ có thể ký kết khế ước]
Dialogue: 0,1:23:53.74,1:23:55.25,Default,,0,0,0,,[với người tình nguyện.]
Dialogue: 0,1:23:56.06,1:23:57.18,Default,,0,0,0,,[Làm anh hùng]
Dialogue: 0,1:23:57.42,1:23:59.78,Default,,0,0,0,,[thì phải chấp nhận cô độc hết kiếp này.]
Dialogue: 0,1:24:04.70,1:24:06.50,Default,,0,0,0,,[Thần cũng có số mệnh của mình,]
Dialogue: 0,1:24:07.10,1:24:08.06,Default,,0,0,0,,[ngươi phải chấp nhận]
Dialogue: 0,1:24:08.42,1:24:09.70,Default,,0,0,0,,[cô độc mãi mãi.]
Dialogue: 0,1:24:10.46,1:24:12.29,Default,,0,0,0,,Tuỳ Tâm Sở Dục!
Dialogue: 0,1:24:10.46,1:24:12.29,Default,,0,0,0,,[Theo Luận ngữ: tùy muốn làm gì thì làm]
Dialogue: 0,1:24:34.58,1:24:36.61,Default,,0,0,0,,[Ta không muốn gánh vác số mệnh của đệ mà sống]
Dialogue: 0,1:24:37.86,1:24:39.25,Default,,0,0,0,,[nhưng ta chỉ có thể chấp nhận số mệnh.]
Dialogue: 0,1:24:40.70,1:24:42.58,Default,,0,0,0,,[Ta sinh ta đã không phải Tướng Tinh.]
Dialogue: 0,1:24:50.69,1:24:52.26,Default,,0,0,0,,Văn lão tặc à Văn lão tặc,
Dialogue: 0,1:24:52.62,1:24:54.54,Default,,0,0,0,,ông tính toán mọi đường thì đã sao?
Dialogue: 0,1:24:55.34,1:24:57.22,Default,,0,0,0,,Tên tiểu tử thối mà ông xem thường
Dialogue: 0,1:24:57.38,1:25:00.30,Default,,0,0,0,,mới là Tướng Tinh mang mệnh trời thật sự.
Dialogue: 0,1:25:02.90,1:25:03.89,Default,,0,0,0,,Đại vương,
Dialogue: 0,1:25:05.54,1:25:06.38,Default,,0,0,0,,Văn Trọng ta
Dialogue: 0,1:25:07.38,1:25:08.78,Default,,0,0,0,,vì Đại Thương này
Dialogue: 0,1:25:09.86,1:25:11.14,Default,,0,0,0,,mà cố hết sức rồi.
Dialogue: 0,1:25:57.37,1:25:58.18,Default,,0,0,0,,Hạnh Nhi.
Dialogue: 0,1:26:02.06,1:26:03.02,Default,,0,0,0,,Lôi Chấn Tử,
Dialogue: 0,1:26:03.70,1:26:05.78,Default,,0,0,0,,ta chỉ là một đoạn ký ức của Hạnh Nhi.
Dialogue: 0,1:26:07.34,1:26:09.78,Default,,0,0,0,,Rất vui vì có thể\Nđi cùng ngươi đến cuối đường.
Dialogue: 0,1:26:11.98,1:26:12.54,Default,,0,0,0,,Không được,
Dialogue: 0,1:26:13.14,1:26:14.38,Default,,0,0,0,,Hạnh Nhi, cô không thể đi nữa,
Dialogue: 0,1:26:15.14,1:26:16.50,Default,,0,0,0,,ta còn chưa cãi nhau đủ với cô mà.
Dialogue: 0,1:26:20.46,1:26:22.26,Default,,0,0,0,,Vì ngươi mà từ bỏ thành thần,
Dialogue: 0,1:26:22.82,1:26:24.02,Default,,0,0,0,,từ bỏ tất cả,
Dialogue: 0,1:26:24.53,1:26:25.41,Default,,0,0,0,,ta rất mãn nguyện.
Dialogue: 0,1:26:27.29,1:26:29.18,Default,,0,0,0,,Nhưng nếu muốn khởi động trận Phong Thần,
Dialogue: 0,1:26:30.10,1:26:31.50,Default,,0,0,0,,còn một ải cuối cùng nữa.
Dialogue: 0,1:26:32.26,1:26:32.86,Default,,0,0,0,,Hạnh Nhi.
Dialogue: 0,1:26:33.26,1:26:34.10,Default,,0,0,0,,Lôi Chấn tử,
Dialogue: 0,1:26:34.38,1:26:35.62,Default,,0,0,0,,thời gian của ta không nhiều nữa,
Dialogue: 0,1:26:35.78,1:26:36.94,Default,,0,0,0,,ngươi nghe cho kỹ đây.
Dialogue: 0,1:26:38.02,1:26:39.30,Default,,0,0,0,,Nếu muốn khởi động trận,
Dialogue: 0,1:26:39.82,1:26:42.22,Default,,0,0,0,,thì sẽ bị quả thánh Kim Long\Nnuốt chửng hoàn toàn.
Dialogue: 0,1:26:43.18,1:26:44.45,Default,,0,0,0,,Mà sức mạnh của quả thánh
Dialogue: 0,1:26:44.98,1:26:46.94,Default,,0,0,0,,sẽ khiến ngươi dần biến thành tên xấu xí,
Dialogue: 0,1:26:47.38,1:26:48.62,Default,,0,0,0,,trở nên đáng sợ,
Dialogue: 0,1:26:49.22,1:26:51.98,Default,,0,0,0,,trở nên khiến người đời không thể nhìn rõ\Nnội tâm thật sự của ngươi.
Dialogue: 0,1:26:52.98,1:26:54.78,Default,,0,0,0,,Ngươi sẽ không còn\Nđược người đời truyền tụng,
Dialogue: 0,1:26:55.10,1:26:56.77,Default,,0,0,0,,sẽ mãi mãi cô độc.
Dialogue: 0,1:26:57.21,1:26:58.50,Default,,0,0,0,,Nếu khởi động trận,
Dialogue: 0,1:26:58.82,1:27:00.10,Default,,0,0,0,,kiếp này xem như bỏ.
Dialogue: 0,1:27:00.73,1:27:01.38,Default,,0,0,0,,Sau này
Dialogue: 0,1:27:01.98,1:27:03.78,Default,,0,0,0,,cũng không được phong thần\Ntrên Đài Phong Thần nữa.
Dialogue: 0,1:27:05.94,1:27:07.30,Default,,0,0,0,,Ngươi có bằng lòng không?
Dialogue: 0,1:27:13.49,1:27:14.62,Default,,0,0,0,,Thật ra
Dialogue: 0,1:27:15.54,1:27:17.06,Default,,0,0,0,,ta đã lựa chọn từ lâu rồi.
Dialogue: 0,1:28:11.14,1:28:12.62,Default,,0,0,0,,Chúc mừng các thần linh
Dialogue: 0,1:28:13.22,1:28:15.54,Default,,0,0,0,,đã xây dựng lại thái bình thịnh thế.
Dialogue: 0,1:29:07.30,1:29:08.89,Default,,0,0,0,,Tây Kỳ là cố hương,
Dialogue: 0,1:29:10.46,1:29:12.22,Default,,0,0,0,,cố hương là Tây Kỳ.
Dialogue: 0,1:29:13.98,1:29:15.70,Default,,0,0,0,,Đôi ta thuở thiếu niên khi ấy
Dialogue: 0,1:29:16.78,1:29:18.69,Default,,0,0,0,,không phụ thời gian tươi đẹp.
Dialogue: 0,1:29:20.86,1:29:21.74,Default,,0,0,0,,Tân Hoàn,
Dialogue: 0,1:29:22.77,1:29:23.70,Default,,0,0,0,,Hạnh Nhi,
Dialogue: 0,1:29:25.62,1:29:26.94,Default,,0,0,0,,ta đưa hai người về nhà.
Dialogue: 0,1:30:05.45,1:30:06.38,Default,,0,0,0,,Lão tiên sinh,
Dialogue: 0,1:30:06.82,1:30:08.30,Default,,0,0,0,,kỹ thuật thả câu của ông
Dialogue: 0,1:30:08.30,1:30:10.73,Default,,0,0,0,,ta chưa từng nghe nói bao giờ.
Dialogue: 0,1:30:13.50,1:30:15.42,Default,,0,0,0,,Người tình nguyện sẽ cắn câu.
Dialogue: 0,1:30:19.42,1:30:23.21,Default,,0,0,0,,[Lôi Chấn Tử: Phong Thần Duyên Khởi]
