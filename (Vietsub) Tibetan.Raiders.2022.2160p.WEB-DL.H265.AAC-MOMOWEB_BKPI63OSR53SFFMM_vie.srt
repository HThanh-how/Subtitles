Dialogue: 0,0:00:05.00,0:00:15.00,nen den,,0,0,0,,Phim do zinzuno remux và upload
Dialogue: 0,0:00:30.08,0:00:33.54,nen den,,0,0,0,,[Bộ phim được cải biên từ tiểu thuyết cùng tên "Tạng Địa Kỳ Binh" của tác giả Ba Oa]
Dialogue: 0,0:00:36.39,0:00:37.25,nen den,,0,0,0,,Sau đó thì sao ạ?
Dialogue: 0,0:00:38.17,0:00:38.77,nen den,,0,0,0,,Sau đó
Dialogue: 0,0:00:39.94,0:00:41.27,nen den,,0,0,0,,nhóm ma quỷ đã tới.
Dialogue: 0,0:00:45.53,0:00:47.36,nen den,,0,0,0,,Trên cao nguyên Tây Tạng,
Dialogue: 0,0:00:49.27,0:00:50.48,nen den,,0,0,0,,làn gió yêu quái hoành hành khắp nơi.
Dialogue: 0,0:00:51.17,0:00:53.81,nen den,,0,0,0,,Tứ Đại Ma Vương lạm sát người vô tội,
Dialogue: 0,0:00:54.37,0:00:55.74,nen den,,0,0,0,,làm hại nhân gian,
Dialogue: 0,0:00:56.18,0:00:57.72,nen den,,0,0,0,,mang tới
Dialogue: 0,0:00:58.41,0:01:02.97,nen den,,0,0,0,,chết chóc và sự khiếp sợ cho thảo nguyên.
Dialogue: 0,0:01:07.55,0:01:08.51,nen den,,0,0,0,,Cho đến khi
Dialogue: 0,0:01:10.33,0:01:11.61,nen den,,0,0,0,,ông ấy xuất hiện.
Dialogue: 0,0:01:14.88,0:01:16.78,nen den,,0,0,0,,Vua Gesar của xứ Ling
Dialogue: 0,0:01:17.02,0:01:19.39,nen den,,0,0,0,,phát động chiến đấu kịch liệt với Tứ Đại Ma Vương,
Dialogue: 0,0:01:21.19,0:01:22.42,nen den,,0,0,0,,khó phân thắng bại.
Dialogue: 0,0:01:23.57,0:01:24.71,nen den,,0,0,0,,Ở bước ngoặt cuối cùng,
Dialogue: 0,0:01:24.90,0:01:26.81,nen den,,0,0,0,,kiếm Thanh Thiên phát ra thần lực,
Dialogue: 0,0:01:27.69,0:01:31.10,nen den,,0,0,0,,giúp Vua Gesar chiến thắng ma quỷ.
Dialogue: 0,0:01:35.78,0:01:36.32,nen den,,0,0,0,,Bố ơi,
Dialogue: 0,0:01:36.64,0:01:39.80,nen den,,0,0,0,,đoạn kịch Tây Tạng này kể về Vua Gesar đúng không ạ?
Dialogue: 0,0:01:40.56,0:01:41.28,nen den,,0,0,0,,Đúng rồi.
Dialogue: 0,0:01:41.41,0:01:44.00,nen den,,0,0,0,,Kiếm Thanh Thiên lợi hại hơn cả gậy Như Ý ạ?
Dialogue: 0,0:01:45.07,0:01:46.91,nen den,,0,0,0,,Tề Thiên Đại Thánh là thần thoại,
Dialogue: 0,0:01:47.21,0:01:49.44,nen den,,0,0,0,,còn Vua Gesar thật sự tồn tại đó.
Dialogue: 0,0:01:51.80,0:01:53.08,nen den,,0,0,0,,Nhưng tại sao
Dialogue: 0,0:01:53.44,0:01:55.47,nen den,,0,0,0,,trong các bức tranh về Vua Gesar
Dialogue: 0,0:01:55.75,0:01:57.49,nen den,,0,0,0,,đều không có kiếm Thanh Thiên vậy ạ?
Dialogue: 0,0:01:58.97,0:02:00.13,nen den,,0,0,0,,Con cũng chú ý tới sao?
Dialogue: 0,0:02:01.63,0:02:02.60,nen den,,0,0,0,,Bố cho rằng
Dialogue: 0,0:02:03.12,0:02:06.03,nen den,,0,0,0,,trong chuyện này nhất định đang ẩn giấu chuyện gì đó.
Dialogue: 0,0:02:06.43,0:02:07.08,nen den,,0,0,0,,Bố ơi,
Dialogue: 0,0:02:07.51,0:02:10.00,nen den,,0,0,0,,bố nhất định phải đi tìm kiếm Thanh Thiên sao ạ?
Dialogue: 0,0:02:11.03,0:02:13.25,nen den,,0,0,0,,Đây chính là ước mơ khảo cổ của bố đó.
Dialogue: 0,0:02:14.09,0:02:16.60,nen den,,0,0,0,,Bố tìm kiếm Thanh Thiên xong sẽ về.
Dialogue: 0,0:02:17.47,0:02:18.85,nen den,,0,0,0,,Tây Tây phải nghe lời chú Đạt,
Dialogue: 0,0:02:19.28,0:02:20.56,nen den,,0,0,0,,không được chọc giận chú Đạt đâu đấy.
Dialogue: 0,0:02:22.46,0:02:23.05,nen den,,0,0,0,,Tây Tây.
Dialogue: 0,0:02:24.18,0:02:24.81,nen den,,0,0,0,,Tây Tây.
Dialogue: 0,0:02:25.44,0:02:26.44,nen den,,0,0,0,,Được không vậy?
Dialogue: 0,0:02:28.07,0:02:31.19,nen den,,0,0,0,,[Hai mươi năm sau, tại một sơn động nào đó ở lưu vực Trường Giang]
Dialogue: 0,0:02:36.38,0:02:37.45,nen den,,0,0,0,,Cháu cẩn thận một chút.
Dialogue: 0,0:02:42.22,0:02:44.71,nen den,,0,0,0,,Tây Tây à, đó là chủ hay khách vậy?
Dialogue: 0,0:02:46.01,0:02:46.75,nen den,,0,0,0,,Xương cốt còn khá mới.
Dialogue: 0,0:02:46.98,0:02:48.35,nen den,,0,0,0,,Vậy thì là Mô Kim Hiệu Uý rồi.
Dialogue: 0,0:02:55.53,0:02:57.04,nen den,,0,0,0,,Chú Đạt, đây là loại bọ gì vậy?
Dialogue: 0,0:03:10.21,0:03:10.71,nen den,,0,0,0,,Tây Tây,
Dialogue: 0,0:03:11.26,0:03:13.93,nen den,,0,0,0,,cháu có chắc là cái động này có liên quan tới kiếm Thanh Thiên không?
Dialogue: 0,0:03:15.61,0:03:18.01,nen den,,0,0,0,,Cháu cảm thấy chỗ này sẽ có huyền cơ gì đó.
Dialogue: 0,0:03:29.70,0:03:30.57,nen den,,0,0,0,,Quả nhiên là xứ Ling.
Dialogue: 0,0:03:31.57,0:03:32.79,nen den,,0,0,0,,Sứ giả của Vua Gesar.
Dialogue: 0,0:03:33.16,0:03:33.89,nen den,,0,0,0,,Vua Gesar
Dialogue: 0,0:03:33.89,0:03:34.70,nen den,,0,0,0,,từng phái sứ giả Trạch Nhân
Dialogue: 0,0:03:34.70,0:03:35.55,nen den,,0,0,0,,tới triều Tống thiết lập quan hệ ngoại giao.
Dialogue: 0,0:03:36.26,0:03:37.75,nen den,,0,0,0,,Cuối đời Trạch Nhân mất ở tại triều Tống.
Dialogue: 0,0:03:40.11,0:03:40.98,nen den,,0,0,0,,Thái Dương Hoả Diễm.
Dialogue: 0,0:03:41.37,0:03:42.89,nen den,,0,0,0,,Đây chắc là nơi tu hành của Trạch Nhân.
Dialogue: 0,0:03:43.16,0:03:44.00,nen den,,0,0,0,,Vậy xác thịt của ông ta đâu?
Dialogue: 0,0:03:45.84,0:03:46.72,nen den,,0,0,0,,Xác thịt.
Dialogue: 0,0:04:10.55,0:04:11.10,nen den,,0,0,0,,Tìm thấy rồi.
Dialogue: 0,0:04:11.89,0:04:13.97,nen den,,0,0,0,,Ở giữa hình này là hình dáng một thanh kiếm.
Dialogue: 0,0:04:14.36,0:04:15.56,nen den,,0,0,0,,Rất có thể là kiếm Thanh Thiên.
Dialogue: 0,0:04:16.43,0:04:17.87,nen den,,0,0,0,,Thiên Vương tàng cổ đao.
Dialogue: 0,0:04:18.50,0:04:20.11,nen den,,0,0,0,,Ngũ âm độ ngũ hành.
Dialogue: 0,0:04:44.02,0:04:45.68,nen den,,0,0,0,,Tây Tây, mau chạy đi.
Dialogue: 0,0:05:18.30,0:05:24.56,nen den,,0,0,0,,[Tạng Địa Kỳ Binh]
Dialogue: 0,0:05:46.75,0:05:47.85,nen den,,0,0,0,,Cởi ra.
Dialogue: 0,0:06:08.30,0:06:08.79,nen den,,0,0,0,,Được ròi.
Dialogue: 0,0:06:19.22,0:06:20.29,nen den,,0,0,0,,Xin chào, Tần.
Dialogue: 0,0:06:20.31,0:06:21.44,nen den,,0,0,0,,Xin chào Diana.
Dialogue: 0,0:06:21.61,0:06:22.85,nen den,,0,0,0,,Đã lâu không gặp.
Dialogue: 0,0:06:22.92,0:06:23.57,nen den,,0,0,0,,Đúng vậy.
Dialogue: 0,0:06:25.26,0:06:26.26,nen den,,0,0,0,,Chuyến đi vẫn thuận lợi chứ?
Dialogue: 0,0:06:26.30,0:06:27.14,nen den,,0,0,0,,Tuyệt vời.
Dialogue: 0,0:06:27.42,0:06:29.22,nen den,,0,0,0,,Tôi có quà tặng ông đây.
Dialogue: 0,0:06:29.24,0:06:29.70,nen den,,0,0,0,,Hay lắm.
Dialogue: 0,0:06:33.69,0:06:35.50,nen den,,0,0,0,,Đây là kiếm Việt Vương
Dialogue: 0,0:06:35.53,0:06:37.26,nen den,,0,0,0,,mà chúng tôi mất rất nhiều công sức mới lấy được đó.
Dialogue: 0,0:06:38.92,0:06:42.69,nen den,,0,0,0,,Tôi tìm kiếm thanh kiếm này lâu lắm rồi.
Dialogue: 0,0:06:42.80,0:06:43.76,nen den,,0,0,0,,Đúng vậy, tôi biết mà.
Dialogue: 0,0:06:43.79,0:06:44.22,nen den,,0,0,0,,Mời ngồi.
Dialogue: 0,0:06:48.43,0:06:48.57,nen den,,0,0,0,,Tuyệt quá.
Dialogue: 0,0:06:48.57,0:06:49.65,nen den,,0,0,0,,[Nhà khảo cổ học Lâm Tây phát hiện di tích nghi ngờ là của Vua Gesar]\NTuyệt quá.
Dialogue: 0,0:06:49.65,0:06:50.48,nen den,,0,0,0,,[Nhà khảo cổ học Lâm Tây phát hiện di tích nghi ngờ là của Vua Gesar]
Dialogue: 0,0:06:50.48,0:06:50.69,nen den,,0,0,0,,Nhìn chi tiết này xem.\N[Nhà khảo cổ học Lâm Tây phát hiện di tích nghi ngờ là của Vua Gesar]
Dialogue: 0,0:06:50.69,0:06:51.60,nen den,,0,0,0,,Nhìn chi tiết này xem.
Dialogue: 0,0:06:58.48,0:06:59.30,nen den,,0,0,0,,Cái này cho tôi sao?
Dialogue: 0,0:07:01.75,0:07:02.76,nen den,,0,0,0,,Hy vọng cô thích nó.
Dialogue: 0,0:07:02.93,0:07:03.62,nen den,,0,0,0,,Tôi cũng vậy
Dialogue: 0,0:07:06.99,0:07:09.20,nen den,,0,0,0,,Đúng, tôi rất thích nó.
Dialogue: 0,0:07:15.51,0:07:16.55,nen den,,0,0,0,,Đây là Louis sao?
Dialogue: 0,0:07:18.00,0:07:19.95,nen den,,0,0,0,,Hàng mà lần trước bị hải quan giữ lại
Dialogue: 0,0:07:20.33,0:07:21.32,nen den,,0,0,0,,chỉ có mấy người biết.
Dialogue: 0,0:07:22.42,0:07:25.43,nen den,,0,0,0,,Ông hiểu nhầm rồi, hãy nghe tôi giải thích.
Dialogue: 0,0:07:25.46,0:07:26.00,nen den,,0,0,0,,Tất nhiên.
Dialogue: 0,0:07:26.14,0:07:27.32,nen den,,0,0,0,,Đây thật sự chỉ là hiểu lầm thôi.
Dialogue: 0,0:07:27.71,0:07:28.70,nen den,,0,0,0,,Xin ông hãy tha thứ cho tôi.
Dialogue: 0,0:07:28.99,0:07:29.80,nen den,,0,0,0,,Tôi xin hứa.
Dialogue: 0,0:07:31.13,0:07:32.58,nen den,,0,0,0,,- Đừng đi, đừng đi.\N- Đừng làm bẩn kiếm.
Dialogue: 0,0:07:32.61,0:07:33.01,nen den,,0,0,0,,Vâng.
Dialogue: 0,0:07:33.78,0:07:36.88,nen den,,0,0,0,,Xin hãy dừng lại, xin hãy tha thứ cho tôi.
Dialogue: 0,0:07:42.30,0:07:44.59,nen den,,0,0,0,,Theo dõi sát sao cô ta, tìm ra kiếm Thanh Thiên.
Dialogue: 0,0:07:47.89,0:07:49.82,nen den,,0,0,0,,[Rất vui vì hôm nay có thể phỏng vấn Tiến sĩ Lâm Tây.]
Dialogue: 0,0:07:50.49,0:07:52.13,nen den,,0,0,0,,[Tiến sĩ Lâm Tây, cô nghĩ sao]
Dialogue: 0,0:07:52.15,0:07:53.03,nen den,,0,0,0,,[về phát hiện lần này?]
Dialogue: 0,0:07:53.78,0:07:54.46,nen den,,0,0,0,,[Tôi tin rằng]
Dialogue: 0,0:07:54.96,0:07:56.35,nen den,,0,0,0,,[người tu hành đã chết trong động kia]
Dialogue: 0,0:07:56.92,0:07:58.90,nen den,,0,0,0,,[chính là sứ giả Vua Gesar phái tới triều Tống.]
Dialogue: 0,0:08:00.27,0:08:02.11,nen den,,0,0,0,,[Hiện tại bên ngoài đang có nhiều ý kiến bất đồng về chuyện này.]
Dialogue: 0,0:08:02.26,0:08:03.54,nen den,,0,0,0,,[Rất nhiều người tỏ ra nghi ngờ]
Dialogue: 0,0:08:04.06,0:08:06.05,nen den,,0,0,0,,[trong nước có nhiều đề tài khảo cổ như vậy,]
Dialogue: 0,0:08:06.21,0:08:07.50,nen den,,0,0,0,,[tại sao cô vẫn luôn muốn nghiên cứu]
Dialogue: 0,0:08:07.54,0:08:09.52,nen den,,0,0,0,,[về đề tài bị nghi ngờ nhiều như kiếm Thanh Thiên vậy?]
Dialogue: 0,0:08:23.42,0:08:24.16,nen den,,0,0,0,,Trên vách đá
Dialogue: 0,0:08:24.32,0:08:25.41,nen den,,0,0,0,,có treo túi của bố cô.
Dialogue: 0,0:08:26.35,0:08:26.98,nen den,,0,0,0,,Nhưng
Dialogue: 0,0:08:27.49,0:08:28.25,nen den,,0,0,0,,không tìm thấy người.
Dialogue: 0,0:08:29.31,0:08:30.05,nen den,,0,0,0,,Hai năm sau
Dialogue: 0,0:08:30.37,0:08:31.91,nen den,,0,0,0,,cô có thể xin giấy chứng nhận mất tích.
Dialogue: 0,0:08:32.35,0:08:33.23,nen den,,0,0,0,,Nhưng giấy chứng tử
Dialogue: 0,0:08:33.94,0:08:35.04,nen den,,0,0,0,,thì phải chờ bốn năm sau mới xin được.
Dialogue: 0,0:08:39.57,0:08:41.20,nen den,,0,0,0,,Năng lực của ông ấyrất mạnh,
Dialogue: 0,0:08:41.72,0:08:43.23,nen den,,0,0,0,,chỉ tiếc là lại có chứng hoang tưởng.
Dialogue: 0,0:08:43.67,0:08:44.24,nen den,,0,0,0,,Nếu không phải
Dialogue: 0,0:08:44.28,0:08:45.53,nen den,,0,0,0,,ông ấy cứ nhất định phải đi tìm thanh kiếm không tồn tại kia
Dialogue: 0,0:08:45.69,0:08:46.85,nen den,,0,0,0,,thì cũng không đến mức mất mạng.
Dialogue: 0,0:08:49.20,0:08:51.28,nen den,,0,0,0,,[Lâm Quốc Hoa]
Dialogue: 0,0:08:55.09,0:08:55.67,nen den,,0,0,0,,[Tiến sĩ Lâm,]
Dialogue: 0,0:08:55.99,0:08:56.73,nen den,,0,0,0,,[cô vẫn chưa trả lời]
Dialogue: 0,0:08:56.75,0:08:57.50,nen den,,0,0,0,,[câu hỏi vừa nãy.]
Dialogue: 0,0:08:57.84,0:08:58.73,nen den,,0,0,0,,[Tại sao phải kiên trì như vậy?]
Dialogue: 0,0:09:01.41,0:09:02.15,nen den,,0,0,0,,[Bởi vì tôi tin,]
Dialogue: 0,0:09:04.98,0:09:06.18,nen den,,0,0,0,,[cũng vì người khác không tin.]
Dialogue: 0,0:09:08.23,0:09:09.03,nen den,,0,0,0,,[Vâng, cảm ơn]
Dialogue: 0,0:09:12.14,0:09:12.68,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,0:09:13.21,0:09:14.44,nen den,,0,0,0,,Chú chụp màn hình ảnh phản chiếu của chiếc cốc
Dialogue: 0,0:09:14.64,0:09:15.80,nen den,,0,0,0,,và hình ảnh trên tường trong cái động kia
Dialogue: 0,0:09:15.82,0:09:16.54,nen den,,0,0,0,,gửi cho cháu.
Dialogue: 0,0:09:41.87,0:09:43.21,nen den,,0,0,0,,Hình ảnh về kiếm Thanh Thiên
Dialogue: 0,0:09:44.04,0:09:44.85,nen den,,0,0,0,,nhất định không sai.
Dialogue: 0,0:10:19.17,0:10:20.80,nen den,,0,0,0,,Chỉ cần tìm được kiếm Thanh Thiên
Dialogue: 0,0:10:21.12,0:10:23.10,nen den,,0,0,0,,thì có thể chứng minh được sự tồn tại của Vua Gesar.
Dialogue: 0,0:10:23.90,0:10:26.26,nen den,,0,0,0,,Bố có dự cảm ngày đó không còn xa nữa.
Dialogue: 0,0:10:53.96,0:10:55.45,nen den,,0,0,0,,[Thảo nguyên Trác Khê Ca]
Dialogue: 0,0:11:00.27,0:11:02.03,nen den,,0,0,0,,Thiên Vương tàng cổ đao.
Dialogue: 0,0:11:04.52,0:11:05.01,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,0:11:05.16,0:11:06.65,nen den,,0,0,0,,Chuẩn bị thủ tục, báo cáo lại với đơn vị.
Dialogue: 0,0:11:06.79,0:11:07.27,nen den,,0,0,0,,Đi đâu?
Dialogue: 0,0:11:08.87,0:11:09.91,nen den,,0,0,0,,Tô-tem Thái Dương Hoả Diễm
Dialogue: 0,0:11:09.94,0:11:11.95,nen den,,0,0,0,,từng là tô-tem bộ lạc của thảo nguyên Trác Khê Ca.
Dialogue: 0,0:11:12.65,0:11:14.40,nen den,,0,0,0,,Chúng ta phải tới Thạch Khúc tìm cờ Thái Dương.
Dialogue: 0,0:11:14.79,0:11:15.36,nen den,,0,0,0,,Thạch Khúc?
Dialogue: 0,0:11:16.06,0:11:17.06,nen den,,0,0,0,,Chỗ đó cao như vậy
Dialogue: 0,0:11:17.45,0:11:17.84,nen den,,0,0,0,,chú không đi đâu.
Dialogue: 0,0:11:17.84,0:11:18.13,nen den,,0,0,0,,[Lối ra vào sân bay]\Nchú không đi đâu.
Dialogue: 0,0:11:18.13,0:11:19.53,nen den,,0,0,0,,[Lối ra vào sân bay]
Dialogue: 0,0:11:23.00,0:11:28.06,nen den,,0,0,0,,[Độ cao 3950m trên mặt nước biển ]
Dialogue: 0,0:11:28.29,0:11:29.35,nen den,,0,0,0,,Chào anh, chào người đẹp.
Dialogue: 0,0:11:29.37,0:11:29.88,nen den,,0,0,0,,Người đẹp.
Dialogue: 0,0:11:29.90,0:11:31.22,nen den,,0,0,0,,Đi xe hả? Đi xe đi.
Dialogue: 0,0:11:31.26,0:11:32.33,nen den,,0,0,0,,Đi xe tôi đi.
Dialogue: 0,0:11:32.35,0:11:33.23,nen den,,0,0,0,,Cô đi đâu vậy?
Dialogue: 0,0:11:33.27,0:11:33.87,nen den,,0,0,0,,Tôi đi Thạch Khúc.
Dialogue: 0,0:11:34.17,0:11:34.70,nen den,,0,0,0,,Thạch Khúc?
Dialogue: 0,0:11:34.94,0:11:35.46,nen den,,0,0,0,,Thạch Khúc.
Dialogue: 0,0:11:35.46,0:11:36.67,nen den,,0,0,0,,Thạch Khúc thì không đi đâu.
Dialogue: 0,0:11:36.70,0:11:37.83,nen den,,0,0,0,,Xa lắm, không đi được.
Dialogue: 0,0:11:37.86,0:11:38.54,nen den,,0,0,0,,Thạch Khúc xa quá.
Dialogue: 0,0:11:38.54,0:11:39.65,nen den,,0,0,0,,Không đi được, không đi được.
Dialogue: 0,0:11:40.24,0:11:42.20,nen den,,0,0,0,,Có chút đạo đức nghề nghiệp được không vậy?
Dialogue: 0,0:11:42.33,0:11:44.42,nen den,,0,0,0,,Có chút đạo đức nghề nghiệp được không vậy?
Dialogue: 0,0:11:44.75,0:11:46.03,nen den,,0,0,0,,Có ai có thể đi Thạch Khúc không?
Dialogue: 0,0:11:46.20,0:11:47.35,nen den,,0,0,0,,Có ai đi Thạch Khúc không?
Dialogue: 0,0:11:55.19,0:11:55.77,nen den,,0,0,0,,[Ngọc Thụ]
Dialogue: 0,0:11:55.77,0:11:56.77,nen den,,0,0,0,,Để ông đây,\N[Ngọc Thụ]
Dialogue: 0,0:11:56.77,0:11:57.21,nen den,,0,0,0,,[Ngọc Thụ]
Dialogue: 0,0:11:57.21,0:11:58.18,nen den,,0,0,0,,nhà trinh thám...\N[Ngọc Thụ]
Dialogue: 0,0:11:58.18,0:11:58.42,nen den,,0,0,0,,[Ngọc Thụ]
Dialogue: 0,0:11:58.82,0:11:59.69,nen den,,0,0,0,,Đời thứ năm,
Dialogue: 0,0:12:01.44,0:12:02.31,nen den,,0,0,0,,không tới một trăm
Dialogue: 0,0:12:02.89,0:12:04.08,nen den,,0,0,0,,chỉ bảy phẩy sáu giây.
Dialogue: 0,0:12:05.22,0:12:05.85,nen den,,0,0,0,,Tây Tây.
Dialogue: 0,0:12:06.62,0:12:08.12,nen den,,0,0,0,,Nếu chúng ta đi xe này tới Thạch Khúc
Dialogue: 0,0:12:08.20,0:12:09.14,nen den,,0,0,0,,thì nhanh lắm đó.
Dialogue: 0,0:12:11.33,0:12:12.76,nen den,,0,0,0,,Có người à?
Dialogue: 0,0:12:12.78,0:12:13.73,nen den,,0,0,0,,Không có mắt à?
Dialogue: 0,0:12:14.33,0:12:15.18,nen den,,0,0,0,,Có đi Thạch Khúc không?
Dialogue: 0,0:12:19.94,0:12:20.58,nen den,,0,0,0,,Hỏi anh đó.
Dialogue: 0,0:12:23.98,0:12:24.90,nen den,,0,0,0,,Đi theo hướng dẫn chỉ đường
Dialogue: 0,0:12:24.92,0:12:25.80,nen den,,0,0,0,,hay đi theo tôi chỉ?
Dialogue: 0,0:12:28.27,0:12:28.75,nen den,,0,0,0,,- Hướng dẫn chỉ đường.\N- Hướng dẫn chỉ đường.
Dialogue: 0,0:12:30.17,0:12:30.67,nen den,,0,0,0,,Hai nghìn tệ.
Dialogue: 0,0:12:33.01,0:12:33.74,nen den,,0,0,0,,Vậy theo cậu chỉ thì sao?
Dialogue: 0,0:12:34.33,0:12:34.79,nen den,,0,0,0,,Một nghìn tệ.
Dialogue: 0,0:12:35.65,0:12:36.15,nen den,,0,0,0,,Cái gì?
Dialogue: 0,0:12:36.69,0:12:38.57,nen den,,0,0,0,,Tôi đi máy bay mất có 500 tệ.
Dialogue: 0,0:12:39.64,0:12:40.76,nen den,,0,0,0,,Cậu lái tên lửa à?
Dialogue: 0,0:12:41.12,0:12:41.72,nen den,,0,0,0,,Chê đắt sao?
Dialogue: 0,0:12:42.21,0:12:42.87,nen den,,0,0,0,,Chê đắt thì đi bộ đi.
Dialogue: 0,0:12:43.36,0:12:43.83,nen den,,0,0,0,,Khỏi mất tiền.
Dialogue: 0,0:12:47.02,0:12:47.95,nen den,,0,0,0,,Hai nghìn tệ thì hai nghìn tệ.
Dialogue: 0,0:12:48.34,0:12:49.11,nen den,,0,0,0,,Đi theo hướng dẫn chỉ đường.
Dialogue: 0,0:13:01.57,0:13:02.23,nen den,,0,0,0,,Không phải xe này.
Dialogue: 0,0:13:40.53,0:13:41.38,nen den,,0,0,0,,Mã Cường?
Dialogue: 0,0:13:41.66,0:13:43.76,nen den,,0,0,0,,Tên này không giống tên người Tây Tạng lắm.
Dialogue: 0,0:13:46.91,0:13:48.07,nen den,,0,0,0,,Hai người cũng đâu giống khách du lịch.
Dialogue: 0,0:13:50.06,0:13:51.01,nen den,,0,0,0,,Cẩn thận chút đi.
Dialogue: 0,0:13:51.28,0:13:52.27,nen den,,0,0,0,,Vợ tôi sẽ tức giận đó.
Dialogue: 0,0:13:53.67,0:13:54.95,nen den,,0,0,0,,Hoá ra là sợ vợ à?
Dialogue: 0,0:13:55.06,0:13:55.74,nen den,,0,0,0,,Lái nhanh chút.
Dialogue: 0,0:13:56.27,0:13:57.56,nen den,,0,0,0,,Nhất định phải tới Thạch Khúc trước khi trời tối.
Dialogue: 0,0:13:59.77,0:14:00.91,nen den,,0,0,0,,Hai người còn bạn bè khác không?
Dialogue: 0,0:14:02.49,0:14:03.14,nen den,,0,0,0,,Ý anh là gì?
Dialogue: 0,0:14:03.59,0:14:04.26,nen den,,0,0,0,,Có một cái xe
Dialogue: 0,0:14:04.78,0:14:05.78,nen den,,0,0,0,,liên tục theo sát phía sau
Dialogue: 0,0:14:05.93,0:14:06.72,nen den,,0,0,0,,kể từ lúc rời khỏi sân bay tới giờ.
Dialogue: 0,0:14:10.38,0:14:11.67,nen den,,0,0,0,,Cậu nghĩ nhiều rồi.
Dialogue: 0,0:14:12.73,0:14:14.42,nen den,,0,0,0,,Có lẽ họ đúng lúc cùng đường với mình thôi.
Dialogue: 0,0:14:15.50,0:14:16.26,nen den,,0,0,0,,Đừng chắn tầm nhìn.
Dialogue: 0,0:14:16.82,0:14:17.59,nen den,,0,0,0,,Lát nữa sẽ biết thôi.
Dialogue: 0,0:15:12.20,0:15:13.50,nen den,,0,0,0,,Mấy người này là loại người gì vậy?
Dialogue: 0,0:15:13.66,0:15:14.84,nen den,,0,0,0,,Không khác gì chó cả.
Dialogue: 0,0:15:16.69,0:15:17.70,nen den,,0,0,0,,Anh nợ tiền người ta đúng không?
Dialogue: 0,0:15:17.73,0:15:18.48,nen den,,0,0,0,,Không phải đi theo tôi.
Dialogue: 0,0:15:18.86,0:15:19.80,nen den,,0,0,0,,Vậy đuổi theo chúng ta làm gì?
Dialogue: 0,0:15:20.95,0:15:21.83,nen den,,0,0,0,,Anh dừng xe lại
Dialogue: 0,0:15:21.97,0:15:22.78,nen den,,0,0,0,,tôi đi nói chuyện với họ.
Dialogue: 0,0:15:23.35,0:15:24.35,nen den,,0,0,0,,Nếu là cướp đường
Dialogue: 0,0:15:24.58,0:15:25.29,nen den,,0,0,0,,thì bây giờ cô xuống xe
Dialogue: 0,0:15:25.32,0:15:26.28,nen den,,0,0,0,,sẽ không có tiền trả tôi nữa.
Dialogue: 0,0:15:58.15,0:15:58.72,nen den,,0,0,0,,Ngồi vững nhé.
Dialogue: 0,0:17:00.58,0:17:01.37,nen den,,0,0,0,,Không phải đã nói rõ
Dialogue: 0,0:17:01.39,0:17:02.82,nen den,,0,0,0,,là phải tới Thạch Khúc trước khi trời tối sao?
Dialogue: 0,0:17:04.87,0:17:05.33,nen den,,0,0,0,,Giúp tôi một việc.
Dialogue: 0,0:17:09.21,0:17:10.16,nen den,,0,0,0,,Không có chuyện gấp
Dialogue: 0,0:17:10.45,0:17:11.02,nen den,,0,0,0,,thì không đi đêm.
Dialogue: 0,0:17:11.34,0:17:12.52,nen den,,0,0,0,,Thế cái gì mới được coi là chuyện gấp?
Dialogue: 0,0:17:14.43,0:17:15.56,nen den,,0,0,0,,Chuyện mà tôi cho là gấp
Dialogue: 0,0:17:16.22,0:17:17.13,nen den,,0,0,0,,là đặt phòng đơn cho tôi,
Dialogue: 0,0:17:17.85,0:17:18.65,nen den,,0,0,0,,bữa tối phải thêm thịt.
Dialogue: 0,0:17:24.41,0:17:25.06,nen den,,0,0,0,,Đây là gì?
Dialogue: 0,0:17:26.01,0:17:26.77,nen den,,0,0,0,,Tiền sửa xe.
Dialogue: 0,0:17:28.80,0:17:30.29,nen den,,0,0,0,,Anh điên rồi à?
Dialogue: 0,0:17:30.57,0:17:32.06,nen den,,0,0,0,,Số này đủ để mua xe mới rồi đó
Dialogue: 0,0:17:32.07,0:17:32.53,nen den,,0,0,0,,được không hả?
Dialogue: 0,0:17:33.68,0:17:35.14,nen den,,0,0,0,,Số lẻ phía trước
Dialogue: 0,0:17:35.77,0:17:36.81,nen den,,0,0,0,,là biển số xe của họ.
Dialogue: 0,0:17:37.27,0:17:37.70,nen den,,0,0,0,,Cảm ơn.
Dialogue: 0,0:18:03.89,0:18:05.72,nen den,,0,0,0,,Ngay cả một người phụ nữ mà cũng để chạy mất.
Dialogue: 0,0:18:12.45,0:18:13.04,nen den,,0,0,0,,Anh Sâm.
Dialogue: 0,0:18:13.80,0:18:14.69,nen den,,0,0,0,,Tài xế đó
Dialogue: 0,0:18:15.55,0:18:16.31,nen den,,0,0,0,,tên Mã Cường.
Dialogue: 0,0:18:30.53,0:18:31.20,nen den,,0,0,0,,Tìm tiếp đi.
Dialogue: 0,0:18:33.60,0:18:34.04,nen den,,0,0,0,,Vâng.
Dialogue: 0,0:18:54.77,0:18:55.38,nen den,,0,0,0,,Đồ lừa đảo,
Dialogue: 0,0:18:55.38,0:18:56.65,nen den,,0,0,0,,chú bảo tuần trước tới mà.
Dialogue: 0,0:18:58.18,0:18:59.29,nen den,,0,0,0,,Bé đáng yêu.
Dialogue: 0,0:19:02.55,0:19:03.46,nen den,,0,0,0,,Có nhớ Mã Cường không?
Dialogue: 0,0:19:06.94,0:19:07.86,nen den,,0,0,0,,Quá nửa là cướp đường.
Dialogue: 0,0:19:08.09,0:19:09.20,nen den,,0,0,0,,Nơi này hoang vu, vắng vẻ
Dialogue: 0,0:19:09.90,0:19:11.41,nen den,,0,0,0,,chúng ta chỉ có thể
Dialogue: 0,0:19:11.44,0:19:12.33,nen den,,0,0,0,,cung cấp biển số xe của đối phương.
Dialogue: 0,0:19:14.14,0:19:15.28,nen den,,0,0,0,,Cháu có biết chú đang nghĩ gì không?
Dialogue: 0,0:19:16.42,0:19:17.49,nen den,,0,0,0,,Chú đang nghĩ tên Mã Cường này
Dialogue: 0,0:19:18.48,0:19:19.68,nen den,,0,0,0,,có lẽ cũng có vấn đề đó.
Dialogue: 0,0:19:21.50,0:19:22.72,nen den,,0,0,0,,Nghe chú nói như vậy
Dialogue: 0,0:19:23.17,0:19:24.34,nen den,,0,0,0,,hình như cũng có vấn đề thật đấy.
Dialogue: 0,0:19:25.15,0:19:25.74,nen den,,0,0,0,,Nếu không
Dialogue: 0,0:19:25.77,0:19:26.46,nen den,,0,0,0,,tại sao anh ta không dám
Dialogue: 0,0:19:26.46,0:19:27.62,nen den,,0,0,0,,báo án cùng chúng ta chứ?
Dialogue: 0,0:19:27.85,0:19:29.43,nen den,,0,0,0,,Vậy nên chú luôn nói
Dialogue: 0,0:19:30.04,0:19:32.39,nen den,,0,0,0,,ra ngoài thì cần phải đề phòng nhiều hơn.
Dialogue: 0,0:19:33.08,0:19:34.71,nen den,,0,0,0,,Mã Cường, Mã Cường.
Dialogue: 0,0:19:34.74,0:19:35.59,nen den,,0,0,0,,Mã Cường tới rồi.
Dialogue: 0,0:19:35.73,0:19:37.53,nen den,,0,0,0,,Mã Cường, Mã Cường.
Dialogue: 0,0:19:37.56,0:19:38.27,nen den,,0,0,0,,Chuyện gì thế này?
Dialogue: 0,0:19:38.30,0:19:39.44,nen den,,0,0,0,,Mã Cường mang quà tới cho mọi người đây.
Dialogue: 0,0:19:39.85,0:19:40.15,nen den,,0,0,0,,Nhìn xem.
Dialogue: 0,0:19:40.56,0:19:41.12,nen den,,0,0,0,,Nào.
Dialogue: 0,0:19:41.48,0:19:43.08,nen den,,0,0,0,,Mã Cường mang quà gì cho mấy đứa đây?
Dialogue: 0,0:19:44.07,0:19:44.91,nen den,,0,0,0,,Ai cũng có cả.
Dialogue: 0,0:19:45.73,0:19:47.25,nen den,,0,0,0,,Mấy cái súng cao su này cho nhóc.
Dialogue: 0,0:19:47.62,0:19:49.11,nen den,,0,0,0,,Nào, sách chú tặng cho mấy đứa đây.
Dialogue: 0,0:19:50.40,0:19:50.96,nen den,,0,0,0,,Cầm lấy nào.
Dialogue: 0,0:19:52.05,0:19:53.95,nen den,,0,0,0,,Chăm chỉ học tập, không được lười biếng,
Dialogue: 0,0:19:54.15,0:19:54.65,nen den,,0,0,0,,nghe rõ chưa?
Dialogue: 0,0:19:55.10,0:19:55.58,nen den,,0,0,0,,Nghe rõ rồi ạ.
Dialogue: 0,0:19:55.62,0:19:56.93,nen den,,0,0,0,,Mỗi người một quyển.
Dialogue: 0,0:19:57.99,0:19:58.48,nen den,,0,0,0,,Cầm lấy.
Dialogue: 0,0:19:59.31,0:20:00.94,nen den,,0,0,0,,Uống nhiều sữa vào cho mau lớn,
Dialogue: 0,0:20:02.51,0:20:03.75,nen den,,0,0,0,,Nhìn thấy Mã Cường có vui hay không?
Dialogue: 0,0:20:03.78,0:20:04.78,nen den,,0,0,0,,Vui ạ.
Dialogue: 0,0:20:04.80,0:20:05.39,nen den,,0,0,0,,Có thích chú không?
Dialogue: 0,0:20:05.63,0:20:06.43,nen den,,0,0,0,,Thích ạ.
Dialogue: 0,0:20:06.46,0:20:07.06,nen den,,0,0,0,,Nhớ chú không?
Dialogue: 0,0:20:07.13,0:20:07.93,nen den,,0,0,0,,Nhớ ạ
Dialogue: 0,0:20:08.29,0:20:10.05,nen den,,0,0,0,,Nào, vui thì cùng ngồi xuống nào.
Dialogue: 0,0:20:11.62,0:20:13.41,nen den,,0,0,0,,- Chú trách nhầm cậu ta rồi.\N- Chăm chỉ đọc sách
Dialogue: 0,0:20:13.49,0:20:14.20,nen den,,0,0,0,,có nghe thấy chưa?
Dialogue: 0,0:20:14.71,0:20:15.78,nen den,,0,0,0,,Nghe rồi ạ.
Dialogue: 0,0:20:18.85,0:20:19.79,nen den,,0,0,0,,Đây là sách của cháu.
Dialogue: 0,0:20:20.10,0:20:20.81,nen den,,0,0,0,,Của cháu.
Dialogue: 0,0:20:22.70,0:20:26.11,nen den,,0,0,0,,Mã Cường, Mã Cường, Mã Cường.
Dialogue: 0,0:20:26.13,0:20:33.05,nen den,,0,0,0,,Mã Cường, Mã Cường, Mã Cường.
Dialogue: 0,0:20:33.14,0:20:38.81,nen den,,0,0,0,,Mã Cường, Mã Cường, Mã Cường.
Dialogue: 0,0:20:41.45,0:20:42.64,nen den,,0,0,0,,Tối qua anh muốn ở lại
Dialogue: 0,0:20:42.87,0:20:43.99,nen den,,0,0,0,,là vì những đứa trẻ đó sao?
Dialogue: 0,0:20:48.14,0:20:49.05,nen den,,0,0,0,,Phần thời gian chậm trễ
Dialogue: 0,0:20:49.51,0:20:50.30,nen den,,0,0,0,,tôi giảm giá cho cô.
Dialogue: 0,0:21:00.04,0:21:00.87,nen den,,0,0,0,,Đây là người nhà anh sao?
Dialogue: 0,0:21:20.94,0:21:21.92,nen den,,0,0,0,,Lễ hội đua ngựa!
Dialogue: 0,0:21:22.24,0:21:24.09,nen den,,0,0,0,,Lâu rồi không gặp.
Dialogue: 0,0:21:36.36,0:21:36.82,nen den,,0,0,0,,Mã Cường.
Dialogue: 0,0:21:44.79,0:21:45.30,nen den,,0,0,0,,Tôi nói rồi
Dialogue: 0,0:21:45.78,0:21:46.40,nen den,,0,0,0,,giảm giá cho cô.
Dialogue: 0,0:21:47.38,0:21:48.07,nen den,,0,0,0,,Cho bọn trẻ đó.
Dialogue: 0,0:21:51.94,0:21:52.78,nen den,,0,0,0,,Bạn của bọn trẻ
Dialogue: 0,0:21:52.80,0:21:53.55,nen den,,0,0,0,,cũng là bạn của tôi.
Dialogue: 0,0:21:54.01,0:21:54.92,nen den,,0,0,0,,Nếu gặp phải khó khăn
Dialogue: 0,0:21:55.10,0:21:55.67,nen den,,0,0,0,,thì gọi cho tôi.
Dialogue: 0,0:21:56.58,0:21:56.93,nen den,,0,0,0,,Cảm ơn.
Dialogue: 0,0:21:57.32,0:21:57.84,nen den,,0,0,0,,Chờ chút.
Dialogue: 0,0:21:59.28,0:21:59.85,nen den,,0,0,0,,Đây là
Dialogue: 0,0:22:00.61,0:22:01.37,nen den,,0,0,0,,tiền cho chuyến khứ hồi.
Dialogue: 0,0:22:02.41,0:22:03.22,nen den,,0,0,0,,Chờ chúng tôi ở đây đi.
Dialogue: 0,0:22:10.68,0:22:11.76,nen den,,0,0,0,,Giúp người thì giúp cho chót.
Dialogue: 0,0:22:17.78,0:22:19.00,nen den,,0,0,0,,Nghèo mà tiêu hoang.
Dialogue: 0,0:22:30.25,0:22:30.70,nen den,,0,0,0,,Ông chủ.
Dialogue: 0,0:22:53.39,0:23:02.14,nen den,,0,0,0,,Đẩy Mã Cường.
Dialogue: 0,0:23:42.47,0:23:47.20,nen den,,0,0,0,,Đẩy Mã Cường, đẩy Mã Cường, đẩy Mã Cường.
Dialogue: 0,0:23:54.50,0:23:55.27,nen den,,0,0,0,,Anh bạn nhỏ.
Dialogue: 0,0:23:56.12,0:23:57.58,nen den,,0,0,0,,Đây là lốp xe của Mã Cường đúng không?
Dialogue: 0,0:24:02.50,0:24:02.95,nen den,,0,0,0,,Nói đi chứ.
Dialogue: 0,0:24:04.68,0:24:05.64,nen den,,0,0,0,,Đừng sợ, đừng sợ.
Dialogue: 0,0:24:08.58,0:24:09.96,nen den,,0,0,0,,Chú là bạn của Mã Cường.
Dialogue: 0,0:24:11.54,0:24:12.44,nen den,,0,0,0,,Nói cho chú biết
Dialogue: 0,0:24:13.77,0:24:15.02,nen den,,0,0,0,,Mã Cường đi đâu rồi?
Dialogue: 0,0:24:55.73,0:24:56.44,nen den,,0,0,0,,Nào, uống trà đi.
Dialogue: 0,0:24:57.54,0:24:58.10,nen den,,0,0,0,,Tashi delek.
Dialogue: 0,0:24:58.13,0:24:58.61,nen den,,0,0,0,,Tashi delek.
Dialogue: 0,0:25:04.17,0:25:04.68,nen den,,0,0,0,,Tài Đán.
Dialogue: 0,0:25:05.85,0:25:06.29,nen den,,0,0,0,,Anh xem thử xem
Dialogue: 0,0:25:06.71,0:25:08.02,nen den,,0,0,0,,anh đã từng nhìn thấy tô-tem này chưa?
Dialogue: 0,0:25:09.46,0:25:10.01,nen den,,0,0,0,,Chưa thấy bao giờ.
Dialogue: 0,0:25:10.84,0:25:11.62,nen den,,0,0,0,,Không thể nào.
Dialogue: 0,0:25:12.65,0:25:14.17,nen den,,0,0,0,,Nó giống huy hiệu cờ của mấy người mà.
Dialogue: 0,0:25:14.43,0:25:16.17,nen den,,0,0,0,,Chỉ là ở giữa nó có thêm thanh kiếm Thanh Thiên thôi.
Dialogue: 0,0:25:16.44,0:25:16.89,nen den,,0,0,0,,Kiếm Thanh Thiên?
Dialogue: 0,0:25:17.05,0:25:17.30,nen den,,0,0,0,,Đúng vậy.
Dialogue: 0,0:25:17.90,0:25:18.75,nen den,,0,0,0,,Thanh kiếm ở giữa
Dialogue: 0,0:25:18.92,0:25:20.26,nen den,,0,0,0,,chính là kiếm Thanh Thiên của Vua Gesar.
Dialogue: 0,0:25:21.63,0:25:22.13,nen den,,0,0,0,,Không rõ lắm.
Dialogue: 0,0:25:24.00,0:25:25.34,nen den,,0,0,0,,Nhưng tôi nhớ tôi từng nhìn thấy
Dialogue: 0,0:25:25.73,0:25:27.57,nen den,,0,0,0,,hình về thanh kiếm này ở điện Mã Ni.
Dialogue: 0,0:25:28.52,0:25:29.23,nen den,,0,0,0,,Điện Mã Ni?
Dialogue: 0,0:25:29.38,0:25:29.66,nen den,,0,0,0,,Đúng.
Dialogue: 0,0:25:32.61,0:25:32.96,nen den,,0,0,0,,Mở.
Dialogue: 0,0:25:34.14,0:25:34.46,nen den,,0,0,0,,Bảy.
Dialogue: 0,0:25:34.63,0:25:36.09,nen den,,0,0,0,,Một, hai, ba, bốn, năm, sáu, bảy.
Dialogue: 0,0:25:44.58,0:25:45.38,nen den,,0,0,0,,Nào, thêm.
Dialogue: 0,0:26:04.38,0:26:05.55,nen den,,0,0,0,,Này, cậu thay tôi đi.
Dialogue: 0,0:26:05.57,0:26:06.06,nen den,,0,0,0,,Được.
Dialogue: 0,0:26:40.38,0:26:41.49,nen den,,0,0,0,,Tiếp theo chúng ta sẽ đi tới điện Mã Ni.
Dialogue: 0,0:26:44.85,0:26:45.44,nen den,,0,0,0,,Thu xe rồi,
Dialogue: 0,0:26:45.61,0:26:46.26,nen den,,0,0,0,,tới đồn cảnh sát.
Dialogue: 0,0:26:46.72,0:26:47.38,nen den,,0,0,0,,Tại sao?
Dialogue: 0,0:26:48.62,0:26:49.65,nen den,,0,0,0,,Người hôm qua đuổi theo mấy người
Dialogue: 0,0:26:49.77,0:26:50.48,nen den,,0,0,0,,không phải là cướp đường.
Dialogue: 0,0:26:52.21,0:26:53.73,nen den,,0,0,0,,Cậu bảo đó không phải là cướp đường
Dialogue: 0,0:26:54.01,0:26:54.93,nen den,,0,0,0,,thì đó là ai chứ?
Dialogue: 0,0:26:55.46,0:26:55.90,nen den,,0,0,0,,Không biết.
Dialogue: 0,0:26:56.88,0:26:57.90,nen den,,0,0,0,,Hai người bị theo dõi rồi.
Dialogue: 0,0:26:58.59,0:26:59.83,nen den,,0,0,0,,Tại sao họ lại theo dõi chúng tôi?
Dialogue: 0,0:27:02.69,0:27:03.33,nen den,,0,0,0,,Tây Tây, cháu nhìn này.
Dialogue: 0,0:27:04.65,0:27:05.81,nen den,,0,0,0,,Có người xông vào nhà cháu rồi.
Dialogue: 0,0:27:09.38,0:27:10.34,nen den,,0,0,0,,Bây giờ có hack được vào
Dialogue: 0,0:27:10.57,0:27:11.47,nen den,,0,0,0,,camera máy tính của cháu không?
Dialogue: 0,0:27:32.86,0:27:33.53,nen den,,0,0,0,,Là ông ta.
Dialogue: 0,0:27:34.94,0:27:36.08,nen den,,0,0,0,,Tần Trung.
Dialogue: 0,0:27:51.33,0:27:52.95,nen den,,0,0,0,,Nếu thực sự là Tần Trung đang bám theo chúng ta
Dialogue: 0,0:27:55.18,0:27:56.34,nen den,,0,0,0,,thì ông ấy hẳn là vì kiếm Thanh Thiên.
Dialogue: 0,0:27:58.54,0:27:59.70,nen den,,0,0,0,,Cậu làm cái gì đấy?
Dialogue: 0,0:28:04.75,0:28:05.58,nen den,,0,0,0,,Rốt cuộc các người là ai?
Dialogue: 0,0:28:12.42,0:28:12.69,nen den,,0,0,0,,Chúng tôi là nhân viên nghiên cứu khảo cổ
Dialogue: 0,0:28:12.69,0:28:13.40,nen den,,0,0,0,,[Thẻ nhân viên: Viện nghiên cứu khảo cổ tỉnh Tứ Xuyên]\NChúng tôi là nhân viên nghiên cứu khảo cổ
Dialogue: 0,0:28:13.40,0:28:13.62,nen den,,0,0,0,,[Thẻ nhân viên: Viện nghiên cứu khảo cổ tỉnh Tứ Xuyên]
Dialogue: 0,0:28:13.94,0:28:15.23,nen den,,0,0,0,,đến tìm kiếm Thanh Thiên của Vua Gesar.
Dialogue: 0,0:28:17.48,0:28:18.11,nen den,,0,0,0,,Tần Trung là ai?
Dialogue: 0,0:28:18.61,0:28:19.12,nen den,,0,0,0,,Ngoài mặt
Dialogue: 0,0:28:19.15,0:28:20.30,nen den,,0,0,0,,ông ta là một nhà sưu tầm văn vật nổi tiếng,
Dialogue: 0,0:28:20.81,0:28:22.58,nen den,,0,0,0,,nhưng thực ra ông ta vẫn luôn thao túng chợ đen văn vật.
Dialogue: 0,0:28:22.83,0:28:23.96,nen den,,0,0,0,,Rất nhiều vụ văn vật bị tuồn ra hải ngoại
Dialogue: 0,0:28:24.00,0:28:24.66,nen den,,0,0,0,,đều liên quan đến ông ta.
Dialogue: 0,0:28:25.75,0:28:27.08,nen den,,0,0,0,,Giờ chúng ta nên báo cảnh sát thì hơn.
Dialogue: 0,0:28:27.59,0:28:28.44,nen den,,0,0,0,,Đã báo cảnh sát rồi.
Dialogue: 0,0:28:28.51,0:28:29.46,nen den,,0,0,0,,Nhưng việc lập án cần thời gian.
Dialogue: 0,0:28:29.83,0:28:31.32,nen den,,0,0,0,,Nếu chúng ta tiếp tục để mất thời gian,
Dialogue: 0,0:28:31.49,0:28:32.72,nen den,,0,0,0,,nhất định họ sẽ trộm văn vật đi mất.
Dialogue: 0,0:28:36.86,0:28:37.42,nen den,,0,0,0,,Mã Cường.
Dialogue: 0,0:28:38.00,0:28:38.68,nen den,,0,0,0,,Giúp tôi đi.
Dialogue: 0,0:28:54.83,0:28:55.72,nen den,,0,0,0,,Cô không sợ đám người đó à?
Dialogue: 0,0:28:56.16,0:28:57.18,nen den,,0,0,0,,Chỉ cần tìm được kiếm Thanh Thiên,
Dialogue: 0,0:28:58.24,0:28:59.12,nen den,,0,0,0,,tôi chẳng sợ gì cả.
Dialogue: 0,0:29:02.24,0:29:04.87,nen den,,0,0,0,,[Điện Mã Ni - Tân Thiên Hương.]
Dialogue: 0,0:29:05.18,0:29:06.96,nen den,,0,0,0,,Cuộc thi sắp bắt đầu rồi.
Dialogue: 0,0:29:08.73,0:29:10.99,nen den,,0,0,0,,Tài Đán! Tài Đán!
Dialogue: 0,0:29:12.66,0:29:14.39,nen den,,0,0,0,,Tài Đán! Tài Đán!
Dialogue: 0,0:29:24.54,0:29:28.38,nen den,,0,0,0,,[Độ cao 4195 mét trên mực nước biển]
Dialogue: 0,0:29:40.95,0:29:41.72,nen den,,0,0,0,,Để kỷ niệm
Dialogue: 0,0:29:42.34,0:29:43.49,nen den,,0,0,0,,những dũng sĩ dưới trướng Vua Gesar
Dialogue: 0,0:29:43.52,0:29:44.81,nen den,,0,0,0,,đã hy sinh trên chiến trường,
Dialogue: 0,0:29:45.68,0:29:47.72,nen den,,0,0,0,,những hậu nhân hành hương đã dùng những viên đá Mã Ni
Dialogue: 0,0:29:48.13,0:29:49.10,nen den,,0,0,0,,để xây cung điện này.
Dialogue: 0,0:29:50.04,0:29:50.60,nen den,,0,0,0,,Cô từng đến đây rồi à?
Dialogue: 0,0:29:51.32,0:29:52.40,nen den,,0,0,0,,Người nghiên cứu về Vua Gesar
Dialogue: 0,0:29:52.43,0:29:53.45,nen den,,0,0,0,,đều biết đến cung điện này.
Dialogue: 0,0:29:54.26,0:29:55.01,nen den,,0,0,0,,Chỉ có điều,
Dialogue: 0,0:29:55.06,0:29:56.69,nen den,,0,0,0,,lúc trước tôi không tìm được manh mối gì.
Dialogue: 0,0:29:57.65,0:29:58.97,nen den,,0,0,0,,Chắc là tôi đã bỏ sót gì đó.
Dialogue: 0,0:30:09.25,0:30:10.75,nen den,,0,0,0,,Thiên vương tàng cổ đao,
Dialogue: 0,0:30:12.09,0:30:13.46,nen den,,0,0,0,,ngũ âm độ ngũ hành.
Dialogue: 0,0:30:14.37,0:30:14.87,nen den,,0,0,0,,Ngũ hành?
Dialogue: 0,0:30:17.35,0:30:18.12,nen den,,0,0,0,,Chú biết.
Dialogue: 0,0:30:37.55,0:30:37.97,nen den,,0,0,0,,Kim.
Dialogue: 0,0:30:38.82,0:30:39.43,nen den,,0,0,0,,Tây Tây.
Dialogue: 0,0:30:39.72,0:30:40.77,nen den,,0,0,0,,Chú tìm được kim rồi.
Dialogue: 0,0:30:42.83,0:30:43.36,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,0:30:43.68,0:30:44.85,nen den,,0,0,0,,Chú di chuyển nó thử xem.
Dialogue: 0,0:30:52.90,0:30:53.59,nen den,,0,0,0,,Trong đó có gì vậy?
Dialogue: 0,0:30:55.19,0:30:55.72,nen den,,0,0,0,,Tượng Phật.
Dialogue: 0,0:30:57.12,0:30:58.21,nen den,,0,0,0,,Thế cũng chẳng có gì đặc biệt.
Dialogue: 0,0:30:59.20,0:31:00.02,nen den,,0,0,0,,Ở đây đầy tượng Phật.
Dialogue: 0,0:31:01.18,0:31:01.66,nen den,,0,0,0,,Không đúng.
Dialogue: 0,0:31:02.93,0:31:04.38,nen den,,0,0,0,,Không phải ngũ hành kim mộc thủy hỏa thổ,
Dialogue: 0,0:31:05.33,0:31:06.55,nen den,,0,0,0,,mà là năm nguyên tố lớn trong vũ trụ
Dialogue: 0,0:31:06.82,0:31:08.22,nen den,,0,0,0,,mà Tạng Địa Noãn Sinh đã nói
Dialogue: 0,0:31:08.84,0:31:09.88,nen den,,0,0,0,,không phong hỏa mộc thổ.
Dialogue: 0,0:31:46.56,0:31:48.05,nen den,,0,0,0,,Ốc Hữu Tuyền là thánh vật của vùng Tây Tạng.
Dialogue: 0,0:31:48.56,0:31:49.56,nen den,,0,0,0,,Lần này chắc chắn là đúng rồi.
Dialogue: 0,0:31:49.86,0:31:51.19,nen den,,0,0,0,,Đi tìm bốn cái còn lại đi.
Dialogue: 0,0:31:52.30,0:31:52.77,nen den,,0,0,0,,Chia ra tìm.
Dialogue: 0,0:32:30.60,0:32:32.08,nen den,,0,0,0,,Tôi tìm được nguyên tố thổ cuối cùng rồi.
Dialogue: 0,0:32:44.24,0:32:44.78,nen den,,0,0,0,,Tìm được cả rồi.
Dialogue: 0,0:32:47.35,0:32:47.85,nen den,,0,0,0,,Rồi sao nữa?
Dialogue: 0,0:32:58.69,0:33:00.14,nen den,,0,0,0,,Ngũ âm độ ngũ hành.
Dialogue: 0,0:33:01.79,0:33:02.83,nen den,,0,0,0,,Ngũ độ phiếm âm.
Dialogue: 0,0:33:04.46,0:33:05.99,nen den,,0,0,0,,Ngũ độ phiếm âm.
Dialogue: 0,0:33:11.28,0:33:12.17,nen den,,0,0,0,,Nào, cầm giúp tôi chút.
Dialogue: 0,0:33:28.98,0:33:30.92,nen den,,0,0,0,,Quay con ốc theo ngũ độ phiếm âm.
Dialogue: 0,0:33:55.03,0:33:55.80,nen den,,0,0,0,,Kêu rồi.
Dialogue: 0,0:34:40.73,0:34:42.30,nen den,,0,0,0,,Vân hồng mạn cổ tự.
Dialogue: 0,0:34:43.09,0:34:44.65,nen den,,0,0,0,,Vô ngôn thích di ngôn.
Dialogue: 0,0:34:45.84,0:34:47.25,nen den,,0,0,0,,Vân hồng mạn cổ tự.
Dialogue: 0,0:34:48.63,0:34:49.76,nen den,,0,0,0,,Vô ngôn thích di ngôn.
Dialogue: 0,0:35:40.82,0:35:42.46,nen den,,0,0,0,,Vân hồng mạn cổ tự.
Dialogue: 0,0:35:42.76,0:35:44.69,nen den,,0,0,0,,Vô ngôn thích di ngôn.
Dialogue: 0,0:35:45.25,0:35:48.04,nen den,,0,0,0,,Tôi đoán chắc địa điểm tiếp theo sẽ ở chùa Phong Lam.
Dialogue: 0,0:35:49.65,0:35:50.37,nen den,,0,0,0,,Mã Cường
Dialogue: 0,0:35:51.21,0:35:52.36,nen den,,0,0,0,,từng làm vệ sĩ đặc chủng,
Dialogue: 0,0:35:53.63,0:35:54.33,nen den,,0,0,0,,đừng có chủ quan.
Dialogue: 0,0:35:55.10,0:35:56.07,nen den,,0,0,0,,Vân hồng mạn cổ tự,
Dialogue: 0,0:35:56.47,0:35:58.16,nen den,,0,0,0,,không phải là mây và cầu vồng ngang qua chùa,
Dialogue: 0,0:35:58.51,0:36:00.26,nen den,,0,0,0,,mà là chùa đã lên đến độ cao của tầng mây.
Dialogue: 0,0:36:00.98,0:36:01.80,nen den,,0,0,0,,Trên thảo nguyên Trác Khê Ca,
Dialogue: 0,0:36:01.82,0:36:03.05,nen den,,0,0,0,,có ba ngôi chùa cổ xưa nhất.
Dialogue: 0,0:36:03.44,0:36:04.54,nen den,,0,0,0,,Một trong số đó, chùa Phong Lam
Dialogue: 0,0:36:04.93,0:36:05.82,nen den,,0,0,0,,hơn một nghìn năm trước,
Dialogue: 0,0:36:05.86,0:36:07.67,nen den,,0,0,0,,độ cao được nâng lên do chuyển động biến dạng của vỏ trái đất,
Dialogue: 0,0:36:08.08,0:36:09.29,nen den,,0,0,0,,cũng vì vậy mà từng được xây lại một lần.
Dialogue: 0,0:36:09.97,0:36:10.98,nen den,,0,0,0,,Hơn nữa quan trọng nhất là,
Dialogue: 0,0:36:11.70,0:36:13.19,nen den,,0,0,0,,vị trí của chùa Phong Lam trong truyền thuyết
Dialogue: 0,0:36:13.23,0:36:14.87,nen den,,0,0,0,,chính là di chỉ của Đại tướng Đặng Mã.
Dialogue: 0,0:36:16.54,0:36:17.46,nen den,,0,0,0,,Đại tướng Đặng Mã?
Dialogue: 0,0:36:18.74,0:36:20.77,nen den,,0,0,0,,Một trong 30 đại tướng dưới trướng Vua Gesar?
Dialogue: 0,0:36:20.90,0:36:21.21,nen den,,0,0,0,,Đúng vậy.
Dialogue: 0,0:36:21.99,0:36:22.97,nen den,,0,0,0,,Tôi đoán là những manh mối này
Dialogue: 0,0:36:23.26,0:36:24.36,nen den,,0,0,0,,đều do Đại tướng Đặng Mã để lại.
Dialogue: 0,0:36:26.67,0:36:27.49,nen den,,0,0,0,,Không đến được chùa Phong Lam
Dialogue: 0,0:36:28.10,0:36:28.85,nen den,,0,0,0,,trong hôm nay đâu.
Dialogue: 0,0:36:29.84,0:36:31.29,nen den,,0,0,0,,Nhất định phải đến trước Tần Trung.
Dialogue: 0,0:36:31.98,0:36:33.07,nen den,,0,0,0,,Anh có bằng lòng đi xuyên đêm một lần không?
Dialogue: 0,0:36:40.54,0:36:41.28,nen den,,0,0,0,,Vội thế cơ à?
Dialogue: 0,0:36:42.38,0:36:43.27,nen den,,0,0,0,,Thế phải thêm tiền.
Dialogue: 0,0:36:49.81,0:36:51.24,nen den,,0,0,0,,Anh đột nhiên đổi hành trình thế
Dialogue: 0,0:36:52.03,0:36:53.48,nen den,,0,0,0,,có cần hỏi ý kiến vợ không?
Dialogue: 0,0:36:57.07,0:36:57.61,nen den,,0,0,0,,Vợ ơi,
Dialogue: 0,0:36:58.48,0:36:59.23,nen den,,0,0,0,,nghe thấy chưa?
Dialogue: 0,0:37:03.30,0:37:04.07,nen den,,0,0,0,,Vợ tôi bảo
Dialogue: 0,0:37:05.51,0:37:06.10,nen den,,0,0,0,,OK nhé.
Dialogue: 0,0:37:23.98,0:37:24.70,nen den,,0,0,0,,Chú nghĩ
Dialogue: 0,0:37:24.95,0:37:26.12,nen den,,0,0,0,,liệu chúng ta có thể tìm được
Dialogue: 0,0:37:26.14,0:37:26.95,nen den,,0,0,0,,di tích Vua Gesar không?
Dialogue: 0,0:37:29.52,0:37:32.12,nen den,,0,0,0,,Con bé này mất mẹ từ sớm
Dialogue: 0,0:37:32.65,0:37:34.35,nen den,,0,0,0,,một tay Lão Lâm nuôi nó lớn.
Dialogue: 0,0:37:35.56,0:37:37.11,nen den,,0,0,0,,Từ sau khi Lão Lâm ra đi,
Dialogue: 0,0:37:37.13,0:37:39.75,nen den,,0,0,0,,con bé chưa từng được ngủ một giấc ngon.
Dialogue: 0,0:37:55.18,0:37:58.95,nen den,,0,0,0,,[Độ cao 4632 mét trên mực nước biển]
Dialogue: 0,0:38:03.98,0:38:04.82,nen den,,0,0,0,,Nhớ phải luôn báo cho chúng tôi
Dialogue: 0,0:38:04.85,0:38:05.52,nen den,,0,0,0,,tình hình bên ngoài nhé.
Dialogue: 0,0:38:05.69,0:38:06.02,nen den,,0,0,0,,Được.
Dialogue: 0,0:38:25.30,0:38:26.09,nen den,,0,0,0,,Lại mất tín hiệu rồi.
Dialogue: 0,0:39:58.28,0:39:58.82,nen den,,0,0,0,,Mời ngồi.
Dialogue: 0,0:40:02.23,0:40:02.75,nen den,,0,0,0,,Đại sư.
Dialogue: 0,0:40:03.53,0:40:04.51,nen den,,0,0,0,,Tôi muốn hỏi
Dialogue: 0,0:40:05.74,0:40:06.58,nen den,,0,0,0,,trong chùa có
Dialogue: 0,0:40:06.61,0:40:07.69,nen den,,0,0,0,,manh mối hay di vật nào
Dialogue: 0,0:40:07.71,0:40:09.00,nen den,,0,0,0,,liên quan đến Vua Gesar
Dialogue: 0,0:40:09.02,0:40:10.06,nen den,,0,0,0,,mà Đại tướng Đặng Mã để lại không?
Dialogue: 0,0:40:14.06,0:40:15.74,nen den,,0,0,0,,Lão Lâm không tìm được,
Dialogue: 0,0:40:16.66,0:40:18.21,nen den,,0,0,0,,cháu lại bước lên vết xe đổ của ông ấy.
Dialogue: 0,0:40:19.29,0:40:22.31,nen den,,0,0,0,,Hai cha con nhà cháu đều cứng đầu vậy sao?
Dialogue: 0,0:40:25.53,0:40:26.94,nen den,,0,0,0,,Thí chủ hỏi vật này
Dialogue: 0,0:40:26.97,0:40:28.13,nen den,,0,0,0,,để làm gì vậy?
Dialogue: 0,0:40:28.20,0:40:29.29,nen den,,0,0,0,,Tôi muốn tìm kiếm Thanh Thiên.
Dialogue: 0,0:40:29.50,0:40:31.04,nen den,,0,0,0,,Tại sao lại muốn tìm kiếm Thanh Thiên?
Dialogue: 0,0:40:31.32,0:40:32.67,nen den,,0,0,0,,Để chứng minh sự tồn tại của Vua Gesar.
Dialogue: 0,0:40:35.13,0:40:35.97,nen den,,0,0,0,,Mặc dù Đại tướng Đặng Mã
Dialogue: 0,0:40:35.99,0:40:37.66,nen den,,0,0,0,,không phải là truyền thuyết không có thật,
Dialogue: 0,0:40:38.89,0:40:41.02,nen den,,0,0,0,,nhưng chùa chúng tôi chưa từng nghe nói đến
Dialogue: 0,0:40:41.04,0:40:42.34,nen den,,0,0,0,,di vật của Đại tướng Đặng Mã.
Dialogue: 0,0:41:31.67,0:41:33.05,nen den,,0,0,0,,Nhưng tại sao
Dialogue: 0,0:41:33.33,0:41:35.41,nen den,,0,0,0,,trong tất cả tranh vẽ về Vua Gesar
Dialogue: 0,0:41:35.65,0:41:37.30,nen den,,0,0,0,,đều không có kiếm Thanh Thiên ạ?
Dialogue: 0,0:41:37.88,0:41:38.79,nen den,,0,0,0,,Bố nghĩ,
Dialogue: 0,0:41:39.38,0:41:42.32,nen den,,0,0,0,,trong chuyện này nhất định còn giấu bí mật nào đó.
Dialogue: 0,0:41:42.35,0:41:42.87,nen den,,0,0,0,,Tây Tây.
Dialogue: 0,0:41:43.63,0:41:44.15,nen den,,0,0,0,,Tây Tây.
Dialogue: 0,0:41:44.65,0:41:45.71,nen den,,0,0,0,,Cháu qua đây xem chút đi.
Dialogue: 0,0:41:45.96,0:41:47.28,nen den,,0,0,0,,Hình như đây không phải kinh văn.
Dialogue: 0,0:41:47.79,0:41:48.61,nen den,,0,0,0,,Nào, để cháu xem nào.
Dialogue: 0,0:41:52.76,0:41:53.64,nen den,,0,0,0,,Cầm giúp tôi một chút nhé.
Dialogue: 0,0:41:59.51,0:42:00.52,nen den,,0,0,0,,Đây quả thực không phải kinh văn.
Dialogue: 0,0:42:01.58,0:42:02.67,nen den,,0,0,0,,Đây là một chương
Dialogue: 0,0:42:02.69,0:42:03.66,nen den,,0,0,0,,trong truyền thuyết về Vua Gesar.
Dialogue: 0,0:42:05.35,0:42:06.73,nen den,,0,0,0,,Nhìn chất liệu này
Dialogue: 0,0:42:07.18,0:42:08.52,nen den,,0,0,0,,thì có vẻ là phiên bản rất cổ xưa.
Dialogue: 0,0:42:09.26,0:42:09.87,nen den,,0,0,0,,Tiểu sư phụ,
Dialogue: 0,0:42:10.49,0:42:12.18,nen den,,0,0,0,,truyền thuyết Vua Gesar này là từ bao giờ vậy?
Dialogue: 0,0:42:12.48,0:42:14.40,nen den,,0,0,0,,Đây là sách sưu tầm cổ xưa nhất,
Dialogue: 0,0:42:14.43,0:42:15.63,nen den,,0,0,0,,chùa chúng tôi chỉ có ba cuốn.
Dialogue: 0,0:42:15.98,0:42:16.86,nen den,,0,0,0,,Thế hai cuốn còn lại ở đâu?
Dialogue: 0,0:42:26.90,0:42:27.34,nen den,,0,0,0,,Tây Tây.
Dialogue: 0,0:42:28.03,0:42:28.67,nen den,,0,0,0,,Cái cuối cùng.
Dialogue: 0,0:42:32.47,0:42:32.86,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,0:42:32.90,0:42:33.62,nen den,,0,0,0,,Chú xem trang này đi.
Dialogue: 0,0:42:34.81,0:42:36.25,nen den,,0,0,0,,Sao trang này không in hết nhỉ?
Dialogue: 0,0:42:36.58,0:42:37.89,nen den,,0,0,0,,Các trang khác đều in kín,
Dialogue: 0,0:42:37.91,0:42:38.64,nen den,,0,0,0,,có mỗi trang này như thế.
Dialogue: 0,0:42:38.86,0:42:39.79,nen den,,0,0,0,,Có lẽ là thiếu mất hai câu.
Dialogue: 0,0:42:41.52,0:42:42.20,nen den,,0,0,0,,Tiểu sư phụ xem đi.
Dialogue: 0,0:42:45.12,0:42:46.49,nen den,,0,0,0,,Lạ thật, lạ thật.
Dialogue: 0,0:42:46.67,0:42:47.56,nen den,,0,0,0,,Lạ thế nào cơ?
Dialogue: 0,0:42:47.69,0:42:48.85,nen den,,0,0,0,,Ở cao nguyên thời Cổ Tạng,
Dialogue: 0,0:42:49.11,0:42:50.17,nen den,,0,0,0,,vì giấy rất đắt,
Dialogue: 0,0:42:50.24,0:42:51.71,nen den,,0,0,0,,nên thường đều in hết giấy,
Dialogue: 0,0:42:52.08,0:42:53.11,nen den,,0,0,0,,không để lại khoảng trống nào.
Dialogue: 0,0:42:54.16,0:42:56.55,nen den,,0,0,0,,Trừ khi câu vốn định in vào đây
Dialogue: 0,0:42:57.04,0:42:58.57,nen den,,0,0,0,,đã không được in vào vì một nguyên nhân nào đó.
Dialogue: 0,0:43:02.95,0:43:05.67,nen den,,0,0,0,,Vô ngôn thích di ngôn.
Dialogue: 0,0:43:07.14,0:43:07.77,nen den,,0,0,0,,Tiểu sư phụ.
Dialogue: 0,0:43:08.11,0:43:08.91,nen den,,0,0,0,,Chỗ này có
Dialogue: 0,0:43:09.00,0:43:10.16,nen den,,0,0,0,,kinh bản của kinh thư không?
Dialogue: 0,0:43:11.76,0:43:13.45,nen den,,0,0,0,,Lắc đầu là sao?
Dialogue: 0,0:43:13.70,0:43:14.32,nen den,,0,0,0,,Không có à?
Dialogue: 0,0:43:14.50,0:43:15.13,nen den,,0,0,0,,Không phải không có
Dialogue: 0,0:43:15.16,0:43:16.81,nen den,,0,0,0,,mà là chỗ đó không được vào.
Dialogue: 0,0:43:31.66,0:43:33.26,nen den,,0,0,0,,Người ngoài tuyệt đối không được vào đây đâu.
Dialogue: 0,0:43:34.50,0:43:35.14,nen den,,0,0,0,,Tôi hiểu.
Dialogue: 0,0:43:35.46,0:43:36.27,nen den,,0,0,0,,Chúng tôi tìm kinh bản
Dialogue: 0,0:43:36.30,0:43:36.90,nen den,,0,0,0,,để xem chút thôi,
Dialogue: 0,0:43:37.12,0:43:38.00,nen den,,0,0,0,,tuyệt đối không nói ra ngoài.
Dialogue: 0,0:43:40.42,0:43:41.60,nen den,,0,0,0,,Còn phải leo thang à?
Dialogue: 0,0:43:42.38,0:43:43.52,nen den,,0,0,0,,Tôi không lên nữa đâu.
Dialogue: 0,0:43:45.30,0:43:46.28,nen den,,0,0,0,,Mở bộ đàm lên xem.
Dialogue: 0,0:43:46.84,0:43:47.70,nen den,,0,0,0,,- Bên dưới không có tín hiệu.\N- Hiểu rồi.
Dialogue: 0,0:43:48.34,0:43:48.91,nen den,,0,0,0,,Hiểu.
Dialogue: 0,0:43:50.57,0:43:50.93,nen den,,0,0,0,,Nào.
Dialogue: 0,0:43:51.93,0:43:52.35,nen den,,0,0,0,,Đi.
Dialogue: 0,0:44:21.42,0:44:22.43,nen den,,0,0,0,,Hai người vào tìm đi.
Dialogue: 0,0:44:43.01,0:44:44.61,nen den,,0,0,0,,Tìm xe của con chuột kia đi.
Dialogue: 0,0:46:09.08,0:46:09.53,nen den,,0,0,0,,Anh Sâm.
Dialogue: 0,0:46:10.63,0:46:12.66,nen den,,0,0,0,,Đám Ngô Thiên có làm nổi không?
Dialogue: 0,0:46:57.41,0:46:58.05,nen den,,0,0,0,,Tìm được rồi.
Dialogue: 0,0:47:21.54,0:47:22.30,nen den,,0,0,0,,Tiểu sư phụ xem này.
Dialogue: 0,0:47:23.63,0:47:25.15,nen den,,0,0,0,,Trên này đúng là có dấu vết từng được khắc lên
Dialogue: 0,0:47:25.49,0:47:26.82,nen den,,0,0,0,,nhưng không biết tại sao lại bị gọt đi mất,
Dialogue: 0,0:47:26.86,0:47:28.01,nen den,,0,0,0,,hơn nữa còn là trước khi được sơn lên.
Dialogue: 0,0:47:28.59,0:47:29.79,nen den,,0,0,0,,Cái này thì tôi không rõ.
Dialogue: 0,0:47:32.24,0:47:32.59,nen den,,0,0,0,,Đi.
Dialogue: 0,0:47:33.98,0:47:35.37,nen den,,0,0,0,,Giờ chúng ta đi hỏi sư phụ xem.
Dialogue: 0,0:48:01.16,0:48:01.93,nen den,,0,0,0,,Các người là ai?
Dialogue: 0,0:48:02.22,0:48:03.21,nen den,,0,0,0,,Người ngoài không được vào đây.
Dialogue: 0,0:48:05.97,0:48:06.96,nen den,,0,0,0,,Các người là người của Tần Trung.
Dialogue: 0,0:48:15.89,0:48:16.58,nen den,,0,0,0,,Cô không sao chứ?
Dialogue: 0,0:48:16.62,0:48:17.26,nen den,,0,0,0,,Tôi không sao.
Dialogue: 0,0:48:19.14,0:48:19.63,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,0:48:20.04,0:48:20.93,nen den,,0,0,0,,Kinh bản bị cướp rồi.
Dialogue: 0,0:48:20.98,0:48:21.56,nen den,,0,0,0,,Sao cơ?
Dialogue: 0,0:49:02.76,0:49:03.57,nen den,,0,0,0,,Mã Cường.
Dialogue: 0,0:49:07.69,0:49:08.38,nen den,,0,0,0,,Đâm chết hắn.
Dialogue: 0,0:49:11.90,0:49:13.11,nen den,,0,0,0,,Anh Sâm, cần tiếp ứng.
Dialogue: 0,0:49:13.85,0:49:14.57,nen den,,0,0,0,,Đi giúp Minh Nguyệt.
Dialogue: 0,0:49:17.97,0:49:18.49,nen den,,0,0,0,,Đứng lại.
Dialogue: 0,0:49:21.75,0:49:22.51,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,0:49:22.85,0:49:23.70,nen den,,0,0,0,,Tây Tây.
Dialogue: 0,0:49:31.06,0:49:31.72,nen den,,0,0,0,,Đừng có chạy.
Dialogue: 0,0:49:33.93,0:49:34.44,nen den,,0,0,0,,Đừng có chạy.
Dialogue: 0,0:49:37.83,0:49:38.47,nen den,,0,0,0,,Bên kia, đuổi theo.
Dialogue: 0,0:49:49.13,0:49:49.77,nen den,,0,0,0,,Vô dụng.
Dialogue: 0,0:49:54.77,0:49:55.25,nen den,,0,0,0,,Lên xe.
Dialogue: 0,0:50:13.48,0:50:14.01,nen den,,0,0,0,,Tây Tây.
Dialogue: 0,0:50:14.66,0:50:15.38,nen den,,0,0,0,,Đằng trước có dốc.
Dialogue: 0,0:50:15.67,0:50:17.11,nen den,,0,0,0,,Sau khi qua dốc thì sang trái.
Dialogue: 0,0:50:20.13,0:50:21.68,nen den,,0,0,0,,Sao lại chạy sang phải thế?
Dialogue: 0,0:50:39.13,0:50:39.64,nen den,,0,0,0,,Lên xe.
Dialogue: 0,0:50:51.17,0:50:51.93,nen den,,0,0,0,,Nguy hiểm quá!
Dialogue: 0,0:50:55.08,0:50:55.59,nen den,,0,0,0,,Chú Đạt!
Dialogue: 0,0:50:56.07,0:50:56.57,nen den,,0,0,0,,Chú Đạt!
Dialogue: 0,0:50:56.90,0:50:57.34,nen den,,0,0,0,,Chú Đạt!
Dialogue: 0,0:50:58.21,0:50:58.56,nen den,,0,0,0,,Chú Đạt...
Dialogue: 0,0:51:00.13,0:51:00.95,nen den,,0,0,0,,Chú Đạt bị bắn trúng rồi,
Dialogue: 0,0:51:01.04,0:51:02.16,nen den,,0,0,0,,mau đưa chú ấy đến bệnh viện.
Dialogue: 0,0:51:02.45,0:51:03.21,nen den,,0,0,0,,Phải cắt đuôi bọn chúng đã.
Dialogue: 0,0:51:43.92,0:51:44.43,nen den,,0,0,0,,Anh Sâm.
Dialogue: 0,0:51:55.00,0:51:55.51,nen den,,0,0,0,,Lâm Tây.
Dialogue: 0,0:52:07.69,0:52:08.15,nen den,,0,0,0,,Không sao chứ?
Dialogue: 0,0:52:08.54,0:52:09.06,nen den,,0,0,0,,Không sao.
Dialogue: 0,0:52:18.80,0:52:20.50,nen den,,0,0,0,,Đi chết đi!
Dialogue: 0,0:53:11.07,0:53:11.65,nen den,,0,0,0,,Anh Sâm.
Dialogue: 0,0:53:52.93,0:53:53.58,nen den,,0,0,0,,Bố.
Dialogue: 0,0:53:54.03,0:53:56.66,nen den,,0,0,0,,Bố nhất định phải đi tìm kiếm Thanh Thiên sao?
Dialogue: 0,0:53:56.72,0:53:59.15,nen den,,0,0,0,,Bố tìm được kiếm Thanh Thiên rồi sẽ về,
Dialogue: 0,0:53:59.65,0:54:01.00,nen den,,0,0,0,,Tây Tây phải nghe lời chú Đạt,
Dialogue: 0,0:54:01.42,0:54:02.83,nen den,,0,0,0,,đừng làm chú Đạt nổi giận nhé.
Dialogue: 0,0:54:20.53,0:54:21.19,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,0:54:43.13,0:54:44.12,nen den,,0,0,0,,Chú Đạt sao rồi?
Dialogue: 0,0:54:46.63,0:54:47.65,nen den,,0,0,0,,Chú Đạt không sao chứ?
Dialogue: 0,0:54:49.82,0:54:50.86,nen den,,0,0,0,,Chúng ta đưa chú ấy đi bệnh viện đi.
Dialogue: 0,0:54:50.93,0:54:51.71,nen den,,0,0,0,,Đi ngay bây giờ.
Dialogue: 0,0:54:51.73,0:54:52.66,nen den,,0,0,0,,Xa quá.
Dialogue: 0,0:54:52.91,0:54:53.66,nen den,,0,0,0,,Không kịp đâu.
Dialogue: 0,0:54:54.95,0:54:56.02,nen den,,0,0,0,,Cô cứu được chú ấy đúng không?
Dialogue: 0,0:54:56.68,0:54:57.37,nen den,,0,0,0,,Đúng không?
Dialogue: 0,0:54:59.17,0:55:01.17,nen den,,0,0,0,,Nếu tối nay chú ấy không hạ sốt...
Dialogue: 0,0:55:06.85,0:55:07.92,nen den,,0,0,0,,Tôi đi thay nước đã.
Dialogue: 0,0:55:17.91,0:55:18.50,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,0:55:19.98,0:55:20.67,nen den,,0,0,0,,Xin lỗi.
Dialogue: 0,0:55:21.62,0:55:22.70,nen den,,0,0,0,,Là cháu đã hại chú.
Dialogue: 0,0:55:27.56,0:55:32.31,nen den,,0,0,0,,Nếu nước suối không tự cạn,
Dialogue: 0,0:55:34.74,0:55:39.67,nen den,,0,0,0,,thì bùn không thể làm nghẽn được nó.
Dialogue: 0,0:55:42.45,0:55:43.51,nen den,,0,0,0,,Chỉ còn một chút nữa thôi
Dialogue: 0,0:55:44.11,0:55:45.02,nen den,,0,0,0,,là thành công rồi.
Dialogue: 0,0:55:46.60,0:55:50.71,nen den,,0,0,0,,Vua Gesar là anh hùng chúng tôi sùng bái nhất.
Dialogue: 0,0:55:51.61,0:55:53.00,nen den,,0,0,0,,Khi còn trẻ,
Dialogue: 0,0:55:53.71,0:55:55.48,nen den,,0,0,0,,chúng tôi đều từng muốn đi tìm ông ấy.
Dialogue: 0,0:55:56.78,0:55:57.47,nen den,,0,0,0,,Nhưng
Dialogue: 0,0:55:57.70,0:55:59.97,nen den,,0,0,0,,trong nước trắng không thể chảy ra bơ,
Dialogue: 0,0:56:00.08,0:56:00.88,nen den,,0,0,0,,trong đá không thể ép ra dầu.
Dialogue: 0,0:56:02.86,0:56:05.69,nen den,,0,0,0,,Tôi đã tìm được chứng cứ rồi.
Dialogue: 0,0:56:08.27,0:56:11.51,nen den,,0,0,0,,Vì sùng bái Vua Gesar,
Dialogue: 0,0:56:11.82,0:56:13.35,nen den,,0,0,0,,rất nhiều người đã tạo ra
Dialogue: 0,0:56:13.69,0:56:14.76,nen den,,0,0,0,,dấu vết về sự tồn tại của ngài ấy.
Dialogue: 0,0:56:15.29,0:56:15.75,nen den,,0,0,0,,Có điều,
Dialogue: 0,0:56:16.57,0:56:18.64,nen den,,0,0,0,,thần thoại không nên bị tìm ra.
Dialogue: 0,0:56:19.85,0:56:21.52,nen den,,0,0,0,,Ngài ấy nên sống trong tim chúng ta.
Dialogue: 0,0:56:26.66,0:56:27.49,nen den,,0,0,0,,Nói vậy thì,
Dialogue: 0,0:56:30.03,0:56:31.35,nen den,,0,0,0,,chứng cứ tôi tìm ra
Dialogue: 0,0:56:35.00,0:56:35.96,nen den,,0,0,0,,đều là giả sao?
Dialogue: 0,0:56:40.13,0:56:40.87,nen den,,0,0,0,,Lão Lâm.
Dialogue: 0,0:56:43.51,0:56:44.17,nen den,,0,0,0,,Lão Lâm
Dialogue: 0,0:56:46.37,0:56:47.28,nen den,,0,0,0,,Tây Tây.
Dialogue: 0,0:56:48.69,0:56:50.78,nen den,,0,0,0,,Tây, Tây Tây.
Dialogue: 0,0:58:01.27,0:58:02.15,nen den,,0,0,0,,Túi của tôi bị cướp rồi.
Dialogue: 0,0:58:03.13,0:58:04.14,nen den,,0,0,0,,Đây là tất cả tiền của tôi.
Dialogue: 0,0:58:04.87,0:58:05.78,nen den,,0,0,0,,Bao giờ về tôi trả bù cho anh.
Dialogue: 0,0:58:06.55,0:58:07.22,nen den,,0,0,0,,Cả xe của anh nữa,
Dialogue: 0,0:58:07.81,0:58:08.41,nen den,,0,0,0,,tôi cũng sẽ đền cho anh.
Dialogue: 0,0:58:12.57,0:58:13.39,nen den,,0,0,0,,Tôi không cho nợ đâu.
Dialogue: 0,0:58:24.22,0:58:25.16,nen den,,0,0,0,,Nhiệm vụ còn chưa hoàn thành,
Dialogue: 0,0:58:25.95,0:58:26.74,nen den,,0,0,0,,tính tiền sau đi.
Dialogue: 0,0:58:28.03,0:58:28.59,nen den,,0,0,0,,Kết thúc rồi.
Dialogue: 0,0:58:31.10,0:58:32.27,nen den,,0,0,0,,Chú Đạt đã hạ sốt rồi.
Dialogue: 0,0:58:33.52,0:58:34.36,nen den,,0,0,0,,Tôi phải dẫn ông ấy về nhà.
Dialogue: 0,0:58:35.80,0:58:36.87,nen den,,0,0,0,,Cô không đi tìm kiếm Thanh Thiên nữa à?
Dialogue: 0,0:58:39.19,0:58:40.17,nen den,,0,0,0,,Ông ấy nói đúng.
Dialogue: 0,0:58:41.46,0:58:42.10,nen den,,0,0,0,,Có lẽ
Dialogue: 0,0:58:42.73,0:58:43.91,nen den,,0,0,0,,mọi thứ chỉ là một giấc mộng.
Dialogue: 0,0:58:45.60,0:58:47.28,nen den,,0,0,0,,Hơn nữa chúng ta cũng không biết nên đi đâu.
Dialogue: 0,0:58:50.57,0:58:52.01,nen den,,0,0,0,,Lúc đầu, tại sao cô muốn tìm kiếm Thanh Thiên?
Dialogue: 0,0:59:18.97,0:59:19.89,nen den,,0,0,0,,Tôi chỉ vì muốn chứng minh
Dialogue: 0,0:59:21.05,0:59:22.70,nen den,,0,0,0,,cố gắng mà bố tôi bỏ cả tính mạng
Dialogue: 0,0:59:24.10,0:59:25.31,nen den,,0,0,0,,không phải không đáng một xu.
Dialogue: 0,0:59:27.45,0:59:28.25,nen den,,0,0,0,,Vậy bản thân cô thì sao?
Dialogue: 0,0:59:38.77,0:59:40.04,nen den,,0,0,0,,Trước khi chứng minh giúp bố tôi,
Dialogue: 0,0:59:42.66,0:59:43.95,nen den,,0,0,0,,tôi chưa từng nghĩ cho bản thân.
Dialogue: 0,0:59:58.53,0:59:59.96,nen den,,0,0,0,,Cô có ghét việc cô đang làm không?
Dialogue: 0,1:00:40.74,1:00:43.01,nen den,,0,0,0,,Đây từng là người bạn thân nhất,
Dialogue: 0,1:00:44.10,1:00:44.99,nen den,,0,0,0,,người anh em tốt nhất của tôi.
Dialogue: 0,1:00:46.46,1:00:47.35,nen den,,0,0,0,,Mong ước của cậu ấy
Dialogue: 0,1:00:47.69,1:00:48.83,nen den,,0,0,0,,là sau khi xin thôi việc
Dialogue: 0,1:00:49.41,1:00:50.57,nen den,,0,0,0,,sẽ trở về quê nhà Tây Tạng
Dialogue: 0,1:00:51.61,1:00:52.55,nen den,,0,0,0,,giúp đỡ
Dialogue: 0,1:00:52.82,1:00:53.78,nen den,,0,0,0,,những đứa trẻ nghèo khó đó
Dialogue: 0,1:00:55.41,1:00:56.60,nen den,,0,0,0,,để chúng có sách học,
Dialogue: 0,1:00:57.09,1:00:58.49,nen den,,0,0,0,,để chúng có thể lớn lên một cách vui vẻ.
Dialogue: 0,1:01:02.70,1:01:03.29,nen den,,0,0,0,,Đáng tiếc là
Dialogue: 0,1:01:05.14,1:01:07.73,nen den,,0,0,0,,tiểu tử này đã ra đi trong thời gian còn làm việc.
Dialogue: 0,1:01:09.89,1:01:11.74,nen den,,0,0,0,,Sau đó sau khi xin thôi việc tôi đến đây.
Dialogue: 0,1:01:13.77,1:01:15.39,nen den,,0,0,0,,Vốn dĩ muốn giúp cậu ấy hoàn thành mong muốn của cậu ấy
Dialogue: 0,1:01:16.66,1:01:17.78,nen den,,0,0,0,,nhưng dần dần phát hiện
Dialogue: 0,1:01:18.98,1:01:19.68,nen den,,0,0,0,,hóa ra bản thân tôi
Dialogue: 0,1:01:21.29,1:01:22.73,nen den,,0,0,0,,cũng yêu những đứa trẻ nơi đây.
Dialogue: 0,1:01:27.11,1:01:27.63,nen den,,0,0,0,,Vì thế,
Dialogue: 0,1:01:28.73,1:01:30.23,nen den,,0,0,0,,đây vốn là di nguyện của đồng đội của anh
Dialogue: 0,1:01:31.41,1:01:32.67,nen den,,0,0,0,,nhưng rồi lại trở thành một phần
Dialogue: 0,1:01:32.69,1:01:33.89,nen den,,0,0,0,,không thể thiếu trong cuộc sống của anh.
Dialogue: 0,1:01:38.56,1:01:39.72,nen den,,0,0,0,,Thực ra cô cũng giống tôi.
Dialogue: 0,1:01:42.75,1:01:43.86,nen den,,0,0,0,,Tìm kiếm kiếm Thanh Thiên
Dialogue: 0,1:01:45.62,1:01:46.94,nen den,,0,0,0,,là chuyện cô tự muốn làm.
Dialogue: 0,1:01:47.63,1:01:48.23,nen den,,0,0,0,,Không phải sao?
Dialogue: 0,1:01:53.06,1:01:54.65,nen den,,0,0,0,,Chắc chắn là vậy rồi.
Dialogue: 0,1:01:57.42,1:01:58.46,nen den,,0,0,0,,Chú Đạt, sao chú lại ra ngoài rồi?
Dialogue: 0,1:01:58.60,1:01:59.73,nen den,,0,0,0,,Chú vẫn cứ không nghe lời cháu.
Dialogue: 0,1:01:59.75,1:02:00.66,nen den,,0,0,0,,Bây giờ chú không được ra ngoài.
Dialogue: 0,1:02:00.69,1:02:01.76,nen den,,0,0,0,,Bây giờ chú phải về tĩnh dưỡng.
Dialogue: 0,1:02:01.79,1:02:02.98,nen den,,0,0,0,,- Chú có biết không?\N- Vội cái gì chứ?
Dialogue: 0,1:02:03.00,1:02:04.03,nen den,,0,0,0,,Chú vẫn ổn lắm.
Dialogue: 0,1:02:04.06,1:02:06.05,nen den,,0,0,0,,Ông già ngủ lâu phết rồi đấy nhỉ?
Dialogue: 0,1:02:06.18,1:02:07.01,nen den,,0,0,0,,Cơn buồn ngủ đến rồi.
Dialogue: 0,1:02:07.03,1:02:08.51,nen den,,0,0,0,,Nghỉ một lát là được mà.
Dialogue: 0,1:02:08.66,1:02:10.66,nen den,,0,0,0,,Chú Đạt, thay băng thôi.
Dialogue: 0,1:02:10.85,1:02:12.82,nen den,,0,0,0,,- Đến đây.\N- Chú mau về đi.
Dialogue: 0,1:02:13.55,1:02:15.61,nen den,,0,0,0,,Diệu thủ hồi xuân,
Dialogue: 0,1:02:16.02,1:02:18.24,nen den,,0,0,0,,thần y Trác Mã.
Dialogue: 0,1:02:23.13,1:02:23.61,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,1:02:29.79,1:02:30.53,nen den,,0,0,0,,Làm gì thế?
Dialogue: 0,1:02:34.83,1:02:35.75,nen den,,0,0,0,,Nhẹ nhàng chút.
Dialogue: 0,1:02:36.73,1:02:37.77,nen den,,0,0,0,,Cái áo này từ đâu mà có?
Dialogue: 0,1:02:38.18,1:02:39.59,nen den,,0,0,0,,Đó là của một người Hán để lại.
Dialogue: 0,1:02:41.10,1:02:41.85,nen den,,0,0,0,,Ông ấy ở đâu?
Dialogue: 0,1:02:42.42,1:02:43.90,nen den,,0,0,0,,Ông ấy đã rời đi nhiều năm rồi.
Dialogue: 0,1:02:45.69,1:02:46.25,nen den,,0,0,0,,Chú Đạt,
Dialogue: 0,1:02:46.78,1:02:47.52,nen den,,0,0,0,,chú xem đi,
Dialogue: 0,1:02:47.79,1:02:48.81,nen den,,0,0,0,,đây là áo của bố cháu.
Dialogue: 0,1:02:53.33,1:02:54.44,nen den,,0,0,0,,Đúng thật là của Lão Lâm.
Dialogue: 0,1:02:55.73,1:02:56.68,nen den,,0,0,0,,Ông ấy đến lúc nào vậy?
Dialogue: 0,1:02:58.94,1:03:00.13,nen den,,0,0,0,,Tháng 9 năm 2009.
Dialogue: 0,1:03:01.23,1:03:03.12,nen den,,0,0,0,,Bố tôi mất tích vào tháng 10.
Dialogue: 0,1:03:04.73,1:03:05.98,nen den,,0,0,0,,Ông ấy đến đây trước khi mất.
Dialogue: 0,1:03:06.65,1:03:08.12,nen den,,0,0,0,,Trác Mã nói là lịch của Tây Tạng.
Dialogue: 0,1:03:09.10,1:03:10.96,nen den,,0,0,0,,Lịch chúng tôi sử dụng ở đây đều là lịch của Tây Tạng.
Dialogue: 0,1:03:11.61,1:03:12.57,nen den,,0,0,0,,Vậy ngày mà cô nói
Dialogue: 0,1:03:12.60,1:03:13.58,nen den,,0,0,0,,dương lịch là lúc nào?
Dialogue: 0,1:03:14.22,1:03:15.60,nen den,,0,0,0,,Dương lịch
Dialogue: 0,1:03:16.13,1:03:17.97,nen den,,0,0,0,,thì chắc là tháng 11 năm đó.
Dialogue: 0,1:03:18.76,1:03:19.88,nen den,,0,0,0,,Cũng tức là
Dialogue: 0,1:03:20.63,1:03:21.48,nen den,,0,0,0,,rơi xuống vách đá
Dialogue: 0,1:03:21.91,1:03:22.97,nen den,,0,0,0,,nhưng không sao hết.
Dialogue: 0,1:03:23.82,1:03:25.14,nen den,,0,0,0,,Lão Lâm vẫn còn sống.
Dialogue: 0,1:03:29.10,1:03:29.87,nen den,,0,0,0,,Cô có biết
Dialogue: 0,1:03:30.49,1:03:31.53,nen den,,0,0,0,,ông ấy ở đâu không?
Dialogue: 0,1:03:32.38,1:03:34.46,nen den,,0,0,0,,Ông ấy nói có một nơi ông ấy bắt buộc phải đi.
Dialogue: 0,1:03:36.80,1:03:37.77,nen den,,0,0,0,,Ông ấy đang tìm kiếm Thanh Thiên.
Dialogue: 0,1:03:38.68,1:03:39.77,nen den,,0,0,0,,Ông ấy đang tìm kiếm Thanh Thiên.
Dialogue: 0,1:03:51.07,1:03:51.56,nen den,,0,0,0,,Chủ tịch Tần.
Dialogue: 0,1:03:52.18,1:03:52.91,nen den,,0,0,0,,Kinh bản đã phục hồi xong.
Dialogue: 0,1:03:56.10,1:03:57.86,nen den,,0,0,0,,Anh Sâm tìm người dùng công nghệ mô phỏng 3D
Dialogue: 0,1:03:57.93,1:03:59.45,nen den,,0,0,0,,phục hồi lại nó
Dialogue: 0,1:03:59.47,1:04:00.31,nen den,,0,0,0,,theo kết cấu của khối gỗ trước khi nó bị gọt.
Dialogue: 0,1:04:01.14,1:04:02.23,nen den,,0,0,0,,Được, cô ra ngoài đi.
Dialogue: 0,1:04:02.78,1:04:03.13,nen den,,0,0,0,,Vâng.
Dialogue: 0,1:04:04.51,1:04:05.37,nen den,,0,0,0,,Kiếm của Vua
Dialogue: 0,1:04:06.06,1:04:07.81,nen den,,0,0,0,,xuyên qua tim Tân Xích,
Dialogue: 0,1:04:08.17,1:04:10.84,nen den,,0,0,0,,nằm trong biển lửa với thang trời.
Dialogue: 0,1:04:12.85,1:04:13.53,nen den,,0,0,0,,Biển lửa.
Dialogue: 0,1:04:15.62,1:04:16.35,nen den,,0,0,0,,Kiếm của Vua.
Dialogue: 0,1:04:20.35,1:04:21.19,nen den,,0,0,0,,Vua Gesar.
Dialogue: 0,1:04:23.06,1:04:24.54,nen den,,0,0,0,,Nơi đánh bại Vua Tân Xích.
Dialogue: 0,1:04:28.59,1:04:29.36,nen den,,0,0,0,,Biển lửa.
Dialogue: 0,1:04:33.26,1:04:34.45,nen den,,0,0,0,,Viêm Nhân Ung Thố.
Dialogue: 0,1:04:52.38,1:04:54.53,nen den,,0,0,0,,Bố tin rằng bố nhất định có thể tìm được kiếm Thanh Thiên.
Dialogue: 0,1:04:55.40,1:04:56.14,nen den,,0,0,0,,Bố có dự cảm
Dialogue: 0,1:04:56.69,1:04:57.77,nen den,,0,0,0,,ngày đó không xa nữa.
Dialogue: 0,1:05:05.89,1:05:06.43,nen den,,0,0,0,,Trác Mã.
Dialogue: 0,1:05:07.02,1:05:08.70,nen den,,0,0,0,,Sau khi bố tôi dưỡng thương xong đi đâu vậy?
Dialogue: 0,1:05:09.34,1:05:11.16,nen den,,0,0,0,,Cụ thể đi đâu ông ấy không nói.
Dialogue: 0,1:05:11.94,1:05:13.78,nen den,,0,0,0,,Nhưng trước khi ông ấy đi
Dialogue: 0,1:05:13.81,1:05:16.25,nen den,,0,0,0,,từng hỏi tôi có một nơi cần đi như thế nào.
Dialogue: 0,1:05:16.73,1:05:17.48,nen den,,0,0,0,,Nơi nào?
Dialogue: 0,1:05:18.82,1:05:20.02,nen den,,0,0,0,,Viêm Nhân Ung Thố.
Dialogue: 0,1:05:21.22,1:05:23.17,nen den,,0,0,0,,Đó là nơi Vua Gesar đánh bại Vua Tân Xích.
Dialogue: 0,1:05:24.03,1:05:25.98,nen den,,0,0,0,,Trác Mã, vậy cô có thể dẫn tôi qua đó không?
Dialogue: 0,1:05:26.82,1:05:28.29,nen den,,0,0,0,,Thời gian cha cô dưỡng thương
Dialogue: 0,1:05:28.33,1:05:29.77,nen den,,0,0,0,,đã dạy cho tôi rất nhiều kiến thức,
Dialogue: 0,1:05:30.14,1:05:31.47,nen den,,0,0,0,,cũng coi như là sư phụ của tôi.
Dialogue: 0,1:05:32.26,1:05:33.25,nen den,,0,0,0,,Tôi bằng lòng dẫn đường.
Dialogue: 0,1:05:34.31,1:05:35.84,nen den,,0,0,0,,Trác Mã, cảm ơn cô.
Dialogue: 0,1:05:36.79,1:05:38.56,nen den,,0,0,0,,Không ai có thể gây chuyện trong địa bàn của tôi.
Dialogue: 0,1:05:39.05,1:05:39.62,nen den,,0,0,0,,Nếu có
Dialogue: 0,1:05:39.87,1:05:41.02,nen den,,0,0,0,,tôi sẽ đuổi người đó ra ngoài.
Dialogue: 0,1:05:41.42,1:05:41.99,nen den,,0,0,0,,Mã Cường,
Dialogue: 0,1:05:42.68,1:05:43.76,nen den,,0,0,0,,chuyến này không thêm tiền chứ?
Dialogue: 0,1:05:44.18,1:05:45.54,nen den,,0,0,0,,Cứ đền vợ cho tôi đi.
Dialogue: 0,1:05:46.33,1:05:47.06,nen den,,0,0,0,,Có hai cô gái bên cạnh cậu
Dialogue: 0,1:05:47.06,1:05:47.85,nen den,,0,0,0,,[Độ cao 5617 mét trên mực nước biển]\NCó hai cô gái bên cạnh cậu
Dialogue: 0,1:05:47.85,1:05:47.87,nen den,,0,0,0,,[Độ cao 5617 mét trên mực nước biển]
Dialogue: 0,1:05:47.87,1:05:49.12,nen den,,0,0,0,,mà trong đầu cậu chỉ có xe.\N[Độ cao 5617 mét trên mực nước biển]
Dialogue: 0,1:05:49.12,1:05:49.50,nen den,,0,0,0,,[Độ cao 5617 mét trên mực nước biển]
Dialogue: 0,1:05:49.50,1:05:50.07,nen den,,0,0,0,,Thằng nhóc như cậu cũng là độc thân do thực lực mà thôi.\N[Độ cao 5617 mét trên mực nước biển]
Dialogue: 0,1:05:50.07,1:05:51.73,nen den,,0,0,0,,Thằng nhóc như cậu cũng là độc thân do thực lực mà thôi.
Dialogue: 0,1:06:28.91,1:06:30.22,nen den,,0,0,0,,Ba mươi đại tướng.
Dialogue: 0,1:06:31.26,1:06:32.53,nen den,,0,0,0,,Nơi đây lẽ nào thực sự là
Dialogue: 0,1:06:32.57,1:06:34.02,nen den,,0,0,0,,lăng mộ của Vua Gesar sao?
Dialogue: 0,1:06:36.24,1:06:37.17,nen den,,0,0,0,,Nơi đây không giống lăng mộ vua
Dialogue: 0,1:06:38.54,1:06:40.62,nen den,,0,0,0,,mà giống là một địa cung có dụng ý khác.
Dialogue: 0,1:06:43.22,1:06:43.61,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,1:06:43.64,1:06:44.26,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,1:06:46.83,1:06:47.51,nen den,,0,0,0,,Sợ chết khiếp.
Dialogue: 0,1:06:48.29,1:06:49.04,nen den,,0,0,0,,Sợ chết khiếp.
Dialogue: 0,1:06:57.56,1:06:58.36,nen den,,0,0,0,,Họ đã vào trong rồi.
Dialogue: 0,1:07:00.80,1:07:01.53,nen den,,0,0,0,,Toàn là cơ quan,
Dialogue: 0,1:07:01.81,1:07:02.44,nen den,,0,0,0,,cẩn thận một chút.
Dialogue: 0,1:07:24.55,1:07:25.17,nen den,,0,0,0,,Cẩn thận.
Dialogue: 0,1:07:25.50,1:07:26.37,nen den,,0,0,0,,Không sao, không sao.
Dialogue: 0,1:07:36.00,1:07:36.83,nen den,,0,0,0,,Phía trước có con mương,
Dialogue: 0,1:07:37.48,1:07:38.08,nen den,,0,0,0,,tôi qua đó trước.
Dialogue: 0,1:08:04.18,1:08:05.65,nen den,,0,0,0,,Trác Mã, chú Đạt.
Dialogue: 0,1:08:06.21,1:08:07.37,nen den,,0,0,0,,Hai người có thể đi dọc
Dialogue: 0,1:08:07.39,1:08:07.98,nen den,,0,0,0,,bên cạnh vách núi.
Dialogue: 0,1:08:08.51,1:08:09.81,nen den,,0,0,0,,Bọn cháu tiếp ứng hai người ở đây.
Dialogue: 0,1:08:37.79,1:08:38.81,nen den,,0,0,0,,Đuổi kịp họ rồi.
Dialogue: 0,1:09:09.84,1:09:10.64,nen den,,0,0,0,,Là đá.
Dialogue: 0,1:09:12.60,1:09:13.06,nen den,,0,0,0,,An toàn.
Dialogue: 0,1:09:13.77,1:09:14.14,nen den,,0,0,0,,Đi.
Dialogue: 0,1:10:08.89,1:10:09.28,nen den,,0,0,0,,Thiên Nhi.
Dialogue: 0,1:10:10.33,1:10:10.69,nen den,,0,0,0,,Minh Nguyệt.
Dialogue: 0,1:10:57.03,1:10:57.72,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,1:10:58.23,1:10:58.75,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,1:11:01.24,1:11:02.25,nen den,,0,0,0,,Giữ chặt lấy.
Dialogue: 0,1:11:12.61,1:11:13.20,nen den,,0,0,0,,Lâm Tây.
Dialogue: 0,1:11:19.92,1:11:21.20,nen den,,0,0,0,,Bỏ chú ra.
Dialogue: 0,1:11:21.60,1:11:23.96,nen den,,0,0,0,,Các cháu bỏ tay chú ra, Tây Tây.
Dialogue: 0,1:11:42.86,1:11:43.26,nen den,,0,0,0,,Lâm Tây.
Dialogue: 0,1:11:44.89,1:11:45.41,nen den,,0,0,0,,Lâm Tây.
Dialogue: 0,1:11:45.92,1:11:46.73,nen den,,0,0,0,,Không có vấn đề.
Dialogue: 0,1:12:03.09,1:12:04.31,nen den,,0,0,0,,Không có đường lui nữa.
Dialogue: 0,1:12:07.01,1:12:08.17,nen den,,0,0,0,,Chúng ta bắt buộc phải xông qua đó.
Dialogue: 0,1:12:15.55,1:12:16.60,nen den,,0,0,0,,Cứ như thế này không phải là cách.
Dialogue: 0,1:12:17.10,1:12:18.04,nen den,,0,0,0,,Tôi đi dụ hỏa lực.
Dialogue: 0,1:12:19.17,1:12:19.73,nen den,,0,0,0,,Đợi một lát.
Dialogue: 0,1:12:25.09,1:12:25.56,nen den,,0,0,0,,Né.
Dialogue: 0,1:12:28.97,1:12:29.42,nen den,,0,0,0,,Xông thôi.
Dialogue: 0,1:12:46.63,1:12:47.04,nen den,,0,0,0,,Đi.
Dialogue: 0,1:12:51.80,1:12:52.76,nen den,,0,0,0,,Khay đạn cuối cùng rồi.
Dialogue: 0,1:13:01.04,1:13:01.85,nen den,,0,0,0,,Lâm Tây giao cho tôi đi.
Dialogue: 0,1:13:31.98,1:13:32.70,nen den,,0,0,0,,Hai người đi giúp Lâm Tây.
Dialogue: 0,1:13:32.95,1:13:33.40,nen den,,0,0,0,,Được.
Dialogue: 0,1:14:11.76,1:14:12.23,nen den,,0,0,0,,Thuốc nổ.
Dialogue: 0,1:15:04.05,1:15:04.74,nen den,,0,0,0,,Cẩn thận.
Dialogue: 0,1:16:23.54,1:16:24.16,nen den,,0,0,0,,Chuẩn bị xong rồi.
Dialogue: 0,1:19:18.14,1:19:18.60,nen den,,0,0,0,,Trái.
Dialogue: 0,1:19:20.25,1:19:20.69,nen den,,0,0,0,,Phải.
Dialogue: 0,1:19:49.33,1:19:50.01,nen den,,0,0,0,,Đưa tay cho tôi.
Dialogue: 0,1:19:58.03,1:19:58.91,nen den,,0,0,0,,Hai cháu không sao chứ?
Dialogue: 0,1:19:59.37,1:20:00.71,nen den,,0,0,0,,Không sao, hai người có sao không?
Dialogue: 0,1:20:00.94,1:20:02.02,nen den,,0,0,0,,- Không sao.\N- Bọn chú không sao.
Dialogue: 0,1:20:31.61,1:20:32.06,nen den,,0,0,0,,Đây là
Dialogue: 0,1:20:33.13,1:20:34.48,nen den,,0,0,0,,cây nghìn mắt của Lỗ Tán,
Dialogue: 0,1:20:35.15,1:20:36.99,nen den,,0,0,0,,kẻ địch đầu tiên mà Vua Gesar đánh bại.
Dialogue: 0,1:20:38.24,1:20:40.01,nen den,,0,0,0,,Hồi nhỏ có nghe cha nhắc đến.
Dialogue: 0,1:20:41.26,1:20:42.20,nen den,,0,0,0,,Chắc là nơi đây rồi.
Dialogue: 0,1:20:44.46,1:20:45.26,nen den,,0,0,0,,Trong những quan tài này
Dialogue: 0,1:20:45.92,1:20:47.12,nen den,,0,0,0,,hóa ra toàn là binh khí.
Dialogue: 0,1:20:51.00,1:20:52.77,nen den,,0,0,0,,Vua Gesar buộc phải dùng cả đời chấm dứt chiến tranh bằng cách chiến đấu,
Dialogue: 0,1:20:53.95,1:20:55.47,nen den,,0,0,0,,thật ra trong lòng vô cùng khát vọng hòa bình.
Dialogue: 0,1:20:56.60,1:20:57.91,nen den,,0,0,0,,Trong truyền thuyết, ông ấy có
Dialogue: 0,1:20:58.22,1:20:59.01,nen den,,0,0,0,,một đàn thờ phong đao.
Dialogue: 0,1:21:01.46,1:21:02.44,nen den,,0,0,0,,Tôi nghĩ chắc là đây rồi.
Dialogue: 0,1:21:24.21,1:21:25.00,nen den,,0,0,0,,Những hòn đá này
Dialogue: 0,1:21:25.42,1:21:27.24,nen den,,0,0,0,,chắc chính là cơ quan mở bức tường đá.
Dialogue: 0,1:21:38.09,1:21:38.69,nen den,,0,0,0,,Đây là...
Dialogue: 0,1:21:40.50,1:21:41.46,nen den,,0,0,0,,Là Hòa Mục Tứ Thụy Đồ.
Dialogue: 0,1:21:42.35,1:21:43.00,nen den,,0,0,0,,Trong dân tộc Tây Tạng,
Dialogue: 0,1:21:44.43,1:21:46.07,nen den,,0,0,0,,đây chính là tượng trưng cho tinh thần đoàn kết hợp tác.
Dialogue: 0,1:21:47.29,1:21:47.85,nen den,,0,0,0,,Tây Tây.
Dialogue: 0,1:21:49.41,1:21:51.37,nen den,,0,0,0,,Nếu trình tự các cơ quan ấn sai
Dialogue: 0,1:21:51.72,1:21:52.90,nen den,,0,0,0,,chúng ta đều mất mạng,
Dialogue: 0,1:21:53.28,1:21:54.19,nen den,,0,0,0,,cứ nghĩ kỹ xem đi.
Dialogue: 0,1:21:58.39,1:21:59.22,nen den,,0,0,0,,Chúng tôi tin tưởng cô.
Dialogue: 0,1:23:08.39,1:23:10.49,nen den,,0,0,0,,Rất vui hôm nay có thể phỏng vấn được Tiến sĩ Lâm Tây.
Dialogue: 0,1:23:10.84,1:23:11.76,nen den,,0,0,0,,Có tin đồn
Dialogue: 0,1:23:11.79,1:23:12.78,nen den,,0,0,0,,cô đã tìm được kiếm Thanh Thiên
Dialogue: 0,1:23:12.97,1:23:14.31,nen den,,0,0,0,,chứng minh sự tồn tại của Vua Gesar.
Dialogue: 0,1:23:14.39,1:23:14.91,nen den,,0,0,0,,Có thật không vậy?
Dialogue: 0,1:23:17.45,1:23:18.54,nen den,,0,0,0,,Người khôn không tin lời đồn.
Dialogue: 0,1:23:19.53,1:23:20.52,nen den,,0,0,0,,Sao cô có thể tin được chứ?
Dialogue: 0,1:23:21.06,1:23:22.36,nen den,,0,0,0,,Nếu tìm được thật
Dialogue: 0,1:23:22.66,1:23:23.93,nen den,,0,0,0,,thì có thể chứng minh được sự cố gắng của cha cô
Dialogue: 0,1:23:23.96,1:23:24.59,nen den,,0,0,0,,không vô ích.
Dialogue: 0,1:23:42.46,1:23:44.43,nen den,,0,0,0,,Tề Thiên Đại Thánh là truyền thuyết,
Dialogue: 0,1:23:44.70,1:23:46.83,nen den,,0,0,0,,còn Vua Gesar thì thật sự tồn tại.
Dialogue: 0,1:23:49.73,1:23:51.87,nen den,,0,0,0,,Nếu có thể tìm được Vua Gesar,
Dialogue: 0,1:23:52.68,1:23:54.86,nen den,,0,0,0,,điều chúng ta nên làm là bảo vệ ngài ấy.
Dialogue: 0,1:23:55.62,1:23:56.78,nen den,,0,0,0,,Không nên dùng ngài ấy để chứng minh
Dialogue: 0,1:23:56.98,1:23:57.91,nen den,,0,0,0,,sự ích kỷ của bản thân.
Dialogue: 0,1:23:59.54,1:24:01.21,nen den,,0,0,0,,Vậy cô sẽ tiếp tục tìm kiếm sao?
Dialogue: 0,1:24:05.69,1:24:08.31,nen den,,0,0,0,,Thần thoại, vốn không nên được tìm thấy.
Dialogue: 0,1:24:09.35,1:24:10.96,nen den,,0,0,0,,Ngài ấy nên sống trong tim của mỗi người.
Dialogue: 0,1:24:12.09,1:24:13.08,nen den,,0,0,0,,Vâng, cảm ơn cô.
Dialogue: 0,1:24:13.28,1:24:13.78,nen den,,0,0,0,,Chú Đạt.
Dialogue: 0,1:24:14.55,1:24:15.40,nen den,,0,0,0,,Chú có từng nghĩ
Dialogue: 0,1:24:15.75,1:24:16.44,nen den,,0,0,0,,tại sao bố cháu
Dialogue: 0,1:24:16.46,1:24:17.58,nen den,,0,0,0,,nhất định phải tìm được kiếm Thanh Thiên không?
Dialogue: 0,1:24:18.02,1:24:19.17,nen den,,0,0,0,,Còn có thể vì cái gì chứ?
Dialogue: 0,1:24:20.54,1:24:22.05,nen den,,0,0,0,,Tẩu hỏa nhập ma thôi.
Dialogue: 0,1:24:23.06,1:24:23.53,nen den,,0,0,0,,Không đúng.
Dialogue: 0,1:24:24.19,1:24:25.71,nen den,,0,0,0,,Cháu phát hiện hình vẽ ẩn trong Thiên Châu
Dialogue: 0,1:24:25.73,1:24:26.58,nen den,,0,0,0,,mà bố cháu để lại
Dialogue: 0,1:24:26.82,1:24:28.26,nen den,,0,0,0,,rất giống với
Dialogue: 0,1:24:28.28,1:24:29.17,nen den,,0,0,0,,Giáng Ma Chử trên tay cầm của kiếm Thanh Thiên.
Dialogue: 0,1:24:29.66,1:24:30.10,nen den,,0,0,0,,Hơn nữa
Dialogue: 0,1:24:30.32,1:24:30.99,nen den,,0,0,0,,hình vẽ này
Dialogue: 0,1:24:31.02,1:24:32.50,nen den,,0,0,0,,giống hệt với
Dialogue: 0,1:24:32.52,1:24:33.36,nen den,,0,0,0,,tô-tem của một Vương quốc cổ xưa.
Dialogue: 0,1:24:34.28,1:24:34.97,nen den,,0,0,0,,Vương quốc,
Dialogue: 0,1:24:35.85,1:24:36.60,nen den,,0,0,0,,Vương quốc nào?
Dialogue: 0,1:24:39.74,1:24:40.35,nen den,,0,0,0,,Có lẽ
Dialogue: 0,1:24:41.44,1:24:43.52,nen den,,0,0,0,,đây chỉ là một bắt đầu của một manh mối quan trọng khác.
Dialogue: 0,1:24:59.31,1:25:00.75,nen den,,0,0,0,,Shambhala.
Dialogue: 0,1:25:01.89,1:25:02.39,nen den,,0,0,0,,Tây Tây,
Dialogue: 0,1:25:02.85,1:25:04.27,nen den,,0,0,0,,cháu lại có chủ ý quỷ quái gì thế?
Dialogue: 0,1:25:42.39,1:25:43.59,nen den,,0,0,0,,[Tập đoàn tội phạm của Tần Trung đã bị tiêu diệt thành công. Bản thân Tần Trung đã bị trừng trị theo pháp luật.]
Dialogue: 0,1:25:43.59,1:25:44.59,nen den,,0,0,0,,[Các thành viên khác trong băng đảng tội phạm của Tần Trung cũng bị trừng trị nghiêm khắc theo pháp luật.]
Dialogue: 0,1:25:44.93,1:25:46.87,nen den,,0,0,0,,[Tần Trung phạm tội cố ý giết người, tội trộm cắp di chỉ văn hóa cổ, tội khai quật cổ mộ và tội mua bán văn vật,]
Dialogue: 0,1:25:46.87,1:25:47.97,nen den,,0,0,0,,[buôn lậu văn vật, hình phạt tổng hợp cho nhiều tội, bị kết án tử hình và tước quyền chính trị suốt đời]
Dialogue: 0,1:25:47.97,1:25:48.67,nen den,,0,0,0,,[và phạt một triệu nhân dân tệ nhân dân tệ.]
Dialogue: 0,1:25:49.27,1:25:52.24,nen den,,0,0,0,,[Cảm ơn  toàn thể tất cả các diễn viên và nhân viên của "Tạng Địa Kỳ Binh"]
